  "title": "213 Niwe Nao Uzuri",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Niwe nao uzuri wa Mwokozi,",
        "Nazo huruma zake na usafi,",
        "Roho Mtakatifu anibadilishe,",
        "Aonekane Yesu ndani yangu."
      ],
      "number": 1
    }
  ]
}