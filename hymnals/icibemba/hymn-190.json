  "number": 190,
  "language": "bem",
  "title": "190 - ISHINA LISUMA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "PRECIOUS NAME"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1. Bule shina lyakwa Yesu,",
        "we mwana wa bulanda,",
        "Li ka kupelo buseko,",
        "nakonse kuntu kaya.",
        "CHORUS:",
        "Ishina -- lisuma --",
        "lya cetekelwa muno;",
        "Ishina -- lisuma --",
        "lishina lya katula.",
        "2. Bule shina lyakwe lyonse,",
        "Libe cishangu cobe;",
        "Lintu waba no kweshiwa,",
        "Lilumbule upepe.",
        "3. Yesu eshina lisuma!",
        "Liletupo buseko!",
        "Lintwa katupokelela,",
        "Nyimbo tukamwimbila.",
        "4. Tota peshina lya Yesu,",
        "Muwe pa nkasa shakwe;",
        "Tuka mupele citebo,",
        "Ngolwendo lwesu lwapwa."
      ],
      "number": 2
    }
  ]
}