{
  "number": 202,
  "language": "bem",
  "title": "202 - YESU MUNGABUSHE 'NE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "JESUS SAVIOUR PILOT ME"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1. Yesu mu-ngabushe 'ne,",
        "Kwishilya lya cimana",
        "Ndetina amabimbi,",
        "Amabwe yaluluma",
        "Mwalishi-ba inshila",
        "Yesu mu-ngabushe 'ne.",
        "2. Nge fyo nyi-na no mwana,",
        "Alamu-talalika",
        "Amabi-mbi yaumfwa,",
        "Nga mwaya-talalika",
        "Mwe Mfumu ya cimana",
        "Yesu mu-ngabushe 'ne",
        "3. Ilyo nka-ba pa-calo,",
        "Mabimbi yakantinya;",
        "Nkasanga ukutusha,",
        "Nkafuka-tilwa ne mwe;",
        "Ngumfwe mu lenjeba 'ti",
        "Witina ndekwabusha."
      ],
      "number": 2
    }
  ]
}