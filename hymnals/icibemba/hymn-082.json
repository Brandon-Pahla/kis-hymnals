{
  "number": 82,
  "language": "bem",
  "title": "82 - INFUMU MULUCELO PE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "LORD IN THE MORNING"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1. Imfumu mu lucelo pe,",
        "Mukang'ufwe shiwi;",
        "Mu kupepa kuli Imwe,",
        "Lesa wa ku mulu.",
        "2. Ndefwayo Mupashi wenu,",
        "Wakuntungulula;",
        "Unange mulimo wandi,",
        "Ni lufyanishiwa.",
        "3. Abantu abamutemwa,",
        "Babe na mapalo;",
        "Lesa akabafwa bonse,",
        "Inshiku shonse pe."
      ],
      "number": 2
    }
  ]
}