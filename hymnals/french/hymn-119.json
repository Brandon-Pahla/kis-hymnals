{
  "number": 119,
  "language": "fr",
  "title": "Saint-Esprit, écoute",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Saint-Esprit, écoute,",
        "Inspire nos chants.",
        "Écarte le doute",
        "Loin de tes enfants."
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Saint-Esprit, inspire",
        "Ton peuple à genoux.",
        "Sous ton seul empire,",
        "Ce moment est doux."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Saint-Esprit, vient vivre",
        "En nous qui prenons",
        "Le pain de ton livre.",
        "Nous te bénissons."
      ],
      "number": 3
    }
  ]
}