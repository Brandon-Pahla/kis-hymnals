{
  "number": 170,
  "language": "fr",
  "title": "Louez le Seigneur",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Louez le Seigneur",
        "Du haut des cieux.",
        "Chantez sa grandeur.",
        "Vous, anges de Dieu ;",
        "Brillantes étoiles,",
        "Soleil, firmament,",
        "Louez, dans sa gloire,",
        "Le Dieu tout-puissant."
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Louez le Seigneur,",
        "Terrestres voix.",
        "De tout votre cœur.",
        "Chantez ses exploits ;",
        "Sa droite éternelle",
        "Soutient l'univers,",
        "D'une âme fidèle",
        "Louez le Seigneur."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Louez le Seigneur",
        "Pour tous ses biens.",
        "Rendez-lui honneur",
        "Aux siècles sans fin ;",
        "Sa grâce est immense,",
        "Multiples ses dons ;",
        "Louez sa clémence,",
        "Sa paix, son pardon."
      ],
      "number": 3
    }
  ]
}