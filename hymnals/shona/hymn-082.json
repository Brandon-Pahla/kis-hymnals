{
  "number": 82,
  "language": "sn",
  "title": "82 Mangwanani",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Tenzi muchanzwa ndoimba,",
        "Zve pamangwanani,",
        "Ndinonamata kunemwi,",
        "Ndinochema imwi"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ndinoda Mweya Mutsvene,",
        "Unditungamire,",
        "Itanyi nzira ichene,",
        "Kuneni munzira"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Avo vanhu varikuda,",
        "Vachapiwa tuso,",
        "Mwari anova chengeta, ,",
        "Nemutsa nenhowo"
      ],
      "number": 3
    }
  ]
}