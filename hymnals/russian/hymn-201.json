{
  "number": 201,
  "language": "ru",
  "title": "О, услышь, Спаситель, нас",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### О, услышь, Спаситель, нас"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "О, услышь, Спаситель, нас,",
        "Тех, кого Ты смертью спас,",
        "Дух Святой на нас излей,",
        "Как живительный елей."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Светит нам в ночи скорбей",
        "Солнце милости Твоей.",
        "Всех, уставших на пути,",
        "Поддержи и укрепи."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "О, пошли, Спаситель, Сам",
        "Мир Твой страждущим сердцам.",
        "Пусть сквозь сумрак зла и бед",
        "Нам сияет правды свет."
      ],
      "number": 4
    }
  ]
}