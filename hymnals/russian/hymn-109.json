{
  "number": 109,
  "language": "ru",
  "title": "Творец мой и Господь",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Творец мой и Господь"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Творец мой и Господь!",
        "Вся сила от Тебя.",
        "Ты льёшь обильно благодать",
        "Святую на меня."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Я сотворён Тобой,",
        "Ты жизнь мне подарил,",
        "Открылся как Спаситель мой,",
        "В любви благословил."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Пусть Дух Твой Всеблагой",
        "Меня животворит,",
        "На основании святом",
        "Навеки утвердит."
      ],
      "number": 4
    }
  ]
}