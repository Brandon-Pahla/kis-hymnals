{
  "number": 155,
  "language": "ru",
  "title": "На Бога уповай",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### На Бога уповай"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "На Бога уповай:",
        "Он - Избавитель твой,",
        "Рассеет страх, как ночи тень,",
        "И даст душе покой."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Вручи Ему свой путь,",
        "Поведай боль души;",
        "И Он услышит голос твой,",
        "На помощь поспешит."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Живи, как верный сын",
        "Небесного Отца,",
        "И следуй по Его стопам",
        "С надеждой до конца!"
      ],
      "number": 4
    }
  ]
}