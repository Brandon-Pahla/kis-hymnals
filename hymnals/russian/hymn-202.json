{
  "number": 202,
  "language": "ru",
  "title": "О, внемли, Господи",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### О, внемли, Господи"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "О, внемли, Господи!",
        "Тебя мы славим вновь",
        "За милость и любовь.",
        "О, внемли, Господи."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Помилуй, Господи!",
        "С высот Твоих призри",
        "И руку к нам простри.",
        "Помилуй, Господи."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Веди нас, Господи,",
        "Ты Духом истины",
        "К небесной пристани.",
        "Веди нас, Господи!",
        "Веди нас, Господи!"
      ],
      "number": 4
    }
  ]
}