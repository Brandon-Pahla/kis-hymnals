{
  "number": 31,
  "language": "ru",
  "title": "Звёзды в небе, прославляйте",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Звёзды в небе, прославляйте"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Звёзды в небе, прославляйте,",
        "Воспевает пусть Земля",
        "Вифлеемское рожденье",
        "Бога - Вечного Царя.",
        "В небесах, на земле",
        "Его имя славьте все."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Сам Господь, Творец Вселенной,",
        "К нам благоволил прийти,",
        "И Божественная слава",
        "Воссияла во плоти.",
        "Бог любовь нам явил -",
        "Славьте имя Бога сил!"
      ],
      "number": 3
    }
  ]
}