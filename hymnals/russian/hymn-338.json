{
  "number": 338,
  "language": "ru",
  "title": "Ночь простёрла крылья",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Ночь простёрла крылья"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ночь простёрла крылья",
        "Тихо над землёй",
        "И зажгла на небе",
        "Месяц золотой."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Отче, утомлённым",
        "Сон спокойный дай,",
        "Ангельскою стражей",
        "Всех нас охраняй."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "От забот житейских",
        "Нам пошли покой",
        "И простри над нами",
        "Твой покров святой."
      ],
      "number": 4
    }
  ]
}