{
  "number": 82,
  "language": "ru",
  "title": "Приди, Дух Божий, вновь",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Приди, Дух Божий, вновь"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Приди, Дух Божий, вновь,",
        "Открой источник благ,",
        "Чтоб я являл Твою любовь",
        "Во всех моих делах."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Приди, Дух Божий, вновь,",
        "Всё злое сокруши,",
        "Раскрой глубины Божьих слов",
        "И в сердце их впиши."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Приди, Дух Божий, вновь,",
        "Очисти храм души",
        "И полнотой Твоих даров",
        "Нас, слабых, надели."
      ],
      "number": 4
    }
  ]
}