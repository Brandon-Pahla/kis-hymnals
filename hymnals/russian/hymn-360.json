      "lines": [
        "### Снежок пушистый"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Снежок пушистый, беленький",
        "Под ножками скрипит,",
        "Одёжки белоснежные",
        "Берёзкам он дарит.",
        "Одёжки белоснежные",
        "Берёзкам он дарит."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "И я хочу быть чистеньким,",
        "Как этот белый снег,",
        "Чтоб радовал я Господа,",
        "Примером был для всех.",
        "Чтоб радовал я Господа,",
        "Примером был для всех."
      ],
      "number": 3
    }
  ]
