{
  "number": 115,
  "language": "ru",
  "title": "Солнца закат золотистый",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Солнца закат золотистый"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Солнца закат золотистый",
        "Медленно догорает,",
        "День благодатный субботний",
        "Близится, наступает."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Припев:",
        "Аллилуйя! Радостно воспевайте!",
        "Аллилуйя! Господа прославляйте."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "С нами Господь обещает",
        "Быть в этот тихий вечер,",
        "Сердце своё открываю",
        "Божьей любви навстречу."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Припев:",
        "Аллилуйя! Радостно воспевайте!",
        "Аллилуйя! Господа прославляйте!"
      ],
      "number": 5
    }
  ]
}