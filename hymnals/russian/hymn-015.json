{
  "number": 15,
  "language": "ru",
  "title": "Чудный, дивный Боже Вечный",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Чудный, дивный Боже Вечный"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Чудный, дивный Боже Вечный,",
        "Зрим Тебя мы, Бесконечный,",
        "Среди гор и средь морей.",
        "Всех цветов благоуханье,",
        "Звёзд таинственных мерцанье -",
        "Полн весь мир любви Твоей."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Вся природа поклониться",
        "Богу Вечному стремится,",
        "Наши помыслы, сердца",
        "Шлют хвалу в небес просторы,",
        "Гимну вторят птичьи хоры,",
        "Славя Бога и Творца."
      ],
      "number": 3
    }
  ]
}