{
  "number": 347,
  "language": "ru",
  "title": "Мой Христос-Спаситель",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Мой Христос-Спаситель"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Мой Христос-Спаситель",
        "Умер за меня",
        "И в Свою обитель",
        "Примет и меня."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Любит мой Спаситель",
        "Малое дитя,",
        "Любит Он и взрослых,",
        "Любит и меня."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ангелы святые",
        "Охраняют нас",
        "Во все дни земные",
        "И во всякий час."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Будем славить Бога,",
        "Будем петь Ему",
        "И к Его престолу",
        "Вознесём хвалу!"
      ],
      "number": 5
    }
  ]
}