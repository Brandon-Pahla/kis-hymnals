      "lines": [
        "### Всё Иисусу отдаю я"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Всё Иисусу отдаю я,",
        "Весь Ему принадлежу.",
        "В упованье и смиренье",
        "Пред лицом Его хожу."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Припев:",
        "Всё я отдаю,",
        "Всё я отдаю,",
        "Всё Тебе, мой Искупитель,",
        "Всё я отдаю."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Всё Иисусу отдаю я,",
        "Всё кладу к Его ногам.",
        "Суету отверг земную,",
        "Направляясь к небесам."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Припев:",
