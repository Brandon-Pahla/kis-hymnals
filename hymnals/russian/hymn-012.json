{
  "number": 12,
  "language": "ru",
  "title": "Свят, свят, свят Господь наш",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Свят, свят, свят Господь наш"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Свят, свят, свят Господь наш,",
        "Бог Всемогущий!",
        "Утренней хвалой наша песнь Ему звучит.",
        "Свят, свят, свят Господь наш,",
        "Щедрый и могучий,",
        "Славимый вечно Триединый Бог."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Свят, свят, свят - взывают",
        "Ангелов хоры,",
        "В радости венцы полагая пред Тобой.",
        "Пенье оглашает",
        "Чистые просторы -",
        "Ты был всегда, Ты есть и будешь ввек."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Свят, свят, свят Господь наш,",
        "Бог Всемогущий!",
        "Все Твои дела возвещают мощь Твою.",
        "Свят, свят, свят Господь наш,",
        "Щедрый и могучий,",
        "Славимый вечно Триединый Бог!"
      ],
      "number": 4
    }
  ]
}