{
  "number": 194,
  "language": "ru",
  "title": "Господь, Тебя люблю",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Господь, Тебя люблю"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Господь, Тебя люблю",
        "И песнь Тебе пою.",
        "О Боже мой, молю:",
        "Услышь меня!",
        "Ты по Своей любви",
        "Милость ко мне яви -",
        "Дух сердца обнови,",
        "Ведь грешен я."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Припев:",
        "Господь, приди ко мне -",
        "Ты мощь моя!",
        "О, помоги в борьбе -",
        "Я жду Тебя."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Услыши голос мой,",
        "Прошу Тебя с мольбой:",
        "Дай жертвовать собой",
        "В делах святых.",
        "Всё, чем я дорожу,",
        "Тебе я приношу;",
        "Как жертву всё сложу",
        "У ног Твоих."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Припев:",
        "Господь, приди ко мне -",
        "Ты мощь моя!",
        "О, помоги в борьбе -",
        "Я жду Тебя."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Господь, пребудь со мной",
        "Здесь, в страшной тьме земной,",
        "Чтоб мне идти тропой",
        "В град золотой.",
        "Пусть грозный враг страшит",
        "И зло кругом кипит -",
        "Ты, верный, крепкий щит,",
        "Всегда со мной!"
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "Припев:",
        "Господь, приди ко мне -",
        "Ты мощь моя!",
        "О, помоги в борьбе -",
        "Я жду Тебя!"
      ],
      "number": 7
    }
  ]
}