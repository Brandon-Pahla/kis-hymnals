  "title": "Да будет Тебе",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Да будет Тебе"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Да будет Тебе, наш Отец Всеблагой,",
        "Вовек прославленье предвечной хвалой!"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Припев:",
        "Аллилуйя, будь прославлен,",
        "Аллилуйя, аминь!",
        "Аллилуйя, честь и славу",
        "Тебе воздадим."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Спаситель, прими поклоненье от нас:",
        "Ты всесильной любовью от смерти нас спас."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Припев:",
        "Аллилуйя, будь прославлен,",
        "Аллилуйя, аминь!",
        "Аллилуйя, честь и славу",
        "Тебе воздадим."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Честь, славу Творцу вознесём за любовь,",
        "Мы прославим хваленьем во веки веков."
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "Припев:",
        "Аллилуйя, будь прославлен,",
        "Аллилуйя, аминь!",
        "Аллилуйя, честь и славу",
        "Тебе воздадим!"
      ],
      "number": 7
    }
  ]
