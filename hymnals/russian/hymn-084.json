{
  "number": 84,
  "language": "ru",
  "title": "О Дух Святой, с небес приди",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### О Дух Святой, с небес приди"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "О Дух Святой, с небес приди,",
        "Всех нас утешь, наставь,",
        "Сердце огнём любви зажги",
        "И к небесам направь."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Верою дай подняться нам",
        "Над суетой земной,",
        "Следовать по стопам Христа",
        "Узкой святой тропой."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "О Дух Святой, с небес приди,",
        "Нас просвещая вновь,",
        "Силы даруй нам крест нести,",
        "Славя Твою любовь!"
      ],
      "number": 4
    }
  ]
}