{
  "number": 2,
  "language": "ru",
  "title": "Всё, сотворённое Творцом",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Всё, сотворённое Творцом"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Всё, сотворённое Творцом,",
        "Пой с ликованьем, с торжеством -",
        "Аллилуйя! Аллилуйя!",
        "Солнца горячие лучи,",
        "Месяц, сияющий в ночи, -"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Припев:",
        "Славьте Бога! Славьте Бога!",
        "Аллилуйя! Аллилуйя! Аллилуйя."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Звон серебристый чистых вод",
        "Богу хвалебный гимн поёт.",
        "Славьте Бога! Аллилуйя!",
        "О пламенеющий огонь!",
        "Свет и тепло даруя, пой -"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Припев:",
        "Славьте Бога! Славьте Бога!",
        "Аллилуйя! Аллилуйя! Аллилуйя."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Славьте Отца во век веков,",
        "Сына за дивную любовь -",
        "Славьте Бога! Аллилуйя!",
        "Духа Святого благодать",
        "Радостно будем воспевать -"
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "Припев:",
        "Славьте Бога! Славьте Бога!",
        "Аллилуйя! Аллилуйя! Аллилуйя!"
      ],
      "number": 7
    }
  ]
}