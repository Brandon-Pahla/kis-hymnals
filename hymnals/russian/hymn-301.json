{
  "number": 301,
  "language": "ru",
  "title": "Господь, Ты, полн смиренья",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Господь, Ты, полн смиренья"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Господь, Ты, полн смиренья,",
        "В последний жизни час",
        "Оставил в поученье",
        "Обряд святой для нас."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Ученикам в тот вечер",
        "Ты Сам слугою был,",
        "Смиренно преклонившись,",
        "Ты ноги им омыл."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Дух кротости, смиренья",
        "Мне в сердце обнови,",
        "Чтоб мог служить я ближним",
        "В терпенье и любви."
      ],
      "number": 4
    }
  ]
}