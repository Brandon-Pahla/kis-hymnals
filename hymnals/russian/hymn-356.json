{
  "number": 356,
  "language": "ru",
  "title": "Милый край родной",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Милый край родной"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Милый край родной,",
        "Там, за синевой,",
        "Край мой любимый,",
        "Оком незримый -",
        "Как я стремлюсь к тебе."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Дивный край святой,",
        "Нет там тьмы ночной;",
        "Солнца светлее,",
        "Звёзд всех яснее",
        "Светит Спаситель там."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Край мой неземной,",
        "Город золотой -",
        "Даст мой Спаситель",
        "Мне там обитель",
        "В дивных твоих стенах."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Край мечты святой,",
        "Будь моей судьбой,",
        "Чтобы вседневно",
        "Я неизменно",
        "Мыслить о горнем мог."
      ],
      "number": 5
    }
  ]
}