{
  "number": 306,
  "language": "ru",
  "title": "Тебе поём",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Тебе поём"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Тебе поём, Тебе поём,",
        "Тебя благодарим",
        "За все Твои дары,",
        "За Вечерю Твою."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Спаситель наш, Спаситель наш,",
        "Дай нам не забывать,",
        "Всем людям возвещать",
        "Твой подвиг ради нас."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "И молим мы, и молим мы,",
        "Господь, дай быть с Тобой",
        "На Вечере святой",
        "Во Царствии Твоём."
      ],
      "number": 4
    }
  ]
}