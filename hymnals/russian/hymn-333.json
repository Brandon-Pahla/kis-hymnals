  "language": "ru",
  "title": "Золотые искры",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Золотые искры"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Золотые искры сыплет",
        "Солнышко в окно;",
        "Детки, встаньте, Бога славьте! -",
        "Вас зовёт оно."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Припев:",
        "Новый день - погожий, ясный -",
        "Нам Господь даёт.",
        "Детки, встаньте, Бога славьте -",
        "Солнышко зовёт."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Иисус не забывает",
        "Маленьких детей,",
        "Им охрану посылает",
        "Из любви Своей."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Припев:",
        "Новый день - погожий, ясный -",
        "Нам Господь даёт.",
        "Детки, встаньте, Бога славьте -",
        "Солнышко зовёт."
      ],
      "number": 5
    }
  ]
}