{
  "number": 321,
  "language": "ru",
  "title": "Прославим Бога и Творца",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Прославим Бога и Творца"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Прославим Бога и Творца,",
        "Который нас хранил,",
        "Своей могучею рукой",
        "Он нас путеводил."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Припев:",
        "Слава, слава Иисусу",
        "За минувший год!",
        "Аллилуйя! С Ним без страха",
        "Мы пойдём вперёд."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Не знаем мы, что новый год",
        "Нам в жизни принесёт,",
        "Но знаем, что Спаситель Сам",
        "Детей Своих ведёт."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Припев:",
        "Слава, слава Иисусу",
        "За минувший год!",
        "Аллилуйя! С Ним без страха",
        "Мы пойдём вперёд."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Господь, благослови же нас,",
        "Храни, не оставляй,",
        "К святым и чистым небесам",
        "Ты путь наш направляй."
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "Припев:",
        "Слава, слава Иисусу",
        "За минувший год!",
        "Аллилуйя! С Ним без страха",
        "Мы пойдём вперёд."
      ],
      "number": 7
    }
  ]
}