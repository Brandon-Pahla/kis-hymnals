  "title": "За Тобою, мой Спаситель",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### За Тобою, мой Спаситель"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "За Тобою, мой Спаситель,",
        "Всюду следовать готов;",
        "Пред Тобою преклоняюсь -",
        "Я познал Твою любовь."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Припев:",
        "По Твоим следам, Спаситель,",
        "Всюду следовать готов",
        "Неизменно, непрестанно,",
        "Как послушный раб Христов."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Хоть и труден путь тернистый,",
        "Устаёт дух слабый мой,",
        "Всё же следовать я буду",
        "Неуклонно за Тобой."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Припев:",
        "По Твоим следам, Спаситель,",
        "Всюду следовать готов",
        "Неизменно, непрестанно,",
        "Как послушный раб Христов."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Если встречу искушенье",
        "Иль опасность на пути,",
        "Ты даруешь мне терпенье",
        "По следам Твоим идти."
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "Припев:",
        "По Твоим следам, Спаситель,",
        "Всюду следовать готов",
        "Неизменно, непрестанно,",
        "Как послушный раб Христов."
      ],
      "number": 7
    },
    {
      "type": "verse",
      "lines": [
        "Ты прошёл путём страданья",
        "В славу, к светлым небесам.",
        "Я стезёю послушанья",
        "По Твоим иду следам."
      ],
