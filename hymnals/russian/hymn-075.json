{
  "number": 75,
  "language": "ru",
  "title": "Верный Спаситель, мой Бог",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Верный Спаситель, мой Бог"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Верный Спаситель, мой Бог,",
        "Ты - мой испытанный Друг,",
        "В руки Твои отдаю",
        "Сердце и жизни весну."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "В мире я долго блуждал,",
        "Правды Твоей не искал,",
        "Ты же меня возлюбил,",
        "Принял и грех мой простил."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Песню о дивной любви",
        "В сердце моё Ты вложи,",
        "Дай силы крест Твой нести,",
        "К родине вечной дойти."
      ],
      "number": 4
    }
  ]
}