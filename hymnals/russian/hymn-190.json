  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Жить в мире зла мне суждено"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Жить в мире зла мне суждено,",
        "И путь мой труден и тернист.",
        "В душе желание одно:",
        "Хочу быть чист, хочу быть чист."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Припев:",
        "Молю, Господь, меня омой",
        "Пречистой и живой водой,",
        "Святым огнём любви Своей",
        "Коснись, Господь, души моей."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Добра желанье есть во мне,",
        "Хочу по правде Божьей жить,",
        "Но средь греха здесь, на земле,",
        "Сам не могу я чистым быть."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Припев:",
        "Молю, Господь, меня омой",
        "Пречистой и живой водой,",
        "Святым огнём любви Своей",
        "Коснись, Господь, души моей."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Лишь сердцем чистые войдут",
        "В град новый Иерусалим,",
        "Пред Иисусом воспоют",
        "И вечно там пребудут с Ним."
      ],
      "number": 6
    },
