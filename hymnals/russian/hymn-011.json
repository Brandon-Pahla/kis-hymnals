{
  "number": 11,
  "language": "ru",
  "title": "Бога прославьте",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Бога прославьте"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Бога прославьте за Его заботу,",
        "Благость и милость к грешникам заблудшим.",
        "Ангелы, славьте, небеса, хвалите!",
        "Славьте Иегову."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Славьте Иисуса за Его страданья,",
        "Подвиг спасенья ради нас, погибших,",
        "Старцы и дети, юноши, девицы,",
        "Славьте Иисуса."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Духа Святого славьте, величайте,",
        "Послан Он Богом нам для утешенья.",
        "Бога, и Сына, и Святого Духа -",
        "Троицу славьте!"
      ],
      "number": 4
    }
  ]
}