{
  "number": 292,
  "language": "ru",
  "title": "Приидите, поклонимся",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Приидите, поклонимся"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Приидите, поклонимся",
        "Пред Спасителем Христом.",
        "Из борьбы великой вышел",
        "Он с победой, с торжеством."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Припев:",
        "Возложите, возложите, возложите",
        "На главу Его венец!",
        "На главу Его венец."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Возведите с ликованьем",
        "Победителя на трон.",
        "Миллионами спасённых",
        "Он навек превознесён."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Припев:",
        "Возложите, возложите, возложите",
        "На главу Его венец!",
        "На главу Его венец."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Восклицайте, восклицайте,",
        "Пойте в радости сердец:",
        "То, о чём в веках мечтали,",
        "Совершилось наконец!"
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "Припев:",
        "Возложите, возложите, возложите",
        "На главу Его венец!",
        "На главу Его венец!"
      ],
      "number": 7
    }
  ]
}