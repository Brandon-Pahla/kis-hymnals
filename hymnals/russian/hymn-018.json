{
  "number": 18,
  "language": "ru",
  "title": "Воспойте песнь заветную",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Воспойте песнь заветную"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Воспойте песнь заветную,",
        "Хвалите Бога сил,",
        "Он нас любовью вечною",
        "К спасенью возродил."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Возьмите арфы с гуслями -",
        "Воспеть Благую весть,",
        "Своей игрой искусною",
        "Царю воздайте честь."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Моря и цепи горные,",
        "Цветной ковёр долин,",
        "Луга, поля просторные -",
        "Всё славит Бога сил."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Народ Твой, Боже, радостно",
        "Тебя с надеждой ждёт.",
        "Твой суд великий, благостный",
        "Спасительно грядёт!"
      ],
      "number": 5
    }
  ]
}