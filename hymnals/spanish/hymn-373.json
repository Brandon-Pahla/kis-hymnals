{
  "number": 373,
  "language": "es",
  "title": "Mi Redentor es Cristo",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Mi Redentor es Cristo"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Mi Redentor es Cristo,",
        "mi gozo y mi canción;",
        "me salva, y me consuela",
        "en horas de aflicción."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Coro:",
        "¡Oh, Salvador bendito,",
        "mi canto elevo a ti;",
        "cual tú no tengo amigo",
        "que me ame tanto aquí!"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "En todos mis afanes",
        "alivio y luz me das;",
        "aunque ande en negras sombras",
        "no dudaré jamás."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Jesús, en ti confío,",
        "amigo y guía fiel;",
        "te quiero más que al mundo",
        "y cuanto se halla en él."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Serás de mi alma el gozo",
        "aquí donde hay maldad;",
        "también serás mi dicha",
        "allá en la eternidad."
      ],
      "number": 6
    }
  ]
}