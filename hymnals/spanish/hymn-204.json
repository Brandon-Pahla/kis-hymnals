{
  "number": 204,
  "language": "es",
  "title": "Oh, cantádmelas otra vez",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Oh, cantádmelas otra vez"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Oh cantádmelas otra vez,",
        "bellas palabras de vida;",
        "hallo en ellas mi gozo y luz,",
        "bellas palabras de vida.",
        "Sí, de luz y vida;",
        "son sostén y guía."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Coro:",
        "¡Qué bellas son! ¡Qué bellas son!",
        "Bellas palabras de vida.",
        "¡Qué bellas son! ¡Qué bellas son!",
        "Bellas palabras de vida."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Jesucristo a todos da",
        "bellas palabras de vida;",
        "hoy escúchalas pecador,",
        "bellas palabras de vida.",
        "Bondadoso te salva,",
        "y al cielo te llama."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Grato el cántico sonará:",
        "bellas palabras de vida;",
        "tus pecados perdonará,",
        "bellas palabras de vida.",
        "Sí, de luz y vida;",
        "son sostén y guía."
      ],
      "number": 5
    }
  ]
}