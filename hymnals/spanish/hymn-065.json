{
  "number": 65,
  "language": "es",
  "title": "El mundo es de mi Dios",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### El mundo es de mi Dios"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "El mundo es de mi Dios;",
        "su eterna posesión.",
        "Eleva a Dios su dulce voz",
        "la entera creación.",
        "El mundo es de mi Dios;",
        "conforta así pensar.",
        "El hizo el sol y el arrebol,",
        "la tierra, cielo y mar."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "El mundo es de mi Dios;",
        "escucho alegre son",
        "del ruiseñor, que al Creador",
        "eleva su canción.",
        "El mundo es de mi Dios;",
        "y en todo mi redor",
        "las flores mil con voz sutil",
        "declaran fiel su amor."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "El mundo es de mi Dios;",
        "jamás lo olvidaré.",
        "Aunque infernal parezca el mal,",
        "mi Padre Dios es Rey.",
        "El mundo es de mi Dios;",
        "y al Salvador Jesús",
        "hará vencer, por su poder,",
        "con la obra de la cruz."
      ],
      "number": 4
    }
  ]
}