{
  "number": 409,
  "language": "es",
  "title": "Si mi débil fe flaqueare",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Si mi débil fe flaqueare"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Si mi débil fe flaqueare,",
        "él me sostendrá.",
        "Si el mal me amenazare,",
        "él me sostendrá."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Coro:",
        "Él me sostendrá,",
        "él me sostendrá.",
        "Porque me ama el Salvador,",
        "él me sostendrá."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Nunca yo podré afirmarme",
        "con tan débil fe;",
        "mas él puede dirigirme,",
        "y me sostendrá."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Son su gozo y complacencia",
        "cuantos él salvó,",
        "y al salvarme su clemencia,",
        "él me sostendrá."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Él no quiso ver perdida",
        "mi alma en la maldad;",
        "dio su sangre por mi vida,",
        "y él me sostendrá."
      ],
      "number": 6
    }
  ]
}