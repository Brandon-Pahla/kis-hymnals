{
  "number": 393,
  "language": "es",
  "title": "Mi fe contempla a ti",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Mi fe contempla a ti"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Mi fe contempla a ti,",
        "Cordero celestial,",
        "mi Salvador.",
        "Oye mi petición,",
        "quita mi transgresión,",
        "sea tu posesión",
        "desde hoy, Señor."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Al débil corazón",
        "tu gracia pueda dar",
        "fe y valor.",
        "Moriste tú por mí,",
        "sea mi amor a ti",
        "puro y ferviente así,",
        "cual vivo ardor."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Por senda oscura voy,",
        "no veo en derredor;",
        "mas me guiarás.",
        "Enjuga mi llorar,",
        "mi noche haz brillar",
        "y así no pueda errar",
        "de ti jamás."
      ],
      "number": 4
    }
  ]
}