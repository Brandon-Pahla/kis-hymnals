{
  "number": 380,
  "language": "es",
  "title": "Ando con Cristo",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Ando con Cristo"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ando con Cristo, somos",
        "amigos,",
        "y mantenemos fiel comunión;",
        "ya de su lado nunca me aparto;",
        "¡cuánto me alienta su",
        "comprensión!"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Coro:",
        "Ando con Cristo, somos amigos,",
        "todas mis cuitas las llevo a él.",
        "Ando con Cristo, marcho a su",
        "lado,",
        "oigo la suave voz de Emanuel."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Los oropeles vanos del mundo",
        "abandonelos sin vacilar.",
        "Siendo su amor tan caro y",
        "profundo,",
        "llena de encanto nuestra amistad."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Hasta las pruebas que en mi",
        "camino",
        "quieren quitarme todo valor,",
        "solo son ayos que me conducen",
        "a la presencia del Salvador."
      ],
      "number": 5
    }
  ]
}