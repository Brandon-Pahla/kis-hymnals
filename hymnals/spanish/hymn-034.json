  "number": 34,
  "language": "es",
  "title": "En momentos así",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### En momentos así"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "En momentos así",
        "yo canto canción,",
        "canción de amor a mi Cristo.",
        "En momentos así",
        "yo busco al Señor,",
        "elevo mi alma a Dios.",
        "Hoy te adoro, Señor,",
        "y te alabo, mi Dios;",
        "te amo, mi Jesús,",
        "te amo, mi Señor."
      ],
      "number": 2
    }
  ]
}