{
  "number": 295,
  "language": "es",
  "title": "Las manos, Padre",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Las manos, Padre"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Las manos, Padre, extiendo a ti;",
        "mi fiel ayuda sé.",
        "Si tú no cuidas ya de mí,",
        "¿a quién y adónde iré?"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Coro:",
        "Yo creo que en el Gólgota",
        "Jesús por mí murió,",
        "y con su sangre, del pecar",
        "a mi alma libertó."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "En tu Hijo amado creo yo,",
        "conozco tu poder.",
        "¡Oh! suple mi necesidad,",
        "renueva tú mi ser."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Los ojos alzo a ti, Señor,",
        "Autor tú de la fe.",
        "¡Oh!, dame el tan precioso don,",
        "pues sin él yo moriré."
      ],
      "number": 5
    }
  ]
}