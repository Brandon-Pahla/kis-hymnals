{
  "number": 443,
  "language": "es",
  "title": "Hay un lugar do quiero estar, cerca de ti",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Hay un lugar do quiero estar, cerca de ti"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Hay un lugar do quiero estar,",
        "cerca de ti, Señor;",
        "allí podré yo descansar",
        "en tu divino amor.",
        "Oh, ven, Jesús bendito,",
        "lléname de tu amor;",
        "mantenme siempre firme",
        "cerca de ti, Señor."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Hay un lugar de dulce paz,",
        "cerca de ti, Señor.",
        "Vivir yo quiero donde estás,",
        "querido Salvador.",
        "Oh, ven, Jesús bendito,",
        "lléname de tu amor;",
        "mantenme siempre firme",
        "cerca de ti, Señor."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Solo hay segura salvación",
        "cerca de ti, Señor;",
        "hay gozo y luz y bendición",
        "cerca de ti, Señor.",
        "Oh, Salvador bendito,",
        "recibe a un pecador,",
        "y tómame en tus brazos",
        "de paternal amor."
      ],
      "number": 4
    }
  ]
}