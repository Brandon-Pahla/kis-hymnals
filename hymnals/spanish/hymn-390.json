  "title": "Soy yo, Señor",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Soy yo, SeñorSoy yo, soy yo, soy yo, Señor,",
        "el que necesita orar.",
        "Soy yo, soy yo, soy yo, Señor,",
        "el que necesita orar.",
        "No es mi madre, ni mi padre,",
        "sino yo, Señor,",
        "el que necesita orar.",
        "No es mi hermano, ni mi",
        "hermana,",
        "sino yo, Señor",
        "el que necesita orar."
      ],
      "number": 1
    }
  ]
