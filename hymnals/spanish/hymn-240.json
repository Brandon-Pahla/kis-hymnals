{
  "number": 240,
  "language": "es",
  "title": "Te ruego, oh Dios",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Te ruego, oh Dios"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Te ruego, oh Dios:",
        "¡Escúchame a mí!,",
        "¡ven, gran Libertador!,",
        "pues agobiado, anhelo ir a ti.",
        "¡ven, gran Libertador!"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Coro:",
        "Por áridas montañas yo vagué,",
        "muy lejos de mi buen Pastor.",
        "¡Oh, sálvame y llévame al redil,",
        "ven, gran Libertador!"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "No puedo abrigo en noche oscura",
        "hallar,",
        "¡ven, gran Libertador!,",
        "mas luz y vida tú me puedes dar,",
        "¡ven, gran Libertador!"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Atormentado, sin descanso y paz,",
        "¡ven, gran Libertador!,",
        "los ojos alzo a tu benigna faz,",
        "¡ven, gran Libertador!"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Mi voz contrita tú no desoirás,",
        "¡ven, gran Libertador!,",
        "mi humilde ruego pronto atenderás,",
        "¡ven, gran Libertador!"
      ],
      "number": 6
    }
  ]
}