{
  "number": 78,
  "language": "es",
  "title": "¡Al mundo paz!",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### ¡Al mundo paz!"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "¡Al mundo paz, nació Jesús,",
        "nació ya nuestro Rey!",
        "El corazón ya tiene luz,",
        "y paz su santa grey,",
        "y paz su santa grey,",
        "y paz, y paz, su santa grey."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "¡Al mundo paz; el Salvador,",
        "supremo reinará!",
        "Ya es feliz el pecador:",
        "Jesús perdón le da,",
        "Jesús perdón le da,",
        "Jesús, Jesús perdón le da."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Al mundo él gobernará",
        "con gracia y con poder;",
        "y a toda nación demostrará",
        "su amor y su poder,",
        "su amor y su poder,",
        "su amor, su amor y su poder."
      ],
      "number": 4
    }
  ]
}