{
  "number": 453,
  "language": "es",
  "title": "Cómo ser cual Cristo",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Cómo ser cual Cristo"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Dime cómo debo amar,",
        "dime cómo trabajar;",
        "cómo en mi trato ser",
        "para ser cual Cristo."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Coro:",
        "Cómo ser cual Cristo,",
        "cómo ser cual Cristo;",
        "dime siempre al andar",
        "cómo ser cual Cristo."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Dime qué preparación",
        "puedo emprender aquí,",
        "esperando al Señor,",
        "para ser cual Cristo."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Dime, Padre, cómo hablar;",
        "dime, Padre, cómo orar;",
        "dime siempre al andar",
        "cómo ser cual Cristo."
      ],
      "number": 5
    }
  ]
}