{
  "number": 133,
  "language": "es",
  "title": "Venid, con cánticos venid",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Venid, con cánticos venid"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Venid, con cánticos venid,",
        "del trono en redor;",
        "con ángeles loor rendid",
        "a Cristo, Salvador.",
        "Con ángeles loor rendid",
        "a Cristo, Salvador."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "De alabanzas digno es él,",
        "quien en la cruz bebió",
        "la copa de amarga hiel,",
        "que vida al hombre dio,",
        "la copa de amarga hiel,",
        "que vida al hombre dio."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Cantad, mortales, por doquier,",
        "cantadle con fervor.",
        "Él siempre es digno de poder,",
        "dominio y honor.",
        "Él siempre es digno de poder,",
        "dominio y honor."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Con gozo, pues, alzad la voz;",
        "a él alegres id,",
        "y con los ángeles de Dios,",
        "a Cristo bendecid.",
        "Con gozo, pues, alzad la voz;",
        "a él alegres id."
      ],
      "number": 5
    }
  ]
}