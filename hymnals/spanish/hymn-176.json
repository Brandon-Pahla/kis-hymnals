{
  "number": 176,
  "language": "es",
  "title": "Tú verás al Rey viniendo",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Tú verás al Rey viniendo,",
        "y oirás son de trompetas.",
        "Tú verás al Rey viniendo,",
        "falta poco ya."
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Coro:",
        "Hoy escucha el coro, oye las",
        "trompetas.",
        "Hoy escucha el coro que anuncia",
        "al Señor."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Resucitarán los justos,",
        "miles de ángeles los llevan",
        "a los pies de Jesucristo;",
        "falta poco ya."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Es Gabriel el que proclama",
        "y nosotros cantaremos:",
        "\"Los pesares terminaron\".",
        "Falta poco ya."
      ],
      "number": 4
    }
  ]
}