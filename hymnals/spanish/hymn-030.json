  "number": 30,
  "language": "es",
  "title": "Abre mis ojos",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Abre mis ojos"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Abre mis ojos,",
        "Jesús, quiero verte,",
        "conmigo tenerte,",
        "decir que te amo.",
        "Abre mi oido,",
        "hoy quiero escucharte.",
        "¡Abre mis ojos",
        "para contemplarte!"
      ],
      "number": 2
    }
  ]
}