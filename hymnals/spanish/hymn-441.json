{
  "number": 441,
  "language": "es",
  "title": "Jesús, te necesito",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Jesús, te necesito"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Jesús, te necesito,",
        "pues soy tan pobre y vil;",
        "recorro, peregrino,",
        "un mundo muy hostil.",
        "Tu amor será mi apoyo;",
        "me anima ver tu faz;",
        "en rnedio del peligro",
        "descanso en tu paz."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Jesús, te necesito,",
        "anhelo a ti servir;",
        "amargas aflicciones",
        "tú puedes compartir.",
        "Tu Espíritu me ayuda",
        "en cada tentación,",
        "me da en cada prueba",
        "consuelo y salvación."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Jesús, te necesito;",
        "muy pronto te veré.",
        "y en calles celestiales",
        "contigo andaré.",
        "Y con ios redimidos",
        "elevaré mi voz",
        "cantándote loores,",
        "mi Salvador, mi Dios."
      ],
      "number": 4
    }
  ]
}