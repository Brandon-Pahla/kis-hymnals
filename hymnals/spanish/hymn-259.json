{
  "number": 259,
  "language": "es",
  "title": "Mi espíritu, alma y cuerpo",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Mi espíritu, alma y cuerpo"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Mi espíritu, alma y cuerpo,",
        "mi ser, mi vida entera,",
        "cual viva, santa ofrenda",
        "te entrego a ti, mi Dios."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Coro:",
        "Mi todo a Dios consagro",
        "en Cristo, el vivo altar.",
        "¡Descienda el fuego santo,",
        "su sello celestial!"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Soy tuyo, Jesucristo,",
        "comprado con tu sangre;",
        "haz que contigo ande",
        "en plena comunión."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Espíritu divino,",
        "del Padre la promesa,",
        "sedienta, mi alma anhela",
        "de ti la santa unción."
      ],
      "number": 5
    }
  ]
}