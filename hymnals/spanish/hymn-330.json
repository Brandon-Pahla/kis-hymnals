{
  "number": 330,
  "language": "es",
  "title": "Hay un feliz Edén",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Hay un feliz Edén"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Hay un feliz Edén",
        "lejos de aquí,",
        "y gozará del bien",
        "el justo allí.",
        "Cantemos con fervor:",
        "\"Digno eres, oh Señor,",
        "de gloria y de honor;",
        "¡loor a ti!\""
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Marchad a aquel lugar,",
        "partid de aquí;",
        "un bello y dulce hogar",
        "tendréis allí.",
        "¡Oh, cuán feliz seré",
        "cuando a tu lado esté!",
        "Bendito viviré",
        "morando en ti."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Eterno resplandor",
        "fulgura allí.",
        "Eterno es el amor",
        "de Dios por mí.",
        "Corramos, pues, allá:",
        "bello aquel hogar será,",
        "por siempre habitará",
        "el santo allí."
      ],
      "number": 4
    }
  ]
}