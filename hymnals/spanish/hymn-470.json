  "language": "es",
  "title": "Guíame, Dios",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Guíame, DiosGuíame, Dios,",
        "guía en tu justicia;",
        "tu senda allana frente a mí.",
        "Pues solo tú, Dios,",
        "solo tú, Dios,",
        "me haces vivir confiado."
      ],
      "number": 1
    }
  ]
}