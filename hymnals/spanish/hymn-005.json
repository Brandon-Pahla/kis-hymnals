{
  "number": 5,
  "language": "es",
  "title": "A ti, glorioso Dios",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### A ti, glorioso Dios"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "A ti, glorioso Dios,",
        "cantamos alabanzas;",
        "rendímoste honor",
        "por todas tus grandezas.",
        "Nos das tu bendición",
        "en nuestra senda aquí;",
        "nos guiarás, Señor,",
        "a tu mansión allí."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Ven siempre, oh gran Dios,",
        "muy cerca de nosotros;",
        "con celo y con fervor",
        "queremos ir a otros.",
        "Tu brazo protector",
        "del mal nos guardará;",
        "en lucha y aflicción",
        "consuelo nos será."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Oh Padre, eterno Dios,",
        "cantámoste loores,",
        "y al Hijo Redentor,",
        "Señor de los señores,",
        "y al Santo Espíritu,",
        "el gran Consolador:",
        "al grande, trino Dios",
        "cantamos con fervor."
      ],
      "number": 4
    }
  ]
}