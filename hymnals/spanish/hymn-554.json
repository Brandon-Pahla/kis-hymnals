{
  "number": 554,
  "language": "es",
  "title": "Con Cristo avanza hoy",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Con Cristo avanza hoy"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Con Cristo avanza hoy,",
        "él te llamó a ti.",
        "Te ha guiado",
        "y te guardará en su amor.",
        "Él cumplirá",
        "fielmente lo que a",
        "su iglesia prometió."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Con Cristo avanza hoy,",
        "resuelto a alcanzar",
        "victorias en su nombre",
        "y por su poder.",
        "Te guardará,",
        "su Santo Espíritu",
        "te fortalecerá."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Con Cristo avanza hoy,",
        "las buenas nuevas da;",
        "que por su sangre",
        "salvación ofrece ya.",
        "Avanza hoy,",
        "imita al Salvador,",
        "con Cristo avanza hoy."
      ],
      "number": 4
    }
  ]
}