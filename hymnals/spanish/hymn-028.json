{
  "number": 28,
  "language": "es",
  "title": "Tu pueblo jubiloso",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Tu pueblo jubiloso"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Tu pueblo jubiloso",
        "se acerca a ti, Señor,",
        "y con triunfantes voces",
        "hoy canta tu loor;",
        "por todas tus bondades",
        "que das en plenitud,",
        "tu pueblo humildemente",
        "te expresa gratitud."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Aunque el hermano nunca",
        "te pueda aquí palpar,",
        "tú siempre con los tuyos",
        "has prometido estar;",
        "los cielos te revelan",
        "Rey nuestro y Creador,",
        "sentimos tu presencia",
        "en nuestro ser, Señor."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "¡Oh, Cristo!, te adoramos,",
        "te damos nuestro amor;",
        "¡oh!, llena nuestras vidas",
        "de fuerza, fe y valor;",
        "impártanos tu gracia",
        "la vida celestial;",
        "que siempre te rindamos",
        "adoración real."
      ],
      "number": 4
    }
  ]
}