{
  "number": 550,
  "language": "es",
  "title": "Día santo del Señor",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Día santo del Señor"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Día santo del Señor,",
        "¡oh, cuán pronto en pasar!",
        "Solo vino poco ha;",
        "ya lo vemos terminar.",
        "Y volando al cielo va,",
        "fiel testigo allí será;",
        "y volando al cielo va,",
        "fiel testigo allí será."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "¿Qué informe llevará",
        "al celeste tribunal?",
        "¿De maldades hablará?",
        "¿de cuidado mundanal?",
        "¿O de santa adoración,",
        "con Jesús en comunión?",
        "¿O de santa adoración,",
        "con Jesús en comunión?"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "¡Oh, perdónanos, Señor,",
        "el mal uso de tu don!",
        "Los preceptos de tu Ley",
        "graba en nuestro corazón.",
        "Es tu sábado, Señor,",
        "sello santo de tu amor;",
        "es tu sábado, Señor,",
        "sello santo de tu amor."
      ],
      "number": 4
    }
  ]
}