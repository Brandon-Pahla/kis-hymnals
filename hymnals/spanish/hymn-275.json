  "title": "Humilde oración",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Humilde oración"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "En humilde oración",
        "quiero dedicar, Señor,",
        "mi indigno corazón",
        "por completo a ti.",
        "Hazme puro, santo y fiel,",
        "quiero obedecer tu Ley.",
        "Dame amor, bondad y fe,",
        "dame tu poder."
      ],
      "number": 2
    }
  ]
}