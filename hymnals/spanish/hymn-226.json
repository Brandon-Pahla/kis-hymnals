{
  "number": 226,
  "language": "es",
  "title": "Buscad primero",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Buscad primero"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Buscad primero el reino",
        "de Dios",
        "y su perfecta justicia;",
        "y lo demás añadido será.",
        "Alelu, aleluya."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "No solo de pan el hombre",
        "vivirá,",
        "sino de toda palabra",
        "que sale de la boca de Dios.",
        "Alelu, aleluya."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Pedid, pedid, y se os dará;",
        "buscad, buscad, y hallaréis;",
        "llamad, llamad, y se os abrirá.",
        "Alelu, aleluya."
      ],
      "number": 4
    }
  ]
}