{
  "number": 17,
  "language": "es",
  "title": "Oh Padre, eterno Dios",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Oh Padre, eterno Dios"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Oh Padre, eterno Dios,",
        "alzamos nuestra voz",
        "en gratitud",
        "por lo que tú nos das",
        "con sin igual amor,",
        "y hallamos dulce paz",
        "en ti, Señor."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Bendito Salvador,",
        "te damos con amor",
        "el corazón;",
        "acepta, oh Señor,",
        "lo que en tu altar",
        "venimos a ofrendar",
        "cual vivo don."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Espíritu de Dios,",
        "escucha nuestra voz,",
        "y en tu bondad",
        "derrama en nuestro ser",
        "divina claridad,",
        "copiosa bendición",
        "y santidad."
      ],
      "number": 4
    }
  ]
}