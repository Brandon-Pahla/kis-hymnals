{
  "number": 166,
  "language": "ny",
  "title": "166 - Thanthwe Long’ambika",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "ROCK OF AGES"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1. Thanthwe long’ambikatu,",
        "Ndibisale momwemu!",
        "Madzi ndipo mwaziwo",
        "Zotuluka m’nthitimo,",
        "Zinditsuke m’mtima mbu!",
        "Zindilimbikitsetu."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2. Ntchito zanga zonsezo,",
        "Sizikondweretsa ’Nu;",
        "Ndikazigwiritsadi,",
        "Ndikalira nsonizi,",
        "Zonse sizikwanazi,",
        "Koma Yesu yekhayo."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3. M’manja mwanga chabe bi,",
        "Mtanda ndingogwiradi;",
        "Maliseche mveke ’ne,",
        "Ndili ndekha, msunge ’ne,",
        "Ndithawire mtsinjewo,",
        "Munditsuke m’mtimamo."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4. M’moyo uno kunsiku,",
        "Pena ndimwaliratu,",
        "Mmene ndidzaukanso",
        "Tionane masowo;",
        "Thanthwe long’ambikatu,",
        "Ndibisale momwemu."
      ],
      "number": 5
    }
  ]
}