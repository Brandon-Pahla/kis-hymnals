{
  "number": 186,
  "language": "ny",
  "title": "186 - Tabwera M’nyumbayi",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "LORD, WE HAVE MET"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1. Tabwera m’nyumbayi, Pa maso panupo;",
        "Idzani, Mbuye, mkhaletu Pamodzi nafenso."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2. Mitima yathuyo Isowa Inudi;",
        "Tilakalaka Yesuyo Akhale m’moyomu."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3. Ndi Mzimu wanuyo Atithangatebe",
        "Kumvera mau anuwo, Tikhale anudi."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4. Mutichotseretu Zoipa zathuzo;",
        "Mukadzafika tsikulo, Mtitenge kwanuko."
      ],
      "number": 5
    }
  ]
}