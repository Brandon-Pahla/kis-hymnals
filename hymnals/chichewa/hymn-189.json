{
  "number": 189,
  "language": "ny",
  "title": "189 - Tilibe Mzinda M’dzikoli",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "NO ABIDING CITY HERE"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1. Tilibe mzinda m’dzikoli",
        "Wolimba, wokhalitsanso;",
        "Komatu tikondwera ’fe,",
        "Tifuna mzinda udzawo,",
        "Tifuna mzinda udzawo."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2. Tilibe mzinda m’dzikoli,",
        "Wolimba, wokhalitsanso;",
        "Tifuna wina m’mwambamo,",
        "Wowala ndi wosathanso,",
        "Wowala ndi wosathanso."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3. Ee, malo a mtenderewo,",
        "Otopawo apumako!",
        "Ndikhala ndi mapikowo,",
        "Ndikadaulukirako,",
        "Ndikadaulukirako."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4. Tonthola moyo wanga ’we,",
        "Adziwa nthawi Mlunguyo;",
        "Ndichite ntchito yakeyo,",
        "Apatsa ’ne mpumulowo,",
        "Apatsa ’ne mpumulowo."
      ],
      "number": 5
    }
  ]
}