{
  "number": 317,
  "language": "ny",
  "title": "317 - Anakwatira Kale",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "THE VOICE THAT BREATHED O’ER EDEN"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1. Anakwatira kale Pa Eden pajapo;",
        "Ulipo lero lino, Mdalitso womwewo",
        "2. Awiri aimamu, Mwamma ndi mkaziyu,",
        "O! Mzimu wakuyera Mdalitso mpatsetu.",
        "3. Mulunzanitse awa, Atate leroli;",
        "Munakwatits’ Adamu ndi Hava kalelo.",
        "4. Mukhale nawo Yesu Mongatukaleko,",
        "Ukwati mnadalitsa Pa Kana pajapo.",
        "5. Mwasunge, mwayendetse Mu njira yanuyo,",
        "Akhale ndi kudala Pa banja pawopo."
      ],
      "number": 2
    }
  ]
}