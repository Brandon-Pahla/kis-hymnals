{
  "number": 173,
  "language": "ny",
  "title": "173 - Ndikadakhala Nacho Chikhulupiro",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "O FOR A FAITH!"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1. Ndikadakhala nachotu Chikhulupirocho,",
        "Sindinganjenjemeretu M’mavuto monsemo."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2. Choti ndikalangidwa ’ne, Ndisang’ung’udzatu,",
        "Popwetekedwa ineyo Ndikhulupira ’Ye."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3. Chomwe chiwala m’mtimanga, Pokhala m’mdimamo,",
        "Chimene sichiwopatu, Sichikaikanso."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4. Chosasunthika konsetu, Pokwiya dzikoli,",
        "Machimo sangakope ’ne Akandidzera ’ne."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "5. Mbuye, patseni mphamvuyo, Chikhulupirocho.",
        "Chinkana pansi panotu Ndidzakondwerabe."
      ],
      "number": 6
    }
  ]
}