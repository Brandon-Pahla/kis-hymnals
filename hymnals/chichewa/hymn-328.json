{
  "number": 328,
  "language": "ny",
  "title": "328 - NATIONAL ATHEM (Chichewa)",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "1. Mlungu dalitsani Malawi,",
        "Mumsunge m’mtendere,",
        "Khonjetsani adani onse,",
        "Njala nthenda nsanje,",
        "Lunzitsani mitima yathu,",
        "Kuti tisaope,",
        "Mdalitse mtsogoleri nafe,",
        "Ndi Mayi Malawi,",
        "2. Malawi dziko lokongola,",
        "Lachonde ndi ufulu,",
        "Nyanja ndi mphepo ya m’mapiri,",
        "Ndithudi tadala,",
        "Zigwa, mapiri, nthaka, Dzinthu",
        "Mphatso zaulere",
        "Nkhalango, madambo abwino",
        "Ngokoma Malawi.",
        "3. O’ Ufulu tigwirizane,",
        "Kukweza Malawi",
        "Ndi chikondi, khama kumvera,",
        "Timutumikire",
        "Pa nkhondo nkana pa mtendere",
        "Cholinga n’chimodzi",
        "Mayi, Bambo, tidzipereke",
        "Pokweza Malawi"
      ],
      "number": 1
    }
  ]
}