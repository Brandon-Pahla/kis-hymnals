{
  "number": 174,
  "language": "ny",
  "title": "174 - Akhristu Ali Nacho Chiyembekezo",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "HOW CHEERING!"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1. Akhristu ali nachotu Chiyembekezocho.",
        "Chimatikondweretsatu M’mavuto monsemo.",
        "Chimatikondweretsatu M’mavuto monsemo."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2. Chinena za mpumulowo Pokhala ndi Khristu.",
        "Pamodzi ndi anzangawo Tidzakhalitsako.",
        "Pamodzi ndi anzangawo Tidzakhalitsako."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3. Mayeso ndi machimowo Kulibe mdzikolo.",
        "Misozi ndipo imfanso Kulibe komweko.",
        "Misozi ndipo imfanso Kulibe komweko."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4. Masiku athe msangatu, Ambuye adzetu.",
        "Tilakalaka kuona Dzikolo ndi Inu.",
        "Tilakalaka kuona Dzikolo ndi Inu."
      ],
      "number": 5
    }
  ]
}