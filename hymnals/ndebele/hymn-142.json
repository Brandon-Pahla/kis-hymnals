{
  "number": 142,
  "language": "nd",
  "title": "142 Yeka Ubumnandi!",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Yek’ ubumnandi bosuku!",
        "Luhle kuzo zonke;",
        "Si lahl’ im’camango yezwe;",
        "Si camang’izulu."
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "O! mahl’ amazw’ okuthula,",
        "Kowamukelayo;",
        "A nosizi lwenguquko;",
        "Esisa ekhaya."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Nanxa si xosha ukona;",
        "Amandla engekho,",
        "Lo obona inhliziyo;",
        "Yena wosisiza."
      ],
      "number": 3
    }
  ]
}