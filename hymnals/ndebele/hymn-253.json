{
  "number": 253,
  "language": "nd",
  "title": "253 Nangu uMalusi Umile",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Nang’ uMalusi umile",
        "Ethathekisile",
        "Uyamemeza izimvu",
        "Az’ gone zonke-du!"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Uth’ azize kuYe manje",
        "Kafuni zesabe",
        "Lok’ ibusisa ezinje",
        "INkosi yez’ngcwele"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "S’bek’ izimvu kuz’ izandla",
        "Nkosi unamandla",
        "Loku nathi singabAkho",
        "Nans’ inzalo yAkho"
      ],
      "number": 3
    }
  ]
}