{
  "number": 159,
  "language": "nd",
  "title": "159  Ngi Kudinga Kangaka",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Nkos’ enhle ngi yakudinga",
        "Ngi mubi a ngi boni",
        "Ngibamb’ isandl’ u ngi hole",
        "Amandla kuW’ a khona"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ngezikhathi zonke,",
        "O, Nkosi ngi yakudinga",
        "Ngezikathi zonke,",
        "O, Msindisi ngi gcine"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "O, ngembese ngobunene",
        "Eyam’ ingubo yimbi",
        "Ngi fundise ngi zibone",
        "Ku nokufun’ amandla"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ngi phephile nxa ngeluswa",
        "Ngokuz’themba ngi yawa",
        "Ngi bambe ngi ngadukiswa",
        "Wena kuphila kwami"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Noma yin’ evela ngomso",
        "Ubuhle nenyembezi",
        "Umoya wami u kuWe",
        "Wena u yawuphasa"
      ],
      "number": 5
    }
  ]
}