{
  "number": 164,
  "language": "nd",
  "title": "164 Ezinyaweni Zika Jesu",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Ez’nyaweni zika Jesu",
        "Ngi zw’ amazwi amahle",
        "Indaw’ enhle eligugu",
        "Ngiy’ thand’ insuku zonke",
        "Ez’nyaweni ngi qaphele",
        "Konke osekudlule",
        "Ngob’ uthando lwAkh’ oluhle",
        "Lwazuz’ inhliziyo yam’"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ngi ngabusiswa ngaphi na?",
        "Kuphel’ ezinyaweni",
        "Kona ngi lahla izono",
        "Ngi thol’ ukuphumula",
        "Ez’nyaweni zika Jesu",
        "Ngi zuz’ intethelelo",
        "Ngamukele umsa wAkhe",
        "Ngi thabe nsuku zonke"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "U ngi busise, Msindisi",
        "Ngi lind’ ezinyaweni",
        "Ngi bheke mina ngothando",
        "Na ngomusa omkhulu",
        "Ngi phe ingqondo kaJesu",
        "Ngize ngi fane naYe",
        "Kuze kube bufakazi",
        "Bokuthi, Ngi noJesu"
      ],
      "number": 3
    }
  ]
}