  "title": "90: YAMANA IMWI IMVWIKI",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 430AH 384CH 456",
        "90YAMANA IMWI IMVWIKI",
        "ANOTHER SIX DAYS&#213; WORK",
        "Doh is Bb",
        "1",
        "Ya mana imwi imvwiki,",
        "Ya talika I nSabata",
        "Moyo wangu ko lyokezya.",
        "Mbuzuba busetekene.",
        "2",
        "Ko bola, ko lumba Leza",
        "Wa ka tupa ku lyokezya;",
        "Sunu mbuzuba bubotu",
        "Kuli mazuba a inda.",
        "3",
        "Sunu atuyeeye Leza",
        "A tu lumba izye Mwami",
        "Elyo tu la ku swilila",
        "Kulyo kezya kwini kwini",
        "4",
        "No tujisi kuumuna,",
        "Ngu mwende lezyi mupati,",
        "U latu sinizya kuti",
        "Muvuni uli mu moyo"
      ],
      "number": 1
    }
  ]
}