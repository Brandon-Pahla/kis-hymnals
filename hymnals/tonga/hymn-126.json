  "title": "126: MBUNDA KAFUTULWA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 236AH 194CH 638",
        "126MBUNDA KAFUTULWA",
        "SINCE I HAVE BEEN REDEEMED",
        "Doh is G",
        "1",
        "Ndila lwimbo ndwe yandisya, Nkaambo ndanununwa,",
        "Ilwa Mufutuli wangu, Nkaambo ndanununwa.",
        "CIINDULULO:",
        "Nkaambo ndanununwa, Nkaambo ndanununwa",
        "Ndila kulumba Mwami",
        "Nkaambo ndanununwa, Ndila kulumbaizya Mwami.",
        "2",
        "Ndijisi Klistu mpa zyoonse, Nkaambo ndanununwa,",
        "Kucita luyando lwakwe, Nkaambo ndanununwa.",
        "3",
        "Ndijisi kamboni mwini, Nkaambo ndanununwa,",
        "Ugusya kuyoowa kwangu, Nkaambo ndanununwa.",
        "4",
        "Ndijisi munzi mubotu, Nkaambo ndanununwa,",
        "Nkweya kuba kutamani, Nkaambo ndanununwa."
      ],
      "number": 1
    }
  ]
