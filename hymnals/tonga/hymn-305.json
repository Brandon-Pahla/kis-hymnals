      "lines": [
        "305COOLWE KU BALILA",
        "THE MOURNERS\\' BLESSING",
        "1",
        "Coolwe ku balila, Ndila kubavuna,",
        "Mbonda kapenga bunene, Tabukandigwasyi",
        "2",
        "Ndakali acuumbwe, Ndalila, balila,",
        "Mbondakakubona kufwa, Ndakakulanduka",
        "3",
        "Ndijisi cijuzyo, Ndakaciabilwa,",
        "Ndila kuuita muya, Ndacijula cuumbwe",
        "4",
        "Tula lila lino, Tula kuseka &#212;ko;",
        "Jesu naya kwiita bakwe, Bala kubusigwa."
      ],
      "number": 1
    }
  ]
}