  "title": "333: KULAYA KWA MUBELESYI",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "333KULAYA KWA MUBELESYITHE MISSIONARY FAREWELL",
        "1",
        "Kunyika &#201;. Ili kule, Nkobali &#201; bazyimini;",
        "Baita &#201; bala ita Andiye &#201; Nkuko lino.",
        "CIINDULULO:",
        "Andiye &#201; nditakali pe, Ngu Mwami.. Odita,",
        "Andiye &#201; a ndivuwe, Syaala nyika ya kwesu.",
        "2",
        "Ndamumvwa &#201; Mwami wamba, Mufwambe &#201; batebuli;",
        "Ku munda &#201; uli kule, Bakwesu &#201; Mundi leke.",
        "3",
        "Kwitala &#201; lyokuya, Ndabona &#201; basowegwa;",
        "Mulyongo &#201; ujulilwe, Bazyimi &#201; ni bandita.",
        "4",
        "Tata ba &#201; ma mwana &#212;ngu, Musyale &#201; nyonse nene;",
        "Kwitala &#201; &#212;lyo kuya, Ndi jisi &#201; Mulimo ko."
      ],
      "number": 1
    }
  ]
}