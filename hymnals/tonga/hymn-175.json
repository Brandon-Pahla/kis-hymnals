  "title": "175: MUFUTULI MBULI MWEMBEZI",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 729AH 294CH 394SDAH 545",
        "175MUFUTULI MBULI MWEMBEZI",
        "SAVIOUR LIKE A SHEPHERD",
        "Doh is Eb",
        "1",
        "Mbuli Mwembezi kotuenzya twakulomba Mufutuli;",
        "Kotusanina muya wako, Tukwabilile tonse;",
        "Mufutuli Jesu wesu, Wakatuula tonse,",
        "Mufutuli Jesu wesu, Wakatuula tonse.",
        "2",
        "Tuli bako kotuenzya, Ba musololi wesu;",
        "Tugusye mu zibi zyesu, Tuta soweki limbi,",
        "Mufutuli Jesu wesu, Kotumvwa twapaila,",
        "Mufutuli Jesu wesu, Kotumvwa twapaila.",
        "3",
        "Watusyomya kututambula, Muzibi naa twaboola;",
        "Uli aluse kutugwasya, Ula tuangulula,",
        "Mufutuli Jesu wesu, Twabola kuli ndiwe",
        "Mufutuli Jesu wesu, Twabola kuli ndiwe."
      ],
      "number": 1
    }
  ]
