  "title": "183: JATISYA LIMWI NDIBOOLE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 894AH 333CH 548SDAH 600",
        "183JATISYA LIMWI NDIBOOLE",
        "HOLD FAST TILL I COME",
        "Doh is G",
        "1",
        "Lusyomo lwapengwa ku bakondwa,",
        "Langa ndaboola kucinga ibangu;",
        "Linda ndaboola ntenda nimpati,",
        "Uta oni linda, ndila fwambana.",
        "CIINDULULO:",
        "Jatisya lyoonse cisyomyo cangu,",
        "Bamwi ku julu bwapegwa ndiwe,",
        "Jata ndaboola Njila mu munzi,",
        "Kala acuno misyini imweka.",
        "2",
        "Tula lindila malamb&#213; ayake;",
        "Ku bamwi ula boola mbuli mubbi;",
        "Ula afwaafwi ciindi tatuzyi,",
        "Andelema yazyibya maiza &#212;swena.",
        "3",
        "Lusyomo lwesu uli yakidwe,",
        "Bulemu bwa kulibonya kwa Mwami;",
        "Lulaindisya zisyomyo zyoonse,",
        "Langa ndil&#213;afwaafwi Jata ndaboola."
      ],
      "number": 1
    }
  ]
}