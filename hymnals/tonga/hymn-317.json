  "title": "317: JERUSALEMA NGOTULINDILA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "317JERUSALEMA NGOTULINDILAWE AWAIT JERUSALEM",
        "1",
        "Jelusalema uyo ngotulindila (2)",
        "Oyo ngo munzi mubotu umwekamweka",
        "2",
        "Aswebo atweende kuya, Tukaimbe (2)",
        "Tukaimbe abangele ku Jelusalema",
        "3",
        "Joshua a Kalebi bakasala (2)",
        "Bakasala baisilaili kuya ku Keenani",
        "4",
        "Basa atweende sunu tukaimbe (2)",
        "Tukaimbe abangele ku Jelusalema."
      ],
      "number": 1
    }
  ]
}