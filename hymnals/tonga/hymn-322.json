  "title": "322: NYIKA YOONSE YAKAZIMINA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "322NYIKA YOONSE YAKAZIMINA THE WHOLE WORLD WENT ASTRAY",
        "1",
        "Nyika yoonse yakazimina (2)",
        "Mu musinze wa cinyonyono.",
        "CIINDULULO:",
        "Koya mumuni umweka",
        "Bwaca mumoyo wako,",
        "Uta mucengi kuti komuzyi,",
        "A moyo wako ulikuzyi.",
        "2",
        "Sunu bwaca mbwakumwiimbila, (2)",
        "Tumwiimbile tulumbaizye.",
        "3",
        "Kunyina musinze kwa Jesu (2)",
        "Swebo tukala mumusinze."
      ],
      "number": 1
    }
  ]
}