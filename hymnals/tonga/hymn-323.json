  "title": "323: CIINDI CA KU PAILA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "323CIINDI CA KU PAILATHE HOUR OF PRAYER",
        "1",
        "Nkocili ciindi cibotu, Mbobwacela likabbile;",
        "Mbuli ciindi cindiita ku paila.",
        "2",
        "Cabota cifumofumo, Kwabota amangolezya;",
        "Ndaluba nyika nondaya kupaila.",
        "3",
        "Ali ceeco ndaba &#212;nguzu, Ndapaila kubisya;",
        "Elyo wandikomanisya aku julu.",
        "4",
        "Limwi Leza ndikasike, Kunyina ciindi cibotu;",
        "Mbuli ciindi cakuya kukupaila."
      ],
      "number": 1
    }
  ]
}