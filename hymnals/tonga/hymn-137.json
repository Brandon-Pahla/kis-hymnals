  "language": "to",
  "title": "137: NDIPA JESU",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 315AH 104CH 596SDAH 305",
        "137NDIPA JESU",
        "GIVE ME JESUS",
        "Doh is F",
        "1",
        "Tola nyika ndipa Jesu, Zya nyika Zili buyo,",
        "Luyando lwakwe luliimvwi, Kukabe kutamani.",
        "CIINDULULO:",
        "Langa bulamfu bwa luse, Aluyando lupati",
        "Langa Lufutuko lunji, A buumi butamani.",
        "2",
        "Tola nyika ndipe Jesu, Ulandikomanisya;",
        "Mufutuli na ndibamba, Njakwiimba mumapenzi.",
        "3",
        "Tola nyika ndipe Jesu, Mwami ukomanisya;",
        "Mulweendo lwangu lwanyika, Lonse ulasalazya.",
        "4",
        "Tola nyika ndipe Jesu, Ndasyoma muciingano;",
        "Limwi meso ajaluke, Nkabone Mwami wangu."
      ],
      "number": 1
    }
  ]
}