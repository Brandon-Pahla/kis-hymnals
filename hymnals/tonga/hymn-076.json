  "title": "76: KUFUTUSYA BAFWA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 479AH 348CH 623SDAH 367",
        "76KUFUTUSYA BAFWA",
        "RESCUE THE PERISHING",
        "Doh is Bb",
        "1",
        "Kofutula bantu bayanda kufwa",
        "Ubagusye mu zyumbwe zya bubi",
        "Lilila babisyi ubaimike,",
        "U baambile Jesu ngu futusya.",
        "CIINDULULO:",
        "Kofutula bantu bayanda kufwa",
        "Jesu ula luse ngu futusya.",
        "2",
        "Ba musulaika ula balinda,",
        "Uyanda kutambula babisyi",
        "Na waba tondeezya luyando lwakwe",
        "Mukalo wa Satani ulazwa.",
        "3",
        "Mu moyo wa muntu mulakatazya",
        "Pesi Jesu ula ngusya zyoonse",
        "Ko ku mya ku luyando lwakwe Jesu",
        "Ntambo zya Satani zyanguluka",
        "4",
        "Kofutula bantu bayanda kufwa",
        "Koyumya balenga ababombe",
        "Kobapilusyila munzila yakwe",
        "Ndibambila bakafutusyigwe."
      ],
      "number": 1
    }
  ]
}