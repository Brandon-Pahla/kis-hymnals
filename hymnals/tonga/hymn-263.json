  "number": 263,
  "language": "to",
  "title": "263: LETA KWAKUMI KOONSE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 239263LETA KWAKUMI KOONSE",
        "BRING YE ALL THE TITHES",
        "Doh is F",
        "1",
        "Komvwa majwi azwa mwibbuku lya Mwami,",
        "Koleta kwakumi kuli Leza,",
        "Kulipa kwako ku Mwami kube kwini",
        "Syoma jwi lilelekedwe.",
        "CIINDULULO:",
        "Leta konse kwakumi ku Mwami,",
        "Undisole lino ulamba,",
        "Ndila ku kupa zileleko,",
        "Uya kubula kwaku zitambwida.",
        "2",
        "Hena uyanda kugwasyigwa amoza,",
        "Koleta kwakumi kuli Leza,",
        "kokala anguwe ciindi coonse,",
        "Ulakupa zileleko.",
        "3",
        "Hena kuli cikusinkila ku Mwami,",
        "Koleta kwakumi kuli Leza,",
        "Leta kwakumi mbuli cisyomyo cakwe",
        "Ulakupa zileleko.",
        "4",
        "Moyo wako usyome mu Mufutuli,",
        "Koleta kwakumi kuli Leza,",
        "Lumbaizya Mwami akusyoma ijwi,",
        "Moyo wako upe Leza.",
        "5",
        "Imba nyimbo zibotu kulumba Mwami",
        "Wa leta kwakumi kuli Leza,",
        "Myoyo yesu ila kutangala bwini",
        "Twabona cipego cakwe."
      ],
      "number": 1
    }
  ]
}