  "title": "158: FWAMBA BUZUBA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 589158FWAMBA BUZUBA",
        "HASTEN ON GLAD DAY",
        "Doh is Eb",
        "1",
        "I kutebula kwa nyika kwaswena,",
        "Mwami uita batebuli bakwe,",
        "Michelo yabizwa koonse koonse,",
        "Yali bambila kuti &#212;tebulwe.",
        "CIINDULULO:",
        "Buzuba &#201;&#201;&#201;&#201;Bola",
        "Kobweza &#201;&#201;&#201;.Micelo",
        "Tutole",
        "2",
        "Cifumo fum &#212;eco buzuba obo,",
        "Bwaswena afwafwi myaka yainda,",
        "Zibi zyavulisya munyika eyi,",
        "Atu libambile cindi camana.",
        "3",
        "Mulimo iwa luyando mubotu,",
        "Mulonga ukunka kuzwa ku julu,",
        "Luyando lutu tamba ku mulimo",
        "I micelo mibotu ya luyando."
      ],
      "number": 1
    }
  ]
}