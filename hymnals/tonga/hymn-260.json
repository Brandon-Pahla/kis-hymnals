  "title": "260: CIBANDA CA BEULA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 610260CIBANDA CA BEULA",
        "VALE OF BEULAH",
        "Doh is C",
        "1",
        "Ndayendesya mucibanda muli luumuno,",
        "Ciband&#213;eci camaluba maluba mabotu,",
        "Cibanda eci ca Beula ninzila imbotu,",
        "Nkambo Mufutuli wangu undisololela!",
        "CIINDULULO:",
        "Cibanda eci ca Beula ndilaciyandisya,",
        "Nyika eyo ya cisyomyo musinzo wamana.",
        "2",
        "Kunyina cimvule konse, Civumba inzila,",
        "Bulemu bwakwe bumweka, Bulatubya konse,",
        "Makamu aku julu alaimbila Mwami,",
        "Ndilakomana kumvwa nyimbo zyakwe zibotu.",
        "3",
        "Ndilaya aku komana, Ku munzi umweka,",
        "Ndila komanisya lyonse, Inzila nituba,",
        "Ndila&#213;fwafwi kubona Milyango I jukide,",
        "Njenzila itutola kubwami bwa luyando."
      ],
      "number": 1
    }
  ]
}