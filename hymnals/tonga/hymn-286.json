  "language": "to",
  "title": "286: KO NDI TOLA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 946286KO NDI TOLA",
        "BEAR ME ON",
        "Doh is D",
        "1",
        "Nda yanda ku bona cindi,",
        "No tu ya ku unka ku Zioni,",
        "Twa salala ku munzi mubotu.",
        "CIINDULULO:",
        "Ko ndi tola, Ndi tole ku Zioni,",
        "Ndi tole ku munzi wa luyando,",
        "Munzi wa ba Jesu.",
        "2",
        "Ndi la ku telela kwimba,",
        "Kwe nkamu impati, Ndi la kwimba,",
        "Abo, Lwimbo lwa ku nununwa, ko.",
        "3",
        "Ndi la ku ba bona ba Isreali,",
        "Mu munzi a Jesu Mwami,",
        "Mulengi a cuno ca bwami.",
        "4",
        "Nda yanda ku Jerusalema,",
        "Munzi wa Mwami, nda yanda,",
        "Kulya micelo I futusya bantu."
      ],
      "number": 1
    }
  ]
}