  "title": "169: BAKOMI BA BULELO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 679AH 413CH 185",
        "169BAKOMI BA BULELO",
        "HIERS OF THE KINGDOM",
        "Doh is Bb",
        "1",
        "We Mukoni wa bulelo &#212;taoni;",
        "Ncinzi nco ona munsi kwa Munzi?",
        "Buka yamuka bweza ntobo yako;",
        "Fwamba kobinda ciindi camana.",
        "2",
        "Malelo a munyika ala lwana;",
        "Bakankama kumwi a kuyowa teelela",
        "Kusika kwa Inkalaki,",
        "&#212;Mukoni wa bulelo &#212;tamuki.",
        "3",
        "Uta cengwi a bubotu bwa nyika;",
        "Bulemu bwa nyika bulamana,",
        "Nketani zya Satani zikosole;",
        "&#212;Mukoni wa bulelo piluka.",
        "4",
        "Sumpula mutwe ulange ku julu;",
        "Ko langila ku bola kwa Mwami",
        "Langa Ul&#213;afwa &#212;fwi a kulibonya,",
        "&#212;Mukoni wa bulelo koimba."
      ],
      "number": 1
    }
  ]
}