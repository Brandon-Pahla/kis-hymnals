  "number": 267,
  "language": "to",
  "title": "267: KOMWAYA MICELO YA LUSE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 568267KOMWAYA MICELO YA LUSE",
        "SCATTER SEEDS OF KINDNESS",
        "Doh is G",
        "1",
        "Atubunganye micelo notuenda munzila,",
        "Azyakulya a maluba tugusye mankwisyita,",
        "Atujane kukomana muzileleko sunu,",
        "Ajanza lyakwe ligusya masunko a munzila.",
        "CIINDULULO:",
        "Tumwaye I micelo, Tumwaye I micelo,",
        "I micelo ya luse kuti tukatebule.",
        "2",
        "Tatulumbaizyi nyimbo bayuni nobaimba amaluba,",
        "Amwekesya a kusika naa yuma amakumbi a izuba,",
        "Taku nozimwekesya mbuli caanda mumupeyo,",
        "Nociloka muluwo.",
        "3",
        "Na tozi minwe ya Mwana noyaba azicisa takwe,",
        "Kusobana limbi tajati zi mucisa- sena,",
        "Mufutuli wesu takatulekeleli? Sena minwe,",
        "Imwekesya taikwe kutupenzya.",
        "4",
        "Oto tunwe tutontola tulatuyeya lyonse,",
        "Mbo twakatupenzya kale mazuba akainda,",
        "Tulatuyeya O to tunwe notwamba mucamba,",
        "Notusalala imaluba ciindi cakutebula."
      ],
      "number": 1
    }
  ]
}