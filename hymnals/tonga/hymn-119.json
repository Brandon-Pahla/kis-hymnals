  "title": "119: GUWO LISANZYA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 110AH 375CH 598SDAH 332",
        "119GUWO LISANZYA",
        "THE CLEANSING WAVE",
        "Doh is G",
        "1",
        "Lino ndabona iguwo, Mwiinzo utasimpi",
        "Jesu Mwami utuvuna waka fwida toonse.",
        "CIINDULULO:",
        "Ndabona tinti libotu ndinjile, Linditubye bu,",
        "Alumbwe Mwami mubotu, A tinti linditubye bu.",
        "2",
        "Ndabona cilengwa cipya, Bulowa buvuna",
        "Cilaamba buumi bumana, Bukunka a meenda.",
        "3",
        "Ndaenda mukusalala, Ndasiya zya nyika,",
        "Moyo wangu wakomana kutambula Jesu",
        "4",
        "Luse lukankamanisya, Watila bulowa,",
        "Jesu alike ulizyi wakafwida ime."
      ],
      "number": 1
    }
  ]
}