  "title": "297: KWAKALI UZUMINA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 6AH 88CH 123",
        "297KWAKALI UZUMINA",
        "THERE WAS ONE WHO WAS WILLING",
        "Doh is F",
        "1",
        "Kwakali uzumina, Kufwida ime;",
        "Kuti ndi mubisyi ndipone,",
        "Nzila ya Ciingano, wakaizumina;",
        "Akundi lekelela zibi.",
        "CIINDULULO:",
        "Zyakagagailwa, Zyakagagailwa;",
        "Wakazumina kuzibweza,",
        "Akucisa kunji, Jesu a Ciingano;",
        "Wakanyamuna zibi zyangu.",
        "2",
        "Ngu luyando Siluse, Ula bikilila;",
        "Nandi sanzya moyo ku bubi,",
        "Kunyina kuzulwa, Ndizyi nda&#213;nguluka,",
        "Nkambo zibi zyangu zyamana.",
        "3",
        "Ndiya kuba &#212;Muvuni, Nsika musiyi",
        "Ndiya kwenda muku komana,",
        "Alwiimbo, ndiimbe, Mumoyo ndilumbe;",
        "Kuti zibi zyangu zya tolwa."
      ],
      "number": 1
    }
  ]
}