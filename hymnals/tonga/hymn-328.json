  "number": 328,
  "language": "to",
  "title": "328: NDILA KUKOMBELA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "328NDILA KUKOMBELAI PRAY FOR THEE",
        "1",
        "Muvuni wangu ula ndikombelezya;",
        "Ula &#212;luse beenzuma bandiluba,",
        "Lino ula ndilindila mulu zyalo,",
        "Ndayanda kuti abe Jesu wako,",
        "CIINDULULO:",
        "Ndila kukombela, Ndilakukombela,",
        "Ndilakukombela, Ndikombela ndiwe.",
        "2",
        "Ndili Jisi Taata waka ndisyomya;",
        "Cilawo cibotu citamani,",
        "Uya kundiita kumu cinga kujulu,",
        "Pesi ndayanda kuunka ambebo.",
        "3",
        "Cikobelo cisalala cituba;",
        "Ndaciyobolelwa Mwami kujulu,",
        "Nondacitambula cisalala ngwa,",
        "Musa ndayanda uka pegwe cimwi.",
        "4",
        "Jesu wakujana ambila bamwi;",
        "Mufutuli wakwe nguwangu lino,",
        "Kolomba Jesu kuti aba futusye,",
        "Wa &#212;ndifwida, Wakafwida andime."
      ],
      "number": 1
    }
  ]
}