  "title": "140: IJWI LYA JESU",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 359AH 171CH 226SDAH 465",
        "140IJWI LYA JESU",
        "THE VOICE OF JESUS",
        "Doh is D",
        "1",
        "Ndamvwa Jesu ulaita &#210;Bola kuli ndime&#211;;",
        "Ulyookezye okatede yama kuli Jesu;",
        "Ndaka bola kuli Jesu, Mukupenga mukusa,",
        "Ndakajana kulyookezya, Akukomanisya.",
        "2",
        "Ndamvwa Jesu ulaita nditambule buumi,",
        "Menda abuumi ulapa, Konywa ufutuke,",
        "Nda bola kuli Jesu, Ndanywa menda &#212;bumi,",
        "Nyota yamana ndapona, Lino ndaba wakwe",
        "3",
        "Ndamvwa Jesu ulaita, &#210;Ndime kusalala,&#211;",
        "Langa ndime ndimumuni, Ukafutusigwe,",
        "Ndaka langa kuli Jesu, Ndajana kumweka,",
        "Mukumweka ndiya kwenda, Limwi ndikasike."
      ],
      "number": 1
    }
  ]
}