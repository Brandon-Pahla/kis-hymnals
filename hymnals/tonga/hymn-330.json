  "number": 330,
  "language": "to",
  "title": "330: KONDIYEEYA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "330KONDIYEEYAREMEMBER ME",
        "1",
        "Hena Muvuni wakafwa, Wakafwida ndime;",
        "Wakazumina kufwila mvunyu mbuli ndime.",
        "CIINDULULO:",
        "Ndigwasye kuzumina, Akukumvwa lyonse,",
        "Wakala acuuno cako, Kondiyeya Mwami.",
        "2",
        "Hena kubi nkwendicita, Ncaakagagailwa?",
        "Luzyalo lupati pati, Luyando lutaambwa.",
        "3",
        "Izuba lyakalivumba, Lyaleka kubala;",
        "Naakagagailwa Mwami, Kaambo kakubisya."
      ],
      "number": 1
    }
  ]
}