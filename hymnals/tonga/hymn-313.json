{
  "number": 313,
  "language": "to",
  "title": "313: INGUZU ZYA KUTELELA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "313INGUZU ZYA KUTELELA POWER TO OBEY",
        "1",
        "Tulele Lino Mwami, Notuyiya Mwibbuku,",
        "Tutondezye Kasimpe, Kotu zuzya Kuloma,",
        "Notwiya &#212;nzila yako, Tugwasye kuteelela.",
        "2",
        "Twa lomba kusanzyigwa, Mu kwenda kwesu, konse,",
        "Sanina myoyo yesu, I nywisye menda eni,",
        "Mu kubi kwa munyika, A tu tube mu myoyo."
      ],
      "number": 1
    }
  ]
}