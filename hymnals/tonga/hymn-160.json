  "title": "160: KUBAMBILA JESU",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 598160KUBAMBILA JESU",
        "KEPT FOR JESUS",
        "Doh is D",
        "1",
        "O, &#210;Kubambilwa Jesu, Mu nguzu zya Leza;",
        "Kutabisigwa &#212;nyika, Kweenda mbuli Jesu",
        "CIINDULULO:",
        "O, Kubambilwa Jesu Ime ndili &#210;buyo,&#211;",
        "Takwe ncejisi, Ndilibuyo tola Moyo wangu.",
        "2",
        "O, &#210;Kubambilwa Jesu! Ku mu sebenzela;",
        "I kuyanda kwa Mwami, Sebenzela Mwami.",
        "3",
        "O, &#210;Kubambilwa Jesu, I kuzwa ku nyika;",
        "I kuli tola ansi, Bubotu bwa moyo.",
        "4",
        "O, &#210;Kubambilwa Jesu, Kuba wakwe Mwini;",
        "Iku bawakwe lyoonse, Kuba wakw&#213; alike!"
      ],
      "number": 1
    }
  ]
}