  "title": "135: NDAKUYANDISYA MWAMI",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 301135NDAKUYANDISYA MWAMI",
        "I LOVE THEE BETTER LORD",
        "Doh is C",
        "1",
        "Ndizyi ndakuyanda, Mwami kwiinda zya munyika,",
        "Wakandipa ikuumuna, Kutakonzyi kufwa.",
        "CIINDULULO:",
        "Cisela tacina ambwa; Caluyando olu,",
        "Cisela tacina ambwa bulowa busanzya.",
        "2",
        "Ndilizyi uli afwaafwi kwiinda nyika yoonse;",
        "Nkubotu kuyeeya ndiwe kwiinda kwiimba koonse.",
        "3",
        "Wakomanya moyo wangu, &#212;Boobo ndakomana;",
        "Kunze aluyando lwako, Nsikwe kukomana.",
        "4",
        "Mufutuli we mubotu, Mbuti waakuboola?",
        "Na kutukomana sunu, Noenda andiswe."
      ],
      "number": 1
    }
  ]
}