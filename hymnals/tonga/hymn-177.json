  "title": "177: TAAKU MASIKU",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 862AH 421CH 558SDAH 427",
        "177TAAKU MASIKU",
        "NO NIGHT THERE",
        "Doh is Eb",
        "1",
        "Munyika itamani, Muli munzi wini;",
        "Tikafwi akumana, Kunyina busiku.",
        "CIINDULULO:",
        "Mwami uya kugusya, Misozi yabo bonse,",
        "(Mwami ugusya misonzi) (Misozi yabo bonse)",
        "Tababali myaka uko, Kunyina busiku,",
        "(Tababali myaka uko) (Kunyina busiku)",
        "2",
        "Milyango imwekesya, Mu munzi wa bwini,",
        "Migwagwa ya golindi, Kunyina busiku.",
        "3",
        "Milyango tiikajali, Mu munzi wa bwini,",
        "A milonga ikunka, Kunyina busiku.",
        "4",
        "Oko takukwe zuba, Ku munzi wa bwini,",
        "Mufutuli ndi zuba, Kunyina busiku."
      ],
      "number": 1
    }
  ]
}