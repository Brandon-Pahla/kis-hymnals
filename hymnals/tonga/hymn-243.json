      "lines": [
        "CS 123SDAH 38243BUKA BUUMI BUKA",
        "ARISE MY SOUL, ARISE",
        "Doh is Bb",
        "1",
        "Buka moyo wangu kozwa mu ku yoowa,",
        "Ku futuka kwangu mu bulowa bwakwe,",
        "Kusininzya kwangu konse,",
        "Kusininzya kwangu konse,",
        "Zina lyangu lilembedwe.",
        "2",
        "Ukala kujulu kundiiminina,",
        "Luyando lupati waka ndinununa,",
        "Waka fwila bantu bonse,",
        "Waka fwila bantu bonse,",
        "Watila bulowa &#212;cuno.",
        "3",
        "Zilonda zipati zya ku Kalivali,",
        "Nku paila kwangu zindiiminine,",
        "Kolekelela twalomba",
        "Kolekelela twalomba,",
        "Uta leki mubi afwe.",
        "4",
        "Taata wateelela na paila Jesu,",
        "Taakwe ku musiya uyandwa mwanaakwe,",
        "Moza wakwe ulaita,",
        "Moza wakwe ulaita,",
        "Ulati ndi mwana wakwe."
