  "title": "336: KALIVALI",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "336KALIVALICALVARY",
        "1",
        "Boola kobola moyo wangu ku Kalivali;",
        "Kubona muntu uwakakufwida Ciingano.",
        "CIINDULULO:",
        "Hena ndiya kukuluba buti Mwami wangu?",
        "Ndiya kuluba buti ndayeeya Mwami.",
        "2",
        "Kolanga musyini wa mamvwa, Kupenga, kupenga,",
        "Naakali kuyumuna zinyonyono zya bantu.",
        "3",
        "Kolanga musyini wa mamvwa, wamamvwa, wamamvwa.",
        "Wakasampulwa &#212;basi lumamba, Nkambo kangu.",
        "4",
        "Kolanga baka mujaya, bajaya Muvuni,",
        "Mvwa kwiita &#212;Leza wangu wa ndisiila nzi?&#213;",
        "5",
        "Wafwa izuba lyaleka kubala, kubala,",
        "Bamuzika cuumbwe ca Josefa bamulinda.",
        "6",
        "Buzuba bwakutalisya Imvwiki, Imvwiki,",
        "Bangele bakagusya bbwe, Jesu wakabuka."
      ],
      "number": 1
    }
  ]
}