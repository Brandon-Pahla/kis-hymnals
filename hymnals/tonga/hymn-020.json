  "number": 20,
  "language": "to",
  "title": "20: BUKA BUUMI BWANGU",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 793 AH 325CH 355 SDAH 611",
        "20: BUKA BUUMI BWANGU",
        "AWAKE MY SOUL",
        "Doh is G",
        "1",
        "Buka bumi bwangu yuma",
        "Ko ya u taiimi,",
        "Kujulu ku la kuita,",
        "U jane mu syini.",
        "2",
        "Ngu Leza Mwami u Kwita,",
        "Usike ku julu",
        "Janza lya kwe litambika",
        "Musyini wa buumi.",
        "3",
        "I kumbi lya ba kamboni",
        "Lila kuli ndiila",
        "Luba nzila zya kaindi",
        "Koya uta cebi.",
        "4",
        "Mufutuli ko tukwela",
        "Tu ta like kwenda,",
        "Tukome mazuba onse",
        "Tutule mikuli."
      ],
      "number": 1
    }
  ]
}