  "title": "274: NDIJISI MUFUTULI",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 53AH 174CH 575 SDAH 284",
        "274NDIJISI MUFUTULI",
        "I HAVE A SAVIOUR",
        "Doh is G",
        "1",
        "Muvuni Jesu undiita kwakwe,",
        "Siluse Mwami muyandi mubotu,",
        "Ula ndilinda a luse lutamani,",
        "Muvuni wangu ngu wako andime.",
        "CIINDULULO:",
        "Ndi pailila ndiwe, Mwami ab&#213;andiwe,",
        "Tuka kale tonse ku nyika eyo.",
        "2",
        "Ndi jisi Tata undipa nkwelomba,",
        "Cileleko a bumi butamani,",
        "Uya kundiita tukabe anguwe,",
        "Ncibotu kuti tukaye andiwe.",
        "3",
        "Cisamo cangu mu bulemu bwakwe,",
        "Ndila lindila waku boola nkasame,",
        "Uya kundipa ndikasame cimweka,",
        "Komvwa mukwesu tukasame tonse.",
        "4",
        "Ndapegwa luumuno mbuli mulonga,",
        "Luumuno lutana bwenwe mu nyika,",
        "Klistu ngu mutaanzi Klistu nguupa,",
        "Olu luumuno ulapa ndiwe.",
        "5",
        "Wajana Jesu utole mulumbe,",
        "Kuti muyandi wangu ngu Mwami wako,",
        "Inyika yonse ba muzyibe Muvuni,",
        "Baka futuke mu zina lya Jesu."
      ],
      "number": 1
    }
  ]
}