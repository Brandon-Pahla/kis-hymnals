  "title": "255: NDAKUYANDA JESU",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 620AH 605AH 32SDAH 484",
        "255NDAKUYANDA JESU",
        "I NEED THEE PRECIOUS JESUS",
        "Doh is F",
        "1",
        "Ndilakuyanda Jesu nkambo ndimucete,",
        "Ndimwenzu uli mu lwendo I lwa ku julu,",
        "Luyando lwa ba Jesu lundikomanisye,",
        "Lundisololele mukwenda kwangu konse.",
        "2",
        "Ndilayanda moyo wa Jesu Undizuzye,",
        "Undi gusye muma sukusyo a mu kuusa,",
        "Ndayanda moza wako undiyisye kwako,",
        "Unditondezye Jesu ndiye ku Ciingano.",
        "3",
        "Ndilakuyanda Jesu kuti nkakubone,",
        "Uli a cuuno ca bwami utamani",
        "Wakatila bulowa kuti ndikomane,",
        "Kukwiimbila nyimbo zya kuku lumbaizya,",
        "A &#208; ME &#208; NI."
      ],
      "number": 1
    }
  ]
}