  "title": "147: NDIKWELE AFWAAFWI A NDIWE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 534AH 314CH 632",
        "147NDIKWELE AFWAAFWI A NDIWE",
        "DRAW ME CLOSER TO THEE",
        "Doh is A",
        "1",
        "Ndikwele Taata munsi kwako; Ndalomba ndijate,",
        "Mumaboko ako ndivumbe, Nyanda kulyookezya.",
        "CIINDULULO:",
        "Afwafwi aluyando;",
        "(Nditole a fwafwi a luyando)",
        "Ndikwele kwako Mwami",
        "(Bola ndikwele kwako Mwami)",
        "Ko-ndi-to-la",
        "(Ndikwele musi aluyando lwako)",
        "Nditole kwako",
        "(ndikwele musi kwako)",
        "2",
        "Ndikwele Mufutuli kwako; Utansiyi limbi",
        "Mumaboko ako ndivumbe Nditaingaili",
        "3",
        "Amuya wako kondikwela; Limwi ndisanzigwe",
        "Kondisandula kondisazya limwi ndisale."
      ],
      "number": 1
    }
  ]
}