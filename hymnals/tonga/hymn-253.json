  "title": "253: BONA MWEEMBEZI",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 182CH 411253BONA MWEEMBEZI",
        "SEE, ISREAL&#213;S GENTLE SHEPHARD",
        "Doh is Ab",
        "1",
        "Bona mwembezi waima, Mwembezi mubotu,",
        "Ulaita imbelele kuya kulinguwe.",
        "2",
        "Zileke zibole kwangu, Ulaita Mwami",
        "Zilelekedwe kwa Leza Mwami wa makamu.",
        "3",
        "Bana bako Mufutuli, Twaba leta kwako,",
        "Mukukomana twalomba, Tola myoyo yesu."
      ],
      "number": 1
    }
  ]
}