      "lines": [
        "CS 540149ENDA MUMUMUNI",
        "WALK IN THE LIGHT",
        "Doh is D",
        "1",
        "Enda Mumumuni wa Mwami Akusololele;",
        "Muya wakwe usalala Ukumunikile.",
        "CIINDULULO:",
        "Enda mumumuni (Enda mumumuni wini wa Leza)",
        "Enda mumumuni (Enda mumumuni wini wa Leza)",
        "Enda mumumuni (Enda mumumuni wini wa Leza)",
        "Mumumuni wakwe Leza.",
        "2",
        "Enda mumumuni wa ijwi lya kazwa kwa Leza",
        "Mumuni usololela basyomeka boonse.",
        "3",
        "Nakuba musinze wajala, Enda mumumuni,",
        "Musinze ula kutija buzuba bwa Leza.",
        "4",
        "Mumumuni uya kuzyiba luyando lwa Leza,",
        "Bweenzinyina ano ansi akujulu koonse."
      ],
      "number": 1
    }
  ]
