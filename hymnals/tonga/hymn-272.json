  "title": "272: KOWALA INTAMBO IYA BUUMI",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 544AH 349CH 624",
        "272KOWALA INTAMBO IYA BUUMI",
        "THROW OUT THE LIFE LINE",
        "Doh is G",
        "1",
        "Kowaala lutambo ilwa buumi,",
        "Mukwesu uyanda kufutulwa,",
        "Nguni ukonzya kumu futula?",
        "Kuti afutuke ata fwi limbi.",
        "CIINDULULO:",
        "Kowaala ntambo, Kowaala ntambo;",
        "Umwi ula soweka, Kowaala ntambo,",
        "Kowaala ntambo, Umwi ula nyikila.",
        "2",
        "Kowaalisya ntambo anguzu,",
        "Uta sowi ciindi, Kofwambaana,",
        "Langa wanyikila kofwambaana,",
        "Kofwambaana mubwato uka futuke.",
        "3",
        "Kowaala ntambo kuba soweka,",
        "Bala nyikila uko nkotazyi,",
        "Myuuwo yamasunko, amapenzi,",
        "Iya kubatola alwizi lubi.",
        "4",
        "Ciindi caku futusya casika,",
        "Tukabe ambabo kumulonga,",
        "Kofwamba mukwesu uta muki,",
        "Kowaala ntambo uba futule."
      ],
      "number": 1
    }
  ]
}