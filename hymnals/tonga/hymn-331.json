  "title": "331: MUNZI OKUYA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "331MUNZI OKUYATHE HOME OVER THERE",
        "1",
        "Amuyeye munzi okuya, Mumulonga wakusalala;",
        "Basetekene obatafwi, Baya kusama kusalala,",
        "Okuya, Okuya, Amuyeye munzi okuya,",
        "Okuya, Okuya, Okuya, Amuyeye munzi okuya.",
        "2",
        "Mufutuli uli okuya Bakwesu baya kukala ko;",
        "Mukulila amukupenga, Andiilukile okuya,",
        "Okuya, Okuya, Mufutuli uli okuya",
        "Okuya, Okuya, Okuya, Mufutuli uli okuya.",
        "3",
        "Ndiya kusika okuya, Musinzo uyeya kumana;",
        "Konditola kumunzi kuya, Bangele bala kundilonda,",
        "Okuya, Okuya, Ndiya kusika okuya,",
        "Okuya, Okuya, Okuya, Ndiyakusika okuya."
      ],
      "number": 1
    }
  ]
}