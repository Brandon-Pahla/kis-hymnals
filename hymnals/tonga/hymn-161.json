  "title": "161: MWAMI WANGU ANDIME",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 599AH 112CH 532SDAH 456",
        "161MWAMI WANGU ANDIME",
        "MY LORD AND I",
        "Doh is F",
        "1",
        "Nd&#213;amwenzuma mubotu, ndila muyandisya,",
        "Ula ndiyanda kapati, Mweenzuma mubotu",
        "Nsikali katako walo, Nyanda kali munsi,",
        "Kuti tube antoomwe, Mwami ame.",
        "2",
        "Muziindi nda netuka, Ulizi ndimubi;",
        "Kuti ndayama kwakwe, Lugwasyo Ndalomba;",
        "Nandi sololela konse, Munzila zyans&#213;ano",
        "Elyo tulaya &#212;nguwe, Mwami ame",
        "3",
        "Ndamwambila kuusa &#212;kukomana kwangu;",
        "Ndaamba cindi komanisya, A ciindi ne nyema;",
        "Wandaambila nce ti cite, A mbwenga ndasola,",
        "Tula kanana nguwe, Mwami ame.",
        "4",
        "Ulizi ndayandisya, I kuleta bamwi;",
        "Elyo ula ndituma, Ndibaambile ijwi,",
        "Ndi ambe luyando lwakwe, A ncaakatufwida;",
        "Tula sebenza &#212;ntomwe, Mwami ame"
      ],
      "number": 1
    }
  ]
}