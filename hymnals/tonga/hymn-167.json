  "number": 167,
  "language": "to",
  "title": "167: MATALISYO MAYUMU",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 664AH 215CH 255SDAH 509",
        "167MATALISYO MAYUMU",
        "FIRM FOUNDATION",
        "Doh is G",
        "1",
        "Mayumu matalisyo a basale;",
        "A bikidwe kuyumya lusyomo lwenu,",
        "Inga wa&#213;mba kunze ncaambide kale,",
        "Inywe no muyanda ciyubilo kwakwe,",
        "Inywe &#212;Muyanda ciyubilo kwakwe.",
        "2",
        "Mu menda malamfu kuti wanjila;",
        "Mapenzi a Kuusa tokwe kuzundwa",
        "Ndiya kuba a nduwe mu kupenga kwako,",
        "Me ndiya kwa sanzya, Me ndiya ku sanzya",
        "Ndiya kwa salazya mapenzi ako. Doh is G",
        "3",
        "Mu mulilo wa kasunkwa no enda,",
        "Luse lwangu luli zulide kwakwe,",
        "Mu mabanga banga a mulilo tokapyi,",
        "Nja ku Kwiiminina nja ku kwiiminina,",
        "Ndiya ku kwiiminina mu mulilo.",
        "4",
        "Oyo uya ku yaama kuli Jesu,",
        "Takwe ku mu sowa na ku mu kaka,",
        "Nakuba kuti Satani wa sola buti,",
        "Nja kuba anduwe &#212;me nsika kusiyi",
        "Imebo taku nenja kuku siya&#213;."
      ],
      "number": 1
    }
  ]
}