  "title": "284: MAZUBA NA AMANA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 486284MAZUBA NA AMANA",
        "WHILE THE DAYS ARE GOING BY",
        "Doh is Ab",
        "1",
        "Nkobali ba yanda ijwi, Mazuba naamana,",
        "Atusandule I bafwa, Mazuba Naamana,",
        "Tubatondezye nzila, Yakusika Kujulu,",
        "Tugwasye basoweka mazuba naamana.",
        "CIINDULULO:",
        "Mazuba &#201;&#201;&#201;&#201;&#201; Amana&#201;&#201;&#201;&#201;",
        "Mazuba&#201;&#201;&#201;&#201;&#201;&#201; Amana&#201;&#201;&#201;&#201;",
        "Tugwasye&#201;&#201;&#201;&#201; Basoweka &#201;&#201;",
        "Mazuba &#201;&#201;&#201;&#201;&#201; Naamana &#201;&#201;&#201;&#201;",
        "2",
        "Taku ciindi cakukala, Mazuba naamana,",
        "Lekabusyu bumwekesye, Mazuba naamana,",
        "Munyika yamapenzi, Kuusa akulila tubusye,",
        "Bawa cibi Mazuba naamana.",
        "3",
        "Ku luyando twaswangana, Mazuba naamana,",
        "Tatusiyi naba omwe Mazuba naamana,",
        "Imbuto njotusyanga iyo ikakomene,",
        "Mumyoyo itontozye, Mazuba naamana."
      ],
      "number": 1
    }
  ]
}