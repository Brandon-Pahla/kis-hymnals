  "title": "7: BA LA KU SALALA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 852 AH 449CH 418SDAH 218",
        "7:BA LA KU SALALA",
        "WHEN HE COMETH",
        "Doh is E",
        "1",
        "Naya ku bola alimwi, Kulonda lubono,",
        "Lubono lubotu botu; Bonse bana&#213;kwe.",
        "CIINDULULO:",
        "Ba la ku ba mbuli",
        "Inyenyezi mu musyini",
        "Bala ku salalisya",
        "Musyini wakwe",
        "2",
        "U la ku londa mabwe, A mwekesya kabotu,",
        "Mabwe onse a salala, Bonse bana&#213;kwe.",
        "3",
        "Twana, twana, O tu yanda, Mufutuli wato,",
        "Otu, ntu tubwe tu mweka, Tonse twana&#213;kwe."
      ],
      "number": 1
    }
  ]
}