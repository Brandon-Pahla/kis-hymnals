  "title": "265: LUUMUNO LUBOTU",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 271CH 311SDAH 463",
        "265LUUMUNO LUBOTU",
        "PEACE PERFECT PEACE",
        "Doh is C",
        "1",
        "Kuumuna munyika ya cibi,",
        "Bulowa bwa Jesu bwa luumuno.",
        "2",
        "Kuumuna mu milimo yesu,",
        "Kucita luyando lwakwe Jesu.",
        "3",
        "Kuumuuna akuusa kwamba,",
        "Muli Jesu twaba kuumuna.",
        "4",
        "Kuumuna kubayandwa besu,",
        "Muli Jesu tuli a luumuno.",
        "5",
        "Kuumuna kumbele tatuzyi,",
        "Tulizyi Jesu uli a cuuno.",
        "6",
        "Kuumuna lufu lwatuvumba,",
        "Jesu wakoma lufu lwa nyika.",
        "7",
        "Kwamana mapenzi a nyika,",
        "Jesu &#212;la tuita kwakwe ku julu."
      ],
      "number": 1
    }
  ]
}