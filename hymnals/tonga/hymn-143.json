  "title": "143: MWAMI KOTU ANDAANYA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 438AH 22CH 33SDAH 64",
        "143MWAMI KOTU ANDAANYA",
        "LORD DISMISS",
        "Doh is E",
        "1",
        "Mwami kotu andaanya swe, Kotupa kukomana,",
        "Atu yandane mu myoyo, Atu zunde mu luse,",
        "Tukomanye, tukomanye, Notuli mu musinzo.",
        "2",
        "Twakulumba twa kuyanda, Kaambo ka majwi ako,",
        "Bubotu bwa lufutuko bube mu myoyo yesu,",
        "Tusyomeke tusyomeke, Mu kasimpe tujanwe."
      ],
      "number": 1
    }
  ]
}