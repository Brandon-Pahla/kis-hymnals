{
  "number": 308,
  "language": "to",
  "title": "308: BOOLA LUFUTUKO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "308BOOLA LUFUTUKO COME SALVATION",
        "1",
        "Basa Kamuboola kuno uyimbe Inyimbo,",
        "Sunu Tulaimbila Leza Wesu,",
        "Kamubola Kuno Nomataata A bacizyi",
        "Kwipobwe Libotu Lya Mwami Jesu.",
        "CIINDULULO:",
        "Boola Lufutuko, Boola Lufutuko,",
        "Koteelela Kulila Kwesu Akulyeetezya,",
        "Sunu Tulaimbila Leza Wesu",
        "Koteelela Kulila Kwesu Mazuba onse,",
        "Kwipobwe Libotu Lya Mwami Jesu.",
        "2",
        "Hena Wamumvwa Jesu Kalila A Ciingano,",
        "Wakafwa Kambo Kazibi Zyesu,",
        "Hena Wamumvwa Jesu Kalila A Ciingano,",
        "Leza Taata Wandisiila Nzi."
      ],
      "number": 1
    }
  ]
}