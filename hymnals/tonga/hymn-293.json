  "title": "293: NKO NKO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 35293NKO NKO",
        "KNOCKING, KNOCKING",
        "Doh is G",
        "1",
        "Nkonko, nkonko, Nguni uyo? Linda, linda",
        "O kwabota, Ngu silweendo mweenzu wa bwami",
        "Mweenzu utaninga nonwa, Mweenzu ukankamanisya,",
        "Mujulile anjile, Mujulile anjile.",
        "CIINDULULO:",
        "Lelo cibi naciko-sa Nseku shakosha ici-bi",
        "Mishimba naifula, Cibi cakakatila,",
        "Nacikano kwisuka.",
        "2",
        "Nkonko, nkonko, Uciimvwi, Linda, linda,",
        "Ndila jula O mulyango taujuki,",
        "Nkaambo nsaku yavulisya, Tuyanda twajatilila,",
        "Cilyango cagagamina, Cakaka kujaluka.",
        "3",
        "Nkonko, nkonko, Ncinzi ico, Linda linda,",
        "Ndilabola, nguwena ucikonkomona,",
        "Musyini wamamvwa wasama,Meso akwe alaaluse,",
        "Lwa Muvuni ulindila, Jula moyo anjile."
      ],
      "number": 1
    }
  ]
}