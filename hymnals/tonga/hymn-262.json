  "title": "262: KLISTU NA BALABAS",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 1262KLISTU NA BALABAS",
        "CHRIST OF BARABBAS",
        "Doh is C",
        "1",
        "Ndicite nzi a Jesu ngomwandiletela,",
        "Usamide munjila, Amamvwa kumutwe.",
        "CIINDULULO:",
        "Ngoyu mubuzyo ndicite nzi lino?",
        "Ndibambule Jesu Mwami ndileke Balabasi.",
        "2",
        "Mubuzyo utaambiki, Ncinzi cisalala,",
        "Siluse na mubisyi ndicite buti?",
        "3",
        "Acuno calubeta utakaki nyika,",
        "Majwi abo amvwika, &#210;Tutambule Jesu&#211;",
        "4",
        "Mubuzyo ulaita, Kuli bantu bonse,",
        "Muziindi zya masunko muyume ba Klistu."
      ],
      "number": 1
    }
  ]
}