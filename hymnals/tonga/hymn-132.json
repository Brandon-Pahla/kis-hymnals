  "title": "132: AFWAAFWI A CIINGANO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 289AH 240CH 595SDAH 312",
        "132AFWAAFWI A CIINGANO",
        "NEAR THE CROSS",
        "Doh is F",
        "1",
        "Jesu mbe aCiingano, Ku mwinzo mubotu",
        "Nkobasilikwa bonse, Mbe ku Kalivali.",
        "CIINDULULO:",
        "Kalivali, Kalivali, Andiyube kwako;",
        "Ndijane lufutuko, Mubulowa bwako.",
        "2",
        "Afwaafwi aCiingano, Luse lwako ndamvwa;",
        "Nyenyezi yako ndipa, Indisololele.",
        "3",
        "Afwafwi a Ciingano, Ndabona bulemu;",
        "Ndipe nzila ya buumi, Kuti ndi futuke.",
        "4",
        "A Ciingano ndalanga, Lusyomo lutafwi;",
        "Kunyika ya Golide, Kumulonga wako."
      ],
      "number": 1
    }
  ]
}