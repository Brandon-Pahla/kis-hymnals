{
  "number": 99,
  "language": "to",
  "title": "99: NE NDA LANGA KU CINGANO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 172AH 89, 90CH 120SDAH 154",
        "99NE NDA LANGA KU CINGANO",
        "WHEN I SURVEY THE WONDEROUS CROSS",
        "Doh is F",
        "1",
        "Ne nda langa ku ciingano",
        "A ka fwila Mwini Bwami",
        "Nda amba nsi jisi cintu,",
        "Nda bu seka bwami bwangu.",
        "2",
        "Langa mutwe a maboko,",
        "A bulowa a luyando",
        "Sena wa bona luyando",
        "A kupenga kuli bobo?",
        "3",
        "Nda kali musowekeli,",
        "Nda ka nununwa bulowa;",
        "A ndi leke ku li lemya,",
        "Pele mu ciingano cakwe.",
        "4",
        "Ne nda jisi nyika yonse,",
        "Nci cilumbulo cisyonto",
        "Luyando o luli bobo",
        "Lu la ita moyo wangu."
      ],
      "number": 1
    }
  ]
}