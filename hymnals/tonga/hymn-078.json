  "title": "78: KU YANDAULA BA SOWEKI",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 532 SDAH 37378KU YANDAULA BA SOWEKI",
        "SEEKING THE LOST",
        "Doh is Bb",
        "1",
        "Tula yandaula basoweki",
        "Baingaila mu malundu,",
        "Tu la ba bula majwi a Mwami",
        "&#210;A mu bole ku lime I nywe.&#211;",
        "CIINDULULO:",
        "Kule, kule mu malundu malamfu,",
        "Tu la leta, basoweki alimwi,",
        "Muluba, muluba lwa mufutuli,",
        "Mwana &#212;mbelele o wa kayaswa.",
        "2",
        "Tu la yandaula basoweki;",
        "Twa tondezya ba bisyi Jesu",
        "Tu la benzya mu nzila ya Mwami",
        "Nda yanda ku imisya abawi",
        "3",
        "Ndila yanda ku cilila Jesu",
        "Ku engelela aba ciswa,",
        "Kuleta ba soweki kwakwe,",
        "A kufutuka kuta mani."
      ],
      "number": 1
    }
  ]
}