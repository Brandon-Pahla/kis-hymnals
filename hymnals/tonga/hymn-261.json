  "title": "261: CIBANDA CA EDENI",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 850AH 429CH 550",
        "261CIBANDA CA EDENI",
        "BEAUTIFUL VALEY OF EDEN",
        "Doh is Eb",
        "1",
        "Nyika imbotu ya Edeni, Kuli kuumuna,",
        "Mumyoyo yabapengede, Kuli kulyokezya.",
        "CIINDULULO:",
        "Nyika imbotu ya Edeni, Munzi usalala,",
        "Nondili mukupenga kunji, Ndalota kulyokezya.",
        "2",
        "Mu moyo onse ulila, Uyaku komana,",
        "Bangele balaimbisya, Kunyika imbotu.",
        "3",
        "Nkokukala Mufutuli, Bulowa busanzye",
        "Mu malundu abulemu, Bangele baimba."
      ],
      "number": 1
    }
  ]
}