{
  "number": 168,
  "language": "to",
  "title": "168: MWAMI MWEEMBEZI WANGU",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 665AH 295CH 405",
        "168MWAMI MWEEMBEZI WANGU",
        "THE LORD IS MY SHEPHERD",
        "Doh is G",
        "1",
        "Mwami ngo Mwembezi wangu taku ncendibula,",
        "Ndakomana muli nguwe Ndilaku lyokezya,",
        "U la ndi sololela konse nkondiya,",
        "Mumasunko ulavuna Mufutuli wesu.",
        "2",
        "Ne ndi ingaila konse mulufu mucibi,",
        "Mboli imulangi wangu nsikayowi lufu,",
        "Nkoli ya ko, Indi vune ndikale kwako,",
        "Taku cindigusya kwako andibe andiwe.",
        "3",
        "Mukupenga mumasunko ndasyoma muli ndiwe",
        "Zileleko nzyo wa ndipa ndiya kukomana,",
        "Wandinanika mafuta ku mutwe wangu,",
        "Ncinzi ncendilomba Kwako zyonse ulandipa.",
        "4",
        "Bubotu bwako a luse O Leza singuzu,",
        "Ucinditondezya nzila limwi ndizumizye,",
        "Ndaya ndaula nzila mbuli bakale,",
        "Mumusinzo wa ku Julu, Nkaimbe ambabo"
      ],
      "number": 1
    }
  ]
}