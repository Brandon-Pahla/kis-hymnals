  "title": "108: JESU MWEMBEZI MWINI",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 46AH 182CH 572",
        "108JESU MWEMBEZI MWINI",
        "LOVINGLY TENDERLY CALLING",
        "Doh is F",
        "1",
        "Jesu mwembezi mwini, Ula kwita bola,",
        "Muluba luyumu nta, Nkokuli cilawo,",
        "Bola nocil&#213; anguzu, Bola mukubusyi",
        "Njila muluba lwakwe, Enda mukasimpe.",
        "CIINDULULO:",
        "Ulaita ndiwe aluyando, We mubisyi kobola kuli&#213;me",
        "Ula kombelezya ulaita, Jesu mwembezi wangu.",
        "2",
        "Jesu mwembezi mwini, Wakafwida ndiwe,",
        "Ulaita kabotu, Mubi bola kwangu,",
        "Fwamba kozwa mucibi, Bola ula kwita",
        "Njila muluba lwakwe, Uya kulyookezya.",
        "3",
        "Kuingaila nkubi, Baumpe nkobali,",
        "Bayanda basoweki, Bayanda kujaya,",
        "Jesu mwembezi mwini, Ula kwita bola,",
        "Njila muluba lwakwe, Uya kulyookezya."
      ],
      "number": 1
    }
  ]
}