  "title": "131: MBULI JESU",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 286131MBULI JESU",
        "MORE LIKE JESUS",
        "Doh is F",
        "1",
        "Andibe mbuli Jesu, Lyoonse akal &#212;ambebo,",
        "Kondizuzya kuumuna,",
        "Ndibambe mbuli nziba, Andibe mbuli Jesu,",
        "Mulweendo lwangu loonse,",
        "Ndacetala mumuya, Mufutuli ba andime.",
        "2",
        "Kuti komvwa kulila ikwa ba cikwaangala",
        "Ulangisya tuyuni ambebo ulandimvwa;",
        "Undiyisya kupona akundi lekelela;",
        "Ndasalala mumoyo Mufutuli ba andime.",
        "3",
        "Andibe mbuli Jesu Lyoonse nendi paila;",
        "Ndyookezye munsi lyakwe kuli kuumuna kwini",
        "Nda zyalululwa ime muluyando wandiita,",
        "Ndavuba mulusyomo, Mufutuli ba andime"
      ],
      "number": 1
    }
  ]
}