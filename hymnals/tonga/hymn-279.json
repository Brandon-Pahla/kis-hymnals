  "title": "279: BOOLA KULI NDIWE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 16279BOOLA KULI NDIWE",
        "COME UNTO ME",
        "Doh is C",
        "1",
        "Moyo wangu wausa, Meso ata boni;",
        "Kukondwa muli Jesu muli kukomana.",
        "CIINDULULO:",
        "Boola kuli ndime noonse nomupengede;",
        "Ano mulemenwe, Muya ku lyookezya,",
        "Bweza joko lyangu, Cilila nzila zyangu,",
        "Ndime Mufutuli wenu, Mukalyookezye muli ndime.",
        "2",
        "Kukomana kwa nyika koonse kulamana;",
        "Luyando lwakwe Jesu Nduyando lutafwi.",
        "3",
        "Buumi butakamani Klistu ula tupa;",
        "Utafwi kosanduka kokondwa upone.",
        "4",
        "Luumuno lwakwe ndunji, Luyando ndupati;",
        "Mukuli taulemi, Muuba zuba lyoonse."
      ],
      "number": 1
    }
  ]
}