  "title": "151: KUYA KUBA INYENYEZI",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 552AH 355CH 626",
        "151KUYA KUBA INYENYEZI",
        "WILL THERE BE ANY STARS",
        "Doh is A",
        "1",
        "Ndayeya sunu nyika eyo imbotu,",
        "Nje ndiya kwenda ni lya bbila,",
        "Muluse nendiya kuima a Jesu,",
        "Hena ndiya kuba &#212;Nyenyezi?",
        "CIINDULULO:",
        "Hena Musyini ula kuba &#212;Nyenyezi?",
        "Nendiya kusika ku julu?",
        "Nendiya kulyookezya a bal&#213; acolwe,",
        "Hena ndiya kuba &#212;Nyenyezi?",
        "2",
        "Mu nguzu zya Mwami andi munyonene,",
        "Ndibe mbuli uvuna bantu,",
        "Kuti Nyenyezi zimweka zibe zyangu,",
        "Nendiya kulumbwa ku Mwami.",
        "3",
        "Hena ndiya kukomana nda mubona,",
        "Ndila ku mutolela bantu,",
        "Ciya kundikomanisya cini cini,",
        "Kuti nda pegwa Inyenyezi."
      ],
      "number": 1
    }
  ]
}