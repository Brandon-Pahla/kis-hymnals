  "language": "to",
  "title": "310: TABAKWE KULILA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "310TABAKWE KULILANO SORROW THERE",
        "1",
        "Kunyina Masiku, Kujulu Nkubotu,",
        "Tatukabombi Tula Kunyona A Luyando.",
        "CIINDULULO:",
        "Tabakwe Kulila, Tabakwe Kulila,",
        "Kujulu Nkoluli Luyando, Tabakwe Kulila.",
        "2",
        "Tabakalili Ko, Nkokuli Kuseka,",
        "Yaku gusigwa Misozi, Tabakwe Kulila.",
        "3",
        "Kunyina Kubisya, Nkokuli Kukoma,",
        "Baya Kusama Bubotu Okuya Kujulu.",
        "4",
        "Tabakwe Kufwa ko, Boonse Iba koma,",
        "Elyo baya kufutuka, Kukabe Kutamani."
      ],
      "number": 1
    }
  ]
}