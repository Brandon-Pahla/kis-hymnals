  "title": "277: MUMUNI NDISOLOLELE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 631AH 306CH 403",
        "277MUMUNI NDISOLOLELE",
        "LEAD KINDLY LIGHT",
        "Doh isAb",
        "1",
        "Nditondezye kutuba ndazimina konditola",
        "Kwasiya mbi ndicili mumusinzo ndisololele,",
        "Kondi gwasya mulweendo ndita soweki,",
        "Koba &#212;ndime gwasy mulweendo ndita soweki,",
        "Koba &#212;ndime nendienda konse.",
        "2",
        "Ndakali kuenda munzila zibi nkita kuzyi;",
        "Ndakali kuyandisisya zya nyika ndisololele,",
        "Ndaka cita zintu zibi, kuli ndiwe,",
        "Kuliyanda kwakaindi ndasiya.",
        "3",
        "Na ndalelekwa mu nguzu zya Mwami; Ndilaunka,",
        "Nakuba mapenzi andivulila, ndilakuunka",
        "Limwi ndikaimbe a bangele,",
        "Ncindali kuyandisisya lyoonse."
      ],
      "number": 1
    }
  ]
}