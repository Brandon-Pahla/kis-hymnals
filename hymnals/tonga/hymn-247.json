  "title": "247: LIZYA MILANGU YA KU JULU",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 29247LIZYA MILANGU YA KU JULU",
        "RING THE BELLS OF HEAVEN",
        "Doh is Bb",
        "1",
        "Uma milangu ya kujulu sunu,",
        "O wakazimina wa zyoka,",
        "Langa wisi wamucinga munzila,",
        "Wamutambula Mwana wakwe.",
        "CIINDULULO:",
        "Colwe colwe baimba kwakwe,",
        "Colwe colwe balaimbisya,",
        "Basilumamba a bakanununwa,",
        "Baimba lwakuangululwa.",
        "2",
        "Uma milangu ya Kujulu sunu",
        "Waka swekede wapiluka",
        "Mubisyi wafutusigwa mukubi",
        "Wazyalwa Mwana wanununwa.",
        "3",
        "Uma milangu ya kujulu sunu,",
        "Bangele balizya impeta",
        "Koambisya kambo kalufutuko",
        "Mwana uyandika wazyalwa."
      ],
      "number": 1
    }
  ]
}