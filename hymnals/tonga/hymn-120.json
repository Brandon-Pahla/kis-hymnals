  "title": "120: LINO KASIMPE NDAKONDWA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 186AH 202CH 670SDAH 486",
        "120LINO KASIMPE NDAKONDWA",
        "I DO BELIEVE",
        "Doh is G",
        "1",
        "Taata ndila kwita kwangu, A bambi nsi bezyi;",
        "Kuti wazwa kuli ndime, Ndila kuya kuli?",
        "CIINDULULO:",
        "Lino kasimpe ndakondwa, Ndakafwidwa Jesu,",
        "Watila bulowa bwakwe, Kundi angulula.",
        "2",
        "Ndakondwa ku Mwana wako, Ndipe nguzu zyako;",
        "Ndipa konse nkwendibula, Muciindi ceelede.",
        "3",
        "Ndiwe mwini waku kondwa, Ndalanga kuliwe;",
        "Anditambule cipego, Muya uta kafwi."
      ],
      "number": 1
    }
  ]
}