      "type": "verse",
      "lines": [
        "CS 260254LUMBA ZINA LYA JEHOVA",
        "PRAISE YE JEHOV&#213;S NAME",
        "Doh is F",
        "1",
        "Amulumbaizye zina lya Jehova mu mukombe,",
        "Mwami wa kujulu Mwami waluyando",
        "O Mwami siluse Mwami singuzu.",
        "2",
        "Tuimbile Mwami impeta zilile, Koonse koonse,",
        "Tumulumbaizye munyimbo zibotu, Zina litamani,",
        "A tumulumbe.",
        "3",
        "Twakulumbaizya munyimbo tulombe ku sanzigwa,",
        "Wakalenga zyoonse tumulumbaizye,",
        "Mwami utamani tumulumbe."
      ],
      "number": 1
    }
  ]
}