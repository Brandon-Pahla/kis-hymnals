  "title": "138: KULISUNGULA KUZWIDE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 316138KULISUNGULA KUZWIDE",
        "ENTIRE CONSECRATION",
        "Doh is D",
        "1",
        "Tola ibuumi bwangu, Kuti bukabe bwako",
        "Kotola maanza angu muluse ndisandule.",
        "CIINDULULO:",
        "Ndisanzye mubulowa bwako, Kondisalazya nditube bu,",
        "Ndikupe Mwami buumi bwangu boose, Kukabe kutamani.",
        "2",
        "Tola maulu angu asebenzale ndiwe,",
        "Kotola ijwi lyangu, Kuti likuimbile.",
        "3",
        "Tola mulomo wangu uzuzye majwi ako;",
        "Kotola mali angu nsikwe kusisa cintu.",
        "4",
        "Tola mazubaangu aakukulumbaizya;",
        "Tola busongo bwangu busebenzele ndiwe."
      ],
      "number": 1
    }
  ]
}