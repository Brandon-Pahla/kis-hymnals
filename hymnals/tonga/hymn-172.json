  "title": "172: NDILILI?",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 716AH 368CH 354SDAH 618",
        "172NDILILI?",
        "HOW LONG?",
        "Doh is Bb",
        "1",
        "Kube lili O Mwami, &#212;We kotana sika?",
        "I myoyo yakatala, A kumuka kwako;",
        "Ciindi cisike lili, Kumweka kupati?",
        "I kwa bulemu bwako, Nokuya kusika.",
        "2",
        "Kube lili O Mwami, Kosiya inkamu?",
        "Uli mukide kale, Bamwi bakatyompwa;",
        "Banjila mukubisya, I batwanga bako,",
        "Mbasyonto balindila, Ikukutambula.",
        "3",
        "Sinsimuna balede; Tuma jwi babuke;",
        "Basale baambe kuti, &#210;Muvuni wasika&#211;;",
        "I malambe ka &#212;yaka, Tulindile tonse;",
        "Bonse bali bambile, Kuya kumubona."
      ],
      "number": 1
    }
  ]
}