  "title": "270: MUVUNI WESU",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 459AH 156CH 214",
        "270MUVUNI WESU",
        "OUR BLEST REDEEMER",
        "Doh is Eb",
        "1",
        "Muvuni tanaya kwakwe wakatulayila,",
        "Muko manisyi watupa ku julu.",
        "2",
        "Mukomanisyi ngwa tupa mwenzu usalala,",
        "Oko nkwatijane moyo a lyookezye.",
        "3",
        "Ijwi lyakwe lya lubomba litwita kwakwe,",
        "Litondezya nzila yakwe ya ku julu.",
        "4",
        "Kusyomeka mu lindiswe akuzunda konse,",
        "Mizeezo yonse njiyakwe isalala.",
        "5",
        "Muya mubotu wa luse kutuengelela,",
        "Ko kala mumyoyo yesu tusandule."
      ],
      "number": 1
    }
  ]
}