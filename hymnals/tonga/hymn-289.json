  "number": 289,
  "language": "to",
  "title": "289: NDITOLE MBWENDI",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 103289NDITOLE MBWENDI",
        "TAKE ME AS I AM",
        "Doh is Ab",
        "1",
        "Jesu O Mwami ndakwita, Ndigwasye nendi mulufu,",
        "Ndipe lufutuko lwako, Nditole mbwebede.",
        "CIINDULULO:",
        "Nditole mbwebede, Nditole mbwebede,",
        "Ndipe lufutuko lwako, Nditole mbwebede.",
        "2",
        "Nsijisi naba lugwasyo, Nakuba wakandifwida,",
        "Kondilengulula ime, Nditole mbwebede.",
        "3",
        "Nsijisi kulibambila, Ndimuntu wazinyonyono,",
        "Pesi ndifutule Mwami, Nditole mbwebede.",
        "4",
        "Ndalomba ku cuno cako, Ndibone ndakulombesya,",
        "Mulimo wako mubotu, Nditole mbwebede",
        "5",
        "Nditume oko nkoyanda, Ndisandule ndi salazye",
        "Ndibelesye kala andime, Nditole mbwebede.",
        "6",
        "Mulimo wako wamana, Nkondo ya lwanwa ndakoma,",
        "Ndiya kukwita O Mwami, Nditole mbwebede."
      ],
      "number": 1
    }
  ]
}