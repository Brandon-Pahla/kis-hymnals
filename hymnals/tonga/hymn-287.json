  "title": "287: MUCIINDI CAMASUNKO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 709AH 277CH 327SDAH 491",
        "287MUCIINDI CAMASUNKO",
        "IN THE HOUR OF TRAIL",
        "Doh is C",
        "1",
        "Mumasunko angu Tata ndiyumye,",
        "Kuti ntasoweki akukusiya,",
        "Na ndatempauka kondijata luboko,",
        "Ndapaila Taata utandisiyi.",
        "2",
        "Zya munyika zibi Ziya Kumana,",
        "Buvubi bwanyika bujaya moyo,",
        "Muluse ndiimike Taata ndi Mwana wako,",
        "Bubi bwangu gusya akuyandisya.",
        "3",
        "Ndipe luse lwako gusya kupenga;",
        "Mukuciswa kwangu nendi munyika,",
        "Janza lyako liyumu linditole kwako,",
        "Ndibe Mwana wako ndisolole.",
        "Ameni."
      ],
      "number": 1
    }
  ]
}