  "title": "129: JESU KOBOOLA UTULELEKE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 278AH 272CH 602",
        "129JESU KOBOOLA UTULELEKE",
        "JESUS COME AND BLESS US",
        "Doh is A",
        "1",
        "Jesu wakasyomya na mwaba banji,",
        "Mu Mwami mwaswana uyakuba ko,",
        "Akusyoma ijwi, Twalomba lino,",
        "Jesu tuleleke Mwami libonye.",
        "CIINDULULO:",
        "Jesu tuleleke twakulombesya,",
        "Jesu tuleleke koba &#212;swe lyoonse.",
        "2",
        "Jesu twakubona ziindi zya kale,",
        "Lyoonse tulakuyandisya, Ba &#212;ndiswe,",
        "Boola Mufutuli muluse lwako,",
        "Tola myoyo yoonse notupaila.",
        "3",
        "Jesu kotugwasya notukwiimbila,",
        "Komvwa mipailo twakulombesya,",
        "Ilusyomo lwesu uluyumye nta;",
        "Kosalazya nzila Tukayandane."
      ],
      "number": 1
    }
  ]
}