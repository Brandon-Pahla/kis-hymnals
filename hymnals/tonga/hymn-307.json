  "title": "307: NDASANZIGWA NDABA MBULI CAANDA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "307NDASANZIGWA NDABA MBULI CAANDA",
        "WASHED WHITE AS SNOW",
        "1",
        "Zinyonyono Zyangu Zyavula",
        "Ndasamba Mumenda a Vuna",
        "Abulowa wakandisanzya",
        "Ndaba mbuli Caanda.",
        "CIINDULULO:",
        "Lwimbo Lwangu Lwa ku Komana",
        "Bulowa Bwakwe Bwa Ndisanzya",
        "Bwandisanzya Inzya Bwandisanzya",
        "2",
        "Ndakanjila mu Kumukondwa",
        "Ndakazumina Kubi Kwangu",
        "Ndakatolwa kuboko Kwakwe",
        "Ndaba Mbuli Caanda.",
        "3",
        "Ndakamupa Moyo Alike",
        "Waseka Wamba Kofutuka",
        "Ndakatambula Ku imuna",
        "Ndaba Mbuli Caanda.",
        "4",
        "Ndila Kwiimba Inguzu Zyakwe",
        "Ndila Kwiimba Kukoma kwakwe",
        "Ndila Kwiimba Kunze Kwa Lufu",
        "Wakandi Salazya"
      ],
      "number": 1
    }
  ]
}