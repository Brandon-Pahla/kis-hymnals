{
  "number": 251,
  "language": "to",
  "title": "251: KULILA TAKUNDIGWASYI",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 92251KULILA TAKUNDIGWASYI",
        "WEEPING WILL NOT SAVE ME",
        "Doh is Eb",
        "1",
        "Nakuba ndalila tacikwe kundivuna,",
        "Nsikaleki kuyoowa zibi tazikamani,",
        "Taku cindigwasya.",
        "CIINDULULO:",
        "Jesu wakandifwida, Jesu wakapenzegwa,",
        "Mizeezo isalala tiikwe kundisandula,",
        "Taaku cindigwasya.",
        "2",
        "Nendibelekesya tacikwe kundivuna,",
        "Mizeezo isalala tiikwe kundi sandula,",
        "Taaku cindigwasya.",
        "3",
        "Naba ndalindila tacikwe kundivuna,",
        "Luse talundigwasyi Nokuba",
        "Ndilindile taku cindigwasya.",
        "4",
        "Lusyomo lwa Jesu ndoluya kundivuna",
        "Ndisyome aciingano Ndijate kuli Jesu,",
        "Lusyomo luvuna."
      ],
      "number": 1
    }
  ]
}