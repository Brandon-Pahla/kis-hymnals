  "title": "110: BALELEKWA IBACITA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 55110BALELEKWA IBACITA",
        "BLESSED ARE THEY THAT DO",
        "Doh is A",
        "1",
        "Koteelela majwi a Jesu, Imajwi a kufutuka",
        "Iwe o talumbi, yeya teelela, Balalelekwa ibacita.",
        "CIINDULULO:",
        "Bala &#212;colwe babamba milawo, Bala &#212;colwe, bala &#212;colwe.",
        "Bala &#212;colwe babamba milawo, Bala &#212;colwe, bala &#212;colwe.",
        "2",
        "Milawo titwaiteelela, A zisyomyo zyakwe zyoonse,",
        "Kumvwa aku yoowa taku futuli, Balalelekwa ibacita.",
        "3",
        "Baya kunjizigwa mumunzi, Banununwe mukubisya,",
        "Baya kusanzigwa kutamani, Baya kupegwa ibuumi."
      ],
      "number": 1
    }
  ]
}