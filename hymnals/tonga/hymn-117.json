{
  "number": 117,
  "language": "to",
  "title": "117: NDABOOLA MWAMI",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 101AH 173CH 224",
        "117NDABOOLA MWAMI",
        "I AM COMING LORD",
        "Doh is Eb",
        "1",
        "Ndamvwa ijwi lyako, Lindiita kwako,",
        "Mubulowa ndisanzigwe, Ubo ba Kalivali.",
        "CIIINDULULO:",
        "Ndaboola Mwami, Ndaboola lino Ndisanzye,",
        "Mubulowa ubo bwa Kalivali.",
        "2",
        "Kunyina nce jisi, Ulakundiyumya, bubi bwangu",
        "Ubusanzye bube busalala.",
        "3",
        "Ngu Jesu undiita mu luyando lwakwe,",
        "Kuti ndi musyome bwini Mulengi wa zyoonse.",
        "4",
        "Tulumbe bulowa, luse lufutusya,",
        "Cipego Klistu watupa, Tube mbuli nguwe."
      ],
      "number": 1
    }
  ]
}