  "title": "250: TANTAMUKA KU MASUNKO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 66AH 468CH 498",
        "250TANTAMUKA KU MASUNKO",
        "YIELD NOT TO TEMPTATION",
        "Doh is Bb",
        "1",
        "Ta ntamuka sunko mubi naita,",
        "Kula kukugwasya kuleta bamwi,",
        "Kolwanisya lyonse tanda musinze,",
        "Langa kuli Jesu ula kuvuna.",
        "CIINDULULO:",
        "Jesu ula kugwasya,",
        "Ula kukupa nguzu",
        "Ulayanda kugwasya ulakukuvuna.",
        "2",
        "Kozwa kubabisyi kusampaula,",
        "Izina lya Mwami lilemekede,",
        "Koba usyomeka a moyo onse,",
        "Langa kuli Jesu ulakuvuna.",
        "3",
        "Ulapa musyini kuli bakoma,",
        "Mu lusyomo lwakwe tusiye zibi,",
        "Mufutuli wesu ulatuyumya",
        "Langa kuli Jesu ula kuvuna."
      ],
      "number": 1
    }
  ]
}