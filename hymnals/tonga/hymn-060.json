{
  "number": 60,
  "language": "to",
  "title": "60: JESU NDIWE KU KOMANA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 452AH 128CH 154SDAH 242",
        "60JESU NDIWE KU KOMANA",
        "JESUS THOU JOY OF LOVING HEARTS",
        "Doh is A",
        "1",
        "Jesu ndiwe ku komana",
        "Ndiwe mukalo wa bumi",
        "Iwe ndiwe mu muni wanyika",
        "Twa boola kuli ndiwe",
        "2",
        "Jesu nje nsima ya buumi",
        "Itupa ibuumi bwesu",
        "Tunywe mutinti lyako",
        "O Mwami Kuti nyota imane",
        "3",
        "Tula mu yandisya Jesu",
        "Oko koonse anko tu kala",
        "Tuya ku komana ayebo",
        "Na twa kujatilila",
        "4",
        "Kokala aswebo Jesu",
        "Nzila zyesu zimwekesye",
        "Kogusya mudima munzila",
        "A nyika isalale."
      ],
      "number": 1
    }
  ]
}