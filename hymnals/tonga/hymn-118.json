{
  "number": 118,
  "language": "to",
  "title": "118: WANDI ULILA KONSE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 107AH 119CH 527SDAH 184",
        "118WANDI ULILA KONSE",
        "JESUS PAID IT ALL",
        "Doh is Eb",
        "1",
        "Ndamvwa Jesu wamba, Tojisi inguzu;",
        "Mwana kopaila, Ndila kupa konse.",
        "CIINDULULO:",
        "Wa ndi ulila, Konse nku kwakwe;",
        "Kubi kwa kandibisya, Waka ndisalazya.",
        "2",
        "Mwami ndila bona kuti nguzu zya ko,",
        "Zigusya cinsenda, Zila bombya moyo.",
        "3",
        "Mbo nta jisi cintu, Cakuula luyando;",
        "Ndila kuli sanzya, Mu bulowa bwakwe.",
        "4",
        "Kumbele kwa Mwami, Nendi ya kuima;",
        "Konse nko ndasala, Kula kuba kwakwe."
      ],
      "number": 1
    }
  ]
}