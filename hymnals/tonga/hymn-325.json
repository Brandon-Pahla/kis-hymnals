{
  "number": 325,
  "language": "to",
  "title": "325: BALI IFUKA TEEBAKA LUMBA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "325BALI IFUKA TEEBAKA LUMBANINE DID NOT THANK THE LORD",
        "1",
        "Basicinsenda bali kumi, Bakasanzigwa bonse,",
        "Omwe buyo wakalumba Leza, Bamwi bakaya kuli?",
        "CIINDULULO:",
        "Inywe nomupaililwa mulumbe Leza,",
        "Jesu ngo musilisi mu mulumbaizye.",
        "2",
        "Nyika yakanununwa pesi mbasyonto bazumina;",
        "Baluba iwaka bafwida, Inzubo zyonse nene.",
        "3",
        "Kuti akatufutusye Jesu, Baambile benzyinyoko;",
        "Yeya kubisya watazyoka, Kulumbaizya Jesu.",
        "4",
        "Utabi mbuli bali fuka, Mube mbuli mubombe;",
        "Ula kupailila sunu, Ula ubya mukuli."
      ],
      "number": 1
    }
  ]
}