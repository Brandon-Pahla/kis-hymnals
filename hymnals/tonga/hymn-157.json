  "title": "157: CIINDI CA KUTEBULA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 588CH 627",
        "157CIINDI CA KUTEBULA",
        "HARVEST TIME",
        "Doh is Eb",
        "1",
        "Oyo uunka kalila, Mu luyando ula syanga;",
        "Takatali takaoni, Luse luzwa ku julu.",
        "CIINDULULO:",
        "Kolanga maila &#212;tuba, Micelo yalibonya,",
        "Kolanga myuunda ya bizwa, Kutebula kwa swena.",
        "2",
        "Mume wa julu wa loka, Mitunga zuba ya mweka;",
        "Micelo tulaipegwa, Kwiinda mu nguzu zyakwe.",
        "3",
        "Utakatali kosyanga, Utapenzegwi &#212;kuyo&#213;wa;",
        "Sebenza utakatali, Uya kukomanisya."
      ],
      "number": 1
    }
  ]
}