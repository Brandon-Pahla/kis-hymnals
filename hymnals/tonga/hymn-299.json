  "title": "299: LYOONSE KU KOMANA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 284299LYOONSE KU KOMANA",
        "ALWAYS CHEERFUL",
        "Doh is E",
        "1",
        "Myoyo yesu ikomane, Tutabi a kuyowa;",
        "Taata wesu wa luyando, Watu tambula kwakwe.",
        "CIINDULULO:",
        "Tukomane tukomane, Kusalala twabona,",
        "Bubotu mbwatucitila, Tukomane ku Mwami.",
        "2",
        "Janza lyakwe sololela, Notunjila mucibi,",
        "Watu syomya luse lwakwe, Cisyomyo citamani.",
        "3",
        "Notwasiya imulimo, Twanjila mu kubisya",
        "Acimvule catuvumba, Tatu kaimbi nyimbo.",
        "4",
        "Basalala balaseka, Nzila zyabo zibotu,",
        "Atuyiye ijwi lyakwe, Tusiye zibi zyonse."
      ],
      "number": 1
    }
  ]
}