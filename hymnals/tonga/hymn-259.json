  "title": "259: HENA TUYA KUSWAANGANA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 921AH 431CH 557",
        "259HENA TUYA KUSWAANGANA",
        "SHALL WE MEET?",
        "Doh 1",
        "Tula kuswana mulonga, Kuli kuumuna lyonse,",
        "Kuli kusalala lyonse, Myoyo ilakomana.",
        "CIINDULULO:",
        "Tula ku swangana, Kumulonga wa kujulu,",
        "Tula kuswana&#213;mulonga, Kuli kuumuna lyonse.",
        "2",
        "Tula kuswana acito, Lwendo lwesu lwamana,",
        "Ula tulindila Mwami, Kwitala amulonga.",
        "3",
        "Tula kuswana ku munzi, Manda alamwekesya,",
        "Bwanda bwa munzi mbubotu, Bwakayakwa anguwe.",
        "4",
        "Tula kuswana a Klistu naya, Kutola bakwe,",
        "Tukazyibe luse lwakwe, Tukakale anguwe."
      ],
      "number": 1
    }
  ]
}