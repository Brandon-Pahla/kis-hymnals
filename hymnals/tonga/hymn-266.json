  "number": 266,
  "language": "to",
  "title": "266: JESU NDILA KUCILILA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 254AH 444CH 429",
        "266JESU NDILA KUCILILA",
        "JESUS I WILL FOLLOW THEE",
        "Doh is Ab",
        "1",
        "Jesu ndakucilila, Ndamvwa kuita kwako,",
        "Ndabola kuli ndiwe, Nditole kumunzi.",
        "CIINDULULO:",
        "Ndila cilila, Ndila kuunka,",
        "Ndiku cilile konse nkoya.",
        "2",
        "Meso angu taboni, Maulu aswekede,",
        "Nandiba mubisyi Ndiyume muli ndiwe.",
        "3",
        "Munzila zyonse zyangu, Amuzibi ndi gusye,",
        "Kondiyumya mu moyo, Kube kutamani."
      ],
      "number": 1
    }
  ]
}