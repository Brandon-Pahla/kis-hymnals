  "title": "178: KLISTU WA PILIKA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 867AH 402CH 536",
        "178KLISTU WA PILIKA",
        "CHRIST RETURNETH",
        "Doh is D",
        "1",
        "Mafumofumo buzuba bwa talika;",
        "O kusalala nokumwaya mudima,",
        "Jesu &#212;yoboola mubulemu bu mweka,",
        "Kutambula ibana bakwe.",
        "CIINDULULO:",
        "O Mwami ndilili? Lili? Notuya kuimba,",
        "Wa piluka Aleluya, Aleluya Ameni,",
        "Aleluya Ameni.",
        "2",
        "Na masikati nokuba mangolezya;",
        "Nokuba konse musinze wa masiku,",
        "Uya kutuba mu bulemu bwa Mwami,",
        "Jesu naya kucinga bakwe.",
        "3",
        "Naaseluke makam &#212;ala mwiimbila,",
        "Basetekene abangele antoomwe,",
        "Aluse ku busyu bwakwe mu bulemu",
        "Jesu naya kucinga bakwe.",
        "4",
        "Tukomanisye twajanwa tucipona;",
        "Taaku kuciswa, Kuusa naa kulila,",
        "Mukumbi tumutambule mu bulemu,",
        "Jesu naya kucinga bakwe."
      ],
      "number": 1
    }
  ]
}