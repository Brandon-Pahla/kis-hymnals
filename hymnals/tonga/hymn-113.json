  "title": "113: ITA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 76AH 181CH 563SDAH 287",
        "113ITA",
        "CALLING",
        "Doh is Ab",
        "1",
        "Ulakombelezya Jesu siluse, Boola iwe a ndime,",
        "Atumutambule mumyoyo yesu, Ulatukombelezya.",
        "CIINDULULO:",
        "Boola&#201;.Boola, Inywe nomukatede",
        "Boola Boola",
        "Jesu Mufutuli Ulamuita, Uita babi boola.",
        "2",
        "Tulindilanzi Jesu ulaita, Ulatuita toonse,",
        "Tumukilanzi kutambula luse, luse lwa Mufutuli;",
        "3",
        "Ciindi camana katulibambila, Tulibambile toonse,",
        "Izuba lyabbila lufu lwaswena, Tutafwi mucibi.",
        "4",
        "Ko yeya luyando ndwakasyomezya, Kuli ndiwe andime,",
        "Nakuba twakabisya ulatwita, Ulatuengelela."
      ],
      "number": 1
    }
  ]
}