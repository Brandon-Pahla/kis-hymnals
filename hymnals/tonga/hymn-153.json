  "language": "to",
  "title": "153: SUNU LYALO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 563AH 270CH 604",
        "153SUNU LYALO",
        "JUST FOR TODAY",
        "Doh is G",
        "1",
        "Tali mapenzi aciza nce paila,",
        "Kondigusya zibi zyonse, sunu pele,",
        "Ndisale majwi akwaamba, Ziindi zyonse,",
        "Sunu lyalo ndi jale mulomo wangu.",
        "2",
        "Ndiya kukubelekela akukomba,",
        "Kube luse mumilimo, Sunu pele;",
        "Ndi cite luyando lwako, Nditelele,",
        "Sunu lyalo ndizumine, Ndibe wako.",
        "3",
        "Nakuti O buumi bwangu, Bulamana,",
        "Muzisyomyo zyako zyonse, Ndijanike,",
        "Tali mapenzi a ciza, Nce paila;",
        "Sunu lyalo ndisololela, O Mwami."
      ],
      "number": 1
    }
  ]
}