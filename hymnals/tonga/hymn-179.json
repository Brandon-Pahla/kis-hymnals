  "number": 179,
  "language": "to",
  "title": "179: LANGA NACIBINDE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "CS 868179LANGA NACIBINDE",
        "BEHOLD THE BRIDEGROOM",
        "Doh is A",
        "1",
        "Walibamba Nacibinde, Nabola nabola?",
        "Wali bamba Nacibinde, Nabola nabola?",
        "Ula kubola, Ula kubola,",
        "Amu libambile ula kuboola.",
        "CIINDULULO:",
        "Langa Nacibinde uya kubola,",
        "Langa Nacibinde uya kubola,",
        "Uya kubola, Uya kubola,",
        "Amu libambile Uya kuboola.",
        "2",
        "Malambe enu ayake, Nabola, nabola;",
        "Malambe enu ayake, Nabola, nabola,",
        "Ula fwambana, Ula fwambana",
        "Amu libambile mwe Nacibinde.",
        "3",
        "Tuya kumucinga tonse, Nabola, nabola,",
        "Tuya kumucinga tonse, Nabola, nabola,",
        "Ula kubola ula kubola,",
        "Tuya kumucinga swe Nacibinde.",
        "4",
        "Atuimbe haleluya, Nabola, nabola,",
        "Atuimbe haleluya, Nabola, nabola,",
        "Langa wabola, langa wabola,",
        "Imba haleluya kwa Nacibinde."
      ],
      "number": 1
    }
  ]
}