  "title": "319: MUNZI WA GOLINDE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "319MUNZI WA GOLINDETHE GOLDEN HOME",
        "1",
        "Kuli munzi mubotu, Ula tulindila;",
        "Bulemu bwawo tabwaambiki,",
        "Kuya masamu taayumi akuyuma,",
        "Ku munzi uyo wa golide",
        "CIINDULULO:",
        "Zuba talibbili, masamu tayumi,",
        "Kuya basyomekede ku munzi kuya,",
        "Ku munzi uyo wa golide.",
        "2",
        "Bantu boonse ibalanga ku Ciingano;",
        "Ku mwana a Mbelele waka fwa,",
        "Baya kuba ma Bbwe akwe amwekesya;",
        "Ku munzi uyo wa golide."
      ],
      "number": 1
    }
  ]
}