{
  "number": 19,
  "language": "xh",
  "title": "19 Linda Mphefumlo",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Linda mphefumlo wam,",
        "U-Tshaba luzile;",
        "Imikhosi kasathana",
        "Iyakuzulela."
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "O! Lind’ uthandaze,",
        "Uze udl’ ilifa;",
        "Ndibonise inyaniso",
        "Kwa nempilo entsha."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "O! yilwa nomlingi-",
        "Ndiphathise Yesu;",
        "Ndakuyeka ukukhala",
        "Ndijonge phezulu."
      ],
      "number": 3
    }
  ]
}