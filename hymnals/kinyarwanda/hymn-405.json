  "title": "405 Mpa Umugisha Waw'Ubu",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 405 Mpa Umugisha Waw'Ubu",
        "**1**",
        "Mbe Data wo mw'ijuru,",
        "Ngwin'umpire rwos'ubu;",
        "Umbabarir'ibyaha,",
        "Unkiz'ibicumuro.",
        "**Gusubiramo:**",
        "Data wo mw'ijuru,",
        "Mp'umugisha waw'ubu",
        "**2**",
        "Nkeney'ubuntu bwawe,",
        "Nyongerer'imbaraga.",
        "Ndi kwig'Ijambo ryawe,",
        "Ngwino rwos'umpir'ubu.",
        "**3**",
        "Yesu nkiz'umvaneho",
        "imbogamizi zose.",
        "Ni wowe ndangamiye,",
        "Nkorah'unyez'ibyaha.",
        "**4**",
        "Nicishije bugufi,",
        "Mpfukamy'imbere yawe.",
        "Yesu ngwin'unkiz'ubu,",
        "Wumve gutaka kwanjye."
      ],
      "number": 1
    }
  ]
}