  "title": "147 Data Mwiza, Nyigiza Hafi",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 147 Data Mwiza, Nyigiza Hafi",
        "**1**",
        "Data mwiza, nyigiza hafi;",
        "Nkube mu gituza;",
        "Ni wowe buhungiro bwanjye,",
        "N'uburuhukiro.",
        "**Gusubiramo:**",
        "**S.na A:**",
        "Data, nyigiza hafi,",
        "Ndifuza ko tubana;",
        "Data, ndifuza ko tubana.",
        "**T. na B:**",
        "Data, data, nyigiza hafi,",
        "Ndifuza cyane ko tubana;",
        "Data, nyigiza hafi;",
        "ndifuza ko tubana,",
        "Ndifuza ko tubana.",
        "**2**",
        "Data mwiza, nyigiza hafi;",
        "Nkomeza ntundeke;",
        "Nakwishimira ko tubana,",
        "Ne kuraragira.",
        "**3**",
        "Data mwiza, nyigiza hafi;",
        "Ngez'ubwo nsa nawe;",
        "Mpembur'unyuhagir'unyeze,",
        "Mb'umuziranenge."
      ],
      "number": 1
    }
  ]
}