  "title": "387 Iyo Ngukoreye Yesu",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 387 Iyo Ngukoreye Yesu",
        "**1**",
        "Iyo ngukoreye Yesu,",
        "Biranezeza;",
        "Iri turo ntuye,",
        "Mwami rikunezeze.",
        "**Gusubiramo:**",
        "Nguhay'ubugingo bwanjye,",
        "Bwakire Yesu",
        "Kubw'ubuntu bwawe Mwami,",
        "Undind'ibibi.",
        "**2**",
        "Warancunguye Mukiza,",
        "Mb'uwawe rwose.",
        "None rer'umbashishe,",
        "Gukor'iby'ushaka.",
        "**3**",
        "Ungir'umumarayika",
        "wihuta cyane;",
        "Njyan'Ijambo ryawe hose,",
        "Mvug'ubutumwa.",
        "**4**",
        "Rwose kuzaba mw'ijuru,",
        "Ni byo nifuza;",
        "Nta byago nta n'amakuba",
        "bizantezura."
      ],
      "number": 1
    }
  ]
}