  "title": " 96 Nshim'Isabato Yera",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 96 Nshim'Isabato Yera",
        "**1**",
        "Nshim'Isabato yera,",
        "Imana yaduhaye.",
        "Ngo tujye turuhuka,",
        "Tuwunezererwemo.",
        "**2**",
        "N'umuns'ubamw'ituza,",
        "Uhoramw'ibyishimo.",
        "Biduhuz'agahinda,",
        "Kugeza ku mperuka.",
        "**3**",
        "N'umuns'uduhimbara,",
        "Tukawizihirwamo,",
        "Twongerwamw'imbaraga,",
        "Tukuzuzw'umugisha.",
        "**4**",
        "Kaze Sabato nziza,",
        "Munsi w'ikiruhuko,",
        "Duhabwamw'umugisha,",
        "Tukamererwa neza."
      ],
      "number": 1
    }
  ]
}