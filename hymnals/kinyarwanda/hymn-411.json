  "title": "411 Nkeneye Umukiza",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 411 Nkeneye Umukiza",
        "**1**",
        "Nkeney'Umukiza Yesu,",
        "Nshaka kumwigiraho;",
        "Nkager'ikirenge mu cye,",
        "Nca mu nzir'ifunganye.",
        "**Gusubiramo:**",
        "Arankunda, Arankunda,",
        "Yes'arankunda cyane!",
        "Yarapfuy'ari jy'azira;",
        "Ni yo mpamvu mukunda.",
        "**2**",
        "Nzajy'ahw'azantuma hose,",
        "Nzahora niteguye.",
        "Sindi muto byatuma ntabasha",
        "kujy'ahw'antuma.",
        "**3**",
        "Dor'ahagaze ku rugi,",
        "Kand'arampamagara.",
        "Ngo musange anyakire,",
        "Ankiz'ibyaha byanjye."
      ],
      "number": 1
    }
  ]
}