  "title": "141 Mwami Mb'Uwawe Maramaje",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 141 Mwami Mb'Uwawe Maramaje",
        "**1**",
        "Mwami, mb'uwawe maramaje,",
        "Nyigisha, nyigisha.",
        "Nkor'iby'ukunda,",
        "Mwami wanjye;",
        "Komez'umfashe.",
        "**Gusubiramo:**",
        "Mb'uwawe (Mwami) ;",
        "Mb'uwawe (Mwami) ;",
        "Niyo migambi yanjye ;",
        "Mb'uwawe (Mwami);",
        "Mb'uwawe (Mwami) ;",
        "Mb'uwawe Mwami, None.",
        "**2**",
        "Iby'iyi si byamarir'iki,",
        "Ntari kumwe nawe?",
        "Byose nzabihara kubwawe,",
        "Ntungishwe nawe.",
        "**3**",
        "Ndets'umunezero w'iyi si,",
        "Ngwino twibanire;",
        "Nibyo bizajya binogera,",
        "Bimpumurize."
      ],
      "number": 1
    }
  ]
}