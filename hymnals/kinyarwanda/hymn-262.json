  "title": "262 Nyir'Impuhwe Nyinshi",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 262 Nyir'Impuhwe Nyinshi",
        "**1**",
        "Nyir'impuhwe nyinshi cyane,",
        "Iman'ibabarira,",
        "Kwibagirw'ibyaha",
        "ikababarir'uwagomye.",
        "**Gusubiramo:**",
        "**S na A:**",
        "Ndaje Yesu, Ndakwitabye,",
        "Uko ndi kos'unyemere,",
        "Ndaje Yesu ndakwitabye,",
        "Mukiz'unyakire.",
        "**S na A:**",
        "Ndaje Yesu, Ndakwitabye,",
        "Uko ndi ko.. unyemere,",
        "Ndaje Yesu, Ndakwitabye,",
        "Mukiz'unyakire.",
        "**2**",
        "Nari kure y'uwo nkunda,",
        "Mbabaj'umutima we,",
        "Kuko namwimagahw'aba,",
        "N'uwanjye yamfiriye.",
        "**3**",
        "Yesu ndakwiyeguriye,",
        "Ndashaka kubaturwa,",
        "Icyaha cyose cyambase,",
        "Maze ngikire rwose.",
        "**4**",
        "Haleluya yanduhuye,",
        "Ubu sinshidikanya,",
        "Nderekez'iyo mw'ijur,",
        "Nejejwe no kubayo."
      ],
      "number": 1
    }
  ]
}