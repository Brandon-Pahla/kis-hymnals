  "title": "420 Yesu Ngwino Ubane N'Itorero Ryawe",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 420 Yesu Ngwino Ubane N'Itorero Ryawe",
        "**1**",
        "Yesu ngwin'ubane",
        "n'itorero ryawe ryose.",
        "Rifashe, ribashe",
        "gutsind'ibigeragezo.",
        "**2**",
        "Ribashe gutunganya",
        "nez'inshingano zaryo;",
        "Riburir'abo mw'isi,",
        "Bitegur'urubanza.",
        "**3**",
        "Ryite ku batabona,",
        "N'abakene bo mw'isi;",
        "Rihabur'abahabye,",
        "Ribabwir'ibya Yesu.",
        "**4**",
        "Ryuzur'Umwuka Wera,",
        "Maze rinesh'icyaha,",
        "Rihuze mw'isi yose,",
        "Yesu turakwinginze."
      ],
      "number": 1
    }
  ]
}