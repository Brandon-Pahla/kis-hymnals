  "title": "467 Turi Ab'Umwami Yesu",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 467 Turi Ab'Umwami Yesu",
        "**1**",
        "Ntabwo tur'abacu,",
        "Tur'ab'Umwami Yesu.",
        "Yesu ni w'utugenga,",
        "Twaramwiyeguriye;",
        "N'aho rwab'urupfu,",
        "Tuzamunambaho rwose;",
        "Umusaraba we,",
        "Ni wo tuzirata!",
        "**2**",
        "Icyubahiro cye,",
        "Kitugaragaremo;",
        "Dupfe ku cyaha cyose,",
        "Duharanir'ijuru.",
        "Baragwa b'ijuru,",
        "Twer'imbuto z'abihannye;",
        "Mureke twibere",
        "mu Man'Ihoraho.",
        "**3**",
        "Tw'ab'Umwami Yesu,",
        "Ntitugacik'intege;",
        "N'iyo twaba mu kaga",
        "cyangwa mu munezero;",
        "Turi kumwe na we",
        "nta kabuza tuzanesha!",
        "Azabana natwe",
        "ntazaturekura!"
      ],
      "number": 1
    }
  ]
}