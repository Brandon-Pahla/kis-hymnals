  "title": "137 Isi Ndayihararutse, Mpisemo Yesu",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 137 Isi Ndayihararutse, Mpisemo Yesu",
        "**1**",
        "Isi ndayihararutse,",
        "Mpisemo Yesu gusa.",
        "Mpaz'umunezero wayo,",
        "Uzashirana nayo.",
        "**Gusubiramo:**",
        "Imbabazi zidashira,",
        "N'urukundo rw'iteka,",
        "Nibyo bijya binyemeza,",
        "Ko nzaramishwa nawe.",
        "**2**",
        "Isi ndayihararutse,",
        "Mpisemo Yesu gusa.",
        "Mu kaga niwe humure;",
        "Nzahora nezerewe.",
        "**3**",
        "Isi ndayihararutse,",
        "Mpisemo Yesu gusa.",
        "Icyamp'akamwenyurira,",
        "Nkamurikirwa nawe.",
        "**4**",
        "Isi ndayihararutse,",
        "Mpisemo Yesu gusa.",
        "Nzirat'umusaraba we,",
        "Yatangiyeho kera."
      ],
      "number": 1
    }
  ]
}