  "title": "376 Isabato Yera, Munsi W'Uwiteka",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 376 Isabato Yera, Munsi W'Uwiteka",
        "**1**",
        "Isabato yera,",
        "Munsi w'Uwiteka;",
        "Duhabw'amahirwe,",
        "Yo kumv'Ijambo ry'Imana.",
        "**2**",
        "Mbeg'umunsi mwiza,",
        "Umunsi w'Imana!",
        "Ukwiye kubahwa rwose,",
        "tukaramy'Imana.",
        "**3**",
        "Abera b'ijuru",
        "baterana natwe;",
        "Tugafatanya guhimbaz'",
        "Umuremyi wacu.",
        "**4**",
        "Ubwo tuzava mw'iyi",
        "si y'igomero;",
        "Tuzakomereza kwez'",
        "Isabato mw'ijuru."
      ],
      "number": 1
    }
  ]
}