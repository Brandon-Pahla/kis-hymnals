  "title": "252 Nimucane Amatabaza",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 252 Nimucane Amatabaza",
        "**1**",
        "Nimucan'amatabaza,",
        "Umwijim'ubay'icyago,",
        "Hazab'umwijim'ukaze,",
        "Umwam'ataragaruka.",
        "**Gusubiramo:**",
        "Mubonez'amatabaza,",
        "Kandi nimwubah'Imana,",
        "Kuza kw'Umwami kuri hafi,",
        "Mubonez'amatabaza.",
        "**2**",
        "Abant'ibihumbi byinshi,",
        "Bahinyur'ubu butumwa,",
        "Natwe tubushikamyemo,",
        "Dutegerez'Umukiza.",
        "**3**",
        "Ijambo rye n'itabaza,",
        "Ntituzigera tuyoba,",
        "N'ubw'amakub'ari hose,",
        "Ntabw'Iman'izaduhana.",
        "**4**",
        "Dukor'imirimo myiza,",
        "Twe guh'ab'iy'is'urwaho,",
        "Tuzabiherw'imigisha,",
        "Nidukomeza kwizera."
      ],
      "number": 1
    }
  ]
}