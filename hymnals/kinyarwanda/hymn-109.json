  "title": "109 Naratuwe Mu Gipimo",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 109 Naratuwe Mu Gipimo",
        "**1**",
        "Umucamanza n'agera,",
        "Akarondor'ibyacu,",
        "Tuzamera nk'izahabu,",
        "Cyangwa s'inkamb'itabwa?",
        "**Gusubiramo:**",
        "Naratuwe mu gipimo,",
        "Nsangwa ntashyitse;",
        "Naratuwe n'ijambo rye,",
        "Nsangwa ntashyitse.",
        "**2**",
        "Mbese tuzumv'ishimwe ngo:",
        "Nuko, wakoze neza;",
        "Cyangw'iteka ryo gupfa ngo:",
        "Mwasanzwe mudashyitse?",
        "**3**",
        "Nimutyo twite kw'ijwi rye,",
        "Igihe kikiriho.",
        "Tureke gutindiganya,",
        "Tumaramaze rwose."
      ],
      "number": 1
    }
  ]
}