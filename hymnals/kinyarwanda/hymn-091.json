  "title": " 91 Wicogora Muri Gihe",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 91 Wicogora Muri Gihe",
        "**1**",
        "Wicogora mur'iki gihe,",
        "Ba maso n'imperuka.",
        "Rind'udatsindwa n'iby'iyi si,",
        "Mukristo jy'usaba.",
        "Uz'uk'umubir'uremba,",
        "Kand'umwanz'arakaze.",
        "Utegereze nyir'ubukwe,",
        "Ba mas'utarambirwa.",
        "**Gusubiramo:**",
        "Komez'ujy'usaba,",
        "Komez'ujy'usaba.",
        "Jya wirind'iteka",
        "ku manywa na n'ijoro,",
        "Ntugahweme gusaba.",
        "**2**",
        "Tsemb'ibishukw'imbere yawe,",
        "Rek'imitima myinshi.",
        "Uzaruhukira mw'ijuru,",
        "Nihw'uzaturiza.",
        "Jy'uhor'usaba Mukristo,",
        "Umwam'arakwiteze.",
        "Ibuk'uko yaguhorewe,",
        "Bamuvush'amaraso.",
        "**3**",
        "Ntugahweme kwambaza Yesu,",
        "Jy'ubana naw'iteka",
        "Ntugahug'ibitum'utsinda,",
        "Tegerez'Umwami.",
        "Wirushy'uzuyaza rwose,",
        "Reka gushidikanya.",
        "N'igihe cyo kumwitegura,",
        "Ntugahweme kwambaza."
      ],
      "number": 1
    }
  ]
}