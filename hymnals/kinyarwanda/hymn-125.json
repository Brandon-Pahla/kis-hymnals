  "title": "125 Nk'Ukw'Izuba Rituvira",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 125 Nk'Ukw'Izuba Rituvira",
        "**1**",
        "Nkukw'izuba rituvira,",
        "Rikarabagirana,",
        "Nik'umucyo wo mw'ijuru",
        "wansabye mu mutima,",
        "Wawukuyemw'iby'amanjwe,",
        "Utangaz'ibihishwe;",
        "Kuko mu mutima wanjye,",
        "Nahatuj'Umukiza.",
        "**Gusubiramo:**",
        "Mbeg'umucyo w'agatangaza,",
        "Wansabye mu mutima!",
        "Ineza y'Umwami Yesu,",
        "Yambereye nk'umucyo.",
        "**2**",
        "Natay'agahinda kose;",
        "Ntabwo nkikitayeho;",
        "Kuk'unyambik'amahoro,",
        "Ameze nk'umwitero,",
        "Nkwerekejeh'umutima,",
        "Ndakwishimiye cyane.",
        "Nzagezw'i wawe mw'ijuru,",
        "Na rwa rukundo rwawe.",
        "**3**",
        "Mukiza warancunguye;",
        "Nd'uwaw'uko ndi kose.",
        "Nyemerera nguhimbaze,",
        "Nkuber'umucyo waka.",
        "Nkwerekejeh'umutima,",
        "Ndakwishimiye cyane.",
        "Nzagezw'i wawe mw'ijuru,",
        "Na rwa rukundo rwawe."
      ],
      "number": 1
    }
  ]
}