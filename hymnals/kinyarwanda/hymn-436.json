  "title": "436 Yewe Nzimizi We, Garuka",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 436 Yewe Nzimizi We, Garuka",
        "**1**",
        "Yewe nzimizi we, garuka;",
        "Umubyey'aragushaka.",
        "N'Umubyeyi ugukunda,",
        "Kand'aragutegereje.",
        "Ngwino! Ngwino!",
        "**2**",
        "Yewe nzimizi we, garuka;",
        "So aguteze amatwi.",
        "Yita cyane ku bafite",
        "imitima imenetse.",
        "Ngwino! Ngwino!",
        "**3**",
        "Yewe nzimizi we, garuka;",
        "Umukiz'aragushaka.",
        "Az'intege nkeya zawe,",
        "Jya ku birenge bya Yesu.",
        "Ngwino! Ngwino!",
        "**4**",
        "Yewe nzimizi we, garuka;",
        "Uhanagurw'amarira.",
        "Ni So uguhamagara;",
        "Kand'aragukunda cyane.",
        "Ngwino! Ngwino!"
      ],
      "number": 1
    }
  ]
}