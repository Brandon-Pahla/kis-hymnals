  "title": "442 Ugururira Yesu",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 442 Ugururira Yesu",
        "**1**",
        "Ugururira Yesu araguhamagara,",
        "Ahagaze ku rugi umukingurire.",
        "**Gusubiramo:**",
        "**Sop:**",
        "Mwugururire, Mwugururire.",
        "Yaje kugucungura,",
        "Tyo mwugururire.",
        "**Alto:**",
        "Mwuguru… Mwugururire,",
        "Mwuguru… Mwugururire;",
        "Yaje kugucungura,",
        "Mwuguru… Mwugururire.",
        "**Tenor:**",
        "Mwugururire, Mwugururire;",
        "Mwugururire, Mwugururire.",
        "Yaje kugucungura,",
        "Tyo mwugururire, Mwugururire.",
        "**Bass:**",
        "Mwugururire, Mwugururire;",
        "Mwugururire, Mwugururire.",
        "Yaje kugucungura,",
        "Mwugururire …rire.",
        "**2**",
        "Kugira ngw'agukize",
        "Yatanz'ubugingo bwe;",
        "Yatsinz'urupfu,",
        "None mwemer'umwakire.",
        "**3**",
        "Mwumv'aracyakomanga,",
        "Mwiyegurire rwose.",
        "Kingur'umutima",
        "Waw'umwiyegurire.",
        "**4**",
        "Tyo reka kuzarira",
        "Wikwitesh'amahirwe.",
        "Wikwitesh'agakiza",
        "Ashaka kuguha."
      ],
      "number": 1
    }
  ]
}