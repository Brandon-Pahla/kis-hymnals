  "title": "117 Mwami Ndumv'Ijwi Ryawe",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 117 Mwami Ndumv'Ijwi Ryawe",
        "**1**",
        "Mwami ndumv'ijwi ryawe,",
        "Rindarika ngo nze,",
        "Nuhagizwe ya maraso,",
        "Wamviriye kera.",
        "**Gusubiramo:**",
        "Mukiza ndaje,",
        "Ubu nj'ah'uri!",
        "Nyejesha ya maraso,",
        "Wamviriye kera.",
        "**2**",
        "N'ubwo nje nahindanye,",
        "Ntunyirengagize;",
        "Ahubw'umboneze rwose,",
        "Mb'umweru waka de.",
        "**3**",
        "Yesu niw'unkomeza,",
        "Niw'umpa gutsinda;",
        "Ng'urukundo n'amahoro,",
        "Binsabemo rwose.",
        "**4**",
        "Mwese nimwishimire!",
        "Ya maras'akiza,",
        "Yatwujuje na Rurema,",
        "Ngo tub'abana be."
      ],
      "number": 1
    }
  ]
}