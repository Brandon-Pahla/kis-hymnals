{
  "number": 455,
  "language": "rw",
  "title": "455 Ndumv’Ijwi Rinyongorera",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 455 Ndumv'Ijwi Rinyongorera",
        "**1**",
        "Har'ijwi rya Marayika,",
        "Rimbwira ryongorera.",
        "Iryo jwi rirankomeza,",
        "Rinter'ibyiringiro.",
        "N'ubw'ijoro ryegereje,",
        "N'umurab'ar'icyago;",
        "Bigiye gutamuruka,",
        "Maz'umucy'utangaze.",
        "**Gusubiramo:**",
        "**Sop:**",
        "Ijwi numva,",
        "Rinter'ibyiringiro;",
        "Rinter'ibyishimo,",
        "Mu – mu mutima.",
        "**Alto:**",
        "Ijwi numva, …jwi numva ry'ihozo,",
        "Rintera ibyiri – byiringiro;",
        "Rinter'ibyi – byishimo pe!",
        "…shimo mu mutima.",
        "**Tenor na Bass:**",
        "Ijwi numva – mva ry'ihozo,",
        "Rintera ibyiri – byiringiro;",
        "Rinter'ibyi – byishimo pe!",
        "…shimo mu muti… mu mutima.",
        "**2**",
        "No mu gihe cy'akabwibwi,",
        "Umucy'urengarenga;",
        "Inyenyer'ifit'umucyo,",
        "Izamurika cyane.",
        "Ijoro niribudika,",
        "Ntugir'ubwoba rwose;",
        "Hasigay'igihe gito,",
        "Umucy'ugatangaza.",
        "**3**",
        "Ibyo byiringiro bitsika",
        "mu mutima wanjye;",
        "Bitamurur'umwijima,",
        "Umucy'ugatangaza!",
        "Iy'Umwami yimye mu",
        "mutima waw'ushavuye,",
        "Ibyiringiro bishyitse",
        "biguma mu mutima."
      ],
      "number": 1
    }
  ]
}