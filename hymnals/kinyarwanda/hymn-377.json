  "title": "377 Shuri ry'Isabato Nkunda",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 377 Shuri ry'Isabato Nkunda",
        "**1**",
        "Shuri ry'Isabato nkunda,",
        "Munezero wanjye;",
        "Unyongeramw'imbaraga;",
        "Mu rugamba ndwana.",
        "**Gusubiramo:**",
        "Mvugisha Mwami Yesu,",
        "Nyongera kwizera.",
        "Mw'ishuri ryo kw'Isabato;",
        "Ntabwo nzacogora.",
        "**2**",
        "Nig'ibyigisho by'ingenzi,",
        "Umunsi ku wundi.",
        "Bituma nsobanukirwa,",
        "Ibyiza by'ijuru.",
        "**3**",
        "Mw'ishuri ryo kw'Isabato,",
        "Twiga Bibiliya;",
        "Tugahabw'amabang'",
        "Atuma twizera cyane."
      ],
      "number": 1
    }
  ]
}