  "title": "483 Nd'Umukozi Mutoya",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 483 Nd'Umukozi Mutoya",
        "**1**",
        "Nd'umukozi mutoya,",
        "Mwami ntiwankoresha?",
        "N'ubwo nta ntege mfite,",
        "Ndagukunda cyane pe!",
        "Mfit'akaganza gato,",
        "Yesu ugakoreshe.",
        "**Gusubiramo:**",
        "Nd'umukozi mutoya;",
        "Mwami ntiwankoresha?",
        "**2**",
        "Nigire buri munsi,",
        "Mu rukund'udukunda;",
        "Ngender'imbere yawe,",
        "Nkuririmbire Mwami;",
        "Mfat'ikiganza Mwami,",
        "Uhor'umpa gutsinda.",
        "**3**",
        "Unyigishe gukunda,",
        "Mpa kwera no gutuza;",
        "Umpe ku Mwuka wawe,",
        "Umbemo buri gihe;",
        "Mwigisha unyigishe,",
        "Kugukorera neza."
      ],
      "number": 1
    }
  ]
}