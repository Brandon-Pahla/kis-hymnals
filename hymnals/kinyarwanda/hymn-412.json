  "title": "412 Abarembejwe N'Ibyaha",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 412 Abarembejwe N'Ibyaha",
        "**1**",
        "Abarembejwe n'ibyaha,",
        "Musang'Umukiza,",
        "Arabaruhura,",
        "Nimumwiringira rwose.",
        "**Gusubiramo:**",
        "Mwiringire Yesu,",
        "Nimumwiringire rwose.",
        "Mwiringire Yesu,",
        "Mumwiringire mukire.",
        "**2**",
        "Yabaviriy'amaraso,",
        "Ahesh'umugisha;",
        "Mwiyuhagire mo,",
        "Arabeza nk'urubura.",
        "**3**",
        "Yesu ni we kuri,",
        "Ni we nzira y'amahoro,",
        "Nimumwizer'azabah'",
        "ubugingo bw'iteka.",
        "**4**",
        "Nimusang'abera,",
        "Maze musingize Yesu,",
        "Ni bwo muzatura,",
        "Mu gihugu cy'amahoro."
      ],
      "number": 1
    }
  ]
}