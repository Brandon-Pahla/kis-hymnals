  "title": "374 Duhimbaz'Imana Y'Abrahamu",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 374 Duhimbaz'Imana Y'Abrahamu",
        "**1**",
        "Mana y'Abrahamu,",
        "Data wo mw'ijuru;",
        "Mana y'urukundo,",
        "Uhorahw'iteka.",
        "Uwiteka Mana,",
        "Ijuru n'isi bikuramye.",
        "Izina ryawe niryubahwe,",
        "Muremyi wacu.",
        "**2**",
        "Mana y'Abrahamu,",
        "Mu biganza byawe,",
        "Ugabir'is'umunezero",
        "nyakuri.",
        "Ubwema n'ikuzo,",
        "n'ibyawe Mana yacu nziza.",
        "Uri kw'isonga,",
        "Nta wahwana nawe Gitare.",
        "**3**",
        "Twese dusingiz'Iman'",
        "Isumba byose.",
        "Icyubahiro cyose,",
        "n'icyawe Mana;",
        "Wowe Mana Data,",
        "Man'Umwana na Mwuka Wera;",
        "Nyir'ubushoboz'uhimbazwe,",
        "Iteka ryose."
      ],
      "number": 1
    }
  ]
}