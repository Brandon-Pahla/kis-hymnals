  "title": "269 Hari Igihugu Kibamo Umucyo",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 269 Hari Igihugu Kibamo Umucyo",
        "**1**",
        "Har'igihugu kibamw'",
        "umucyo urabagirana,",
        "Ntabwo habayo umwijima,",
        "Yesu n'umucyo waho.",
        "**Gusubiramo:**",
        "**S. na A.:**",
        "Twemer'umusaraba,",
        "Tuzambikw'ikamba,",
        "Dukomez'iby'ukuri,",
        "Nibwo tuzagerayo.",
        "**T. na B.:**",
        "Twemer'u ...sa...saraba,",
        "Tuzambikw'ika... ikamba,",
        "Dukome... by'u... by'ukuri,",
        "Nibwo tuzagerayo.",
        "**2**",
        "Ibihe byaho ni byiza rwose,",
        "Nta mpinduk'ibayo,",
        "Ntabwo habay'impagarara,",
        "Tuhabik'ubutunzi.",
        "**3**",
        "Mw'ijuru hariy'amazu",
        "meza arabagirana,",
        "Tuzahahurira n'inshuti,",
        "Twakiranw'ibyishimo.",
        "**4**",
        "Twifuza cyane kwimuka vuba,",
        "Mur'iyi s'ishaje,",
        "Tujye guturana n'abera,",
        "Maze dut'umuruho."
      ],
      "number": 1
    }
  ]
}