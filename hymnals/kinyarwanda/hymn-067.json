      "type": "verse",
      "lines": [
        "### 67 Abatoni B'Umwami",
        "**1**",
        "Abatoni b'Umwami!",
        "Nimuze dusakaze",
        "hos'umunezero wacu,",
        "Turangururire rimwe,",
        "Dukikij'intebe,",
        "Dukikij'intebe.",
        "**Gusubiramo:**",
        "Turajy'i Siyoni,",
        "Ku murwa mwiz'utunganye.",
        "Turajy'i Siyoni,",
        "Ku murwa mwiz'utunganye w'Imana.",
        "**2**",
        "Murek'ibyigomeke,",
        "Byanga gushim'Umwami.",
        "Arikw'abana b'Imana,",
        "Arikw'abana b'Imana,",
        "Muhore mwishima,",
        "Muhore mwishima.",
        "**3**",
        "Twishimir'i Siyoni,",
        "N'umudugudu wera;",
        "Tuzahahurira twese,",
        "Tuzahahurira twese.",
        "Hazahor'ituza,",
        "Hazahor'ituza.",
        "**4**",
        "Turirimbe cyan'ubu,",
        "Hataba n'umw'urira.",
        "Tugiye kuger'iwacu,",
        "Tugiye kuger'iwacu,",
        "Mu gihugu cyiza,",
        "Mu gihugu cyiza."
      ],
      "number": 1
    }
  ]
}