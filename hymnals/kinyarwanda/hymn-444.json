  "title": "444 Har'Icyo Nsobanukiwe",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 444 Har'Icyo Nsobanukiwe",
        "**1**",
        "Har'icyo nsobanukiwe,",
        "Yes'amp'ikiruhuko.",
        "Mwami nguy'umutima wanjye,",
        "Uwitegekere.",
        "**2**",
        "Iby'isi mbisiz'inyuma,",
        "Niberey'uwa Yesu;",
        "Ahebuj'ibintu byose,",
        "Ni w'ump'umunezero.",
        "**3**",
        "Arabagirana cyane,",
        "Akwiy'icyubahiro;",
        "Agir'urukundo rwinshi,",
        "N'imbabazi n'impuhwe.",
        "**4**",
        "Yesu yarankunze cyane,",
        "Bitum'aza kumpfira;",
        "Kubw'urukundo rw'Imana,",
        "Se kuki ntamukunda?",
        "**5**",
        "Yesu ndakwiyeguriye,",
        "Nkomeza nkunambeho;",
        "Ubuzima bwanjye bwose,",
        "Buhishwe muri wowe."
      ],
      "number": 1
    }
  ]
}