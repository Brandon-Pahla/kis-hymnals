  "title": " 46 Jy'Uhor'Undinganirije",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 46 Jy'Uhor'Undinganirije",
        "**1**",
        "Jy'uhor'undinganirije,",
        "Amagamb'atunganye.",
        "Umpugure meny'ukuri,",
        "Kw'amagambo yawe.",
        "Nkund'amagambo yawe,",
        "Kw'ampa kukwizigira.",
        "**Gusubiramo:**",
        "Amagambo meza cyane,",
        "Anter'ibyishimo byinshi;",
        "Niyo meza, ndayakunda;",
        "Ntabw'atanezeza.",
        "**2**",
        "Krist'ahet'abantu bose,",
        "Amagamb'atunganye.",
        "Nimwitabe banyabyaha!",
        "Mwumv'iby'ubugingo!",
        "Abayoboz'ituza,",
        "N'amagamb'atunganye.",
        "**3**",
        "Menyesha bos'ubutumwa!",
        "N'amagamb'atunganye!",
        "Nimuze mubabarirwe!",
        "Mwumv'iby'ubugingo!",
        "Yesu niwe Mukiza,",
        "Utwonger'ubugingo."
      ],
      "number": 1
    }
  ]
}