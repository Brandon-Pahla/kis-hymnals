  "title": "368 Kwizera Kwanjye",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 368 Kwizera Kwanjye",
        "**1**",
        "Kwizera kwanjye",
        "Kwabonye uburuhukiro;",
        "Niringiye Uhoraho",
        "Wantangiy'incungu.",
        "**Gusubiramo:**",
        "Nta yindi ncungw'ikenewe,",
        "Yesu yarishye byose.",
        "Yarapfuye, arazuka;",
        "Yaranyitangiye.",
        "**2**",
        "Nta bwoba ngifite namba!",
        "Yes'arampagije.",
        "Yankuyehw'ibyaha byose,",
        "Kubw'amaraso Ye.",
        "**3**",
        "Yesu yaje gukiz'",
        "Imitima yazimiye.",
        "Yatanz'amaraso Ye,",
        "Kugira ngo ndokoke.",
        "**4**",
        "Har'Ijambo rihindura,",
        "Kandi rimp'ituza.",
        "N'Ijambo ryiza ry'Iman'",
        "Ijambo ry'ubugingo."
      ],
      "number": 1
    }
  ]
}