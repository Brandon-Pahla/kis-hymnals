  "title": "258 Yasezeranye Kuduh'Imvura",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 258 Yasezeranye Kuduh'Imvura",
        "**1**",
        "Yasezeranye kuduha,",
        "Imvura y'umugisha,",
        "Iri sezerano ryayo,",
        "Ryaturutse mw'ijuru.",
        "**Gusubiramo:**",
        "Duh'umugisha,",
        "Turifuz'umugisha,",
        "Uvuy'iwawe mw'ijuru,",
        "Nyabuneka wuduhe.",
        "**2**",
        "Yasezeranye kuduha,",
        "Imvura y'umugisha,",
        "Yitugoborer'uyu muns'",
        "ikwir'impanda zose.",
        "**3**",
        "Yasezeranye kuduha,",
        "Imvura y'umugisha,",
        "Turayifuz'uy'umunsi,",
        "Sohoz'ibyo wavuze.",
        "**4**",
        "Yasezeranye kuduha,",
        "Imvura y'umugisha,",
        "Icyayiduh'uyu munsi,",
        "Mana tubabarire."
      ],
      "number": 1
    }
  ]
}