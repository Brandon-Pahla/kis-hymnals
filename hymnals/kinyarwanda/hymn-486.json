  "title": "486 Bivuge Ku Musozi",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 486 Bivuge Ku Musozi",
        "**Gusubiramo:**",
        "Bivuge ku musozi,",
        "Bivuge hejuru ku tununga;",
        "Bivuge ku musozi,",
        "Yuko Yesu yavutse.",
        "**4**",
        "Abungeri baragiy'",
        "amatungo yabo,",
        "Mw'ijuru haz'",
        "umucyo urabagirana.",
        "**5**",
        "Bakibon'ibyongibyo,",
        "Bamarwa n'ubwoba;",
        "Marayik'ababwira",
        "ko Yesu yavutse.",
        "**6**",
        "Babony'Umwana Yes'",
        "aryamye mu muvure,",
        "Bamutur'amaturo,",
        "Bashim'ivuka rye!"
      ],
      "number": 1
    }
  ]
}