  "title": "353 Mu Maboko Ya Yesu",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 353 Mu Maboko Ya Yesu",
        "**1**",
        "Mu maboko ya Yesu,",
        "Ndi mu gituza cye!",
        "Mpabon'umunezero,",
        "Kubw'urukundo rwe.",
        "Mbega ngo ndaririmba,",
        "Nk'abamarayika:",
        "Aho hantu heza,",
        "Ku Nyanja ya Yasipi!",
        "**Gusubiramo:**",
        "Mu maboko ya Yesu,",
        "Ndi mu gituza cye;",
        "Mpabon'umunezero,",
        "Kubw'urukundo rwe.",
        "**2**",
        "Mu maboko ya Yesu,",
        "Anyitaho rwose;",
        "Ntsind'ibigeragezo,",
        "Ni ho nesh'icyaha.",
        "Mu maboko ya Yesu,",
        "Hehe n'umubabaro!",
        "Ntabwo ngishidikanya,",
        "Yes'amar'ubwoba.",
        "**3**",
        "Yesu Mukunzi wanjye,",
        "Wowe nihishamo;",
        "Ni wowe niringira,",
        "Wowe wampfiriye.",
        "Gitare gikomeye,",
        "Ndagutegereje;",
        "Nuza ku manywa cyangwa",
        "nijor'uzanjyana!"
      ],
      "number": 1
    }
  ]
}