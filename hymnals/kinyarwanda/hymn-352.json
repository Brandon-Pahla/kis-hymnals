  "title": "352 Nitugera Mw'Ijuru",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 352 Nitugera Mw'Ijuru",
        "**1**",
        "Turirimb'urukundo rwa Yesu;",
        "Impuhwe ndetse n'ubuntu bye.",
        "Ari kudutegurir’aho tuzaba,",
        "mw’ijuru.",
        "**Gusubiramo:**",
        "Nitugera mw'ijuru,",
        "mbeg'umunezero tuzagira!'",
        "Turirimb’indirimbo nziza",
        "z’abanesheje.",
        "**2**",
        "N'ubw'ibicu bidukingirije,",
        "Bitubuza kureba neza,",
        "Turi hafi yo gusoz'",
        "urugendo rukomeye.",
        "**3**",
        "Dukomeze dukorer’Imana,",
        "Tuzabihemberwa nta shiti;",
        "Turi hafi guhabw’ubugingo",
        "bw’iteka ryose!",
        "**4**",
        "Dor’ikamba rir’imbere yacu,",
        "Tuzaryambikirwa mw’ijuru,",
        "N’ahantu h’igikundiro,",
        "harahebuje rwose."
      ],
      "number": 1
    }
  ]
}