  "title": "126 Njya Nkunda Kuririmb'Indirimbo",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 126 Njya Nkunda Kuririmb'Indirimbo",
        "**1**",
        "Njya nkunda kuririmb'indirimbo,",
        "Ngo yarancunguye.",
        "Niwe waciy'ingoyi z'umwanzi,",
        "Maz'arambatura.",
        "**Gusubiramo:**",
        "**Sop:**",
        "Kuk'ubwe yancunguye;",
        "Kuk'ubwe yancunguye,",
        "Nzahimbaz'izina rye.",
        "Kuk'ubwe yancunguye,",
        "Nzaririmbir'uwandokoye.",
        "**A.T.B.:**",
        "Kuk'ubwe yancunguye;",
        "Kuk'ubwe yancunguye;",
        "Kuk'ubwe yancunguye,",
        "Nzahimbaz'izina rye.",
        "Kuk'ubwe yancunguye,",
        "Kuk'ubwe yancunguye;",
        "Nzaririmbir'uwandokoye.",
        "**2**",
        "Mfit'Umukiz'unogeye rwose,",
        "Niwe wancunguye.",
        "Mpimbazwa no gukor'iby'ashaka,",
        "Kuko yancunguye.",
        "**3**",
        "Nzahora mpamya ntacyo nikenga,",
        "Yuko yancunguye.",
        "Sinzashidikanya ngo ncogore,",
        "Kuko yancunguye.",
        "**4**",
        "Mfit'aho yanteguriye heza,",
        "Kuko yancunguye.",
        "Niho nzibanira naw'iteka,",
        "Kuko yancunguye."
      ],
      "number": 1
    }
  ]
}