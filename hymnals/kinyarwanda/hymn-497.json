  "language": "rw",
  "title": "497 Nzogera Murangurure",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 497 Nzogera Murangurure",
        "**1**",
        "Nzogera murangurure,",
        "Mutangaz'ubutumwa!",
        "Mu byaro mu mijyi hose,",
        "Humvikan'indirimbo.",
        "Dor'Imana yakunz'isi,",
        "Yatanz'Umwana wayo,",
        "Aza mw'isi kudupfira,",
        "Is'ishim'iyo Mana.",
        "**Gusubiramo:**",
        "**Sop.:**",
        "Nimurangurure,",
        "Muvug'iby'amahoro!",
        "Nimurangurure,",
        "Nimuvug'iby'agakiza!",
        "**A. T. na B.:**",
        "Nimura-ngurure,",
        "Muvug'iby'amahoro!",
        "Nimura-ngurure,",
        "Muvuge i - gakiza!",
        "**2**",
        "Nzogera nimutangaze",
        "ivuka ry'Umukiza;",
        "Kubw'ineza yagiriye",
        "umunyabyaha wese.",
        "Mubivug'ahantu hose,",
        "N'inkuru y'imbabazi.",
        "Ijuru n'isi nibyamamaz'iyo neza nyinshi.",
        "**3**",
        "Mureb'Umukiza Yesu,",
        "Mureb'asuzugurwa,",
        "Kand'ar'Umwana w'Imana,",
        "Tukabon'agakiza.",
        "Yaducunguj'amaraso ye",
        "y'igiciro cyinshi;",
        "Turangurure tuvuge",
        "iby'uko gucungurwa.",
        "**4**",
        "Nimugende mujye hose,",
        "Mugere mw'isi yose;",
        "Mwingingir'abigometse,",
        "Bagarukir'Imana.",
        "Dore bari mu makuba,",
        "Bakeney'agakiza;",
        "Bemer'Umukiza Yesu,",
        "Abah'umudendezo!"
      ],
      "number": 1
    }
  ]
}