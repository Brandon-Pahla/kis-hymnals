  "title": "383 Mu Gihe Ntugarijwe",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 383 Mu Gihe Ntugarijwe",
        "**1**",
        "Mu gihe ntugarijwe",
        "N'akaga gakomeye;",
        "Yes'umbwiz'ijwi ryiz'uti:",
        "Humura ndahari.",
        "**2**",
        "Iy'umurab'ukaramye,",
        "Ntabwo mpungabana.",
        "Krist'ankomez'ambwir'ati:",
        "Humura ndahari.",
        "**3**",
        "Umwijim'iy'ubuditse,",
        "Nkabur'iyo ngana;",
        "Numv'ijwi rimpabura ngo:",
        "Humura ndahari."
      ],
      "number": 1
    }
  ]
}