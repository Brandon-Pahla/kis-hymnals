  "title": " 88 Ntumpiteho Mukiza We",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 88 Ntumpiteho Mukiza We",
        "**1**",
        "Ntumpiteho Mukiza we!",
        "Wumv'uko nganya.",
        "Ubw'ugenderer'abandi,",
        "Nanjye ntunsige.",
        "**Gusubiramo:**",
        "Yewe Mukiza!",
        "Umv'uko nganya.",
        "Ubw'ugenderer'abandi,",
        "Nanjye ntunsige.",
        "**2**",
        "Njye neger'intebe yawe,",
        "Mpabw'imbabazi.",
        "Ngupfukamire, mbabaye,",
        "Mpa kukwizera.",
        "**3**",
        "Ntaw'undi nakwiringira,",
        "Ni wowe nshaka.",
        "Mvur'ibikomere byanjye,",
        "Umbabarire.",
        "**4**",
        "Ur'isoko y'amahoro,",
        "Nyir'ubugingo.",
        "Singir'undi niyambaza,",
        "Keretse wowe."
      ],
      "number": 1
    }
  ]
}