  "title": "482 Mwam'Ushobora Byose",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 482 Mwam'Ushobora Byose",
        "**1**",
        "Mwam'ushobora byose,",
        "Duhe kukuririmbira;",
        "Duhimbaza,",
        "Dat'usumba byose;",
        "Nyir'icyubahiro,",
        "Mutegeka wac'ibihe byose.",
        "**2**",
        "Mw'Ijambo ryawe Mwami,",
        "Kandi ni ryo nkota yawe.",
        "Mu gusenga,",
        "Hir'ubwoko bwawe;",
        "Hir'Ijambo ryawe,",
        "Umwuka Wer'atumanukire.",
        "**3**",
        "Mwuka Wera w'Imana,",
        "Ur'Umuhamy'ukomeye.",
        "Mw'uyu mwanya,",
        "Ngwin'ubane natwe;",
        "Turagukeneye,",
        "Ngwino wuzur'imitima yacu.",
        "**4**",
        "Ur'umwe mu Butatu,",
        "Uhor'usingizwa Mana.",
        "Nyakubahwa! Gahor'usingizwa!",
        "Ishimwe n'iryawe!",
        "Tuzahora tukuramy'iteka!"
      ],
      "number": 1
    }
  ]
}