  "title": "453 Senga",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 453 Senga",
        "**1**",
        "Senga mu gitondo kare,",
        "Ujy'usenga no ku manywa.",
        "Senga na nimugoroba,",
        "Iman'ihora yumva.",
        "**2**",
        "Haba mu gitondo kare,",
        "Haba ndetse no ku manywa,",
        "Haba na nimugoroba,",
        "Iman'ihora yumva.",
        "**3**",
        "Igih'icy'ari cyo cyose,",
        "Yesu yakugenderera;",
        "Ahor'agutez'amatwi",
        "Ntucogore gusenga."
      ],
      "number": 1
    }
  ]
}