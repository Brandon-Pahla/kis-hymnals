  "title": "177 Mu Gihugu Cy'Ibyiza",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 177 Mu Gihugu Cy'Ibyiza",
        "**1**",
        "Mu gihugu cy'ibyiza, Har'umurwa wera;",
        "Uzahorahw'iteka: Nta joro rihaba.",
        "**Gusubiramo:**",
        "**S:**",
        "Nta marir'azabayo,",
        "Nta rupfu cyangw'ubwoba;",
        "Ibihe ntibibarwayo,",
        "Kuko ntabwo bwira.",
        "**A., T. na B.:**",
        "Nta marir'azabayo,",
        "Nta rupfu cyangw'ubwoba;",
        "Ibihe ibihe ntibibarwayo,",
        "Kuko ntabwo bwira.",
        "**2**",
        "Inkike z'uwo murwa,",
        "Zose n'izahabu;",
        "N'inzira nazo nuko;",
        "Nta joro rihaba.",
        "**3.**",
        "Kand'amarembo yawo,",
        "Ntabwo yugarirwa;",
        "Uruzi rw'ubugingo,",
        "Ruzawutembamo.",
        "**4**",
        "Ntuzagomba kuvirwa,",
        "N'izuba n'ukwezi;",
        "Kand'Umwana w'Intama,",
        "Niwe mucyo wawo."
      ],
      "number": 1
    }
  ]
}