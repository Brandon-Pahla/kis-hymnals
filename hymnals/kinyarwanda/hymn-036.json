  "title": " 36 Ni Wowe Nizeye",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 36 Ni Wowe Nizeye",
        "**1**",
        "Ni wowe nizeye,",
        "Ntama y'i Kalvari,",
        "We Mukiza!",
        "Ngusab'unyumvire,",
        "Nkiz'ibicumuro,",
        "Mperey'ubu Mwami,",
        "Nkuyoboke.",
        "**2**",
        "Nyuzuz'imbabazi,",
        "Ngir'inama nziza,",
        "Nter'umwete;",
        "Kuko wampfiriye,",
        "Nkwerek'urukundo,",
        "Rudatuba rwaka,",
        "Nk'umuriro.",
        "**3**",
        "Ni ngotwa n'ibyaha,",
        "Nkagira n'ishavu,",
        "Unyobore.",
        "Nkura mu mwijima,",
        "Ntsembahw'amaganya,",
        "Kandi ntundekere,",
        "Kure yawe."
      ],
      "number": 1
    }
  ]
}