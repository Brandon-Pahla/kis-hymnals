  "title": " 15 Urukundo Rwa Yesu",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 15 Urukundo Rwa Yesu",
        "**1**",
        "Urukundo rwa Yesu,",
        "Rwageze kuri twebwe;",
        "Rudukuraho rwose,",
        "Urubanza rwo gupfa.",
        "**Gusubiramo:**",
        "Aradukunda, Aradukunda,",
        "Aradukunda, Yesu Mutabazi.",
        "**2**",
        "Abatari bahabwa,",
        "Ubugingo bw'iteka,",
        "Twihute tubabwire,",
        "Urukundo rwa Yesu.",
        "**3**",
        "Kuko yaradukunze,",
        "Yadupfiriye twese,",
        "Natwe dukunde dutyo,",
        "Abakiri mu byaha.",
        "**4**",
        "Aho Yes'azazira,",
        "Aje kwim'ingoma ye,",
        "Nibwo tuzumv'ijwi rye,",
        "Ati: Mwakoze neza.",
        "**5**",
        "Akir'uyu mutima,",
        "Weze maz'ub'uwawe,",
        "Wambambiwe ku giti,",
        "Nzagukunda mb'uwawe."
      ],
      "number": 1
    }
  ]
}