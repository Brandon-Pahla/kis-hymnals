  "title": " 49 Ntabwo Ndatekereza",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 49 Ntabwo Ndatekereza",
        "**1**",
        "Ntabwo ndatekereza,",
        "Kw'iwac'ari bugufi.",
        "Uko bukeye negereza,",
        "Gusohora mw ijuru.",
        "**Gusubiramo:**",
        "Hafi y'iwacu, Hafi yaho;",
        "Ubu ngeze hafi y'iwacu,",
        "Ngiye gusohorayo.",
        "**2**",
        "Ku rurembo rwa Data,",
        "Hariy'amazu menshi;",
        "Yateguriy'abera bose,",
        "Ngo babane na Yesu.",
        "**3**",
        "Ninjya kuger'iwacu,",
        "Nzururuts'umutwaro;",
        "Nzoroherwa n'umubabaro,",
        "Nambar'ikamba ryiza.",
        "**4**",
        "Ningera ku nkengero,",
        "Nzaba ngeze bugufi;",
        "Nzaba negerey'i muhira,",
        "Ngiye kuger'iwacu."
      ],
      "number": 1
    }
  ]
}