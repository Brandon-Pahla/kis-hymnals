  "title": "359 Umucyo Uva Mw'Ijuru",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 359 Umucyo Uva Mw'Ijuru",
        "**1**",
        "Umucyo uva mw'ijuru,",
        "Uyobor'abagenzi,",
        "Uzadufasha tugere,",
        "Ku nkombe yo hakurya.",
        "**Gusubiramo:**",
        "Mwami Yes'umucyo",
        "waw'umurikir'abagenzi",
        "Winjire mu nyanja,",
        "Maz'ubakingir'umuraba.",
        "**2**",
        "Umwijim'urabuditse,",
        "Inyanj'irahorera;",
        "Yes'aduhozahw'amas'",
        "azadufasha twambuke.",
        "**3**",
        "Mutyo ducan'amatara,",
        "Dukomez'urugendo.",
        "Twe kuyobywa n'abasare,",
        "Badafit'urumuri."
      ],
      "number": 1
    }
  ]
}