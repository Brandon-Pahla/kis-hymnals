  "title": "131 Ndusheho Gusa Nawe",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 131 Ndusheho Gusa Nawe",
        "**1**",
        "Ndusheho gusa nawe,",
        "Yesu Mukiza wanjye;",
        "Nyuzuz'ubugwaneza,",
        "Mp'urukundo ruhebuje.",
        "Ndusheho kumera nkawe,",
        "Aho ngenda hose, Mwami;",
        "Ndiyoroheje rwose,",
        "Mwami njye ngukurikiza.",
        "**2**",
        "Nib'irind'icyanira,",
        "Ikamenya n'igishwi;",
        "Sinshidikanya rwose,",
        "Nanjy'ihor'inyitayeho.",
        "Izanyigisha kwitonda,",
        "Imbabarir'amafuti;",
        "Ndiyoroheje rwose,",
        "Mwami njye ngukurikiza.",
        "**3**",
        "Ndusheho gusa nawe,",
        "Ndusheh'uko bukeye;",
        "Nyigiza hafi yawe,",
        "Ahw'amahor'adashira.",
        "Uhor'umpindura mushya,",
        "Kubw'urukund'ungirira;",
        "Ndiyoroheje rwose,",
        "Mwami njye ngukurikiza."
      ],
      "number": 1
    }
  ]
}