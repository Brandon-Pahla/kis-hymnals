  "title": "  1 Murinzi We, Vuz'Impanda",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 1 Murinzi We, Vuz'Impanda.",
        "**1**",
        "Murinzi, jy'uvuz'impanda,",
        "Uhan'umuntu wese;",
        "Ng'uzumv'izo nkuru nziza,",
        "Yihan'akiz'ibyaha.",
        "**Gusubiramo:**",
        "Murinzi we vuz'impanda!",
        "Yivuze cyane hose (hose),",
        "Menyesha bos'ubutumwa,",
        "Imbohe zibohorwe.",
        "**2**",
        "Yivugirize mu mpinga,",
        "Mw'ishyamba mu gisiza;",
        "Ndetse no mu nyanja hose,",
        "Bameny'ako gakiza.",
        "**3**",
        "Yivugirize mu nzira,",
        "Mu rwiherero hose.",
        "Dat'arabategereje,",
        "Ntacy'ataringanije.",
        "**4**",
        "Uyivugiriz'indembe,",
        "Nazo zikizw'ibyaha.",
        "Yes'arabatumir'ati:",
        "Nimuze mbaboneze."
      ],
      "number": 1
    }
  ]
}