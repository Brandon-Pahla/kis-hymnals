  "title": "469 Turakuramya",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 469 Turakuramya",
        "**1**",
        "Turakuramya Mwami udukunda;",
        "Dusonzey'ukwera kwawe.",
        "Mwami Nyir'imigisha, Mwami mwiza,",
        "Dukeneye kuzuzw'urukundo;",
        "Tumurikirwe n'ubwiza bwawe.",
        "**2**",
        "Mwigisha mwiza utagereranywa;",
        "Uduhire, Mwam'utweze.",
        "Duh'umutima mushya, twiyoroshye.",
        "Dukeneye kuzuzw'urukundo;",
        "Tumurikirwe n'ubwiza bwawe.",
        "**3**",
        "Dusagiz'urukundo rwawe Mwami;",
        "Urukundo rudashira.",
        "Ruzan'abanyabyaha kuri Yesu.",
        "Dukeneye kuzuzw'urukundo;",
        "Tumurikirwe n'ubwiza bwawe.",
        "**4**",
        "Mwam'abazajya batubona bose,",
        "Batangare batubonye;",
        "Dufit'ubumwe nyabwo, dufatanya.",
        "Dukeneye kuzuzw'urukundo;",
        "Tumurikirwe n'ubwiza bwawe.",
        "**5**",
        "Wumve Mwami amasengesho yacu,",
        "Utwuzuz'Umwuka wawe;",
        "Twigish'Ijambo ryawe, turimenye.",
        "Dukeneye kuzuzw'urukundo;",
        "Tumurikirwe n'ubwiza bwawe."
      ],
      "number": 1
    }
  ]
}