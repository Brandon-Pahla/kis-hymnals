  "title": "121 Nzasingiz'Urukundo Rwa Yesu",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 121 Nzasingiz'Urukundo Rwa Yesu",
        "**1**",
        "Nzasingiz'urukundo",
        "rwa Yesu, Wambambiwe;",
        "Yemeye kumanuka,",
        "Azanywe no kumpfira.",
        "**Gusubiramo:**",
        "Nzasingiz'urukundo rwe;",
        "Mpimbaze, Yaramfiriye,",
        "Ashaka kumbeshaho;",
        "Nzahora muhimbaza.",
        "**2**",
        "Yesu yambabarijwe,",
        "Ntararushya mbabara;",
        "Yahoze ansabira,",
        "Ntarameny'iyo bijya.",
        "**3**",
        "Urukundo rw'Imana,",
        "Mbega ngo rurankiza!",
        "Ibyaha byanjye byose,",
        "Bikantamururwaho!",
        "**4**",
        "Nta cyiza nakoreye,",
        "Umwami wankunz'atyo;",
        "None maze kwemezwa,",
        "Mwami nguhaye byose."
      ],
      "number": 1
    }
  ]
}