      "lines": [
        "### 272 Yesu Niwe Rufatiro",
        "**1**",
        "Yesu niwe rufatiro,",
        "Twubakahw'ingo zacu,",
        "N'Urutare rukomeye,",
        "Ntabwo runyegenyega.",
        "**Gusubiramo:**",
        "**S. na A.:**",
        "Yesu n'Urutare,",
        "Bwugamo bwacu,",
        "Yesu niwe soko,",
        "Y'umunezero.",
        "**T. na B.:**",
        "Yesu Rutare, Yesu n'Urutare,",
        "Bwugamo bwacu, â€¦cubwacu",
        "Yesu ni soko, Yesu niwe soko,",
        "Y'umunezero.",
        "**2**",
        "Iyo turi kumwe nawe,",
        "Atwuzuz'urukundo,",
        "Niwe byiringiro byacu,",
        "Atwonger'imbaraga.",
        "**3**",
        "Duh'urukundo nyakuri,",
        "Ukomez'ingo zacu,",
        "Turind'imigambi mibi,",
        "Iva kuri sekibi."
      ],
      "number": 1
    }
  ]
