  "title": "393 393 Bana, Nimwitonde",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 393 Bana, Nimwitonde",
        "**1**",
        "Bana nimwitonde,",
        "Kandi mwishime;",
        "Muse n'abo mw'ijuru,",
        "Munezerwe.",
        "**2**",
        "Mur'abavandimwe,",
        "Nimukundane;",
        "Mwubah'ababyeyi,",
        "Kandi mwumvire.",
        "**3**",
        "Mwirind'umujinya;",
        "Ntimukabeshye;",
        "Ibyo muvuga,",
        "Bishimishe Yesu."
      ],
      "number": 1
    }
  ]
}