  "title": "245 Ahw'Umukiza Anyobora",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 245 Ahw'Umukiza Anyobora",
        "**1**",
        "Ahw'Umukiza anyobora,",
        "Nzemera mukurikire,",
        "Nzamukurikir'iteka,",
        "Sinzakorwa n'isoni.",
        "**Gusubiramo:**",
        "Ahw'Umukiza anyobora,",
        "Nzemera mukurikire,",
        "Ahw'azanyobora hose,",
        "Nzajya mukurikira.",
        "**2**",
        "N'ubwo haba mu mwijima,",
        "Nkagenda nyobagurika,",
        "Nzumv'ijwi ry'uwo",
        "Mukiza rinsubizemw'intege.",
        "**3**",
        "N'ubw'isi yambushabusha,",
        "Nkabur'epfo na ruguru,",
        "Mukiza uzandamira,",
        "Ntabwo nzarambarara.",
        "**4**",
        "Ibi byago nibishira,",
        "Azanyijyanir'iwacu,",
        "Ntihabay'umubabaro,",
        "Nzaba ntanye n'ibyaha."
      ],
      "number": 1
    }
  ]
}