  "title": "366 Isoko Itemba",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 366 Isoko Itemba",
        "**1**",
        "Har'isoko y'ubunt'",
        "Isokw'ihor'itemba.",
        "Mu mitim'inaniwe,",
        "Sokw'iryoshye cyane.",
        "N'isoko nziza cyane pe!",
        "N'isokw'itumar'inyota.",
        "**Gusubiramo:**",
        "Iyo Soko, Ihora itemba;",
        "Uyinywaho iramukiza.",
        "**2**",
        "Ndumv'ijwi ryongorera,",
        "Nk'indirimbo nziza!",
        "Rituruka mw'isoko",
        "Nziza idakama.",
        "Ni yo soko y'ubugingo,",
        "Ni y'idukiz'ubwihebe.",
        "**3**",
        "N'isoko y'umugisha,",
        "Itang'ibyishimo.",
        "N'amaraso ya Yes'",
        "Akiz'abanyabyaha.",
        "N'amaraso y'Umukiza,",
        "Kand'aratunganye rwose."
      ],
      "number": 1
    }
  ]
}