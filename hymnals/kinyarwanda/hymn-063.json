  "number": 63,
  "language": "rw",
  "title": " 63 N'Iki Cyankiz'Ibyaha?",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 63 N'Iki Cyankiz'Ibyaha?",
        "**1**",
        "N'iki cyankiz'ibyaha?",
        "N'amaraso yawe Yesu.",
        "N'iki cyanyeza rwose?",
        "N'amaraso yawe Yesu.",
        "**Gusubiramo:**",
        "Maras'atunganye,",
        "Niy'amboneza rwose.",
        "Nta cyampa gukira,",
        "Nk'amaraso y'Umukiza.",
        "**2**",
        "Nta kindi cyantunganya,",
        "N'amaraso yawe Yesu.",
        "Nta kindi cyiru mfite,",
        "N'amaraso yawe Yesu.",
        "**3**",
        "Nta cyantsembahw ibyaha,",
        "N'amaraso yawe Yesu.",
        "Nta byanjye byamboneza,",
        "N'amaraso yawe Yesu.",
        "**4**",
        "Ni wowe niringira,",
        "N'amaraso yawe Yesu.",
        "Ni wow'ump'amahoro,",
        "N'amaraso yawe Yesu.",
        "**5**",
        "Njye ngusingiza cyane,",
        "Ku maraso yawe Yesu.",
        "Mukiz'urakarama,",
        "Ku maraso wamviriye."
      ],
      "number": 1
    }
  ]
}