  "title": " 24 Ndagushak'Iteka",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 24 Ndagushak'Iteka",
        "**1**",
        "Ndagushak'iteka,",
        "Mwami mwiza cyane,",
        "Ijwi ryawe ryiza,",
        "Rizan'amahoro.",
        "**Gusubiramo:**",
        "Ku manywa na n'ijoro,",
        "Ndagushak'iteka.",
        "Mpir'ubu Mukiza we!",
        "Twigumanire.",
        "**2**",
        "Ndagushak'iteka,",
        "Jy'ungum'iruhande.",
        "Ntsind'amoshya yose,",
        "Iyo turi kumwe.",
        "**3**",
        "Ndagushak'iteka,",
        "Ntuje n'iyo nganya.",
        "Ngwin'ubu tubane!",
        "Mbone gutungana.",
        "**4**",
        "Ndagushak'iteka,",
        "Nyigish'iby'ushaka!",
        "Nkwinging'unsohorez'amasezerano!",
        "**5**",
        "Ndagushak'iteka,",
        "Mwami Wera rwose,",
        "Nyobora mb'uwawe,",
        "Wowe munyahirwe!"
      ],
      "number": 1
    }
  ]
}