  "title": "118 Ndumv'Umukiz'Ambwira",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 118 Ndumv'Umukiz'Ambwira",
        "**1**",
        "Ndumv'Umukiz'ambwira,",
        "Ko mbuz'imbaraga;",
        "Ngo: Njye museng'iteka,",
        "Ntacyo nzamuburana.",
        "**Gusubiramo:**",
        "Yanndihiriye imyenda yose;",
        "Yambabariy'ibyaha,",
        "Bihinduk'umweru.",
        "**2**",
        "Niw'ufit'imbaraga,",
        "Zihagije rwose;",
        "Zo gukiz'ababembe,",
        "Zikoroshy'umutima.",
        "**3**",
        "Ubwo nta cyiza nkora,",
        "Nibesherejweho;",
        "N'ubuntu bwawe gusa,",
        "Ni wowe nkesha byose.",
        "**4**",
        "Imbere ya ya ntebe,",
        "Nzahahagarara;",
        "Nzaba njyany'iminyago,",
        "Nyishyikirize Yesu."
      ],
      "number": 1
    }
  ]
}