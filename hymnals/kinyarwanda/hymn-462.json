  "title": "462 Impano Y'Ijuru",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 462 Impano Y'Ijuru",
        "**1**",
        "Ngwino Soko y'ubugingo,",
        "Ngwino Mpano y'ijuru;",
        "Uduhire Mwuka Wera,",
        "Duhishurire Kristo.",
        "**2**",
        "Mwuka Wer'ur'urumuri,",
        "Ntacy'ubasha guhishwa;",
        "Ntacy'usobwa,",
        "Mwuka Wera utuboneshereze.",
        "**3**",
        "Utuyobore kwa Data,",
        "Tumusange twishimye;",
        "Mu masengesho dusenga,",
        "Tuber'Umuyobozi.",
        "**4**",
        "Hindura byose bishyashya,",
        "Duhabwe gusa nawe!",
        "Uzatugeze mw'ijuru,",
        "Mu Bwami buhoraho."
      ],
      "number": 1
    }
  ]
}