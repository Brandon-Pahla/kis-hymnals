  "language": "rw",
  "title": "428 Haguruka, Haguruka",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 428 Haguruka, Haguruka",
        "**1**",
        "Umwam'araguhamagara haguruka.",
        "Akuyobora mu mucyo,",
        "Mu nzira yo kwizera;",
        "Mu rugendo rwo mw'iyi si,",
        "Wigir'ubwoba rwose;",
        "Ngaho haguruka.",
        "**2**",
        "Komera shikama",
        "B'intwari ku rugamba;",
        "N'ubwo Satan'atontoma,",
        "Yarangije gutsindwa;",
        "Yesu yaramunesheje,",
        "Komez'ibendera Rye;",
        "Komera shikama.",
        "**3**",
        "Komeza komeza,",
        "Rwanan'ibyiringiro;",
        "Ririmb'indirimb'igutera",
        "Gushikama cyane.",
        "Ntugashidikanye rwose,",
        "Gir'umutim'utuje;",
        "Komez'ukomere!"
      ],
      "number": 1
    }
  ]
}