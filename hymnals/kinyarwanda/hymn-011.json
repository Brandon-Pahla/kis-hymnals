  "title": " 11 Mukiza Ngukurikire",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 11 Mukiza Ngukurikire",
        "**1**",
        "Mukiza ngukurikire,",
        "Ah'unshakira hose;",
        "Ah'ujya ngukurikire,",
        "Mwami ngukurikire.",
        "**Gusubiramo:**",
        "Mukiza ngukurikire, Wamviriy'amaraso;",
        "Nib'utagir'ukwitaho, Njye nkwikurikirire.",
        "**2**",
        "Nahw'inzira yazitirwa,",
        "Naho yakukumuka,",
        "Ko wayinyuzemo mbere,",
        "Nyicamo mfit'ishimwe.",
        "**3**",
        "Naho naterwa n'ibyago,",
        "Nkageragezwa cyane,",
        "Warageragejwe nawe,",
        "Mwami ngukurikize.",
        "**4**",
        "Nib'unyobora mw'ishavu,",
        "Niba nkena nkanenwa,",
        "Wigeze kuba ruvumwa,",
        "Nanjye ngukurikize."
      ],
      "number": 1
    }
  ]
}