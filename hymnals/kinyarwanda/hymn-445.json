  "language": "rw",
  "title": "445 Bigiye Gusobanuka",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 445 Bigiye Gusobanuka",
        "**1**",
        "Turi hafi kujy'aheza,",
        "Mu gihugu cyiza cyane.",
        "Ntituzongera kurira,",
        "Bigiye gusobanuka!",
        "**Gusubiramo:**",
        "Twiringir'Imana gusa,",
        "Ntitugir'ubwoba rwose.",
        "N'ubwo duhura n'ibiruhije,",
        "Bigiye gusobanuka!",
        "**2**",
        "Ibyiza twari twahombye,",
        "Tuzongera kubihabwa.",
        "Mw'ijuru tuzanezerwa,",
        "Bigiye gusobanuka!",
        "**3**",
        "Tuzamenya nez'impamvu,",
        "Twahuraga n'amakuba,",
        "Indirimbo zigatuza,",
        "Bigiye gusobanuka!",
        "**4**",
        "Ibijya bidushobera,",
        "Tukibaz'impamvu yabyo,",
        "Tukabur'ibyiringiro,",
        "Bigiye gusobanuka!",
        "**5**",
        "Imana yac'iz'inzira,",
        "Ni y'ifit'urufunguzo,",
        "Izadukiz'amarira,",
        "Bigiye gusobanuka!"
      ],
      "number": 1
    }
  ]
}