{
  "number": 52,
  "language": "nso",
  "title": " 52 E TLA SEDIBA SA MAHLATSE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 52 E TLA SEDIBA SA MAHLATSE",
        "AH 197 CH 291",
        "COME FOUNTAIN OF JOY"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Tla sediba sa mahlatse,",
        "O nthute go opela,",
        "Melatšwana ya mogau",
        "e lokela tumišo,",
        "Onthute go go kgabiša,",
        "Ke bontšhe toko ya go,",
        "Ge tshepo ya kgaugelo",
        "yona o mpha fela;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Ke bea kgopolo yaka,",
        "Ka thušo ya 'Go ke mo,",
        "Ke na le tshepo go",
        "fila gae ka lethabo,",
        "Jes 'o nyakile timelong,",
        "Ke tšwile ka lešakeng:",
        "Go mpoloka le dikotsing,",
        "A tšolla madi a 'Gwe;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ka mogau ke wa molato",
        "ka mehla yohle,",
        "\"Mme o ntleme ka dithapo,",
        "Ke be kgauswi le wena;",
        "Morena ge ke timela,",
        "Ke tloga go Modimo,",
        "Tšea pelo yekhwi ya ka,",
        "O e tliše go wena."
      ],
      "number": 4
    }
  ]
}