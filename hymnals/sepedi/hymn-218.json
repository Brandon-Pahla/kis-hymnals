{
  "number": 218,
  "language": "nso",
  "title": " 218  MOTHEO O TEE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 218  MOTHEO O TEE",
        "AH 334 CH 433",
        "ONE FOUNDATION"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Motheo wa phuthego",
        "ke Jeso Morena,",
        "Ke popo mpšha,",
        "Ka madi le lentšu,",
        "O tile go e nyaka,",
        "Go ba monyadiwa,",
        "Ka madi o e rekile,",
        "A ba a e hwela;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Ba ba tšwago",
        "Mahlakoreng ohle,",
        "Bohle ba tee fela,",
        "Morena o tee fela,",
        "Tumelo, go tswalwa,",
        "Ba tumiša leina,",
        "Ba ja sejo se tee,",
        "Ba na le tshepo e tee,",
        "Le tšhegofatšo yohle,"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Le ge go e sotla,",
        "Batho ba e lebelela,",
        "Le ge dira di thuba",
        "leswika la yona,",
        "Bakgethwa ba re,",
        "\"Go fihla neng?",
        "Bosego bja go lla",
        "E tong goba bja tumišo\";"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Mošomong, matshwenyegong,",
        "Le dintweng tša yona,",
        "E letile ka khutšo",
        "Ya ka bosafelego,",
        "Ponong ya kgalalelo,",
        "E šegofaditšwego,",
        "Mme ka phenyo phuthego",
        "E tlo dula khutšong;"
      ],
      "number": 5
    }
  ]
}