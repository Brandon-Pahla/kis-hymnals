{
  "number": 209,
  "language": "nso",
  "title": " 209  GO SEPELA KGAUSWI",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 209  GO SEPELA KGAUSWI",
        "АН 310 CH 383",
        "OH FOR A CLOSER WALK"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Go sepela le Modimo,",
        "Khutšong ya Godimo,",
        "‘Sedi le phadima tseleng,",
        "E nkišago go kwana;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Bowa Moya o Mogethwa,",
        "Morongwa o botse,",
        "Ke ila dibe tšohle,",
        "Tšeo di go lelekilego;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "A metsotso e botse ya,",
        "Lethabo e kwagale,",
        "Mme go šetše moo go ka",
        "Se sahlwego go tlatšwa;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Medimo yeo e ratwago,",
        "Le ge e ka ba efe,",
        "O nthuše ke e lahle,",
        "Ke khunamele wena;"
      ],
      "number": 5
    }
  ]
}