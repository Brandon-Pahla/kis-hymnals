{
  "number": 27,
  "language": "nso",
  "title": " 27 RE TSOŠOLOŠE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 27 RE TSOŠOLOŠE",
        "AH 157 CS 396",
        "WE PRAISE THEE O GOD"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Lebogang Modimo",
        "ka Morwa wa Gagwe",
        "Yena Jeso a re",
        "hwetsego dibeng."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušulošo",
        "Aleluya! Motumišeng!",
        "Aleluya! Amen!",
        "Aleluya! Aleluya!",
        "re tsošološe"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Lebogang Modimo",
        "ka Moya Mokgethwa;",
        "'Sedi le re bontshitsego",
        "M' phološi."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Tumišo le theto go",
        "Kwan 'e hlabilweng;",
        "Ya rwala dibe tšohle",
        "ya re hlatšwišiša"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Tumišo yohle e",
        "be go Morena;",
        "Ya re rekilego",
        "ka mogau wa 'Gwe."
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "Re tsošološe re",
        "llatse lerato;",
        "Re tsošwe moyeng",
        "ka mollo wa \"Go."
      ],
      "number": 7
    }
  ]
}