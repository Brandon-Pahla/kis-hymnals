{
  "number": 106,
  "language": "nso",
  "title": " 106 RE TLO AGA LESWIKENG",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 106 RE TLO AGA LESWIKENG",
        "AH 213 CH 579",
        "BUILD ON THE ROCK"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Re tlo a ga go Jeso,",
        "Leswikeng le le sa felego,",
        "Bjale mohlang difefo",
        "di tšutla, Re tlo ema bang;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Re aga leswikeng,",
        "'Swika la nnete,",
        "Re tho aga leswikeng ruri,",
        "E lego Kreste Jeso;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ba bngwe ba aga lešabašabeng,",
        "La mahumo a lefase,",
        "Ba bangwe maphothong a sebe,",
        "A letlotlo la mabose,"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "A le keng le ageng leswikeng,",
        "Motheong o tiilego,",
        "Tshepo ya polokego ya rena,",
        "Ke eo e tiilego;"
      ],
      "number": 5
    }
  ]
}