{
  "number": 119,
  "language": "nso",
  "title": " 119 SEDIBA SE HLATSWANG",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 119 SEDIBA SE HLATSWANG",
        "AH 375 CH 598",
        "THE CLEANSING WAVE"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ke bona 'diba sa madi,",
        "Se se tseneletšeng,",
        "Jeso Moren' wa phološa,",
        "O šupa hlakoreng;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Sediba ke se se ntlhatšwang,",
        "Bjale ke tsena go hlapa,",
        "Retang Morena wa ntlhatšwa,",
        "O wa ntlhatšwa,",
        "E wa ntlhatšwa;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ke bona bopo le leswa,",
        "Ke kwa madi 'a šuša,",
        "A re tshenyego e a hwa,",
        "Gona mo sedibeng,"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Ke rotogela leseding,",
        "La kua godimo,",
        "Ka pelo e hlwekilego,",
        "Jeso o dutše ntshe;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Go a makatša lefaseng,",
        "Go kwa madi 'a tšwileng,",
        "Mme ke tseba Jeso fela,",
        "Yo a mpapoletšweng;"
      ],
      "number": 6
    }
  ]
}