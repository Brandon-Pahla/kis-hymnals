{
  "number": 237,
  "language": "nso",
  "title": " 237  EMANG, EMELANG JESO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 237  EMANG, EMELANG JESO",
        "АН 368 CH 354",
        "STAND UP FOR JESUS"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Emang, emelang Jeso,",
        "Mašole 'a 'fapano,",
        "Emišang molao wa 'Gwe,",
        "Go sešo senyege,",
        "Le fenye le go fenya,",
        "Le hlatlwa ke Yena,",
        "Dira tšohle di fele,",
        "Kreste ke Morena;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Emang, emelang Jeso,",
        "Le kweng le moKgoši,",
        "Le yeng tweng e thata,",
        "tšatšing la lehono,",
        "Banna mošomeleng,",
        "Dira e le tše ntši,",
        "Le tiišetšeng Moreneng,",
        "Le Iweng ka maatla;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Emang, emelang Jeso,",
        "Ntwa e ya fela,",
        "Lehono ke mokgoši,",
        "Ka moso ke phenyo,",
        "Yo'a fenyago o rwešwa,",
        "Mphapahlog 'a 'phelo,",
        "Le Moren'o tlo buša,",
        "Ka mo gosafeleng;"
      ],
      "number": 4
    }
  ]
}