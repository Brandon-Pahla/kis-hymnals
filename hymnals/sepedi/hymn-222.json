{
  "number": 222,
  "language": "nso",
  "title": " 222  KE BOLOKEGILE MATSOGONG A JESO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 222  KE BOLOKEGILE MATSOGONG A JESO",
        "AH 254 CH 615",
        "SAFE IN THE ARMS OF JESUS"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Matsogong a ga Jeso,",
        "Ke bolokegile,",
        "Tšhireletšo leratong,",
        "Moy'a ka o'ikhutše,",
        "Kwa mantšu'a mageloi,",
        "A tla ka koša go nna,",
        "A tšwa kua kgalalelong,",
        "'watle la Jaspera;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Matsogong a ga Jeso,",
        "Ke bolokegile,",
        "Tšhireletšo leratong,",
        "Moy'a ka o 'khutše;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Matsogong a ga Jeso,",
        "Ke phonyoge tshenyo,",
        "Diteko tša lefase,",
        "Sebe se paletšwe,",
        "Tokollo maswabing,",
        "Tokollo le poifong",
        "Diteko ke tše mmalwa,",
        "E! le dikeledi;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Jeso tšhireletš'wa ka,",
        "Jeso o nkhwetše,",
        "Mo leswikeng la mehla,",
        "Ke tlo tshepela ntshe,",
        "A ke lete tiišetšong,",
        "Bošego bo fete,",
        "Ke be ke bone meso,",
        "Kgaogano'a gauta;"
      ],
      "number": 5
    }
  ]
}