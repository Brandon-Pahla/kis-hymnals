{
  "number": 246,
  "language": "nso",
  "title": " 246  BJALE BOLELA LENTŠU KA JESO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 246  BJALE BOLELA LENTŠU KA JESO",
        "CH 522",
        "NOW JUST A WORD FOR JESUS"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Bolela lentšu ka Jeso,",
        "Mogwer a lerato,",
        "Kgothatša dipelo ka",
        "Seo o se diretšwego;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Boleala lentšu ka Jeso,",
        "Le tlo thusa tseleng,",
        "Lentšwana ka Jeso,",
        "Košeng le thapelong,"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Bolela lentšu ka Jeso,",
        "Tshwarelo ya dibe,",
        "Mme ka mogau o leka,",
        "Go ya gae legodimong,"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Bolela lentšu ka Jeso,",
        "Bjale wena o re,",
        "Ke rata M'phološi,",
        "Yo a nkhwetšego,"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Bjale bolela lentšu ka Jeso,",
        "Se sene nako,",
        "Pelo e tletšego botswa,",
        "Moputso ke maswabi;"
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "Bjale bolela lentšu ka Jeso,",
        "Tumelo ge e fela,",
        "Ragoga phokolong yohle,",
        "Mme o mo nee tšohle;"
      ],
      "number": 7
    }
  ]
}