{
  "number": 145,
  "language": "nso",
  "title": " 145  SE NYAKE SEŠIRO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 145  SE NYAKE SEŠIRO",
        "CS 513",
        "ASK NOT TO BE EXCUSED"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Se nyake seširo,",
        "Go na le mošomo,",
        "'Swanetše go dirwa,",
        "O tšwa go Modimo,",
        "Kgopelo ya Gagwe,",
        "O santš'o e newa,",
        "Hle e amogele,",
        "O šomele Yena;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Tla hle, Tla (bjale),",
        "Senyake seširo,",
        "Tla hle, Tla (bjale),",
        "Lokela mošomo,",
        "Se nyake seširo,",
        "Go tlothwe godimo,",
        "'Ganne thato ya",
        "k'o gomišwe godimong;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Se nyake seširo,",
        "Morena wa bitša,",
        "Be ntš'o sa dumele,",
        "Phakiš 'o theeletše,",
        "Mašem 'a budule,",
        "Bašomi ga ga go,",
        "Thabela mošom 'o,",
        "Šomele Modimo;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Se nyake seširo,",
        "Go kotse tiegong,",
        "Rato le nyaditšwe,",
        "Le tlo ela ruri,",
        "Kgaugelo e bitša,",
        "Mme e šupa tsela,",
        "Jes 'o a go kgopela,",
        "Tla o tlo tshwarelwa;"
      ],
      "number": 5
    }
  ]
}