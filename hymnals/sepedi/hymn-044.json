{
  "number": 44,
  "language": "nso",
  "title": " 44 GE MOŠOMO WA KA O FEDILE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 44 GE MOŠOMO WA KA O FEDILE",
        "CS 849",
        "WHEN MY LIFE-WORK IS ENDED"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ge mošomo wa ka o fedile mokhwi,",
        "Mmele wona o tlo apara go sa hwe,",
        "Ke tlo tseba M'phološi wa ka ka mošola,",
        "O tlo nkamogela a myemyela;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Ke tlo.... motseba,",
        "Ke tlo motseba,",
        "Ke phološitšwe,",
        "Ke tlo ema le Yena,",
        "A na le mabadi,",
        "Diatleng tša 'Gwe;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Lethabo le tlo ata kudu",
        "Ge ke mmona 'fahlegong.",
        "Ge leihlo la kgaugelo",
        "Le ntebelela,",
        "Pelo ya ka e tlo tumiša",
        "Ka mogau le lerato,",
        "Godimo ke lokišeditšwe bodulo;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Ke gopola baratiwa",
        "Ba šetšego ba hwilego kgale,",
        "Ke gopola taelano kua nokeng.",
        "Re ka se sa kgaogana kua edene,",
        "Takatšo ke go bona M'phološi;"
      ],
      "number": 5
    }
  ]
}