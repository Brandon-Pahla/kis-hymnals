{
  "number": 147,
  "language": "nso",
  "title": " 147  NKGOGELE KGAUSWI NAGO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 147  NKGOGELE KGAUSWI NAGO",
        "AH 314 CH 632",
        "DRAW ME CLOSER TO THEE"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Nkgogele kgauswi nago Tate,",
        "Kea go kganyoga,",
        "Kgauswi ntsenye matsogong,",
        "Ke nyaka go khutša;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Kgauswi ka lerato,",
        "Nkgogele go wena,",
        "Nkgogele go wena kgauswi,",
        "(Kgauswi ka thapo tša lerato,",
        "Nkgoge, nkgogele,",
        "Godimo kgauswi ka lerato,",
        "Nkgogele godimo,",
        "Kaguswi nkogele go wena,)"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Nkgogele kgauswi nago M'phološi,",
        "Ke se go tlogele,",
        "Ge ke le matsogong a gago,",
        "Kea itebala;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Nkgogele kgauswi ka moya,",
        "Ke tshwane le wena,",
        "Ntsoše, ntihatšwe,",
        "O ntire botse,",
        "Ke be ke hlweke;"
      ],
      "number": 5
    }
  ]
}