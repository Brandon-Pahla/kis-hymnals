{
  "number": 291,
  "language": "nso",
  "title": " 291  E SWANETSE KONYANA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 291  E SWANETSE KONYANA",
        "AH 145 CH 168",
        "WORTHY, WORTHY IS THE LAMB"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "E swanetše Konyana,",
        "E swanetše Konyana,",
        "E swanetše Konyana,",
        "E hlabilweng;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Tumo haleluya!",
        "Theto haleluya!",
        "Tumo haleluya!",
        "Go konyana;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Mphološi mmušo o tle!",
        "Fetšiša mmus 'a sebe,",
        "Tliša boikhutšo bja",
        "'Go, Konyana;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Eke re ka motheetša!",
        "Re rate re tumiše,",
        "Go filela Sione,",
        "Go konyana;"
      ],
      "number": 5
    }
  ]
}