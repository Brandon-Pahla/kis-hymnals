{
  "number": 30,
  "language": "nso",
  "title": " 30 TŠATŠI LE A PHIRIMA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 30 TŠATŠI LE A PHIRIMA",
        "AH 41 CH 51",
        "DAY IS DYING IN THE WEST"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Tšatši le a phirima,",
        "Boikhuto bja godimo,",
        "Rapela ge bošego bjo,",
        "Bonesa mabone legodimong;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "O a halalela Morena Modimo!",
        "Legodimo le lefase,",
        "Di tumiša wena",
        "Ramaatla ohle;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Morena wa bophelo,",
        "Le wa lefase lohle,",
        "Rena ba go nyakago,",
        "Re phuthe 'ratong,",
        "E ba kgauswi,"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Tšatši le'a phirima,",
        "Re tee 'ratong la Gago,",
        "Kgalalelong le mogaung,",
        "Tša lešira la 'Go re rotoge;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Ge dinaledi, 'tšatši,",
        "bošego, di nyamile,",
        "Moren'a mangeloi re utullele,",
        "Moso re bone Jeso;"
      ],
      "number": 6
    }
  ]
}