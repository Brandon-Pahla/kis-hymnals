{
  "number": 245,
  "language": "nso",
  "title": " 245  GA GO LEINA LE LENGWE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 245  GA GO LEINA LE LENGWE",
        "АН 98 CH 517",
        "THERE'S NO OTHER NAME"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ga go leina le lengwe,",
        "Le swanago le la Jeso,",
        "Ke thabo ya mangeloi",
        "Le mokreste lefaseng,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Leina botse, Leina botse,",
        "Ga go le swanang nalo,",
        "Leina botse, Leina botse,",
        "Ke leina la ga Jeso"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ga go le le swanang nalo,",
        "Ge pelo e nyamile,",
        "Ga go le le swanang nalo,",
        "Ge pelo e thabile;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Ke ne tshepo ya go mmona,",
        "Mohlang a tlo bonala,",
        "Ka tshepo a nkamogele,",
        "A nkhomotše le moyeng;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Ge a rata na ke some,",
        "Ka mehla tshemong ya 'Gwe,",
        "Go lokile ge 'ele Jeso,",
        "A šegofatše tšohle;"
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "Ge a rata gore lehu,",
        "Le kgome mmele ke hwe,",
        "Go lokile ge'ele Jeso,",
        "Tshepo ya lehu la ka;"
      ],
      "number": 7
    }
  ]
}