{
  "number": 164,
  "language": "nso",
  "title": " 164  KE DUTŠE DINAONG TŠA JESO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 164  KE DUTŠE DINAONG TŠA JESO",
        "CH 618",
        "AT THE FEET OF JESUS"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ke dutše 'naong tša Jeso,",
        "Ke kwa mantšu a bose,",
        "Felo le bohlokwa kudu,",
        "Ke tlo dula ka mehla,",
        "Ke dutše naong tša Jeso,",
        "Ke lebelela morago,",
        "Rato la 'Gwe la mogau,",
        "Le tšere pelo ya ka,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Ke kae moo go hwa go ka bago,",
        "Le yona tšhegofatšo",
        "Ke bea dibe le kgaugelo,",
        "ke tle ke be boikhutšong,",
        "Dinaong tša ga Jeso,",
        "Ke tlo lla thapelong,",
        "Ke phutha botlalong bja",
        "'Gwe mogau mehla yohle;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "O ntšhegofatše M'phološi,",
        "Ke let naong tša 'Go,",
        "Ntebelele ka lerato,",
        "Le kgaugelo e kgolo,",
        "Mphe mogopolo wa Jeso,",
        "Ke be bjalo ka Yena,",
        "Ke šupe ge ke le wa 'Gwe,",
        "Yena a ntokafatše;"
      ],
      "number": 4
    }
  ]
}