{
  "number": 113,
  "language": "nso",
  "title": " 113 JESO O A BITŠA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 113 JESO O A BITŠA",
        "АН 169 CH 563",
        "CALLING"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ka bonolo Jeso o a go bitša,",
        "O bitša wena le nna;",
        "O letile monyakong wa pelo,",
        "'Letile wena le nna;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Tlang gae, tlang gae,",
        "Lena ba tennweng, Tlang gae,",
        "Ka bonolo Jeso o a go bitša,",
        "Modiradibe tla gae;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Re duletšeng Jeso a re kgopela,",
        "A rapelela rena,",
        "Re diegelang go kwa mogau,",
        "Mogau wa 'go le nna;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Nako e akgofile e a feta,",
        "E feta wena le nna,",
        "Meriti ya tla bošego bja lehu,",
        "Di tela wena le nna;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Gopola ka tshepišo e makatšang,",
        "Tshepišo ya 'go le nna,",
        "Le ge re sentše O a re swarela,",
        "Swarela wena le nna;"
      ],
      "number": 6
    }
  ]
}