{
  "number": 41,
  "language": "nso",
  "title": " 41 MODIMO A BE LE LENA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 41 MODIMO A BE LE LENA",
        "AH 479 CH 35",
        "GOD BE WITH YOU"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Modimo a be le wena,",
        "a go hlatle a go thekge;",
        "A go šireletše gohle,",
        "Go fila re kopana gape;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Re kopa.........ne, Re Kopane;",
        "Re kopane naong tša Jeso",
        "Re kopa.......ne, Re kopane",
        "Modimo a be le wena;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Modimo a be le wena,",
        "Phukeng tša gwe a go file,",
        "Ka mmana a go fepe,",
        "Modimo a be le wena;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Modimo a be le wena,",
        "Tlalelong le matshwenyegong,",
        "'Atleng tša 'Gwe a go ake,",
        "Modimo a be le wena;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Modimo a be le wena,",
        "Ka lerato a go tswese,",
        "A Sireletše lehung,",
        "Modimo a be le wena."
      ],
      "number": 6
    }
  ]
}