{
  "number": 14,
  "language": "nso",
  "title": " 14 AH EKWA DIPENI DI E WA ",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 14  AH EKWA DIPENI DI E WA",
        "CS 573",
        "HEAR THE PENNIES DROPPING"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Kwa tshelet 'e tshelwa",
        "E kwa ge e wa;",
        "Ke ya Jeso yohle,",
        "e fiwe yena."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Khenkhe! Khenkhe!",
        "Bjalo ke moneelo;",
        "O fiago Morwa,",
        "Ke bananyana."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Pušulošo",
        "Khenkhe! Khenkhe!",
        "Khenkhe! Khenkhe!",
        "\"E kwa e tshelwa",
        "Ke ya Jeso yohle,",
        "E fiwe yena."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Re le bananyana",
        "re tliša peni.",
        "Mohla re godile,",
        "re tliše kudu"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Ge re sena selo",
        "re mofe 'rato;",
        "O tlo thaba kudu",
        "a amogele."
      ],
      "number": 6
    }
  ]
}