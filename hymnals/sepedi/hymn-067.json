  "title": " 67 TLANG BAO RE RATAGO MORENA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 67 TLANG BAO RE RATAGO MORENA",
        "АН 435 CH 640",
        "COME WE THAT LOVE THE LORD"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Tlang bao re ratago,",
        "Morena re thabeng,",
        "Re opele mmogo bohle,",
        "Re opele mmogo bohle,",
        "Re hlomphe bogoši,",
        "Re hlomphe bogoši;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Re ya kua Sione,",
        "O mobotse Sione,",
        "Re ya legodimong,",
        "Motse mobotse wa Modimo;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ba ganang G'opela,",
        "Ba sa tsebeng Modimo,",
        "Mme bana ba legodimo,",
        "Mme bana ba legodimo,",
        "Ba opele thabong,",
        "Ba opele thabong;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Thaba ya Sione,",
        "E tšweletša botse,",
        "Sešo re file Godimong,",
        "Sešo re fihle Godimong,",
        "Mebileng ya gauta,",
        "Mebileng ya gauta;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "A keng re opele,",
        "Megokgo e ome,",
        "Re sepele Imanuele,",
        "Re sepele Imanuele,",
        "Go ya Legodimong.",
        "Go ya Legodimong:"
      ],
      "number": 6
    }
  ]
}