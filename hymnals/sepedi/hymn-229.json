  "title": " 229 KE MANG YA LEGO MORENENG",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 229 KE MANG YA LEGO MORENENG",
        "CS 59",
        "WHO IS ON THE LORD'S SIDE"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ke mang wa Moreneng? Tsemelela,",
        "Mahlakoreng a mabedi, O eme kae?"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Kgetha bjale,",
        "Botse goba bobe o eme kae?",
        "Kgetha bjale,",
        "Botse goba bobe o eme kae?"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "T&B",
        "Ke mang ya lego Moreneng,",
        "Botseng goba bobeng o eme kae?"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Bontši ga bo kgethe botse,",
        "Mme ga se sona sa nepagalo,"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Ema Moreneng, Ka lebaka mang?",
        "Phološo e gona kwa pele;"
      ],
      "number": 6
    }
  ]
}