{
  "number": 77,
  "language": "nso",
  "title": " 77 KE RATA GO BOLELA DITSHWANTŠHETŠO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 77 KE RATA GO BOLELA DITSHWANTŠHETŠO",
        "AH 95",
        "I LOVE TO TELL THE STORY"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ke rata go bolela",
        "tshwantšhetšo ya legodimo,",
        "Ka Jeso kgalalelong,",
        "Le ka 'rato la 'Gwe,",
        "Ke rata go e bušeletša,",
        "Go bane e le nnete,",
        "E nkgodiša kganyogo,",
        "Go fetiša tšohle,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Ke rata go e bušeletša,",
        "Tshwantšhetšo e kgethwa,",
        "Tshantšhetšo 'a kgale-kgale,",
        "Ya rato la Jeso,"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ke rata go e bušeletša,",
        "Tshwantšhetšo e makatšago,",
        "Go feta le gauta",
        "tša tora tša rena,",
        "Ke rata go e bušeletša,",
        "Go batho ba bangwe,",
        "Ge ba seso ba kwe,",
        "Thomo ya Modimo;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Ke rata go e bušeletša",
        "tshwantšhetšo e monate,",
        "Gommo ke tlo e bušeletša,",
        "E ba botse ka kudu,",
        "Ke rata go e bušeletša,",
        "Go batho ba bangwe,",
        "Ge ba sešo ba kwe,",
        "Thomo ya Modimo,"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Ke rata go e bušeletša,",
        "Go ba tsebago,",
        "Ba rata go e kwa,",
        "Le bjaloka ba bangwe,",
        "Ge ke le kgalalelong,",
        "koša ya ka e mpša,",
        "E tlo ba yona ye,",
        "Yeo ke e ratilego;"
      ],
      "number": 6
    }
  ]
}