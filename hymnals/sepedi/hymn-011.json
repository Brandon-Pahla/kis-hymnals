{
  "number": 11,
  "language": "nso",
  "title": " 11 KE TLO LATELA M'PHOLOŠI",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 11  KE TLO LATELA M'PHOLOŠI",
        "AH 282 CH 266",
        "I WILL FOLLOW THEE MY SAVIOUR"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ke tlo latela M'phološi",
        "gohle mo a nkisago,",
        "Mo a yang ke tlo latela",
        "ee Morena ke ye ntshe;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušulošo",
        "Ke tlo latela M'phološi",
        "yo'a ntshololetseng madi",
        "Le ge bangwe ba go lathla",
        "nna ke tlo go latela"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Le ge tsela e le mmitiwa,",
        "Mme e sa bonagale;",
        "O e sepetse ka pele,",
        "Ka thabo ke latela."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Le gare ga matshwenyego,",
        "ke wela le ditekong;",
        "Le wen 'o kile wa lekwa,",
        "ka gona ke latela"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "O ntshepetša tlaisegong,",
        "Mmogo le boditaneng;",
        "Wena o kile wa hloka,",
        "ka gona ke latela."
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "Ge Jordane ka maphotho,",
        "e tonya le go tshosa",
        "O e wetse pele ga ka,",
        "lenna ke tlo latela."
      ],
      "number": 7
    }
  ]
}