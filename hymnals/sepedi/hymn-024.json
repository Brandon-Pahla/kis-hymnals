{
  "number": 24,
  "language": "nso",
  "title": " 24 KE GO HLOKA NAKO TŠOHLE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 24 KE GO HLOKA NAKO TŠOHLE",
        "AH 222 CH 258",
        "I NEED THEE EVERY HOUR"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Kea go hloka hle,",
        "Mokgethwa-kgethwa;",
        "Ga go lentšu le",
        "kang go nkhomotša"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušulošo",
        "Kea go hloka Jeso",
        "ka dinako tšohle,",
        "Ntshegofatse Mpholosi",
        "ke tla go wena;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Kea go hioka hle,",
        "Dula hleng ga ka,",
        "Teko ga dimphenye",
        "ge o le gona"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Kea go hloka hle,",
        "thabong, bohlokong;",
        "E tla ka pejana,",
        "Dula le nna"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Kea go hloka hle,",
        "O tle gonthuta",
        "Masot 'a tshepiso",
        "o ntshollele."
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "Kea go hloka hle,",
        "Wen 'o Mokgethwa",
        "Ntire wa Gago",
        "Morw 'a Modimo,"
      ],
      "number": 7
    }
  ]
}