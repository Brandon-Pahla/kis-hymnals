{
  "number": 140,
  "language": "nso",
  "title": " 140  LENTŠU LA JESO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 140  LENTŠU LA JESO",
        "CH 226 AH 171",
        "THE VOICE OF JESUS"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ke kwa ntšu la Jeso",
        "Le re \"Tia go nna ikhutše.",
        "Ithekge ge o lapile,",
        "Mo sefegeng sa ka,",
        "Ka tla go M'phološi,",
        "Ke lapile ka kudu,",
        "Ka hwetša khutšo,",
        "Yena a nthakgatša;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Ke kwa Jeso a re,",
        "\"Ke go nea fela,",
        "Wena o nyorilwego",
        "Meets šea enwa o phele'",
        "Ka tla go jeso ka",
        "Nwa meetse 'a phedišago,",
        "Ka omologa ka mpšhafala,",
        "Bjale ke phedile;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ke kwa Jeso a re,",
        "\"Ke seetša sa 'fase,",
        "Leba go nna o tihabelwe,",
        "Tšatši la phadimo,",
        "Ka leba Jeso, mme",
        "Yena ke letšatši la ka,",
        "Ke tlo sepela 'seding le,",
        "Ke be ke fetše leeto;"
      ],
      "number": 4
    }
  ]
}