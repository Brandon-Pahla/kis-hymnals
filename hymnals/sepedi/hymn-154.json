{
  "number": 154,
  "language": "nso",
  "title": " 154  MOLETI MPOTŠE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 154  MOLETI MPOTŠE",
        "AH 409 CH 180",
        "WATCHMAN TELL ME"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Moleti mpotše,",
        "A moso wa Sione o a sa?",
        "A tšhupo tša go tla ga 'Gwe,",
        "Di phadimile tseleng?",
        "Moleti! Tsoga, bona,",
        "Mahube a nyarela,",
        "Apara tšwalo tša lenyalo,",
        "Bo sele tsoga, tsoga;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Moleti, bona sedi le",
        "phadima tseleng ya 'Go,",
        "Tšhupo di mo faseng lohle,",
        "Tša letšatši lele tlago,",
        "Ge phalafala e Ila go",
        "Tsoša ba leng lewatle,",
        "Bakgethwa ba robetšego",
        "Ba apare go sehwe;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Moleti tumiša 'sedi",
        "La ngwaga wa Sabata,",
        "Bohle ba bolele gore",
        "Bogoši bo kgauswi,",
        "Mofeti ke bona mošoloa,",
        "Kgalalelo ya Kanana,",
        "Motse le bo botse,",
        "Seding la kwa godimo;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Moleti motseng wa gauta,",
        "O dutše troneng yaGagwe,",
        "Ke Kgoši ya Sione,",
        "O buša ka khutšo,",
        "Kua thabeng tša lesedi,",
        "Di phadima botlalong,",
        "Melatswana e phadimago,",
        "Mo go Melago dikenywa;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Moleti fase le kgauswi,",
        "Le nago le dikenywa,",
        "Ka mošola go botse bjang,",
        "Dienywa tša Edene,",
        "Go kwagala dikoša,",
        "Di duma kua godimo,",
        "Bona bontši, theetša koša,",
        "Bafeti ba ya ntshe;"
      ],
      "number": 6
    }
  ]
}