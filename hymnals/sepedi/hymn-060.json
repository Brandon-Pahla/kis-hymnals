{
  "number": 60,
  "language": "nso",
  "title": " 60 JESO THABO YA PELO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 60 JESO THABO YA PELO",
        "AH 128 CH 154",
        "JESUS THOU JOY OF LOVING HEARTS"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Jeso thabo ya pelo,",
        "Wena noka ya bophelo;",
        "Lethabong la lefase,",
        "Re boela go wena gape;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Wena bogobe bjo 'phelago,",
        "Re lakatša go ja wena",
        "Re nwa go wena sediba,",
        "O tlatše meoya ya rena;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Re go lakatša Meoyeng,",
        "Gohle moo re lahlelwago ntshe;",
        "Go lethabo ge bosele,",
        "Mahlatse ge re go dumela;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Jeso o dule go rena,",
        "O re direle khutšo;",
        "Leleka swiswi la sebe,",
        "O kgantšhetše lefase."
      ],
      "number": 5
    }
  ]
}