{
  "number": 211,
  "language": "nso",
  "title": " 211 RE TLO NELWA KE MASOTO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 211 RE TLO NELWA KE MASOTO",
        "АН 153 CH 652",
        "SHOWERS OF BLESSING"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Re tlo nelwa ke mašoto,",
        "Tshepišo tša lerato;",
        "Dinako tša tapologo,",
        "Tše tšwang go Mophološi;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Go na mašoto,",
        "Mašoto re a hloka,",
        "Re welwa ke tšhegofatšo,",
        "Re kgopela mašoto;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Re tlo nelwa ke mašoto,",
        "A tlo re tsošološa,",
        "Dithabeng le dikoting,",
        "Go duma pula e ntši,"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Re tlo nelwa ke mašoto,",
        "A re wele lehono;",
        "Bjale re fe tapologo,",
        "Tla bjale hlompha mantšu;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Re tlo nelwa ke mašoto,",
        "A re wele lehono",
        "Ge re 'ipolela Modimong,",
        "Re bitša le go Jeso;"
      ],
      "number": 6
    }
  ]
}