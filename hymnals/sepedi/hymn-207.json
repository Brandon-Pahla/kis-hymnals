{
  "number": 207,
  "language": "nso",
  "title": " 207  TIIŠA TSHWARELO YA KA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 207  TIIŠA TSHWARELO YA KA",
        "CH 25",
        "SEAL MY PARDON"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ka bona yo'a leketlago,",
        "A dutla le madi,",
        "A nteba pelong bohloko,",
        "Ke eme fapanong;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Nka se ke ka lebala,",
        "Tebelelo ya 'Gwe,",
        "E be eke wa ntshekiša,",
        "Ka lehu la Gagwe;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Tebo ya bobedi ya re,",
        "\"O swaretšwe dibe!\"",
        "Ke hwile gore o phele,",
        "Madi a lefile;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Ge ke utulla le lehu,",
        "Ke baka la sebe,",
        "Khupamarama mašotong,",
        "Ke swarelo ya ka;"
      ],
      "number": 5
    }
  ]
}