{
  "number": 79,
  "language": "nso",
  "title": " 79 PITŠO YA BABUNI",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 79 PITŠO YA BABUNI",
        "АН 338 CH 448",
        "FAR AND NEAR THE FIELDS"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Kgole, kgauswi mašemong",
        "Go tletše ka puno",
        "Go phatsima gauta ya yona,",
        "Thabeng ke mabele;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Morena roma babuni",
        ", Re ya go rapela,",
        "Ba romele batlo go",
        "Buna sešo go fete nako;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ba romele mo mesong,",
        "Gape le mosegare,",
        "Ge letšatši le dikela,",
        "Batle ba phuthe tšohle;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Wena o rongwago ke Morena,",
        "O phuthe 'ngata;",
        "Legoding mantšiboa,",
        "O tlo ba le lethabo;"
      ],
      "number": 5
    }
  ]
}