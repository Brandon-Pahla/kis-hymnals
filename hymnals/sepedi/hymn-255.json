{
  "number": 255,
  "language": "nso",
  "title": " 255  KE YA GO HLOKA JESO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 255  KE YA GO HLOKA JESO",
        "AH 320 CH 605",
        "I NEED THEE, PRECIOUS JESUS"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ke ya go hloka Jeso,",
        "Ke le modidi,",
        "Moeti le mofeti,",
        "Ga ke na tša lefase,",
        "Ke hloka rato la 'Go,",
        "Go nkhomotša tseleng,",
        "Tiiša kgato tša ka,",
        "Go ba maatla a ka;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Ke hloka pelo 'a Jeso,",
        "Go kwa ke tlalelwa,",
        "Bolela teko 'a ka,",
        "Go nthuša bohlokong,",
        "Ke hloka kgaugelo 'a 'Go,",
        "Go ba thušo ya ka,",
        "Go mpontšha tša Jeso,",
        "Go ntšhupa konyana;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ke ya go hloka Jeso,",
        "Mme ke tlo go bona,",
        "O le kgalalelong,",
        "O dutše teroneng,",
        "Ke na le baphološwa,",
        "Ke tle ke thabele,",
        "Go opela tumišo tša 'Go,",
        "Go bona Morena, Amen;"
      ],
      "number": 4
    }
  ]
}