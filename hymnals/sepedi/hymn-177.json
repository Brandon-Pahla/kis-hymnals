{
  "number": 177,
  "language": "nso",
  "title": " 177  GAGO BOŠEGO KUA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 177  GAGO BOŠEGO KUA",
        "AH 421 CH 558",
        "NO NIGHT THERE"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Lefaseng la lesedi,",
        "\"Go na le motse ntshe,",
        "O ka seke wa feta,",
        "\"Bošego a bo go\";"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Modim'o tlo phumula,",
        "Dikeledi le lehu,",
        "Ga go balwe mengwaga,",
        "Bošego ga bo go;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Mejako ya perela,",
        "E motseng wola,",
        "Mebila ke'a gauta,",
        "\"Ga go bošego gona,"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Mejako ga e tšwalwe,",
        "Gona motseng wola,",
        "Go nako ya bophelo,",
        "“Bošego ga bo go\"."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Ga ba hloke letšatši,",
        "Gona motseng wola,",
        "Lesedi ke konyana,",
        "\"Bošego ga bo go\""
      ],
      "number": 6
    }
  ]
}