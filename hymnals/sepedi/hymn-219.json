{
  "number": 219,
  "language": "nso",
  "title": " 219  NKGOGE HLE WENA JEHOFA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 219  NKGOGE HLE WENA JEHOFA",
        "АН 303 CH 409",
        "GUIDE ME O THOU GREAT JEHOVA"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "O nkgoge wena Jehofa,",
        "Ke mofeti lefaseng,",
        "Ke'a fokola mme O maatla,",
        "O sware ka seatla,",
        "O, M'phološi! O, M'phološi!",
        "O senkgwa sa bophelo,",
        "O senkgwa sa bophelo;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "O bule bjale sediba,",
        "Sa meetse a fodišago,",
        "Leru la moll'o fišago,",
        "Le ntshepetše leeto,",
        "Mophološi! Mophološi",
        "O be kgothatšo ya ka,",
        "O be kgothatšo ya ka;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ke tlo opela",
        "Poloko ka gohle,",
        "Bohle ba kwe",
        "Ba tshepe le Modimo;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Ge ke fihla Joredane,",
        "O tloše 'tšhogo la ka,",
        "O ntlodiše le maphotho,",
        "O mphiliše Kanana,",
        "Mophološi! Mophološi!",
        "Wena ke go tumiše,",
        "Wena ke go tumiše;"
      ],
      "number": 5
    }
  ]
}