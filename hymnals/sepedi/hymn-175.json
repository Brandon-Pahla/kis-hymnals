  "title": " 175  MOPHOLOŠI BJA KA MODIŠI",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 175  MOPHOLOŠI BJA KA MODIŠI",
        "AH 294 CH 394",
        "SAVIOR, LIKE A SHEPHERD"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "M'phološi Bja ka modiši,",
        "Re goge ka bonolo,",
        "Re je mafulong a mabotse,",
        "Re lokišetše lešaka,",
        "Jeso ya šegofaditšwego,",
        "O re rekile rena,",
        "Jeso ya šegofaditšwego,",
        "O re rekile rena;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Re ba gago re 'loko le tee,",
        "o diše tsela ya rena,",
        "O re diše le bobeng,",
        "Re nyaka re timetše,",
        "Jeso ya šegofaditšwego,",
        "Ekwa ge re rapela,",
        "Jeso ya šegofaditšwego,",
        "Ekwa ge re rapela;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "O tshepišitše gore,",
        "O tlo re amogela,",
        "Kgaugelo e tlo re lokolla,",
        "Mogau o re matlafatše,",
        "Jeso ya šegofaditšwego,",
        "'Nna re tla go wena,",
        "Jeso ya šegofaditšwego,",
        "'Nna re tla go wena;"
      ],
      "number": 4
    }
  ]
}