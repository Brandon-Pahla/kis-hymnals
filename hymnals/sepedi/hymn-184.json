{
  "number": 184,
  "language": "nso",
  "title": " 184  THABANG LEFASENG",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 184  THABANG LEFASENG",
        "AH 403 CH 189",
        "JOY TO THE WORLD"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Thabang Morena o a tla,",
        "Amogelang Kgoši,",
        "Pelo tšohle di lokišwe,",
        "Go tsebišwe ka lerato,",
        "Le hlago e opele, Le hago e opele,",
        "Le hla... le hlag 'e opele;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Thabang Moren ‘o tlo buša,",
        "Batho ba opele,",
        "Mašemong, meetseng, matlapeng,",
        "Mebotong le mabaleng,",
        "Go tumišwe gape, Go tumišwe gape,",
        "Go tu... go tumišwe gape;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "O tlo buša lefaseng,",
        "Batho ba tlo šupa,",
        "Kgalalelo toko ya 'Gwe,",
        "Kgalalelo le mogau,",
        "Le go loka ga 'Gwe,",
        "Bo botse bja rato,",
        "bo botse bja rato, Bo botse.",
        "Bo botse bja lerato;"
      ],
      "number": 4
    }
  ]
}