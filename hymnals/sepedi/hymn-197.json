{
  "number": 197,
  "language": "nso",
  "title": " 197  GA GO SEKOTI SE SE LESWISWI",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 197  GA GO SEKOTI SE SE LESWISWI",
        "CS 854",
        "THERE'LL BE NO DARK VALLEY"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ga go 'koti sa 'swiswi Jeso'a tla,",
        "Ga go 'koti sa 'swiswi Jeso'a tla,",
        "Ga go 'koti sa 'swiswi Jeso'a tla,",
        "Go tlo phutha ba Gagwe;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Go tlo phutha ba Gagwe,",
        "Go tlo tšea ba Gagwe,",
        "Ga go 'koti sa 'swiswi Jeso'a tla",
        "Go ba iša kua gae;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Go se be le manyami Jeso' a tla,",
        "Go se be le manyami Jeso' a tla,",
        "E tlo ba moso wa kgalalelo,",
        "Ge a tlo tšea ba 'Gwe"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Go se be le dillo ge a etla,",
        "Go se be le dillo ge a etla,",
        "Ke puno e botse mohlang a tla,",
        "Go phutha bana ba 'Gwe;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "E tlo ba koša tša go tumiša,",
        "E tlo ba koša tša go tumiša,",
        "Le kopano ya 'thabo ge a tla,",
        "Go phutha bana ba 'Gwe;"
      ],
      "number": 6
    }
  ]
}