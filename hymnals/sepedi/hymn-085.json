{
  "number": 85,
  "language": "nso",
  "title": " 85 KE RETA MATLA 'MODIMO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 85 KE RETA MATLA 'MODIMO",
        "AH 67 CH 93",
        "I SING THE MIGHTLY POWER"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ke reta matia \"Modimo,",
        "A thlotšeng dithaba",
        "A adileng le lewatle,",
        "E le legodimo,",
        "Ke reta bohlale bja",
        "go bea le letšatši,",
        "Le ngwedi go buša",
        "bošego le dinaledì;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Ke reta kgaugelo ya 'Gwe,",
        "O refa le dijo,",
        "O tlhotše ka 'ntšu la 'Gwe,",
        "A re go gobotse,",
        "Re bona tše re makatšang,",
        "Mahlakoring ohle,",
        "Tšohle dia re makatša,",
        "Bona Legodimo,"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Tšohle tše melago fase,",
        "Dia go tumiša,",
        "Tšohle di reta Mmopi,",
        "Le ge di homotše,",
        "Le diphidi dia phedišwa,",
        "Di thokomelong,",
        "Re ka tšhabela kae na?",
        "Modimo o gohle;"
      ],
      "number": 4
    }
  ]
}