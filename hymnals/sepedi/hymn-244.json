{
  "number": 244,
  "language": "nso",
  "title": " 244  TLA GO JESO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 244  TLA GO JESO",
        "CS 179",
        "COME TO JESUS"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Tla go Jeso, Tla go Jeso",
        "Tla go Jeso bjale,",
        "Bjale tla go Jeso,",
        "Tla go Jeso Bjale,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Oa boloka, Oa boloka,",
        "Oa boloka bjale,",
        "Bjale wa boloka,",
        "Oa boloka bjale,"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "A ka kgona, A ka kgona,",
        "A ka kgona bjale,",
        "Bjale a ka kgona,",
        "A ka kgona bjale;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Bitša yena, Bitša yena,",
        "Bitša yena bjale,",
        "Bjale bitša yena,",
        "Bitša yena bjale;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "O letile, O letile,",
        "O letile bjale,",
        "Bjale o letile,",
        "O letile bjale;"
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "O mo tshepe, O mo tshepe,",
        "O mo tshepe bjale,",
        "Bjale o mo tshepe,",
        "O motshepe bjale;"
      ],
      "number": 7
    },
    {
      "type": "verse",
      "lines": [
        "Šegofatšwa, Šegofatšwa,",
        "Šegofatšwa bjale,",
        "Bjale Šegofatšwa,",
        "Šegofatšwa bjale;"
      ],
      "number": 8
    }
  ]
}