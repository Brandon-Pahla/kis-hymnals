{
  "number": 97,
  "language": "nso",
  "title": " 97 RE KWELE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 97 RE KWELE",
        "АН 196 CH 637",
        "WE HAVE HEARD"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Re kwele tše di botse,",
        "Jeso Mophološi,",
        "Tsebišang batho bohle,",
        "Jeso o'a phološa,",
        "Goeletšang tlase meeding,",
        "Le godimo dithabeng.",
        "Morena o laetše,",
        "Jeso wa phološa;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Tumišang mo tumišeng,",
        "Jeso Mophološi,",
        "Tsebišang balahlegi,",
        "Jeso ke 'Mphološi,",
        "Opelang kua lešokeng,",
        "Go kwale le maphothong,",
        "Lefase le be thabong,",
        "Jeso ke 'Mphološi;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Goelelang ka moKgoši,",
        "Jeso ke 'Mphološi,",
        "Ka lehu le bophelo,",
        "Jeso ke 'Mphološi,",
        "Opelang ka bonolo,",
        "Ge pelo e swabile,",
        "Lebitla le fentšwe,",
        "Ke Jeso 'Mphološi;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Hlabosa lentšu moyeng,",
        "Jeso ke 'Mphološi,",
        "Lethabo 'tšhabeng tšohle,",
        "Jeso ke 'Mphološi,",
        "Goelela ka phološo,",
        "Dithabeng le magageng,",
        "Ke košaya bafenyi,",
        "Jeso Mophološi;"
      ],
      "number": 5
    }
  ]
}