{
  "number": 289,
  "language": "nso",
  "title": " 289  NTŠEE KE LE BJALE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 289  NTŠEE KE LE BJALE",
        "CS 103",
        "TAKE ME AS I AM"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Jeso Morena ke'a lla,",
        "Morena nthuše ke ya hwa,",
        "Tliša bophelo bja Gago,",
        "\"Nkuke ke le bjale\";"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Ntšee Kgoši,",
        "Ntšee ke le bjale,",
        "Batametša phološo,",
        "\"Ntšee ke le bjale\";"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ke tletšeka dibe Jeso,",
        "Ka madi a Gago nhlatšwe,",
        "O ka ntira seo se ratang,",
        "\"Ntšee ke le bjale\";"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Ga ke na le mešomo,",
        "E mebotse Moreneng,",
        "Mphološe ka ina la 'Go,",
        "\"Ntšee ke le bjale\";"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Ke khunama pele ga 'Go,",
        "Mpone M'phološi dinaong,",
        "Nthome mošomo wa Gago,",
        "\"Ntšee ke le bjale\";"
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "Ge 'o rata ke go šomele,",
        "Mpšafatša hlaologanyo,",
        "Nna o ntšhomiše:",
        "\"Ntšee ke le bjale\";"
      ],
      "number": 7
    },
    {
      "type": "verse",
      "lines": [
        "Pheletšong ya mošomo,",
        "Ge šetše ntwa e fentšwe,",
        "Sello sa ka e tlo ba se,",
        "\"Ntšee ke le bjale\";"
      ],
      "number": 8
    }
  ]
}