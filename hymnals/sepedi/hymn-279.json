{
  "number": 279,
  "language": "nso",
  "title": " 279 PELO E MAHLOMOLENG.",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 279 PELO E MAHLOMOLENG.",
        "CS 16",
        "COME UNTO ME"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Pelo e le maswabing,",
        "Maahlo a lakatšago,",
        "Thabo e gona tumelong,",
        "Lesedi go Jeso,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Tlang (e tlang) keno go nna (lena),",
        "Bohle ba le lapilego, Le imelwago (moyeng)",
        "Ke tlo le lapolosa: (etlang le)",
        "Itšeeleng joko ya ka, Le ithute go nna,",
        "Gobane ke bonolo, Ke pelohlomogi,",
        "Le tlo humana Khutšo meoyeng;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Menate ya lefase ga",
        "e kgotsofatše,",
        "'Rato le šupa 'thabo,",
        "Lerato ga le hwe,"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Khomotšo ya godimo e,",
        "Newa ke Kreste,",
        "Na wena o loketše tahlego?",
        "Itshole (sokologa o phele),"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Khutšo ya 'Gwe eke noka,",
        "'Rato la 'Gwe eke koša,",
        "Joko ya 'Gwe e bofefo,",
        "Letšatši lohle;"
      ],
      "number": 6
    }
  ]
}