{
  "number": 214,
  "language": "nso",
  "title": " 214  A JESO WA HLOKOMELA?",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 214  A JESO WA HLOKOMELA?",
        "АН 122 CH 613",
        "DOES JESUS CARE?"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "A Jeso 'a hlokomela ge ke,",
        "Kwa bohloko pelong?",
        "Ke le mathateng, Le pitlaganong,",
        "Mme ke lapišwa ke tsela?"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Ke'a tseba oa hlokomela,",
        "Go kgoma pelo ya 'Gwe,",
        "Dinakong tše thata ke a tseba,",
        "O nhlokomela kudu;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "A Jeso o hlokomela",
        "Ge tsela e le leswiswing?",
        "Ge tšatši le phirima bošego,",
        "A o dula a le kgauswi?"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "A Jeso o hlokomela",
        "Ge meleko e nhlagela?",
        "Pelong e bohloko ke sa kgaotše,",
        "Ke lla bošego bjohle?"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "A Jeso o hlokomela ge",
        "Ke re \"šala moratwa?",
        "Pelo ya ka eke ya phatloga,",
        "A tše di a mo kgoma?"
      ],
      "number": 6
    }
  ]
}