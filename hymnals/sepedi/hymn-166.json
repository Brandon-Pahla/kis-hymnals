{
  "number": 166,
  "language": "nso",
  "title": " 166 LESWIKA LA BOSAFELEGO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 166 LESWIKA LA BOSAFELEGO",
        "АН 237 CH 474",
        "ROCK OF AGES"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "‘Swika la bosafeleng,",
        "Ke iphihle go wena,",
        "A meetse le madi ao",
        "A tšwang hlakoreng,",
        "A a mphodiše dibe,",
        "Mpoloke mo melatong;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Ga se ka maatla a ka,",
        "Nka lokiša melato,",
        "Le ge ke sa ikhutše,",
        "Keledi di tšhologa,",
        "Ga go tokišo sebeng,",
        "Ge e se wena fela;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ga ke na selo go nna,",
        "Ke kgomaretše wena,",
        "Ke hloka go apešwa,",
        "Ke hloka tšhegofatšo,",
        "Ke fofela motsweding,",
        "Nhlatšwe Morena ke'a hwa;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Ge ke sa le bophelong,",
        "Ke bodulala ke hwa,",
        "Ge ke ya le leswiswing,",
        "Ke letile kahlolo,",
        "Swika la bosafeleng,",
        "Ke iphihle go wena;"
      ],
      "number": 5
    }
  ]
}