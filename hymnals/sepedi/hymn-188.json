{
  "number": 188,
  "language": "nso",
  "title": " 188  MMUŠI WA BADIRADIBE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 188  MMUŠI WA BADIRADIBE",
        "AH 188 CH 238",
        "CHIEF OF SINNERS"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Le ge ke dira dibe,",
        "Jeso o Nkhwetše le nna,",
        "Gore ke tle ke phele,",
        "Ore ke se ke ka hwa,",
        "Bja ka kala morareng,",
        "Ke wa 'Gwe yena wa ka,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Golo bja rato la 'Gwe,",
        "Bo feta legodimo,",
        "Bo feta le lewatle,",
        "Le ka mo bosafelengo,",
        "Le nkhumane kwa kgole,",
        "Ke sešo ke mo nyake,"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Le ge ke dira dibe,",
        "Kreste ke tšohle go nna,",
        "O tseba ka manyami,",
        "Mahloko, mahlomola,",
        "Ke bolokwa ke yena,",
        "O thekga le bophelo;"
      ],
      "number": 4
    }
  ]
}