      "lines": [
        "### 83 GO OPELA KA MALEME",
        "АН 139 CH 155",
        "O FOR A THOUSAND TONGUES"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Go opela ka maleme,",
        "Go retwe Jehova,",
        "Kgalalelo ya Modimo,",
        "Go retwe Jehova;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Le'a halalela,",
        "Le'a halalela",
        "leina la Mophološi,",
        "Le'a halalela,",
        "Lea halalela",
        "leina la Mophološi,"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Jeso ke mo.... phološi,",
        "Go retwe 'ina la Jeso,",
        "Ke koša go molahlegi,",
        "Go retwe 'ina la Jeso;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "O kgaola maatl'a sebe,",
        "Go retwe ina la Jeso,",
        "Re hlapa mading a Gagwe,",
        "go retwe ina la Jeso"
      ],
      "number": 5
    }
  ]
