{
  "number": 130,
  "language": "nso",
  "title": " 130 NKGOGELE KGAUSWI",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 130 NKGOGELE KGAUSWI",
        "АН 235 CH 594",
        "I AM THINE O LORD"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ke wa 'go Jeso, Ke kwa ntšu la 'go,",
        "Le mpotša ka lerato,",
        "Mme ke kganyoga go rotoga,",
        "Ke be kgauswi le wena;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Nkatametše kgauswi Morena,",
        "Le sefapano sa 'Go,",
        "Nkatametše kgauswi Morena,",
        "Hlakoreng le hlabilweng;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Nkgethe Morena ke go šomele,",
        "Ka matla a makgethwa,",
        "A moya waka o lebe Wena,",
        "Ke dire thato ya 'Go;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Go monate nakwana ye ke leng,",
        "Teroneng ya bogoši,",
        "Ge ke khuname ke le thapelong,",
        "Ke bolela segwera;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Go lerato leo le tseneletšeng,",
        "Ka mošola go noka,",
        "Go maiphihlelo a thabo kgolo,",
        "Boikhutšong le Morena."
      ],
      "number": 6
    }
  ]
}