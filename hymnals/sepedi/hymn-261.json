{
  "number": 261,
  "language": "nso",
  "title": " 261  MOLAPONG O MOBOTSE WA EDENE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 261  MOLAPONG O MOBOTSE WA EDENE",
        "CH 550 АН 429",
        "BEAUTIFUL VALLEY OF EDEN"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Molapo o botse wa Edene,",
        "Monate khutšong,",
        "Dipelong tše nyamilego,",
        "Ekwa monko o bose;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Molapo o botse wa Edene,",
        "Legae la bakgethwa,",
        "Gantši ge ke tlalelwa,",
        "Ke lora khutšo ya Gago;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Pelong ya yo a llago,",
        "Go phadima letšatši,",
        "La koša ya magneloi,",
        "Ao a opelago kgole;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Legae le gona la M'phološi,",
        "La sehlopha se sekgethwa,",
        "Lefaseng la kgalalelo,",
        "Go koša tše mpšha;"
      ],
      "number": 5
    }
  ]
}