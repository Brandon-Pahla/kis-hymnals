{
  "number": 15,
  "language": "nso",
  "title": " 15 E JESO O WA NTHATA ",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 15  AH 455  CS 283  E JESO O WA NTHATA JESUS LOVES ME, THIS I KNOW"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ee Jeso a nthata,",
        "Go bolela mangwalo;",
        "Bananyana ke bagwe,",
        "ba bofefo o maatla."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušulošo",
        "Jeso oa nthata,",
        "Ee oa nthata,",
        "Jeso oa nthata,",
        "ke kwa ka mangwalo."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ee Jeso oa nthata,",
        "Mme o ile a nkhwela,",
        "O hlatšwa dibe tšohle,",
        "Bana bagwe ba tsene;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Ee Jeso oa nthata,",
        "Maswabing le bohlokong,",
        "O tšwa kua legodimong,",
        "Go mpona ke robetse;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Ee Jeso wa nthata,",
        "O hiwa gohle le nna;",
        "Gomme ge ke mo rata,",
        "O tlo nkisa 'Godimong"
      ],
      "number": 6
    }
  ]
}