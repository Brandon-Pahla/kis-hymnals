{
  "number": 121,
  "language": "nso",
  "title": " 121 KE OPELA KA RATO LA JESO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 121 KE OPELA KA RATO LA JESO",
        "AH 111 CH 529",
        "I WILL SING OF JESUS LOVE"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "K' opela ka ga Jeso,",
        "Ke yena a nthatileng,",
        "O tlogile godimong,",
        "Go tlo hwa sefapanong,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "K' opela 'rato la 'Gwe,",
        "Jeso ke motumiše,",
        "O nkhwetše nna ke phele,",
        "Ke tlo opela rato,"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Pele ke sešo ke lle,",
        "Jeso o tlo ntellela,",
        "Ke sešo ke rapele,",
        "O ile a nthapelela;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Gotsenelela 'rato,",
        "'Fase ga le go tsebe,",
        "Ge dibe tše kang tša ka,",
        "Tšona di šweufetše;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Ga kena se bohlokwa,",
        "O be'a ka nthata bang,",
        "Morena o khumane,",
        "Nthuše ke go rate;"
      ],
      "number": 6
    }
  ]
}