{
  "number": 69,
  "language": "nso",
  "title": " 69 RE TLO IKHUTŠA SEDIBENG",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 69 RE TLO IKHUTŠA SEDIBENG",
        "AH 149 CS 206",
        "WILL TARRY BY THE LIVING WATERS."
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Re tlo ikhutša sedibeng,",
        "Metsing a itshekileng,",
        "Gona Jeso o re letile,",
        "Go re amogela;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Re tlo ikhutša sedibeng,",
        "Re ikhutše sedibeng,",
        "Re ikhutše sedibeng,",
        "Meetsing a itshekileng;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ge re lapile mo leetong,",
        "re tlo dula khutšong,",
        "Pele ga 'diba sa bophelo,",
        "motsweding wa 'phelo;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Tla go Jeso Kreste o tilšwe,",
        "O kgone go ema;",
        "Ke yena meetsi a bophelo,",
        "Enwa o se nyorwe;"
      ],
      "number": 5
    }
  ]
}