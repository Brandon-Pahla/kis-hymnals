  "title": " 12 SEDIBA SE GONA. ",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 12  SEDIBA SE GONA",
        "AH 135 CH 163",
        "THERE IS A FOUNTAIN"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Sediba se gona sa madi,",
        "A tšwago go M'phološi,",
        "Badiradibe ba tsena gona,",
        "Ba hlapišwa dibe,",
        "Ba hlapišwa dibe,",
        "Ba hlapišwa dibe,",
        "Badiradibe ba tsena gona,",
        "Ba hlapišwa dibe;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Lehodu le hwago le'a thaba,",
        "Go bona sediba",
        "Gobane lenna ke le ditšhila,",
        "Nka hlapa sedibeng,",
        "Nka hlapa sedibeng,",
        "Nka hlapa sedibeng,",
        "Gobane le nna ke le ditšhila,",
        "Nka hlapa sedibeng;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Madi a Gago konyan' e hwago,",
        "Ga' fele maatla,",
        "Phuthego e phološitšweng,",
        "E nyoretšwe Wena,",
        "Ba phološwe sebeng,",
        "Ba phološwe sebeng,",
        "Phuthego e nyoretšwego Wena,",
        "E phološwe sebeng;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Baka ke bone sediba,",
        "Sa madi tumelong,",
        "Lerato la 'Go le pelong,",
        "Go fihlela ke' hwa,",
        "Go fihlela ke' hwa,",
        "Go fihlela ke' hwa,",
        "Lerato la 'Go le pelong,",
        "Go fihiela ke' hwa;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Morena o ntukišeditse,",
        "le ge ke sa swanela,",
        "Tefelo ya neo ya madi,",
        "Bophelo bjo sa felego,",
        "Bophelo bjo sa felego,",
        "Bophelo bjo sa felego,",
        "Tefelo ya neo ya madi,",
        "Bophelo bjo sa felego;"
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "Go na le kos 'e monate,",
        "Ke tumiša M'phološi,",
        "Leleme le ikokobetšang,",
        "Lentšhitšwe lebitleng,",
        "Lentšhitšwe lebitleng,",
        "Lentšhitšwe lebitleng",
        "Leleme le ikokobeditšego,",
        "Lentšhitšwe lebitleng;"
      ],
      "number": 7
    }
  ]
