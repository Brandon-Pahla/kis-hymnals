{
  "number": 284,
  "language": "nso",
  "title": " 284  GE MATŠATŠI A FETA BJALO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 284  GE MATŠATŠI A FETA BJALO",
        "CS 486",
        "WHILE THE DAYS ARE GOING BY."
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Pelo di hloile ge",
        "Matšatši a feta bjalo,",
        "Meoya e a timela ge",
        "Matšatši a feta bjalo,",
        "Re tlo myemyela gona",
        "Tseleng yohle ya rena,",
        "Tse botse re ka di dira",
        "Ge matšatši a feta;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "A feta, a feta,",
        "A feta, a feta,",
        "Bo botse re ka bo",
        "Dira ge matšatši a feta;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ga go nako 'a papadi",
        "Ge matšatši a feta,",
        "Fahlogo di phadime",
        "Ge matšatši a feta,",
        "Faseng la mathata,",
        "Dikeledi di maahlong,",
        "Thuša ngwaneno a tsoge,",
        "Ge matšatši a feta;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Tlemo tšohle tša lerato,",
        "Ge matšatši a feta,",
        "Re di tlogela morago,",
        "Ge matšatši a feta,",
        "Re bjala peu tše botse,",
        "Di gola moriting, leseding,",
        "Di re thabiša dipelo,",
        "Ge matšatši a feta;"
      ],
      "number": 5
    }
  ]
}