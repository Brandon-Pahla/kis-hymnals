{
  "number": 135,
  "language": "nso",
  "title": " 135 KE A GO RATA MORENA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 135 KE A GO RATA MORENA",
        "CS 301",
        "I LOVE THEE BETTER, LORD"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ke a go rata Morena,",
        "Go feta tša 'fase,",
        "Gobane o mphile khutšo,",
        "E sa senyegego,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Ga se go bolelwe tšohle,",
        "Ka rato le ntletšego,",
        "Ga se go bolelwe tšohle,",
        "Madi a ntlhatšwago;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "O kgauswi go feta,",
        "Bohle ba lefase,",
        "Monate go gopola wena,",
        "Go fetisa koša;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "O mphile lethabo,",
        "Mme ke tlo thakgala,",
        "Kantle go lerato la gago,",
        "Ke dula ke swabile;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Mophološi yo botse wa ka,",
        "Go tlo ba bjang gonna,",
        "Ge re ka rwešwa thakgalo,",
        "Go wena 'faseng le;"
      ],
      "number": 6
    }
  ]
}