{
  "number": 117,
  "language": "nso",
  "title": " 117 KE TLA MORENENG",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 117 KE TLA MORENENG",
        "AH 173 CH 224",
        "I AM COMING LORD"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ke kwa ntšu la go,",
        "Le mpitša Morena,",
        "Go botsefatšwa ke madi,",
        "A elang khalfari;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Ke tla Morena,",
        "ke tla go wena,",
        "O ntlhapiše mo mading,",
        "A elang kalfari;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ke tla ke fokola,",
        "Maatla a go wena,",
        "Ntlwekiše dibe tšohle,",
        "Ke be yo' a hwekilego;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Jeso o mpiletša,",
        "Tumelong, Leratong,",
        "Ditshepišong le mo khutšong,",
        "Lefaseng legodimong;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Tumišang madi a 'Gwe,",
        "Tumišang poloko,",
        "Tumišang neo ya Morena,",
        "Tišong ya rena;"
      ],
      "number": 6
    }
  ]
}