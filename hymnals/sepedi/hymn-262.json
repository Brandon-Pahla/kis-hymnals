{
  "number": 262,
  "language": "nso",
  "title": " 262 KRESTE GOBA BARABAS.",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 262 KRESTE GOBA BARABAS.",
        "CS 1",
        "CHRIST OR BARABBAS"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ke tlo dirang ka Jeso?",
        "O eme pele ga ka,",
        "Ka aparo se sekhubedu,",
        "Le mmitšwa hlogong,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Ye ke potšišo bjale,",
        "Karab 'e tlo ba efe?",
        "A ke bapole Kgoši Jeso?",
        "Ke lese Barabas?"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ke lokolle lehodu,",
        "Leo le ikgogomošago?",
        "Sebe goba M'phološi?",
        "A o tlo lahlwa mang?"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Potšišo e bohlokwa ye,",
        "Kgwerana e sa eme,",
        "Kwalobohloko,",
        "E sa ntše e le gona,"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Sedulong sa kahlolo,",
        "A o ka lahla lefase?",
        "O sa ntše o goelela,",
        "Mmapoleng! Mmapoleng!"
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "Ga se modiradibe, fela,",
        "Potšišo e tla ka maatla,",
        "E theetšeng bakreste,",
        "Mo nakong ya diteko;"
      ],
      "number": 7
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Ye ke potšišo bjale,",
        "Karab 'e tlo ba efe?",
        "Ke tlo bapola Barabas,",
        "Ke lokolle Kgoši;"
      ],
      "number": 8
    }
  ]
}