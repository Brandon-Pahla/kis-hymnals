{
  "number": 125,
  "language": "nso",
  "title": " 125 LESEDI PELONG",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 125 LESEDI PELONG",
        "CH 612 АН 256",
        "SUNLIGHT IN THE HEART"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Go letšatši kua dithabeng,",
        "Go letšatši lewatleng,",
        "Mahlasedi a gauta",
        "A bonwa le botaleng,",
        "Mme lesedi le legolo",
        "Le tlatša pelo ya ka,",
        "Gobane o dula ntshe",
        "M'phološi, Lesedi le go Wena;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "A letšatši le lebotse,",
        "Ke letšatši ka pelong,",
        "Jeso o tloša maswabi,",
        "Ke letšatši ka pelong;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Leroleng ke tlogela maswabi,",
        "A matšatši a fetilego,",
        "O nkapeša ka lethabo,",
        "'Mme o ntlatša tumišo,",
        "Legaeng la kgalalelo,",
        "Leo o nthwaletšego lona,",
        "Ke rotoga ka pelo,",
        "Moya wa ka o latele;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "O nthekile M'phološi,",
        "Gohle ga ka ga Gago,",
        "Lebone la 'rato la 'Go,",
        "Le tuke go go tumiša,",
        "Legaeng la kgalalelo",
        "Leo o nthwaletšego lona,",
        "Ke rotoga ka pelo,",
        "Moya wa ka o latele;"
      ],
      "number": 5
    }
  ]
}