{
  "number": 137,
  "language": "nso",
  "title": " 137  MPHE JESO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 137  MPHE JESO",
        "CS 315 AH 104",
        "GIVE ME JESUS"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Tšea lefase mme o mphe Jeso,",
        "Tša lona ga se selo,",
        "Lerato la 'Gwe ga le fele,",
        "Le gona go ya guile;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Bogolo bja kgaugelo,",
        "Bophara bja lerato,",
        "Botialo bja thekololo,",
        "Tshepišo e sa felego;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Tšea lefase mme o mphe Jeso,",
        "Khutšo moyeng wa ka,",
        "Ke na le Mophološi wa ka,",
        "Mme ke tlo opela gohle,"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Tšea lefase mme o mphe Jeso,",
        "Ke mmone a myemyela,",
        "Mo leetong la ka lohle,",
        "Sedi le tla ntlabela,"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Tšea lefase mme o mphe Jeso,",
        "Ke tshepe sefapano",
        "Ke bone ka mo go hlakilego,",
        "Re lebane le Jeso;"
      ],
      "number": 6
    }
  ]
}