{
  "number": 251,
  "language": "nso",
  "title": " 251  GO LLA GO KA SE MPHOLOSE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 251  GO LLA GO KA SE MPHOLOSE",
        "CS 92",
        "WEEPING WILL NOT SAVE ME"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Dillo ga di mphološe,",
        "Ešita le dikeledi,",
        "Ga di alafe matswalo,",
        "Ga di nhlatšwe le dibe,",
        "Dillo ga di mphološe;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Jeso o ile 'a ntellela,",
        "A nkhwela sefapanong,",
        "Jeso o tlo ntokolla,",
        "Mme o tlo mpoloka;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Mešomo ga e phološe,",
        "Ditiro tše hiwekilego,",
        "Kgopolo e halalelago,",
        "E ka se ntokafatše,",
        "Mešomo ga e mphološe;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Tebelelo ga e mphološe,",
        "Ke timela melatong.",
        "Ke kwa ntšu la mogau,",
        "Ge ke leta ke tlo hwa,",
        "Tebelelo ga e mphološe"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Tumelo e a phološa,",
        "Ke tshepa Morw’a Gago,",
        "Ke tshepa tiro ya 'Gwe,",
        "Ke tšhabela go Yena,",
        "Tumelo e a mphološa;"
      ],
      "number": 6
    }
  ]
}