{
  "number": 163,
  "language": "nso",
  "title": " 163  KENNA E! ROMA NNA.",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 163  KENNA E! ROMA NNA.",
        "AH 341 CH 447",
        "HERE AM I, SEND ME"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ekwa Jeso ge a bitša,",
        "Ke mang a tlo go šoma?",
        "Mašemo 'a letile puno,",
        "Ngata di wala ke mang?",
        "Morena o a goelela,",
        "Moputs'o fiwa fela,",
        "Ke mang a tlo go araba,",
        "Kenna e roma nna;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Ge o sa tole lewatle,",
        "Go ya go baheitene,",
        "O ka ba hwetša kgauswi,",
        "ba thuše mo mojakong,",
        "Le ge o se legeloi,",
        "Thero e se tša Paulo,",
        "Bolela rato la Gagwe,",
        "O re \"O hwetše bohle;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ge o se moleta-kgoro,",
        "Lerakong la Sione,",
        "O šupa tsela' godimo,",
        "Ba hwetše 'phelo, khutšo,",
        "Ka thapelo le dineo,",
        "'Ka dira tše hlokwago,",
        "O ka swana le Arone,",
        "Thuša moporofeta;"
      ],
      "number": 4
    }
  ]
}