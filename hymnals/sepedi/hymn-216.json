{
  "number": 216,
  "language": "nso",
  "title": " 216  GA KE NNOŠI",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 216  GA KE NNOŠI",
        "CS 557",
        "NEVER ALONE"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Bodutung? Ga ke nnoši,",
        "Ge Jeso a le gona,",
        "O wa nthabiša,",
        "Ke tsebo' kgauswi,",
        "Bodutu? Aowa'a bogo,",
        "Jeso ke mogwera,",
        "Ke fetoga mme yena",
        "O ka bosafeleng;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Aowa ga ke nnoši,",
        "E ga ke nnoši,",
        "O tshepišitše gore",
        "Nka se be nnoši,"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "T&B",
        "Aowa, aowa ke nnoši,",
        "E ga ke nnoši",
        "O tshepišitše gore",
        "Nka se be nnoši;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Tennwe? Aowa nka se we,",
        "Ge ke le fegeng sa 'Gwe,",
        "Ke thaba boikhutšong",
        "Ka mo bosafeleng, Thušo?",
        "Ga ke na yo mme",
        "Ke 'ithekga Moreneng.",
        "Matsogong'a maatla'a Jeso,",
        "Bjale o mpolokile;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Ke letile? O re ke lete tiišong,",
        "Ke makala gantši,",
        "Ke eng se diegišago Morena,",
        "Lethabo le se nago hlathollo,",
        "Motheo o maatla,",
        "Lethabo Moreneng;"
      ],
      "number": 6
    }
  ]
}