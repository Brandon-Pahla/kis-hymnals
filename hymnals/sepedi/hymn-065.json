{
  "number": 65,
  "language": "nso",
  "title": " 65 GO BOTSE GO TSHEPA JESO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 65 GO BOTSE GO TSHEPA JESO",
        "АН 201 CH 588",
        "'T IS SO SWEET TO TRUST."
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Go botse go tshepa Jeso,",
        "Go theetša ntšu la gage,",
        "Le go boloka tshepišo,",
        "go rialo Morena,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Jeso, Jeso kea mo tshepa,",
        "Ke mo lekile gohle,",
        "Jeso, Jeso ya rategang,",
        "ruri ke tshepa wena;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Go botse go tshepa Jeso,",
        "Go tshepa madi a 'Gwe,",
        "Ka tumelo go hlwekišwa,",
        "Mading a Gagwe fela;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "E go botse go mo tshepa,",
        "Go tlogela le dibe,",
        "Go Jeso go itšeela,",
        "Bophelo, thabo, khutšo;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Ke thabetše go mo tshepa,",
        "Jeso M'phološi wa ka,",
        "Mme ke tseba o na le nna,",
        "O tlo ba le nna gohle;"
      ],
      "number": 6
    }
  ]
}