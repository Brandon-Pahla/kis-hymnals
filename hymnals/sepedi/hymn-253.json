{
  "number": 253,
  "language": "nso",
  "title": " 253  BONA MODIŠI WA ISRAELE YO BOLO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 253  BONA MODIŠI WA ISRAELE YO BOLO",
        "CH 411",
        "SEE, ISRAEL'S GENTLE SHEPHERD"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Modiši wa Israele,",
        "O eme o a bitša,",
        "Ekwa o bitša dikonyana,",
        "Mme o a di akarela;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "O re \"le ba lese ba tle,",
        "Le se ba thibele\",",
        "Gobane Morena o",
        "Tletše go ba phološa;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Morena re ba tlišitše,",
        "Ka lethabo;",
        "Ke ka thabo re lego ba Gago,",
        "Le bana ba rena;"
      ],
      "number": 4
    }
  ]
}