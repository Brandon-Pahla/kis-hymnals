{
  "number": 210,
  "language": "nso",
  "title": " 210 A MOPHOLOŠI YO BOTSE BJANG",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 210 A MOPHOLOŠI YO BOTSE BJANG",
        "АН 101 CH 644",
        "WHAT A WONDERFULL SAVIOUR"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Kreste ke pheko ya dibe,",
        "M'phološi yo'a makatšago!",
        "O lokolotše, o lefile,",
        "M'phološi yo'a makatšago!"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "M'phološi ya fenyago!",
        "Ke Jeso, Jeso wa ka,",
        "M'phološi ya makatšago,",
        "Jeso wa ka,"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ke motumiša ka madi,",
        "M'phološi ya makatšago!",
        "O mpoelantše le Modimo,",
        "M'phološi ya makatšago!"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "O nhlwekišitše le pelong,",
        "M'phološi ya makatšago,",
        "Mme ke yena ya bušago,",
        "M'phološi ya makatšago!"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "O sepela kgauswi le nna,",
        "M'phološi ya makatšago!",
        "Mme o nthekga ka mehla,",
        "M'phološi ya makatšago!"
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "O mpha maatla a phenyo,",
        "M'phološi ya makatiago!",
        "Le phenyo nakong ya teko,",
        "Mpholosi ya makatšago!"
      ],
      "number": 7
    },
    {
      "type": "verse",
      "lines": [
        "Ke mo file pelo yaka,",
        "M'phološi ya makatšago!",
        "'Fase ga le na kabelo,",
        "M'phološi ya makatšago!"
      ],
      "number": 8
    }
  ]
}