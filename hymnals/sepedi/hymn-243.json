{
  "number": 243,
  "language": "nso",
  "title": " 243 TSOGA MOYA WA KA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 243 TSOGA MOYA WA KA",
        "CS 123",
        "PRAISE MY SOUL, ARISE"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Tsoga moya wa ka,",
        "Tloga mo poifong ya 'Go,",
        "Tefelo ya madi e",
        "Šetše ntefeletše,",
        "Monkemedi o teroneng,",
        "Monkemed o teroneng",
        "Ina la ka atleng tša 'Gwe;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "O phele 'godimong,",
        "Go tlo nkgopelala,",
        "Ka rato la Gagwe",
        "ka madi a Gagwe,",
        "Ao a tšhologetšego bohle,",
        "Ao a tšhologetšego bohle;",
        "A gaša terone ya mogau;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Mabadi a mahlano",
        "A kwa khalfari,;",
        "A nkišetša thapelo,",
        "A mpolelela ka kudu,",
        "Mme a re \"mo tshwarele hle\",",
        "Mme a re \"Mo swarele hle\"",
        "Se mo tlogelele go hwa;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Tate o kwa,",
        "Mo tšwa wa Gagwe,",
        "A ka se fularele",
        "Morwa wa Gagwe,",
        "O arabela mo mading,",
        "O arabela mo mading,",
        "A re ke tšwetšwe ke Modimo;"
      ],
      "number": 5
    }
  ]
}