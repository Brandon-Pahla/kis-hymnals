{
  "number": 182,
  "language": "nso",
  "title": " 182  MOSO WA GAUTA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 182  MOSO WA GAUTA",
        "АН 394 CH 547",
        "GLEAMS OF THE GOLDEN MORNING"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Meso ya gauta e batametše,",
        "Jeso o tlo tla,",
        "Go tšeela bana ba Gagwe,",
        "Legaeng la bona,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Re bona 'sedi la moso 'a gauta,",
        "Le phunyeletša bosegong,",
        "Re bona 'sedi la moso 'a gauta,",
        "Le bula lebitla,"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Taelo ya efangedi e tloga,",
        "E eya ditšhabeng,",
        "Monyadi a ka se diege,",
        "Go letša phalafala;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "O tla le mangeloi,",
        "O theoga legodimong,",
        "Moahlodi o tlo iša ba Gagwe,",
        "Moo ba ka se sa hwago;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Ge ba kgogana le baratiwa,",
        "Ba tlo kopana,",
        "Megokgo ya bao ba llago,",
        "E tlo phumolwa;"
      ],
      "number": 6
    }
  ]
}