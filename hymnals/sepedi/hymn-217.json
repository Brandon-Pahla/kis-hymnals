{
  "number": 217,
  "language": "nso",
  "title": " 217  SE SENGWE SA JESO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 217  SE SENGWE SA JESO",
        "CH 283 АН 242",
        "SOMETHING FOR JESUS"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Jeso 'rato la",
        "'Go o mphile lona,",
        "Nka se ke ka gana,",
        "Morena ka,",
        "Ke khunama leratong,",
        "Ke phetha dikano,",
        "Ke tliša myemyelo,",
        "Go sengwe sa Jeso;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Setulong sa kgaugelo,",
        "Wa nthapelela,",
        "Ke lebelela go",
        "Jeso ka tumelo,",
        "Nthuše sefapanong,",
        "Ke bolele lerato;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "O mphe pelo 'a nnete,",
        "Seswantšho sa Gago,",
        "Gore go bonale",
        "Matšatši 'ohle,",
        "Tiro tša lerato,",
        "le ya pelo-nolo,",
        "Motimedi a humane,",
        "Se sengwe sa Jeso;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Nna le tšohle tša ka,",
        "Dineo tša 'Go,",
        "Lethabong, manyaming,",
        "Bophelong bjono,",
        "Ge ke go bona,",
        "Ke tlo dula polokong,",
        "Ka bosafelego,",
        "Se sengwe sa Jeso;"
      ],
      "number": 5
    }
  ]
}