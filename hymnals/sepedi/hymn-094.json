{
  "number": 94,
  "language": "nso",
  "title": " 94 AH 294 SEKOLO SA SABATA SE MONATE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 94 AH 294 SEKOLO SA SABATA SE MONATE",
        "CS 608",
        "SWEET SABBATH SCHOOL"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ke sekolo sa sabata,",
        "Seo ke se ratago,",
        "Pelo ya ka e thabile,",
        "tšatšing la Sabata,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Sabata e botse,",
        "Sabata e botse,",
        "Pelo ya ka e thabile,",
        "tšatšing la Sabata,"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ke ile ka thoma go bona,",
        "Tsela ya bophelo,",
        "Ke 'ile ka nagana botse,",
        "Ka kgetha Sabata"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Jeso o be a le gona,",
        "A mpitšaka 'rato,",
        "Gore ke mo kgethe yena,",
        "Le Sabata ya 'Gwe;"
      ],
      "number": 5
    }
  ]
}