{
  "number": 290,
  "language": "nso",
  "title": " 290  GO SOMELA JESO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 290  GO SOMELA JESO",
        "CH 574",
        "OILING FOR JESUS"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Thabang re šomela Morena,",
        "Re kgahlwa ke mošomo wa Gawe,",
        "Tšohle tšeo a re laelago tšona,",
        "Di tlo phethišwa ka botlalo;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Wena Jeso re tlo ya",
        "Pele ka lethabo,",
        "Re tšweleletše,",
        "Mošomong wa Gago;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ka thabo re tlo ruta,",
        "Lerato la 'Gwe ditšhabeng,",
        "Jeso kgalalelo ya Tate,",
        "O tlo re nešetša ditšhegofatšo;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Ka boikokobetšo re latela Morena,",
        "Teleng ka botlalo,",
        "Batimedi re ba iša nneteng,",
        "Go yona kwana ya Morena;"
      ],
      "number": 5
    }
  ]
}