{
  "number": 63,
  "language": "nso",
  "title": " 63 KE ENG SE KA NHLATSWAGO SEBE?",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 63 KE ENG SE KA NHLATSWAGO SEBE?",
        "WHAT CAN WASH AWAY MY SIN"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ke eng se ka nhlatšwago sebe?",
        "Ke fela madi a Jeso,",
        "Ke eng se ka nhlwekišago?",
        "Ke fela madi a Jeso,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Al ke a bohlokwa a",
        "nhlatšwago bošweu",
        "Ga ke tsebe se sengwe,",
        "Ko feia madi a Jeso,"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ke kae moo nka hiatswiwago?",
        "Ke fela mading a Jeso,",
        "Ke kae moo nka swarelwago?",
        "Ke fela mading a Jeso;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Ga go na polano,",
        "Ke fela mading a Jeso,",
        "E sego go lokeng ga ka,",
        "Ke fela mading a Jeso;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Tshepo ya ka le khutšo,",
        "Ke fela mading a Jeso,",
        "Go loka gohle ga ka,",
        "Ke fela mading a Jeso,"
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "Kgalalelo ke opela,",
        "'Fela mading a Jeso,",
        "Ditebogo tšeo ke tlišago,",
        "Ke fela mading a Jeso;"
      ],
      "number": 7
    }
  ]
}