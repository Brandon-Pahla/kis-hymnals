{
  "number": 224,
  "language": "nso",
  "title": " 224 TSHUBANG MABONE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 224 TSHUBANG MABONE",
        "АН 356 CH 628",
        "LET THE LOWER LIGHTS BE BURNING"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Phadima mogau wa Tate,",
        "Ntlong ya 'Gwe ya lesedi,",
        "Mme rena o re laela,",
        "Go gaša 'sedi bathong,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Kgantšhang mabone'a lena,",
        "A ye kgole mo bathong,",
        "Ba ba lelerang leswiswing,",
        "Ba tle ba bolokege,"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "'Swiswi la sebe le gona,",
        "Go tšutliša maphotho,",
        "Ba letilego ka tshepo,",
        "Lesedi mo go lona,"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Tšhuba leone ngwanešo,",
        "Emongwe o sefefong,",
        "O leka go phopholetša,",
        "Leswiswing wa timela;"
      ],
      "number": 5
    }
  ]
}