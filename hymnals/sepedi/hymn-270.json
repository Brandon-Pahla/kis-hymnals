{
  "number": 270,
  "language": "nso",
  "title": " 270  MOREKOLODI YO A ŠEGOFADITŠWEGO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 270  MOREKOLODI YO A ŠEGOFADITŠWEGO",
        "AH 156 CH 214",
        "OUR BLEST REDEEMER"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Morekolodi a sešo",
        "a re sala botse,",
        "A re tlogela go",
        "Dula le mohomotši;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "A tla go re thabiša moyeng",
        "Yo a thakgetšego,",
        "Ge a humana pelo",
        "Yeo e mo ratago;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Re kwa lentšu la Gagwe,",
        "Ke le le bonolo,",
        "Le tlosa phoso mme,",
        "Le lapološa pelo;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Semelo se re nang naso,",
        "Le phenyo ya rena,",
        "Le kakanyo ye kgethwa,",
        "Ke tša Gagwe;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Moy'o hlwekilego le kgaugelo,",
        "E ba pelohlomogi,",
        "Lokiša dipelo tša rena,",
        "Re swane Nago;"
      ],
      "number": 6
    }
  ]
}