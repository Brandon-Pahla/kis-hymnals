{
  "number": 259,
  "language": "nso",
  "title": " 259  RE TLO KOPANA MOSOLA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 259  RE TLO KOPANA MOSOLA",
        "АН 431 CH 557",
        "SHALL WE MEET"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Re tlo kopana ka mošola,",
        "Moo go sego difefo na?",
        "Kgalalelong e sa felego,",
        "Moo go sego mahloko?"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Na re tlo kopana gona,",
        "Re tlo kopana mošola?",
        "Na re tlo kopana gona,",
        "Moo go senago difefo?"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Re tlo kopana lebopong,",
        "Re fetile difefong,",
        "Re tiiše Boitshwarelong,",
        "Lebopong la legodimo,"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Re tlo kopana motseng,",
        "Meagong e phadimago,",
        "Merakong ya Jaspera,",
        "A! e agetšwego rena?"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Re kopane le M'phološi,",
        "Ge a tlo tšea ba 'Gwe,",
        "A re tlo tseba kgaugelo,",
        "Re dule teroneng ya 'Gwe?"
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "E re tho kopana,",
        "Re tlo kopana mošola,",
        "E re tho kopana ntshe,",
        "Moo go sego leswiswi,"
      ],
      "number": 7
    }
  ]
}