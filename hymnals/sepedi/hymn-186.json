{
  "number": 186,
  "language": "nso",
  "title": " 186  MORENA RE KOPANE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 186  MORENA RE KOPANE",
        "CH 26",
        "LORD WE HAVE MET"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Jeso re kopane,",
        "Gona ntlong ya 'Go,",
        "Tla le wena tsena mmogo,",
        "Re be mahlong a 'Go,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Dipelo tša rena,",
        "Di’ a go golela,",
        "Le wona moya wa rena,",
        "O go hloka kudu;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Kgopelo tša gago,",
        "Tate re tle re kwe,",
        "Ge go bulwa lentšu la 'Go,",
        "Le wena o re kwe,"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "O re lokišetše,",
        "Rena batho ba 'Go,",
        "Mohlang re hwa,",
        "Re bole leroleng la Gago;"
      ],
      "number": 5
    }
  ]
}