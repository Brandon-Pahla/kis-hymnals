{
  "number": 133,
  "language": "nso",
  "title": " 133 KE SEPELA LE MODIMO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 133 KE SEPELA LE MODIMO",
        "AH 284 CH 395",
        "WALKING WITH GOD"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ntese ke sepele nago,",
        "Modimo bia ka Enoke",
        "O sware seatla sa ka,",
        "O bolele ka bonolo,",
        "Le ge ke sa bone tsela,",
        "Jeso o sepele le nna;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Ge ke sepela ke le nnoši,",
        "sefefo se galefile,",
        "Swiss le nswara dinaong,",
        "Dira di ntaletše kgauswi,",
        "O homoditše lewatle,",
        "Morena sepla le nna;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ge ke ikhutša go wena,",
        "Tša lefase ke lefela,",
        "Ke sepele ka sebete,",
        "ke tlo emiša 'fapano,",
        "Ke be ke bone Sione,",
        "M'phološ 'a sepele le nna;"
      ],
      "number": 4
    }
  ]
}