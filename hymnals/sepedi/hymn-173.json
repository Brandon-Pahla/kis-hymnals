{
  "number": 173,
  "language": "nso",
  "title": " 173  A TUMELO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 173  A TUMELO",
        "АН 203 CH 245",
        "O FOR A FAITH"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "A tumelo e sa felego,",
        "Le bontšhing bja dira,",
        "E sa senywego ke",
        "Bodidi le bohloki,",
        "Bodidi le bohloki;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Yeo e sa ngongoregelego",
        "thupa ya golokišwa;",
        "Mme nakong ya bohloko",
        "E tshepe Modimo",
        "O tshepe Modimo fela;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Tumelo yeo e phadimago,",
        "Ge diteko di etla,",
        "Mme mo kotsing ga go na",
        "Poifo, Ga go pelaelo,",
        "Ga go pelaelo;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Ditšhikinyego tša lefase,",
        "Gadi e šikinye,",
        "Ga e fenywe ke bobe,",
        "Le boradia bja bjona,",
        "Le boradia bja bjona;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Morena mphe tumelo ye,",
        "Le ge nka tlelwa ke eng,",
        "Gona mo ke tlo kwa,",
        "Bose bja kwa gae,",
        "Bose bja kwa gae;"
      ],
      "number": 6
    }
  ]
}