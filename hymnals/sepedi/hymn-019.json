{
  "number": 19,
  "language": "nso",
  "title": " 19 IHLOKOMELE MOYA WA KA ",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 19 IHLOKOMELE MOYA WA KA",
        "CH 358",
        "MY SOUL BE ON THY GUARD"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Itiise moya wa ka!",
        "Manaba ke a mantisi,",
        "Mašabašaba a dibe a gona,",
        "Gore a tle a go theose;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Elwa o rapele!",
        "O se ineele ntweng,",
        "Elwa ka mehla yohle,",
        "Gomme o kgopele thuso;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ga o seso wa fenya,",
        "Gomme o se tepelele,",
        "Mošomo wa 'Go o tlo fela,",
        "O humane mphapahlogo;"
      ],
      "number": 4
    }
  ]
}