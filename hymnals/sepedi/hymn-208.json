{
  "number": 208,
  "language": "nso",
  "title": " 208  GATANG KA BONOLO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 208  GATANG KA BONOLO",
        "АН 278 CH 601",
        "TRED SOFTLY"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Homolang, Homolang,",
        "Go tshebo-tshebo,",
        "Homolang, Homolang",
        "Bolokang lentšu"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Gatang ka bonolo,",
        "Moreno mokhwi,",
        "Gatang ka gonanya,",
        "Re batametše,"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "T&B",
        "Le gateng ka bonolo,",
        "Moreno mokhwi,",
        "Le gateng ka gonanya mo,",
        "Re batametše;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Homolang! Homolang!",
        "Re mo go kgethwa,",
        "Aletareng yekhwi,",
        "ya thomo 'e botse,"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Homolang! Homolang!",
        "A re rapeleng,",
        "Re kwe ka edene,",
        "ka nako yekhwi;"
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "Homolong! Homolang!",
        "Ngwalang mogau,",
        "Homolang! Homolang!",
        "Letang Morena;"
      ],
      "number": 7
    }
  ]
}