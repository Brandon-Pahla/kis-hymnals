{
  "number": 264,
  "language": "nso",
  "title": " 264  TŠATŠI LE LEKGETHWA LA JEHOFA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 264  TŠATŠI LE LEKGETHWA LA JEHOFA",
        "АН 318 CH 654",
        "HOLY DAY, JEHOVA'S REST"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Letšatši le le kgethwa,",
        "La Jehofa Morena,",
        "Ke lona la bošupa,",
        "Le kgethilwe ke Yena,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Le 'amogele, Le amogele,",
        "Re a go thabela",
        "Go ikhutša go Jehofa,"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Mošomo o feditšwe,",
        "Sabata sešo e thome,",
        "Mme a le šegofatša,",
        "Ke ka moo re ikhutšago,"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Batho ba le fetotše,",
        "Ba kgetha la pele,",
        "Hlokomela ka mo Beibeleng,",
        "Ga e bolele bjalo,"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Ba bantši bona ba re",
        "\"Sontaga e lokile\",",
        "Eupša puku 'a Morena,",
        "Yona e a ba phegiša;"
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "Ka bala mme ka bona,",
        "Molao wa Sabata,",
        "Ka pele ka ikokobetša,",
        "Tseleng ye ya nnete;"
      ],
      "number": 7
    }
  ]
}