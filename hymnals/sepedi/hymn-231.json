{
  "number": 231,
  "language": "nso",
  "title": " 231  DUMELELA JESO A TSENE PELONG",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 231  DUMELELA JESO A TSENE PELONG",
        "CS 25",
        "LET JESUS COME INTO YOUR HEART"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ge o lapišitšwe ke dibe tša go,",
        "Dumelela Jeso ka pelong,",
        "Ge o duma go phela leswa,",
        "Dumelela Jeso ka pelong,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Bjale se ngongorege,",
        "Bjale o se mo lahle,",
        "Bjale o bule pelo,",
        "Dumelela Jeso ka pelong,"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ge o llela goba le go hlweka,",
        "Dumelela Jeso ka pelong,",
        "Metswedi e hlatšwago e kgauswi,",
        "Dumelela Jeso go tsena,"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Ge go le sefefo se segolo,",
        "Dumelela Jeso ka pelong,",
        "Ge go le gona go se hlatšwešišwe,",
        "Dumelela Jeso go tsena,"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Ge bagwera ba se sa tshepagala,",
        "Dumelela Jeso go sena,",
        "Ge o tlo tsena",
        "'Felong la boikhutšo,",
        "Dumelela Jeso go tsena;"
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "Ge o opela le bakgethwa,",
        "Dumelela Jeso ka pelong,",
        "Ge o tlo tsena",
        "'Felong la boikhutšo,",
        "Dumelela Jeso go tsena;"
      ],
      "number": 7
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Bjale ga go pelaelo,",
        "Bjale ga ke mo lahle,",
        "Bjale ke butše,",
        "Pelo me Jeso o tsena ka gare;"
      ],
      "number": 8
    }
  ]
}