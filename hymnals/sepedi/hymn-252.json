{
  "number": 252,
  "language": "nso",
  "title": " 252  TSHEPA YENA FELA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 252  TSHEPA YENA FELA",
        "CH 567",
        "ONLY TRUST HIM"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Etlang bohle ba imelwago sebeng,",
        "Morena o na le mogau,",
        "O tlo le fa Khutšo,",
        "Ge le mo tshepile,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Mo tshepeng, mo tshepeng,",
        "Mo tshepeng bjale,",
        "O'a boloka o'a boloka,",
        "o'a boloka bjale,"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Jeso o tšholotše madi,",
        "Go re nea mašoto",
        "Ikineleng bjale ka mading,",
        "Ao a hlatswišišago;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "E, Jeso ke nnete, tsela,",
        "Yeo e išago khutšong,",
        "Dumelang bjale go Yena,",
        "Le šegofatšwe;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Etlang bjale tsenang bakgethweng,",
        "Le ye kgalalelong,",
        "Agang faseng la godimo,",
        "Kua lethabong;"
      ],
      "number": 6
    }
  ]
}