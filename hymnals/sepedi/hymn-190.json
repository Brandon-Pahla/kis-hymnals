{
  "number": 190,
  "language": "nso",
  "title": " 190  LEINA LE LEBOTSE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 190  LEINA LE LEBOTSE",
        "AH 100 CH 523",
        "PRECIOUS NAME"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Tšea leina la Jeso,",
        "Ngwana wa mahloko,",
        "Le tlo go neea khutšo,",
        "Ge o le amogela,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Leina... le Botse...",
        "Ke tshepo ya lefase,",
        "Leina... le Botse...",
        "Lethabo la 'Godimong;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Tšeea leina la Jeso,",
        "Ke seširo sa rena,",
        "Ge o tlalelwa ke di teko,",
        "Tshepa 'ina la Jeso,"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Oho! Leina la Jeso,",
        "Le a re thabiša,",
        "Ge a re amogetše,",
        "Re tlo mo opelela;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Re khunama ka 'ina la 'Gwe,",
        "Re wele naong tša 'Gwe,",
        "Ke Kgoši ka mphapahlogo,",
        "Re tumiša ka gosafelego;"
      ],
      "number": 6
    }
  ]
}