{
  "number": 234,
  "language": "nso",
  "title": " 234  JESO 'SEDI LA LEFASE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 234  JESO 'SEDI LA LEFASE",
        "CS 313",
        "JESUS, THE LIGHT OF THE WORLD"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Mangeloi ge 'a opela,",
        "Jeso 'sedi lefaseng,",
        "Kgalalelo Moreneng,",
        "Jeso 'sedi lefaseng;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Re tsama 'a 'seding Le le botse,",
        "Tlang mo phadimong ya tšhegofatšo,",
        "Le re phadimele nako tšohle,",
        "Jeso sedi la 'fase,"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Tsogang bohle ka 'thabo,",
        "Jeso 'sedi la 'fase,",
        "Tlang phenong ya godimo,",
        "Jeso 'edi la 'fase,"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Kreste o kgabišitšwe,",
        "Jeso 'sedi lefaseng,",
        "Moren'a bosafelego,",
        "Jeso 'sedi lefaseng,"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Retang Kgoši ya khutšo,",
        "Jeso 'sedi la 'fase,",
        "Retang tšatši la toka,",
        "Jeso 'sedi la 'fase;"
      ],
      "number": 6
    }
  ]
}