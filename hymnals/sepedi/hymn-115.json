{
  "number": 115,
  "language": "nso",
  "title": " 115 NGAKA E KGOLO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 115 NGAKA E KGOLO",
        "AH 118 CH 530",
        "THE GREAT PHYSICIAN"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ngaka e kgolo e kgauswi,",
        "Jes o pelo tlhomogi,",
        "O bolela tsebe e kwe,",
        "Ekwa lentšu la Jeso,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "O opelwa Godimong,",
        "O ratwa le ke batho,",
        "Monate go Opelwa,",
        "Jeso ya rategang,"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Dibe tšohle di tshwarelwe,",
        "Ekwa lentšu la Jeso,",
        "Godimo ke rotošetšwe,",
        "Ke wale mphapahlogo;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Go išwe kgalalelo,",
        "Go ina la ga Jeso,",
        "Ke rata Mophološi,",
        "Leina la ga Jeso;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Ga go ya tlošang letswalo,",
        "Kantle ga Mophološi",
        "Moya o felela tshepong,",
        "Go ina la ga Jeso;"
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "Ge a tla le mphapahlogo,",
        "Serwalo sa bophelo,",
        "Re tlo dula lehlakoreng,",
        "Re bolele phološo;"
      ],
      "number": 7
    }
  ]
}