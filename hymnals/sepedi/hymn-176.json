{
  "number": 176,
  "language": "nso",
  "title": " 176  TSHEPO E ŠEGOFADITŠWEGO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 176  TSHEPO E ŠEGOFADITŠWEGO",
        "CH 379 АН 308",
        "BLESSED HOPE"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Tshepo 'e šegofaditšwego,",
        "Ke yeo e hlokegago,",
        "Go feta leruo la",
        "Tshotlego ya lefase,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Naledi e gona yeo e",
        "kgantšhago leswiswing,",
        "Yeo e ntšhago kgalalelo",
        "Ya khutšo legitleng;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ntšu le kgothatšago,",
        "Le tsoša le moya,",
        "Leo le tlošago pelaelo,",
        "\"Modimo 'lerato;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Lentšu la Golgota,",
        "Leo le nago le tshwarelo,",
        "'Sedi lela la kutullo,",
        "E lego yona tshepo;"
      ],
      "number": 5
    }
  ]
}