  "title": " 17 RE THABA LERATONG ",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 17  RE THABA LERATONG",
        "AH 454 CS 567",
        "IT'S LOVE THAT MAKES US HAPPY"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Re thabišwa ke 'rato,",
        "Tsela e bonolo,",
        "Le re thusa le kgopolong,",
        "Mogau go bangwe;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Ke Modimo ya tletšego rato,",
        "Re ba 'Gwe re swana le Yena,",
        "Re thabišwa ke 'rato,",
        "Tsela e bonolo,",
        "Le re thusa le kgopolong,",
        "Mogau go bangwe;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Fase le tletše dibe,",
        "Lehu le tshenyego,",
        "Ka lerato re tlo leka,",
        "go sokolla bangwe,"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Ge bophelo bo fela,",
        "Re bitšwa godimo,",
        "košaya rena e tlo ba,",
        "Ka gosafelego;"
      ],
      "number": 5
    }
  ]
}