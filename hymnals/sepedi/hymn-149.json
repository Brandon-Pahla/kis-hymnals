{
  "number": 149,
  "language": "nso",
  "title": " 149 SEPELA LESEDING",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 149 SEPELA LESEDING",
        "CS 540",
        "WALK IN THE LIGHT"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Sepela 'seding la Morena,",
        "Le tlo go kgantšhetša,",
        "Moya wa 'Gwe o tšwago 'godimong,",
        "o tloše leswiswi;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Se...pela Leseding.…",
        "(Le botse la Modimo)",
        "Sepela 'seding la Modimo;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Sepela 'seding la efangedi,",
        "Fokong la Modimo,",
        "Sedi le ntshepedišang bosweng,",
        "Batho ba Morena;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Sepela 'seding le ge",
        "Seswiswi le le tseleng,",
        "Le tlo tloša leswiswi,",
        "Morena a be a tle;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Sepela 'seding o tlo tseba,",
        "Rato la Modimo,",
        "Bokgotse lefaseng legodimong",
        "Bo tloba bjo botse;"
      ],
      "number": 6
    }
  ]
}