{
  "number": 138,
  "language": "nso",
  "title": " 138  BOINEELO BJO BO TSENELETŠEGO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 138  BOINEELO BJO BO TSENELETŠEGO",
        "CH 273",
        "ENTIRE CONSECRATION"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Tšea bophelo bja ka,",
        "O ntokišetše bja Gago,",
        "Tšea 'atla tša ka,",
        "Di dire thato ya Gago,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "(Ntlhatšwe mading a kwana,)",
        "(Ntlhatšwe mading a kwana,)",
        "Nthatšwe mading a fodišago,",
        "Ntšee Morena, E be ka bosafelego;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Tšea dinao tša ka,",
        "Di kitimele Wena,",
        "Tšea ntšu la ka kamehla,",
        "Ke tle ke go opelele;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Tšea molomo wa ka,",
        "O tlale mantšu a Gago,",
        "Tšea tšohle tša ka,",
        "Mme go se šale selo;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Tšea nako tša ka tšohle,",
        "Di go tumiše bjalo,",
        "Tšea tlhaloganyo ya ka,",
        "O e šomiše yohle;"
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "Itšeele thato ya ka,",
        "O mphe ya Gago,",
        "Itšeele pelo ya ka,",
        "E be bjana bodulo bja 'Go;"
      ],
      "number": 7
    },
    {
      "type": "verse",
      "lines": [
        "Tšea rato la ka Jeso,",
        "Ke le bea naong tša Gago,",
        "O ntšee mme ke tlo ba,",
        "Wa Gago ka mehla;"
      ],
      "number": 8
    }
  ]
}