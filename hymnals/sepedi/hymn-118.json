{
  "number": 118,
  "language": "nso",
  "title": " 118 JESO O LEFILE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 118 JESO O LEFILE",
        "АН 119 CS 107",
        "JESUS PAID IT ALL"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ke kwa M'phološ 'a re,",
        "O fokola maatla,",
        "Dula o rapela,",
        "Hwetša tšohle go nna;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Jes 'o lefile,",
        "Ke filwe tšohle,",
        "Sebe se ngaparetše,",
        "O ntlhatswitše sohle;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Bjale Moren 'a ka,",
        "Bona madi a 'Go,",
        "A fetolang lepher 'a,",
        "Tološa pelo,"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Ga go selo gonna,",
        "Se ka gaugelwang,",
        "Ke hlatšwa aparo,",
        "Mading a konyana;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Ke ema teroneng,",
        "Ke yo 'a phethagetšeng,",
        "Ke tlo rola tšohle,",
        "Naong tša go Jeso;"
      ],
      "number": 6
    }
  ]
}