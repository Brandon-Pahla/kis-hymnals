  "title": " 171  MOLETI BOTONG LA SIONE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 171  MOLETI BOTONG LA SIONE",
        "AH 418 CH 184",
        "WATCHMAN ON THE WALLS OF ZION"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Moleti botong la Sion,",
        "Re botše ka bošego,",
        "Mphatlalatsane o kae?",
        "A re tlo bona meso?",
        "A o bona 'sedi le phadimago,",
        "A o bona 'sedi le phadimago;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Re botše ge re fetile,",
        "Dipontšho ka mphela,",
        "A šetše re le kgauswi?",
        "A re ka bona naga?",
        "A ruri legodimo le kgauswi,",
        "A ruri legodimo le kgauswi;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "E re hweditše sešupo,",
        "Mme 'fase le kgauswi,",
        "Re phakišetše kua pele,",
        "Godimo le nyarele,",
        "Goeletšang ka mantšu A lethabo,",
        "Goeletšang ka mantšu A lethabo;"
      ],
      "number": 4
    }
  ]
}