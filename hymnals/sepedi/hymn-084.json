{
  "number": 84,
  "language": "nso",
  "title": " 84 TUMIŠANG MODIMO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 84 TUMIŠANG MODIMO",
        "АН 13 CH 683",
        "PRAISE GOD"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Retang Modimo go tšohle,",
        "Modimo ya re phedišang,",
        "Tumišang bohle Godimong,",
        "Retang Tate, Morwa,",
        "Moya; Amen;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "### 84(a) KOŠA YA TSOŠOLOŠO",
        "SING AS WE JOURNEY (RALLY SONG)"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Re batho ba Morena,",
        "Morena, Morena,",
        "Re batho ba Morena,",
        "Re reta Morena,",
        "Jeso mo okamedi,",
        "Re letile M'phološi,",
        "Re opela leetong, Ke mo"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "E tlang re ye Gae,",
        "Legaeng, Legaeng,",
        "E tlang re ye gae,",
        "Gona kwa mošola,",
        "E tlang re ye gae,",
        "Re bonaneng kwa gae",
        "Jeso re tlo opela,",
        "Šetše re le kgauswi;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Ka lethabo re toge,",
        "re toge, re toge,",
        "Ka lethabo re toge,",
        "Re hlamule tseleng.",
        "Bašweu le ba baso,",
        "Bohle ba ne'koša,",
        "Ba opela le Jeso,",
        "Kwa Jerusalema;"
      ],
      "number": 6
    }
  ]
}