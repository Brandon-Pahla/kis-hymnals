{
  "number": 203,
  "language": "nso",
  "title": " 203  MOHOMOTŠI O TLILE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 203  MOHOMOTŠI O TLILE",
        "AH 154 CH 526",
        "THE COMFORTER HAS COME"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "A re gašeng taba,",
        "Tše botse mo bathong,",
        "Gohle mo ba leng gona,",
        "Le ba leng manyaming,",
        "Bohle bakreste ba tumiše",
        "Ba re \"Mohomotš'o tlile;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Mohomotš 'o gona,",
        "Mohomotš'o gona",
        "Moya o mokgethwa,",
        "Tshepišo ya Tate,",
        "A re gašeng taba,",
        "Gohle-gohle bathong,",
        "Mohomotš'o gona;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Šego bo fetile,",
        "Meso e filile,",
        "Dillo di fetile,",
        "Bogale bo a tuka,",
        "Le tšatši le'a",
        "sobela dithabeng,",
        "Mohomotš'o gona;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Bonang Kgoši Jeso,",
        "O na le phodišo,",
        "Ba kgolegong o",
        "Tlo le lokolla,",
        "Go bohle go tšwe",
        "Sefela sa phenyo,",
        "Mohomotšo gona;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "‘Rato la bohlokwa,",
        "Leleme le la ka,",
        "Le ka bolela bjang,",
        "Bathong ba hwago,",
        "Gore nna mosenyi",
        "ke tshwane le yena,",
        "Mohomotš'o gona;"
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "Opelang ka kudu,",
        "Le gagole marulelo,",
        "Badumedi godimo,",
        "Ba tlo le fetola,",
        "Mantšung a lerato,",
        "koša e sa felego",
        "Mohomotš'o gona,"
      ],
      "number": 7
    }
  ]
}