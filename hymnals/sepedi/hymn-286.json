{
  "number": 286,
  "language": "nso",
  "title": " 286  O NTHWALE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 286  O NTHWALE",
        "CS 946",
        "BEAR ME ON"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ke goletše tšatši leo!",
        "Ge ba rekolotšwego,",
        "Ba apere tše tšhweu Sione,",
        "Gae la lethabo,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "O nthwale, nthwale nkiše Sione,",
        "Nthwale o nkiše motseng wa thabo,",
        "Motse wa bakgethwa;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ke tlo kwa di haleluya,",
        "Go tšwa mašabašabeng,",
        "Moya le wona o thakgetše,",
        "Le bona bakgethwa;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Thabelang tšatši leo,",
        "Le ratwang ke bafeti,",
        "Le tlo fediša le mahloko,",
        "Ba khutše Mohloding;"
      ],
      "number": 5
    }
  ]
}