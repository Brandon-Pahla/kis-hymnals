{
  "number": 144,
  "language": "nso",
  "title": " 144 MOBUNG WA GODIMO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 144 MOBUNG WA GODIMO",
        "АН 317 CH 631",
        "HIGHER GROUND"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ke mo tseleng e rotogang,",
        "ke rotoga 'tšatši ohle;",
        "Ke sepela ke rapela,",
        "Morena mpee godimo;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Morena ntshware ke eme,",
        "Ka tumelo legodimong,",
        "Godimo go feta mokhwi,",
        "Morena mpee godimo;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ga ke kganyoge go dula,",
        "Ngongoregong le poifong,",
        "Le ge bangwe ba go rata,",
        "Nna ke rata godimo;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Ke nyaka go ba godimo,",
        "Le ge Satane a nkgoga,",
        "Ka tumelo ke ekwele,",
        "Koš 'a mangeloi godimo;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Ke nyaka go ya godimo,",
        "Ke bone sedi bophelo,",
        "Ke sa rapela le tseleng,",
        "Morena nkiše godimong;"
      ],
      "number": 6
    }
  ]
}