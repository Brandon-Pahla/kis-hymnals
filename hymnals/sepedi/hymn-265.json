{
  "number": 265,
  "language": "nso",
  "title": " 265 KHUTŠO E PHETHAGETŠEGO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 265 KHUTŠO E PHETHAGETŠEGO",
        "АН 263 CH 311",
        "PEACE, PERFECT PEACE"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Khutšo, Khutšo,",
        "'\"Faseng le la sebe,",
        "Madi a Jeso a re fa khutšo;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Khutéo, Khutšo,",
        "Go lapa mo tseleng.",
        "Go šomela Jeso ke go ikhutša;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Khutšo, Khutšo,",
        "Ge o selekegile,",
        "Sefegeng sa Jeso go Khutšo,"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Khutšo, Khutšo,",
        "Ba ratua ba ile,",
        "Go Jeso re bolokilwe bohle;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Khutšo, Khutšo,",
        "Te di sa tsejwego,",
        "Jes'o re tseba mo teroneng;"
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "Khutšo, Khutšo,",
        "Lehu le širago,",
        "Eupša Jes'o fedišitše lehu;"
      ],
      "number": 7
    },
    {
      "type": "verse",
      "lines": [
        "Khutšo, Khutšo,",
        "Ntwa e tlo fela,",
        "Jeso o re biletša godimo;"
      ],
      "number": 8
    }
  ]
}