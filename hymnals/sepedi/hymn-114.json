{
  "number": 114,
  "language": "nso",
  "title": " 114 SEFERO SE BULETŠWE NNA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 114 SEFERO SE BULETŠWE NNA",
        "AH 177 CH 561",
        "THE GATE AJAR FOR ME"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Go na le 'fero se butšweng,",
        "Menyako e halalelang,",
        "Hlasedi la sefapano,",
        "Kutullo ya lerato;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "O mokakang mogau wona,",
        "Sefero se buletšwe nna,",
        "Le nna, le nna?",
        "Se buletšwe le nna;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Fero seo se buletšwe,",
        "Bohle ba tlago phološong,",
        "Bahumi, bahloki,",
        "Le bahlanka le magoši,"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Dirang ka pejana,",
        "Ge sefero se sa butšwe,",
        "Amogela sefapano,",
        "Pontsho yeo ya lerato;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Mošola re tlo rola,",
        "Sefapano sa bjale,",
        "Re rwale mphapahlogo,",
        "Bophelong ratong la 'Gwe;"
      ],
      "number": 6
    }
  ]
}