{
  "number": 105,
  "language": "nso",
  "title": " 105 A KA PHOLOŠA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 105 A KA PHOLOŠA",
        "ABLE TO DELIVER"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "O goeletše ka kudu",
        "Wena tokologo yeo,",
        "Go lokolla bona bana",
        "Ba nago le hlompo,",
        "Mollong go lahlelwe",
        "Bona ba imelwago;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "A ka go lokolla,",
        "O na le phološo,",
        "Wena hle ngwanešo,",
        "O modumele;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Na a ka lokolla?",
        "O a rereša;",
        "Na agona le matla,",
        "Ao a kago nthuša?",
        "Mpotše ka Kreste,",
        "Yo a bapotšwego,",
        "Na a ka ntokolla",
        "Le go satane?"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Na a ka lokolla!",
        "O tiee o be le matla,",
        "O se tšhabe",
        "Tshwarelo ka Jeso,",
        "O se tšhabe",
        "Tshwarelo ka Jeso,",
        "Leleka pelaelo",
        "O eme go yena (Jeso)"
      ],
      "number": 5
    }
  ]
}