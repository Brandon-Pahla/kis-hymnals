      "lines": [
        "### 170  MMOPI LE KGOŠI YA KA",
        "AH 54 CH 71",
        "MY MAKER AND MY KING"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Mmopi, Kgoši ya ka,",
        "tša ka ke tša Gago,",
        "Ke wena motswedi wa ka,",
        "Wa mahlatse ohle,",
        "Ke wena motswedi wa ka,",
        "Wa mahlatse ohle;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Bopiwa sa Gago,",
        "Ke phela go wena,",
        "Modimo wa ka bohlokwa",
        "bjago bo retwe,",
        "Modimo wa ka bohlokwa",
        "bjago bo retwe;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Morena nka go fa eng?",
        "Tšohle ke tša Gago,",
        "'Rato la 'Go le kgopela",
        "pelo e lebogang,",
        "\"Rato la 'Go le kgopela",
        "pelo e lebogang:"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Ntšhušumetše moyeng,",
        "Takatšo le lentšu,",
        "E be tša gago ka mehla,",
        "Lentšu le takatšo e",
        "be tša Gago ka mehla,",
        "Lentšu le takatšo;"
      ],
      "number": 5
    }
  ]
