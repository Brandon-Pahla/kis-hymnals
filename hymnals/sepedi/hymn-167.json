{
  "number": 167,
  "language": "nso",
  "title": " 167 MOTHEO O TIILEGO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 167 MOTHEO O TIILEGO",
        "AH 215 CH 255",
        "FIRM FOUNDATION"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Motheo o tiilego,",
        "Lena bahalaledi,",
        "O theilwe lentšung",
        "Tumelong ya lena,",
        "Na a ka bolela bjang",
        "go wena ge a re,",
        "Ke mang ya tšhabilego,",
        "Ke mang ya tšhabilego,",
        "Ke mang ya tšhabetšego",
        "Go M'phološi;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Se tšhabe ke na le wena,",
        "Se lepelele,",
        "Kenna Modimo wa",
        "Gago ke tlo go thuša,",
        "Ke tlo go matlafatša",
        "ke go emiše,",
        "O emišwe ke go loka,",
        "O emišwe ke go loka.",
        "O emišwe ke go loka",
        "tsogong la ka;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ge ke go bitša go",
        "Sepela bothateng,",
        "Noka tšamaswabi di",
        "ka se fele maatla,",
        "Kgakanegong ke tlo",
        "Go šegofatša,",
        "Ke go halaletše le",
        "Phenyong ya go,",
        "Ke go halaletše le",
        "Phenyong ya go;"
      ],
      "number": 4
    }
  ]
}