{
  "number": 282,
  "language": "nso",
  "title": " 282 HAREPA TŠE MASOME A DIKETE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 282 HAREPA TŠE MASOME A DIKETE",
        "CS 342",
        "TEN THOUSAND HARPS"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ekwang mantšu a bontšintši,",
        "A tumiša godimong,",
        "Go lethabo Jeso o buša gona,",
        "Modimo wa lerato,",
        "Bona o dutše teroneng,",
        "Jeso o buša lefase,",
        "Haleluya, Haleluya,",
        "Haleluya, Amen;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Kgoši ya kgalalelo,",
        "O buše ka bosafelego,",
        "Ga go se ka kgaoganyago,",
        "Ba Gago 'ratong la 'Go,",
        "Ke bao e lego lethabo,",
        "Go bona sefahlogo sa Gago,",
        "Haleluya, Haleluya,",
        "Haleluya, Amen;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Phakisa o tle M'phološi,",
        "Letšatši la kgalalelo,",
        "Ga go ešo goelelwa gore,",
        "Lefase le a feta,",
        "Re tlo opela re re,",
        "\"Kgalalelo go Kgoši',",
        "Haleluya, Haleluya,",
        "Haleluya, Amen;"
      ],
      "number": 4
    }
  ]
}