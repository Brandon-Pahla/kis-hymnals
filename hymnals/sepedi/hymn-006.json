{
  "number": 6,
  "language": "nso",
  "title": " 6 HLOKOMELANG BAKGETHWA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 6 HLOKOMELANG BAKGETHWA",
        "AH 415 CH 549",
        "WATCH, YE SAINTS"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Hlokomelang bakgethwa,",
        "Thoromo ya legodimo,",
        "Mabone a tuke,",
        "Itukisetseng Morena;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "O etla! Jeso o etla,",
        "bona! O tla kgalalelong",
        "O tla go busa ka phenyo,",
        "O etla Jeso o etla;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Bonang tshepiso ya 'Gwe,",
        "Dibe di tshwaretšwe,",
        "Diaparo le mphapahlogo,",
        "le bolele thekololo,"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Mebuso e ya fediswa,",
        "Ekwang dikoloi di duma,",
        "Bolelang ka tshegofato,",
        "Phalafal'a bosupa e a lla,"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Ditshaba di a thulana,",
        "Go tla bogosi bja Kreste,",
        "Lefase le mo bodulong,",
        "Goelatsang Jeso o etla;"
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "Badiradibe tlang le'a bitšwa,",
        "Jeso a le kopela.",
        "Akgofang go seso gothwe,",
        "Mogau, nako di fedile;"
      ],
      "number": 7
    }
  ]
}