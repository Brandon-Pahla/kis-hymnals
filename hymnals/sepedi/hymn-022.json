{
  "number": 22,
  "language": "nso",
  "title": " 22 MORENA KEA KWA ",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 22 MORENA KEA KWA",
        "AH 151 CH 208",
        "LORD I HEAR"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Morena ke kwa maoto",
        "ao a thololwago;",
        "A kgodisa ba nyorilweng,",
        "A a rothele le nna."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušulošo",
        "Le nna le nna o",
        "rothisetse le nna."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Tate wa pelo hlomogi,",
        "Se mphete nna mosenyi;",
        "Mme le ge o ka mpheta,",
        "Se fete le masoto;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Kgale ke letse dibeng,",
        "Kgale ke go hlomere;",
        "Lefase le ntsere pelo,",
        "Ntshwarele o mphološe,"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Se mphete Moy 'o mokgethwa,",
        "O ka bontsha difofu,",
        "Supakgoni bja ga Jeso,",
        "Bolela khutšo go nna;"
      ],
      "number": 6
    }
  ]
}