{
  "number": 146,
  "language": "nso",
  "title": " 146 PHELA KA TENG GA KA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 146 PHELA KA TENG GA KA",
        "АН 247 CH 279",
        "THY LIFE WITHIN ME"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Phela ka teng ga ka,",
        "Kgoši ya magoši,",
        "Wena o be karabo,",
        "Potšišong tša ka,",
        "O phele teng ga ka,",
        "Dira thato ya 'Go",
        "Mme nna ke bonagatše,",
        "Kgaugelo ya 'Go;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Mmele o ineetšego,",
        "O hiwekišwe sebeng,",
        "A lesedi la Gago,",
        "Le phadime ka teng,",
        "'Fase le homolele,",
        "Mmele o fetolwe,",
        "Mohlanka wa 'Gago,",
        "O mo sepediše,"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ditho tša wona tšohle,",
        "Di ikokobetše,",
        "Di dumele gore",
        "Di ka šomišwa,",
        "Di hloke thologelo,",
        "Botseng le di tekong,",
        "Ditirelong tša Gago,",
        "Go se pelaelo;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Nke ke dule khutšong,",
        "Le go lokeng gohle,",
        "Ke hiwe ke go letile,",
        "Ge wena o ntlhoka,",
        "O phele ka teng ga ka,",
        "Moren 'a marena,",
        "Wena o be karabo,",
        "Potšišong tša ka;"
      ],
      "number": 5
    }
  ]
}