{
  "number": 107,
  "language": "nso",
  "title": " 107 MPOTŠE TABA KA JESO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 107 MPOTŠE TABA KA JESO",
        "AH 82 CH 534",
        "TELL ME THE STORY OF JESUS."
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Mpotše taba ka ga Jeso,",
        "E ngwale pelong ya ka,",
        "Mpotše mafoko 'a mabose",
        "A fetago le todi,",
        "Mpotše koš'a mangeloi,",
        "Ge Jeso a belegwa,",
        "Kgalalelo go Modimo,",
        "Khutšo e be lefaseng;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Mpotše taba ka ga Jeso,",
        "E ngwale pelong ya ka,",
        "Mpotše mafoko 'a mabose",
        "A fetago le todi;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "A ila dijo 'ganateng,",
        "Bolela matšatši ao,",
        "A lekwa dibeng tša rena,",
        "Mme a fenya pheletšong,",
        "Bolela mešomo ya Gwe,",
        "Le mathata a Gagwe,",
        "A nyatšiwa a hlorišwa,",
        "A se na gae a hloka,"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Bolela papolo ya 'Gwe,",
        "A kwešwa le bohloko,",
        "Bolela ka bitla la 'Gwe,",
        "Bolela tsogo ya'Gwe,",
        "Lerato le le bonolo,",
        "Le bonagala botse,",
        "Nke ke lle ge o seba,",
        "Rato la phološ' oa ka;"
      ],
      "number": 5
    }
  ]
}