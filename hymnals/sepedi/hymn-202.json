{
  "number": 202,
  "language": "nso",
  "title": " 202  JESO MOPHOLOŠI NKGOGE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 202  JESO MOPHOLOŠI NKGOGE",
        "AH 297 CH 398",
        "JESUS, SAVIOUR PILOT ME"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Jeso M'phološi nkgoge",
        "Sefefong sa bophelo,",
        "Ke tlelwa ke dikotse",
        "'swika le le sa bonwego,",
        "Thuso e tšwa go wena,",
        "Jeso M'phološi nkgoge,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Bjalo ka mme ngwaneng,",
        "O homotše lewatle,",
        "Maphotho a theetša ge,",
        "O re a homole tu!",
        "Wena Kgoši ya lewatle,",
        "Jeso M'phološi nkgoge;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ge ke batamela 'bopo,",
        "Maphotho a a ferehla,",
        "Ke tletše khutšo ka teng,",
        "Ge ke le 'fegeng sa 'Go,",
        "Nke ke kwe ge o re se",
        "Tšhoge ke'a go goga;"
      ],
      "number": 4
    }
  ]
}