{
  "number": 198,
  "language": "nso",
  "title": " 198  MOGWERA WA POTEGO KE JESO.",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 198  MOGWERA WA POTEGO KE JESO.",
        "CH 528 AH 114",
        "THE BEST FRIEND IS JESUS"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Mogwer' a potego ke Jeso,",
        "Ge o tlelwa ke diteko,",
        "O tlo fodišaa pelo,",
        "O tlo go fa le Maatla,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Mogwera potego ke Jeso,",
        "Mogwer'a potego ke Jeso,",
        "Yena o tlo go thuša",
        "A go kwe ge o bitša,",
        "Mogwer'a potego ke Jeso;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ke humane mogwera Jeso,",
        "O mpha khutšo le khomotšo,",
        "Matlogong a Gagwe ga",
        "ke tšhoge le bjona bobe,",
        "Mogwer'a potego ke Jeso;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Mohlang re tlo kopana gae,",
        "Le badumedi ba le pele,",
        "Re tlo opela lebopong,",
        "Re tumiše ka mehla,",
        "Mogwer' a potego ke Jeso;"
      ],
      "number": 5
    }
  ]
}