{
  "number": 194,
  "language": "nso",
  "title": " 194  LERATO KA GAE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 194  LERATO KA GAE",
        "CS 580",
        "LOVE AT HOME"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Go botse 'felong",
        "'ohle, Ka rato gae,",
        "Go modumo wa thabo,",
        "Ka rato gae,",
        "Khutšo e a oketšega,",
        "Lethabo le ntšhifale,",
        "Nako e feta botse,",
        "Ge rato le 'ntshe;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Le... rato ka gae,",
        "Nako e feta botse.",
        "'Rato le lentshe;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Lethabo le ntshe lapeng,",
        "Ge rato le 'ntshe,",
        "Ga go hlokege selo,",
        "Rato le lentshe,",
        "Matšoba ge a thunya,",
        "Lefase tšhemo 'e botse,",
        "Bophelo bo'a thabiša,",
        "'Rato le lentshe;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "‘Godimo le a thaba,",
        "Rato le lentshe,",
        "‘Fase le tletše rato,",
        "Moo go leng thabo,",
        "Nokana di opela,",
        "Godimo le phadima,",
        "Go e a myemyelago,",
        "Rato le lentshe;"
      ],
      "number": 5
    }
  ]
}