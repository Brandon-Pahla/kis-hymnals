{
  "number": 152,
  "language": "nso",
  "title": " 152 BOLELA MOYENG WA KA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 152 BOLELA MOYENG WA KA",
        "CH 554",
        "SPEAK TO MY SOUL"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Bolela le nna Jeso,",
        "Ka bonolo moyeng,",
        "O sebe ka lerato,",
        "Gore o nnoši,",
        "O bule pelo yaka go",
        "kwa lentšu la 'Go,",
        "O ntlatše ka tumišo,",
        "Ke thabe go wena,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Bolela ka go seba,",
        "Seba ka lerato,",
        "O tho fenya ka gohle,",
        "Gobane ga'o nnoši;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Bolela baneng ba 'Go,",
        "O ba goge tseleng,",
        "Ba tlatše ka lethabo,",
        "Ba rute thapelo",
        "Ba ke ba go neele",
        "Maphelo a bona,",
        "Akgofiša bogoši,",
        "Re bone Morena;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Bolela bja ka kgale,",
        "Go rata ga Gago,",
        "Ke tsebe sentshwanetšego,",
        "Ke phethe molao,",
        "A ke go halaletše,",
        "Nthute go tumiša",
        "Ke dire ka lethabo,",
        "Ke hlomphe wena;"
      ],
      "number": 5
    }
  ]
}