{
  "number": 109,
  "language": "nso",
  "title": " 109 O LEKANYEDITŠWE 'MME O BOFEFO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 109 O LEKANYEDITŠWE 'MME O BOFEFO",
        "WEIGHED AND WANTING"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Mohlang moahlodi a re,",
        "Lekanyetšago bofelong,",
        "Re tlo ba Bja ka gauta?",
        "Goba bja ka malahla?"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "O lekanyeditšwe ke Morena,",
        "Mme o bofefo,",
        "O lekilwe ka molao,",
        "Mme o imetšwe;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "A re tlo kwa gothwe,",
        "\"Mohlanka wa potego,",
        "O šomile gabotse,",
        "Goba o paletšwe\" mosinyi?"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Na re tlo kwa moya?",
        "Go san a le kgaugelo?",
        "Goba re tlo diega,",
        "Go be go laelwe tshenyo?"
      ],
      "number": 5
    }
  ]
}