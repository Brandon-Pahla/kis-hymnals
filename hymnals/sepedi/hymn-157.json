{
  "number": 157,
  "language": "nso",
  "title": " 157  NAKO YA GOBUNA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 157  NAKO YA GOBUNA",
        "CH 627",
        "HARVEST TIME"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Yo a tšwago a dutše",
        "A lla, Ka peu ya lerato,",
        "A se lape a robala,",
        "O humana tšhegofatšo,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Hlokomela ge wena molemi!",
        "Mabele a budule,",
        "Bona mašemo a šweufetše,",
        "Puno e kgauswi,"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Go wa phoka ya godimo,",
        "Letšatši le hlabile,",
        "Go nea tše hlokegago,",
        "Go bakgethwa bohle;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Bjala peu o se lape,",
        "O se ke wa boifa,",
        "Tiišetša sekhwi bothateng,",
        "O tlo buna tše botse;"
      ],
      "number": 5
    }
  ]
}