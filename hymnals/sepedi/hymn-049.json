{
  "number": 49,
  "language": "nso",
  "title": " 49 KAKANYO E TEE E BOTSE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 49 KAKANYO E TEE E BOTSE",
        "АН 324 CH 352",
        "ONE SWEETLY SOLEMN THOUGHT"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Go tla kakanyo,",
        "Go nna e botse,",
        "Ke kgauswi le gae lehono,",
        "Go feta pele,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Kgauswi gae,",
        "Legae la ka,",
        "Kgauswi gae lehono,",
        "Go feta pele;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Kgauswi ntlong ya Tate,",
        "Dintlong tše dintši,",
        "Kgauswi moo Jeso a bušago,",
        "Lewatleng le botse,"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Ke kgauswi go ya gae,",
        "Ke rola merwalo,",
        "Sefapano sa maima,",
        "Ke rwala mphapahlogo;"
      ],
      "number": 5
    }
  ]
}