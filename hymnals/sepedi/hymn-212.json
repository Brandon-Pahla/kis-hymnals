{
  "number": 212,
  "language": "nso",
  "title": " 212  KHUTŠO E MONATE",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 212  KHUTŠO E MONATE",
        "AH 253 CH 609",
        "SWEET PEACE"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Go tsena pelong ya ka (ya ka)",
        "Lethabo le boikhutšo (khutšo)",
        "Ke e opela gape-gape,",
        "Khutšo ya Modimo"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Khutšo neo e",
        "Tšwago godimo,",
        "Khutšo e makatšago,",
        "Khutšo ya Modimo;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ka Jeso khutšo e gona (e gona)",
        "Molato wa ka o lefilwe (lefilwe)",
        "Ga go motheo o šele,",
        "wa neo ya lerato;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Ge ke beile dihlora go Jeso (Jeso)",
        "Ke hwetše khutšo pelong (pelong)",
        "Go yena ke humane lehumo,",
        "Khutšo e monate;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Go Jeso ke dutše khutšong (khutšong),",
        "Ke dutše kgauswi Le Yena (Yena)",
        "Ke bona khutšo fela,",
        "Lerato la Modimo;"
      ],
      "number": 6
    }
  ]
}