{
  "number": 71,
  "language": "nso",
  "title": " 71 NA KE NGWADILWE?",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 71 NA KE NGWADILWE?",
        "AH 281 CH 617",
        "IS MY NAME WRITTEN THERE?"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ga ke' iše felo khumo,",
        "Le ge e le gauta,",
        "Ke hloka legodimo,",
        "Ke tsene lešakeng;",
        "Bukung kgolo ya pušo,",
        "Gona legodimong",
        "O ko mpotše Mmoloki,",
        "A ke ngwadilwe nna;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "A ke ngwadilwe nna,",
        "Bukung tšweu ya 'Go",
        "Bukung kgolo ya mmušo,",
        "A ke ngwadilwe nna;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Dibe tšaka di tletše,",
        "Bja ka 'šabašaba,",
        "Eupša madi a' Mmoloki,",
        "Wona a ntekane,",
        "Go na le ditshepišo,",
        "Tihakeng tše phatsimang,",
        "\"'Lege di le khubedu,",
        "Di tlo šhweufala';"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Motse mobotsebotse,",
        "Felo ga lesedi,",
        "Moo ba halaleditšweng,",
        "Ba aparo tše tšhweu,",
        "Mo bobe bo sa tseneng,",
        "Go senya tše botse,",
        "Go nale mangeloi,",
        "A hlokometšego;"
      ],
      "number": 5
    }
  ]
}