{
  "number": 240,
  "language": "nso",
  "title": " 240  BONANG MOPHOLOŠI MOJAKO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 240  BONANG MOPHOLOŠI MOJAKO",
        "CH 227",
        "BEHOLD THE SAVIOUR AT THE DOOR"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Bonang M'phološi mojako,",
        "O opaopa ka bonolo,",
        "Ga a bolo go leta moo,",
        "O se ke wa mo swabisa,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "O re o reke go Yena,",
        "Gauta le diaparo,",
        "O tlotse maahlo o bone,",
        "O apole dibe tša gago,"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "A ke o kwe lentšu la Gagwe,",
        "O go fa boikgethelo,",
        "Wena o modiradibe,",
        "Nagana o sokologe;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Mošomo wa Gagwe o a fela,",
        "O tloga a se sa rapela,",
        "Wa ditšhila a tšhilafale,",
        "Wa goloka a loke bjalo;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Jeso o eme ka ntle,",
        "O kgopela pelo ya 'go ka bonolo,",
        "Ragoga o bule mojako,",
        "A tle a lalele nago,"
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "Mo tsenye o mo amogele,",
        "Mme o tlo ikhutša go yena,",
        "O thabe le yena,",
        "Le je mmogo dilalelo;"
      ],
      "number": 7
    }
  ]
}