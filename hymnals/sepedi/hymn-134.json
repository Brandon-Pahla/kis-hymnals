{
  "number": 134,
  "language": "nso",
  "title": " 134 LERATO LE MAKATŠAGO LA JESO",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 134 LERATO LE MAKATŠAGO LA JESO",
        "AH 123 CH 650",
        "WONDERFUL LOVE OF JESUS"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "‘Felong la godimo,",
        "Ke tumiše ka ntšu la ka,",
        "Ke mang ya ka opelago tumišo,",
        "'Ratong la Jeso?"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Rato lekhwi le makatsang,",
        "Ke rato la ga Jeso,",
        "Rato lekhwi le makatsang",
        "Ke rato la ga Jeso,"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ke thabo mo mosegareng,",
        "Ke khutšo le mo leswiswing,",
        "Ke phodišo le tiišetšo,",
        "Lona lerato la ga Jeso;"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Ke tshwarelo, tshepo go nna,",
        "Ke thušo ya ka ge ke ewa,",
        "Bophelong le lehung,",
        "Tšohle ke rato la ga Jeso;"
      ],
      "number": 5
    }
  ]
}