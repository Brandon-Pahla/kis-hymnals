{
  "number": 227,
  "language": "nso",
  "title": " 227 KUA SEFAPANONG.",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 227 KUA SEFAPANONG.",
        "CS 212",
        "AT THE CROSS"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "M'phološ'o tšholotše madi,",
        "Ya hwa Kgoši ya ka,",
        "A ineela a ba a nkhwela,",
        "Seboko se bja ka nna,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Sefapanong, Moo ke boneng lesedi,",
        "Mme morwalo wa pelo wa rolwa,",
        "Ka tumelo ka bona lesedi,",
        "Mme bjale ke mo lethabong;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Na ke ka mediro ya ka,",
        "A hwilego mohlareng,",
        "Kwelobohloko,",
        "'Rato ke tše fetišago (tšohle);"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Marothodi a maswabi",
        "A ka se fele,",
        "Kenna yo Morena,",
        "Ke tla ke le fela;"
      ],
      "number": 5
    }
  ]
}