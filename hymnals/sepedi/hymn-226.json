{
  "number": 226,
  "language": "nso",
  "title": " 226  O NYAKILE A DUMELA",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 226  O NYAKILE A DUMELA",
        "АН 183 CH 228",
        "ALMOST PERSUADED"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "O nyakile a dumela,",
        "O nyakile go amogela Kreste,",
        "Eke motho o re tloga moya tloga,",
        "Ke tlo go bitša ka nako e ngwe;"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "O nyakile a dumela,",
        "Etla gona bjale se retologe,",
        "Jeso' wa go laletša,",
        "Mangeloi a gona,",
        "O a rapelelwa, A ke o tle;"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Puna šetše e fetile,",
        "Tshenyego e gona mafelelong,",
        "Go nyaka fela ga go thuše selo,",
        "Sello se bohloko ge o lahlilwe;"
      ],
      "number": 4
    }
  ]
}