{
  "number": 21,
  "language": "nso",
  "title": " 21 KE ETLA SEFAPANONG. ",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 21 KE ETLA SEFAPANONG",
        "AH 184 CH 600",
        "I AM COMING TO THE CROSS"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ke etla sefapanong,",
        "Bofokoding le bodiding,",
        "Tšohle ke'a di lahla,",
        "Ke tlo humana phološo,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Pušološo:",
        "Ke tshepile Morena,",
        "Konyana ya Khalefari,",
        "Ke khunama fapanong,",
        "Mpholose Jeso bjale,"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ke go hlologeletšwe,",
        "Sebe se santse se ntaola,",
        "Jeso o bolela le nna o re,",
        "\"Ke tlo go hlapisa\","
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Ke go fa tšohle tša ka,",
        "Bagwera le lehumo,",
        "Ke be wag ago,",
        "Go ya ka bosafelego;"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Ke tshepa ditshepiso,",
        "Bjale ke kwa madi,",
        "Ke pitikologa fase,",
        "Ke bapotšwe le Jeso,"
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "Jeso o tlatša moyeng,",
        "Gomme ke phethegile,",
        "Gohle ke phethegile,",
        "Go tumiswe konyana;"
      ],
      "number": 7
    }
  ]
}