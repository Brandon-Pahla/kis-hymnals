{
  "number": 58,
  "language": "tum",
  "title": " 58 Thuwuskani Fumu",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 58 Thuwuskani Fumu",
        "ALL HAIL THE POWER"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Muthuwuske Fumu Yesu,  wangelo wagwade,",
        "Mumuvwalikenge mphumphu  yake m'uchindami,",
        "Mumuvwalikenge mphumphu yake m'uchindami."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Imwe mwa wakusoreka mwa wakuwombokwa,",
        "Thuwuskani muwomboli mu m'walike mphumphu,",
        "Thuwuskani muwomboli mu m'walike mphumphu."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Wanthu wose na mitundu pa charu cha pasi,",
        "Wachindikenge m'womboli, wamvwalike mphumphu,",
        "Wachindikenge m'womboli, wamvwalike mphumphu."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Pakufika kuchanya'ko tikamulambire,",
        "Tamkwimba mtatakuya swi,",
        "mu mvwarike mphumphu,",
        "Tamkwimba mtatakuya swi,",
        "mu mvwarike mphumphu."
      ],
      "number": 5
    }
  ]
}