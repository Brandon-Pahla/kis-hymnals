{
  "number": 114,
  "language": "tum",
  "title": " 114 Kuli Chipata Chamwazi",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 114 Kuli Chipata Chamwazi",
        "THE GATE AJAR FOR ME"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Kuli chipata chamwazi",
        "m'miryango mkung'azima,",
        "Mu mphinga mukuvumbukwa",
        "kutemwa kwa mponoski."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Refrain:",
        "Ndusungu Lwa Fumu Yesu,",
        "PakundijuLira ine,",
        "Ine, ine pa kundijuLira."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Chipata wakujulira wanthu",
        "wose wa m'charu,",
        "wasambazi na wakavu wa",
        "mtundu uli wose."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Njiraninge pa chipata",
        "papo chichali mwazi,",
        "Pokererani mu mphinga",
        "ndiko kuli kutemwa."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Mphinga tamkuwuthulira",
        "pa sirya pa muronga,",
        "cuwala mphumphu ya wumi",
        "m'chanya takumiemwesna."
      ],
      "number": 6
    }
  ]
}