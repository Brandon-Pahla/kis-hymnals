      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Kwananga kungasweta kutituwiskikenge,",
        "Kwananga kungasweta kutituwiskikenge,",
        "Nanga kutchesame nadi kwam wa nga weya,",
        "Kwananga kungasweta kwananga kungasweta,",
        "Kutituwiskikenge kutituwiskikenge"
