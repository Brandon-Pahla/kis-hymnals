{
  "number": 367,
  "language": "tum",
  "title": " 367 Mponoski Wati Wizenge",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 367 Mponoski Wati Wizenge"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Mponoski wati wizenge pa bingu,",
        "Charu citentheme,",
        "Wamoyo, wakufwa tiwawungane",
        "panthazi pa Fumu."
      ],
      "number": 2
    },
    {
      "type": "chorus",
      "lines": [
        "Yikwiza nadi Fumu yikwiza",
        "yikufumira kucanya,",
        "Wakumtemwa wadange kuwuka",
        "pakwiza kwa Fumu Yesu."
      ]
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Mwa wakupulikana tirondore",
        "kurongozga kwake",
        "Wali kuya katinozgera",
        "malo kwiza katitora."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Lindizganinge pano na kuromba,",
        "nchilayizgo chake",
        "Wukani, rombani, fukafukani,",
        "potaninge homwa."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Zizipizgani para m'kusuzgika,",
        "mkumanya wakwiza",
        "Kukondwa kulipo para",
        "wakwiza kuwatora wake."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "5.",
        "Suzgikani kuti m'tonde,",
        "kwananga kwazara mu caru,",
        "Kwananga kuwuseso chara",
        "nadi mu matupi ghinu."
      ],
      "number": 6
    }
  ]
}