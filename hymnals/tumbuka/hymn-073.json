{
  "number": 73,
  "language": "tum",
  "title": " 73 Pakwiza Fumu",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 73 Pakwiza Fumu",
        "CALLED TO THE FEAST"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Tachemeka ku chiphikiro,",
        "kukawa nawo wanthu wake,",
        "Kasi tamkwima wuli ise pakwiza Fumu?"
      ],
      "number": 2
    },
    {
      "type": "chorus",
      "lines": [
        "Pakwiza Fumu, mbaL,",
        "pakwiza Fumu, Kasi tamkwima wuLi",
        "'se pakwiza Fumu?"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Lye wakawafwira wanthu,",
        "wayam'kuwara mphumphu yake,",
        "Tamkumuwona nadi nadi pakwiza Fumu."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Ngandileza pakunganima,",
        "vyamchindindi vyamvumbukwa 'mwe,",
        "Tamkumanyana nadi tose, pakwiza Fumu."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Wizamkuwona wanthu wake,",
        "wavwara uchindami wake,",
        "Pakuzizipizga tamkondwa pakwiza Fumu."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "5.",
        "Kupatukana myirayira",
        "wakwananga wamkulira swi!",
        "Kwamkuwa kwakofya nkhanira pakwiza Yesu."
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "6.",
        "Yesu Fumu mtipokerere,",
        "tikhale patupi na imwe,",
        "Tileke kopa zuwa lira, pakwiza kwinu."
      ],
      "number": 7
    }
  ]
}