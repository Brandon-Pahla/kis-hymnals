{
  "number": 146,
  "language": "tum",
  "title": " 146 Mkhale Mkati Mwa Ine",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 146 Mkhale Mkati Mwa Ine",
        "THY LIFE WITHIN ME"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Mkhale mkati mwa ine,",
        "themba la mathemba,",
        "Mundipe imwe zgoro",
        "la mafumbo ghane,",
        "Mkhale mkati mwa ine,",
        "ndiwe winu m'vyose,",
        "Ndiwe chilongolero",
        "cha ukuru winu."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Mnyumba ya mtima wane",
        "mwasuka kwananga,",
        "Na uchindami winu",
        "mndiwalire mkati,",
        "Charu chikhale chete,",
        "thupi lane liwe,",
        "Muzga winu muweme,",
        "kuchita vya imwe."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Viwaro vyane vyose",
        "vimpulikireni, kwinu,",
        "Muchite navyo vyose",
        "mukukhumba kwinu,",
        "Ndingenjerwanga chara",
        "panji kutambula,",
        "Pake kugongowa",
        "panji kukhuwara."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Nditi ndipumuzgike",
        "ku visuzyo vose",
        "Kuti mlangule ndimwe para",
        "mndikhumbenge,",
        "Mkhale mkati mwa ine,",
        "themba la mathemba,",
        "Mundipe imwe zgoro",
        "la mafumbo ghane."
      ],
      "number": 5
    }
  ]
}