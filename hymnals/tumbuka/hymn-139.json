{
  "number": 139,
  "language": "tum",
  "title": " 139 Ndasanga Mbwezi",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 139 Ndasanga Mbwezi",
        "IVE FOUND A FRIEND"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Ndasanga m'bwezi muweme,",
        "wakanditemwa kale,",
        "Wakandiguza m'chitemwa",
        "kundifiska kwa iyo,",
        "Mtima wane ngwake nadi,",
        "ntha tingalekanaso,",
        "Ndine wake iyo ngwane,",
        "kwa muyirayira swi!"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Ndasanga m'bwezi muweme,",
        "wakafwa kundithaska,",
        "Wakapereka umoyo,",
        "na iyo mwene m'mphinga,",
        "Palije na kamo nkhane,",
        "kweni vyose n'vya Fumu,",
        "Mtima, mphavu, 'moyo",
        "vyose n'vyake kwamyirayira."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Ndasanga m'bwezi muweme,",
        "nkhongono zose n'zake,",
        "Kundivikilira m'nthowa,",
        "kundifiska kuchanya,",
        "Uchindami uli m'nthazi,",
        "kundikhwimiska ine,",
        "Sono ndirwe, nditewete",
        "kunthazi nkhapumule."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Ndasanga m'bwezi muweme",
        "wa lusungu, m'neneska.",
        "Ndi mrongozgi wa vinjeru",
        "m vikilin wa mphavu,",
        "Kwa uyo wakunditemwa,",
        "ntha nditipatukeko,",
        "Pa umoyo panji mnyifwa,",
        "ndine wake kwandanda."
      ],
      "number": 5
    }
  ]
}