{
  "number": 65,
  "language": "tum",
  "title": " 65 Nkhuweme Kugomezga",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 65 Nkhuweme Kugomezga",
        "TIS SO SWEET TO TRUST"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Nkhuweme kughagomezga mazgu",
        "gha Fumu Yesu Kukoreska",
        "marayizgo. agho yanena Fumu"
      ],
      "number": 2
    },
    {
      "type": "chorus",
      "lines": [
        "Nkhugomezga mwa ku Yesu,",
        "ndimrumbe mtatakuya, Yesu, Yesu,",
        "ngwalusungu ndimrumbe myirayira."
      ]
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Nkhuweme 'mwe kugomezga,",
        "m'ndopa zake za Yesu,",
        "Kugomezga mwa ku Yesu,",
        "tiwatisavye ise."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Nkhuweme kumugomezga wasavye",
        "zakwananga, Umoyo tiwatipenge,",
        "chimwemwe na mutende."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Nkhukondwa kum'manya iyo,",
        "Yesu mponoski wane",
        "NKnumanya watiwe nane",
        "mpaka ku umaliro."
      ],
      "number": 5
    }
  ]
}