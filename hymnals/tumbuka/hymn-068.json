{
  "number": 68,
  "language": "tum",
  "title": " 68 Ndawombokwa Na Ndopa",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 68 Ndawombokwa Na Ndopa",
        "REDEEMED! HOW I LOVE TO PROCLAIM IT"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Ine nkhukondwa kupharazga,",
        "ndaponoskeka na ndopa,",
        "Ndaponoskeka mu uchizi,",
        "mtatakuya ndine wake."
      ],
      "number": 2
    },
    {
      "type": "chorus",
      "lines": [
        "Mndopa zake, (mndopa zake)",
        "mndopa zake ndawombokwa,",
        "Ndine wake (ndine wake),",
        "mtatakuya ndine wake."
      ]
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Mwa Yesu ndiri wakusangwa,",
        "kwambura mayowoyeko,",
        "Nkhumanya ungweru wa Fumu,",
        "ukhalilire mwa ine."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Nkhukumbuka mponoski wane,",
        "nkhukumbuka zuwa lose,",
        "Nditimwimbire rutaruta,",
        "ndimbenge kutemwa kwake."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Nkhumanya ndizamkuyiwona,",
        "Fumu iyo nkhuyitemwa,",
        "lyo yikundirongozgas0,",
        "na kundisunga usiku."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "5.",
        "Mphumphu yane nadi yiriko,",
        "kukaya yiweme kura,",
        "Tose pamoza na watuwa,",
        "ndam'khara na Fumu Yesu."
      ],
      "number": 6
    }
  ]
}