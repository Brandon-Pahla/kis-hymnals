{
  "number": 166,
  "language": "tum",
  "title": " 166 Yesu Mwa Mphanji Yane",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 166 Yesu Mwa Mphanji Yane",
        "ROCK OF AGES"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Yesu mwa mphanji yane",
        "ndibisame mwa Imwe,",
        "Ndopa zo zikafuma",
        "umo wakamlasani,",
        "Zindichizge ku mtima",
        "zindithaske mkwananga."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Ntchito zane n'zichoko",
        "ndikuru dango linu,",
        "Nanga phamphu liwuke",
        "kulilira mkwananga,",
        "Vyose vikuperera",
        "Mponoski ndimwe pera."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Ndilive na kaweme,",
        "nkhukorako mphinjika,",
        "Mndichitire lusungu",
        "ndawankhule mvwalike,",
        "Ndamukazuzi ine",
        "mundisavye ndingafwa."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Para Iwendo lwamara",
        "ndamuvivya murwani,",
        "Pakukwera kuchanya",
        "kum'wonani m'chizumbe,",
        "Yesu mwa mphanji yane",
        "ndibisame mwa imwe."
      ],
      "number": 5
    }
  ]
}