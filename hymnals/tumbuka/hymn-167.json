{
  "number": 167,
  "language": "tum",
  "title": " 167 Lufura Lwa Kukhora",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 167 Lufura Lwa Kukhora",
        "FIRM FOUNDATION"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Lufura lwa k'khora imwe watuwa,",
        "Lwa chigomezgo luri m'mazgu ghake,",
        "Wangayowoyachi kuruska mazgu,",
        "Imwe mwabisama mwa muponoski,",
        "Imwe mwabisama mwa muponoski."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Mungopanga chara ndiri na imwe,",
        "Ndine Chiuta winu ndovwirenge,",
        "Nakumpani mphavu kum'chinthiskani,",
        "Nakumkhozgani mu urunji wane,",
        "Nakumkhozgani mu urunji wane."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Usange ndati mwende pa mironga,",
        "Mironga ya vitima ntha yizure,",
        "Ndiwenge namwe kuchimbizga suzgo,",
        "Kum'khwimiskani m'vikhuwazgo vyose,",
        "Kum'khwimiskani m'vikhuwazgo vyose."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Usange muna viyezgo mu nthowa,",
        "Wezi wakuzala mupokerenge,",
        "iyezgo vyakofya vitondenge cha,",
        "Vim'photoreninge uheni winu,",
        "Vim'photoreninge uheni winu."
      ],
      "number": 5
    }
  ]
}