{
  "number": 118,
  "language": "tum",
  "title": " 118 Yesu Wamala Mulandu Wane",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 118 Yesu Wamala Mulandu Wane",
        "JESUS PAID IT ALL"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Mponoski wakuti,",
        "\"Ulije nkhongono,",
        "Lindizga mkuromba,",
        "gomezga mwa Ine."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Refrain:",
        "Yesu wamaLa",
        "muLandu wane,",
        "Nkhafipiskika nadi",
        "wandisuka ine."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Mwa Imwe nkhusanga",
        "nkhongono zikuru,",
        "Zakusintha ine",
        "ndiwe m'weme mtima."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Ndilije kaweme",
        "kuti m'ndipe wezi,",
        "Ndisuke chivwalo",
        "m'ndopa za Kalvari."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Pa chizumbe chake",
        "ndamukweruzgika,",
        "Mmalundi gha Fumu",
        "ndamkuwa mkutonda."
      ],
      "number": 6
    }
  ]
}