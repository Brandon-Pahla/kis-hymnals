{
  "number": 210,
  "language": "tum",
  "title": " 210 Khristu Watiphemaniska",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 210 Khristu Watiphemaniska",
        "WHAT A WONDERFUL SAVIOUR"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Khristu watiphemaniska Mtaski wakuzizwiska,",
        "Tawombokwa watigula Mtaski wakuzizwiska."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Refrain:",
        "Mtaski wakuzizwiska Yesu, Fumu yane,",
        "Mtaski wakuzizwiska ndi Yesu Fumu."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Nkhumchindika m'ndopa zake Mtaski wakuzizwiska,",
        "Wandifikiska kwa Dada Mtaski wakuzizwiska."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Wanditozga mtima wane Mtaski wakuzizwiska,",
        "Wakuwusa nak'khalamo Mtaski wakuzizwiska."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Wakwenda nane munthowa Mtaski wakuzizwiska,",
        "Nundipa chipulikano Mtaski wakuzizwiska."
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "5.",
        "Wakundipa 'mwe kutonda Mtaski wakuzizwiska,",
        "Kutonda m'viyezgo vyose Mtaski wakuzizwiska."
      ],
      "number": 7
    },
    {
      "type": "verse",
      "lines": [
        "6.",
        "Kwa lyo ndajipereka Mtaski wakuzizwiska,",
        "Charu ntha chingandiquza Mtaski wakuzizwiska."
      ],
      "number": 8
    }
  ]
}