{
  "number": 225,
  "language": "tum",
  "title": " 225 Wamkhristu Wuka",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 225 Wamkhristu Wuka",
        "C.H. 576 STAND LIKE THE BRAVE"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Wamkhristu wuka yalangula Fumu,",
        "Vwala vya nkhondo na lupanga m'woko,",
        "Kumana na m'rwani ruta wofi cha!",
        "Yima ukhwime jiso pa murwani."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Refrain:",
        "Yima khwima, yima khwima,",
        "yima khwima, Jiso pa murwani."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Kwali ni m'suzgo pwerera umanye,",
        "Lawiska m'nthazi wangakupweteka,",
        "warwani mbanandi uti utonde,",
        "Yima ukhwime jiso pa murwani."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Mlimo wa Fumu uwukolerere,",
        "Linda, ulimbe, urwe, uthereske,",
        "Kose warongozge ruta m'chikanga,",
        "Yima ukhwime jiso pa murwani."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Ungadondanga mrongozgi walipo,",
        "Wakupa \"chizi na kusanguruska,",
        "Chitemwa chake nga ndi mronga m'bozwa,",
        "Yima ukhwime jiso pa murwani."
      ],
      "number": 6
    }
  ]
}