  "title": " 363 Ndimwe Chiuta",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 363 Ndimwe Chiuta"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Yumoza: Ndimwe",
        "Wose: Ndimwe Chiuta,",
        "Mwene-nkhongono.",
        "Yumoza: Ndimwe",
        "Wose: Ndimwe Chiuta,",
        "Mwene-nkhongono.",
        "Yumoza: Ndimwe wawa",
        "Wose: Ndimwve",
        "Yumoza: Ndimwe",
        "Wose: Ndimwe Chiuta, Mwene-nkhongono.",
        "Yumoza: Ndimwe wawa",
        "Wose: Ndimwe",
        "Yumoza: Ndimwe",
        "Wose: Ndimwe Chiuta,",
        "Mwene-nkhongono.",
        "Ndimwe Muliska,",
        "Mwene-nkhongono.",
        "Ndise mberere,",
        "za mskambo winu.",
        "M'linde mberere za mskambo winu.",
        "M'zipe nkhongono mberere zinu.",
        "Mlonde mberere zakutayika."
      ],
      "number": 2
    }
  ]
}