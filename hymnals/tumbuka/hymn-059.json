{
  "number": 59,
  "language": "tum",
  "title": " 59 Yesu Mkunditemwa",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 59 Yesu Mkunditemwa",
        "JESUS LOVER OF MY SOUL"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Yesu mukunditemwa, mu chipakato chinu,",
        "M'nditaske ku vimphunga na visuzgo vya mtima,",
        "M'ndivikilire Yesu, kumayigha ghakofya,",
        "Fiskani mtima wane kuphota la kuchanya."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Mphonokwa ndilive 'mwe, nkhukwapirana namwe,",
        "Lekani kundisida, m'ndikhozge m'ndisangwiske,",
        "Nkhumugomezgani 'mwe, movwiri wane ndimwe,",
        "Mutu wane m'wudike phapindo la uchizi."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Nkhukhumba imwe pera, kundikhuska m'kuruska,",
        "Ndamtechi, ndamurwari, nda chinthuru muchizge,",
        "Ndimwe watuwa nadi, ine ndiri muheni,",
        "Wezi, 'nenesko n'vyinu, ine ndiri wa waka."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Muna lusungu lose, mungandigowokera",
        "Maji ghamoyo ghize, ghanditozge mu mtima,",
        "Mbwiwi yenecho ndimwe, ndimwengeko kwa imwe",
        "Yibwibwituke mwane lero na kunthazi sWi!"
      ],
      "number": 5
    }
  ]
}