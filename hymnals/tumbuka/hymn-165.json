{
  "number": 165,
  "language": "tum",
  "title": " 165 Nkhutemwa Fumu",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 165 Nkhutemwa Fumu",
        "I LOVE THEE"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Nkhutemwa, nkhutemwa, nkhutemwa, Fumu,",
        "Nkutemwa mponoski, nkhutemwa Fumu,",
        "Nkhumtemwani Fumu namwe mkumanya,",
        "Milimo yinene nkhumutemwani."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Ndakondwa, ndakondwa nkhanira nadi,",
        "M'chimwemwe chandanda nkhwima pa phiri,",
        "Nkhudoka kuchanya kukakhalako,",
        "Na Yesu, wangelo na wak'wombokwa."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "A Yesu mponoski ndatumbikika,",
        "Chiponosko chane nchimwemwe chane,",
        "Zina linu m'mtima ndimbe chitemwa,",
        "Uchizi wa imwe uwe mwa ine."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Ndi njani nga ndimwe Fumu ya mtende,",
        "Mkusangwa, mkutemwa mukundovwira,",
        "Ndimbirenge Fumu mwa nkhongono 'mwe,",
        "Chimwemwe mu mtima nkhusanguruka."
      ],
      "number": 5
    }
  ]
}