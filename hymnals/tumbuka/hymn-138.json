{
  "number": 138,
  "language": "tum",
  "title": " 138 Nkhujipereka Kwinu",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 138 Nkhujipereka Kwinu",
        "ENTIRE CONSECRATION"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Mtole umoyo wane,",
        "nkhujipereka kwinu.",
        "Mtole mnawoko ghane,",
        "ghachite khumbo linu"
      ],
      "number": 2
    },
    {
      "type": "chorus",
      "lines": [
        "Mundisuke Fumu",
        "mu ndopa ('mwe mundopa),",
        "M'ndichizge mchiziwa",
        "cha ndopa (za ku chizga).",
        "Nkhujipereka yose kwinu, Fumu,",
        "Kwa imwe nyengo zose"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Mtole malundi ghane,",
        "ghachimbilire imwe,",
        "Mtole mazgu ndimbenge,",
        "ndimwimbireni Fumu."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Mtole milomo yane,",
        "yizule na uthenga,",
        "Mtoleso chuma chane,",
        "nd'reke kusunga kanthu."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Mutole nyengo yane,",
        "kuti ndimtamikeni,",
        "Mtole vinjeru vyane,",
        "ndichite mlimo winu."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "5.",
        "Mtole khumbiro lane,",
        "mulizgore la imwe,",
        "Mtoleso mtima wane,",
        "mukhalenge mwa ine."
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "6.",
        "Mtole chitemwa chane,",
        "ndiwe m'malundi ghinu,",
        "Mnditole yose ndiwe winu,",
        "nyengo zose swi!"
      ],
      "number": 7
    }
  ]
}