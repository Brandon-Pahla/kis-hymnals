{
  "number": 74,
  "language": "tum",
  "title": " 74 Ise Tikusewa",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 74 Ise Tikusewa",
        "SOWING IN THE MORNING"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Ise tikusewa mbuto za lusungu,",
        "Namachero chero na mise wuwo,",
        "Tikulindilira masika na vuna,",
        "Tizam'mwemwetera pakuvironga."
      ],
      "number": 2
    },
    {
      "type": "chorus",
      "lines": [
        "Pakuvironga, pakuvironga,",
        "Tizam mwemwetera, pakuvironga,",
        "Pakuvironga. pakuvironga,",
        "Tizam mwemwetera pakuvironga."
      ]
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Tichali kusewa nanga ndi muhanya,",
        "Panji mu mututu, m'mphepo na mvura,",
        "Masika ghakwiza, mulimo umare,",
        "Tizam mwemwetera pakuvironga."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Nanga tikusewa mu masozi pera,",
        "Nanga zitayike waka zinyake,",
        "Chaleka chiliro wamkutihenera,",
        "Tizam mwemwetera pakuvironga."
      ],
      "number": 4
    }
  ]
}