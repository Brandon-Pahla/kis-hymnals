{
  "number": 171,
  "language": "tum",
  "title": " 171 Imwe Walinda M'ziyoni",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 171 Imwe Walinda M'ziyoni",
        "C.H. 184 WHAT OF THE NIGHT?"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Imwe walinda m'Ziyoni mutimanyiske nyengo,",
        "Kasi nthanda yawoneka? Kasi mlenji wafika?",
        "Pakuwona kasi ungweru wiza?",
        "Pakuwona kasi ungweru wiza?"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "M'tiphalire vimanyisko vya ulendo vyamara,",
        "Kasi 'mwe tiri pafupi kufikako kusirya?",
        "Kasi nadi ufum'uli pafupi?",
        "Kasi nadi ufum'uli pafupi?"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Malazi gha kuwoneka t'yeni tichemerezge,",
        "Nthanda yikufuma nadi yikugadima m'chanya,",
        "Mwawalunji chiponosko chafika,",
        "Mwawalunji chiponosko chafika."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Tamanya kendero kithu mlima uli pafupi,",
        "Tichimbilire munthazi luwiro tampumula,",
        "Msekerere ise taponoskeka,",
        "Msekerere ise taponoskeka."
      ],
      "number": 5
    }
  ]
}