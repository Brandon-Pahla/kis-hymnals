{
  "number": 170,
  "language": "tum",
  "title": " 170 Fumu Mlengi Wane",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 170 Fumu Mlengi Wane",
        "MY MAKER AND MY KING"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Fumu mlengi wane vyose vyane n'vinu,",
        "M'nkhongono zinu Fumu nkhusanga vitumbiko,",
        "Mnkhongono zinu Fumu nkhusanga vitumbiko."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Chilengiwa Chinu nkhukhala mwa imwe,",
        "Uweme winu nchindi zane zikuperera,",
        "Uweme winu nchindi zane zikuperera."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Ndingamupanichi vyose n'vinu kale,",
        "Chitemwa chinu Fumu ntchakwenera viwongo,",
        "Chitemwa chinu Fumu ntchakwenera viwongo."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Mwe uchizi winu undipe nkhongono,",
        "Mazgu na makhumbiro ghane viwe vya imwe,",
        "Mazgu na makhumbiro ghane viwe vya imwe."
      ],
      "number": 5
    }
  ]
}