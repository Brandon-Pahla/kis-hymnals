  "title": " 126 Ndina Sumu Nkhuyitemwa",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 126 Ndina Sumu Nkhuyitemwa",
        "SINCE I HAVE BEEN REDEEMED"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Ndina sumu nkhuyitemwa",
        "Ndati ndawombokwa,",
        "Ya mponoski, mtaski. Fumu",
        "Ndati ndawombokwa."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Refrain:",
        "Ndati ndawombokwa",
        "Ndati ndawombokwa,",
        "N'chindike zina Lake,",
        "Ndati ndawombokwa",
        "Ndam'chindika zina La Fumu."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Khristu wakundikhutiska",
        "Ndati ndawombokwa,",
        "Ndichite khumbiro lake",
        "Ndati ndawombokwa."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "W Ndinal kaboni muweme",
        "Ndati ndawombokwa,",
        "Wakuchimbizga nkhayiko",
        "Ndati ndawombokwa."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Wandinozg chikaya",
        "Ndati ndawombokwa,",
        "Uko ndamkhala kwandan",
        "Ndati ndawombokwa."
      ],
      "number": 6
    }
  ]
