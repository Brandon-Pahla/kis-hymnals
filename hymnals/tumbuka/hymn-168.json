{
  "number": 168,
  "language": "tum",
  "title": " 168 Fumu Ndiyo Mliska",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 168 Fumu Ndiyo Mliska",
        "THE LORD IS MY SHEPHERD"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Fumu ndiyo mliska ntha nd'ngasauka,",
        "Nkhurya m'maruhari ghak'teketera,",
        "Wakundirongozga k'maji ghaweme,",
        "Wakundipa mphavu na kundithaska."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "M'chinkhwawu cha m'nkhorwe wa nyifwa nd'ngenda,",
        "Imwe muli nane ndopenge chara,",
        "Nthonga yinu na ndodo vikund'khozga,",
        "Palije chiheni chingandiwira."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Pamaso pa m'rwani mwanozga kurya,",
        "Nkhombo yazura na vitumbiko fwa!",
        "Mutu wane mwaphakazga mafuta,",
        "Kasi ndingafumbachiso kwa imwe?"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Nadi uweme na lusungu lwinu,",
        "Vindirondezgenge mpaka kuchanya,",
        "Ndende m'nthowa wasekuru wakenda,",
        "M charu ichi mpaka nkhanjire kura."
      ],
      "number": 5
    }
  ]
}