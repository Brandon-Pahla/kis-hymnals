{
  "number": 211,
  "language": "tum",
  "title": " 211 Vitiwire Vitumbiko",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 211 Vitiwire Vitumbiko",
        "C.H. 652 THERE SHALL BE SHOWERS OF BLESSING"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Vitiwire vitumbiko phangano la chitemwa,",
        "Kuwenge nyengo ziweme kufumira kuchanya."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Refrain:",
        "Vitumbiko tikuvikhumbiska nadi,",
        "Manthonyezi gha Lusungu ghatipe vitumbiko.",
        "(A/T)",
        "Vitumbiko vyake tikuvikhumbiska nadi,",
        "Manthonyezi gha Lusungu ghatipe vitumbiko."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Vitiwire vitumbiko vyakutichiska ise,",
        "Pa mapiri na m'vinkhwawu vula yikuru nadi."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Vitiwire vitumbiko mtitumizgire Fumu,",
        "Mutipe nkhongono sono mkwaniske mazgu ghinu."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Vitiwire vitumbiko viwe muhanya uno,",
        "Apo tikuzomerezga nakumuchema Yesu."
      ],
      "number": 6
    }
  ]
}