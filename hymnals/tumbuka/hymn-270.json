{
  "number": 270,
  "language": "tum",
  "title": " 270 Mponoski Withu",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 270 Mponoski Withu",
        "C.H. 214 OUR BLESSED REDEEMER"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Muponoski withu wakatilayizga kuti,",
        "Msanguluski wazamkuwa nkhoswe yithu."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Wakiza kwizakatozga, mulendo muweme,",
        "Wasange yunji m'neneska wakhalemo."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Mazgu ghake ghakuphwepwa kufuma kuchanya,",
        "Ghakususka uheni mkuweyerera."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Uweme wose ndi wake na kutonda kose,",
        "Ghanoghano liweme naloso ndake."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "5.",
        "Mzimu mutuwa mutichitirenge lusungu,",
        "Mukhale m'mitima yithu wana winu."
      ],
      "number": 6
    }
  ]
}