  "title": " 228 Mchindikeni M'chindikeni",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 228 Mchindikeni M'chindikeni",
        "CH. 645 PRAISE HIM PRAISE HIM"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "M'chindikeni Yesu muwomboli withu,",
        "Charu chimbe chiphale chitemwa,",
        "Mtamikeni imwe wangelo kuchanya,",
        "Zina lake ndakwenera ntchindi,",
        "Nga ndi mliska Yesu wakupwerera,",
        "Kutiyegha mumawoko ghake.  Refrain:",
        "Mchindikeni myowoye ukuru wake,",
        "Mchindikeni m'sumu za kukondwa.  2.",
        "Mchindikeni Yesu muwomboli withu,",
        "Wakafwira ise tawaheni,",
        "Vpnanji yithu yachiponosko chandanda,",
        "Mtamikeni wakapayikika,",
        "Mwimbireni vitima wakayegha,",
        "Chitemwa 'mwe chikuru nkhanira.  3.",
        "Mchindikeni Yesu muwomboli withu,",
        "uchanya kose kwimbe Hosana,",
        "Yesu mtaski wakuwusa nyengo Zose,",
        "M pani mphumphu musofi na Fumu,",
        "Yesu wizenge m'charu wakuvivya,",
        "Mazaza na ntchindi vyose n'vyake."
      ],
      "number": 2
    }
  ]
}