{
  "number": 158,
  "language": "tum",
  "title": " 158 Mwe Vuna Ya Charu Yaneng'enera",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 158 Mwe Vuna Ya Charu Yaneng'enera",
        "HASTEN ON, GLAD DAY"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Mwe vuna ya charu yaneng'enera,",
        "Fumu yikuchema wavuni wize,",
        "Mbuto zose zacha mu minda yose,",
        "Zaphya kulindizga kwiza kwa Fumu."
      ],
      "number": 2
    },
    {
      "type": "chorus",
      "lines": [
        "Soprano:",
        "Zani Luwiro, muyeghe vuna,",
        "Mutiyeghere kukwithu."
      ]
    },
    {
      "type": "verse",
      "lines": [
        "ALto, Tenor, Bass",
        "Zani imwe wangeLo Luwiro,",
        "Myeghe vuna ya m'charu kuchanya,",
        "Mutiyeghere ise Luiwiro,",
        "Kwithu kuchanya."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Dazi lambura masozi kuchanya,",
        "Likwiza luwiro, myaka yaluta,",
        "wanthu waphatira muzakwananga",
        "Muwawungaye nyengo yindamare."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Mwe kutowa mlimo wachitemwano",
        "Mronga wa maji kufuma kuchanya,",
        "Nchitemwa chikutirongora mlimo,",
        "Ndicho chikuvuna mbuto ziweme."
      ],
      "number": 5
    }
  ]
}