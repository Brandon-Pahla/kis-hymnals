{
  "number": 178,
  "language": "tum",
  "title": " 178 Khristu Wakwiza",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 178 Khristu Wakwiza",
        "C.H. 536 CHIRST RETURNETH"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Yamkuwa wuli nyengo yakumulenji,",
        "Pakwiza Yesu mu uchindami wake,",
        "Wizamwoneka na nkhongono zikuru,",
        "Kwizakatora wake."
      ],
      "number": 2
    },
    {
      "type": "chorus",
      "lines": [
        "Fumu mwizenge pauLi?",
        "timbe nakukondwa;",
        "Khristu wiza, Khristu wiza,",
        "ALeLuya Amen, ALeLuya Amen."
      ]
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Panji na mhanya, panji nyengo ya mise;",
        "Panji m'usiku Fumu 'mwe yingafika;",
        "Wizamwoneka na nkhongono zikuru, Kwizakatora wake."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Tamkondwa pakuruta tiri wamoyo Kulje nthenda,",
        "vitima na nyitwaso, NWza mmabingu,",
        "na nkhongono zIKUru, Kwizakatora wake."
      ],
      "number": 4
    }
  ]
}