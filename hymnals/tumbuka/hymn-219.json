{
  "number": 219,
  "language": "tum",
  "title": " 219 Mundirongozgenge, Dada",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 219 Mundirongozgenge, Dada",
        "C.H. 409 GUIDE ME, O THOU GREAT JEHOVA"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Mundirongozgenge, Dada ndamuyeni pa charu,",
        "Ndine wakutomboloka mundikore pa woko,",
        "Mundiryeske Mundiryeske ndirye mpaka",
        "ndikhute (ndikhute) Ndirye mpaka ndikhute."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Mujure chisimi chinu maji ghake ghan'chizge,",
        "Moto na mutambo m'nthazi vindirongozge m'nthowa,",
        "Mwankhongono mwankhongono muwenkhongono",
        "zane (mwe zane), Muwe nkhongono zane."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Pakufika pa Yordano mufumye wofi wane,",
        "Mu nyitwa na mpharanyiko nkhatikeko kusirya,",
        "Nkhamtamike, nkhamtamike nkhamutamike",
        "Fumu (m tamike) Nkhamutamike Fumu."
      ],
      "number": 4
    }
  ]
}