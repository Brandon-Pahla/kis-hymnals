{
  "number": 202,
  "language": "tum",
  "title": " 202 Yesu M'ndirongozgenge",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 202 Yesu M'ndirongozgenge",
        "C.H. 398 JESUS, SAVIOUR, PILOT ME"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Yesu m'ndirongozgenge paulendo wam'nyanja;",
        "Mayigha na jalawe vikujandizga m'nthazi;",
        "Wendeskero ngwa Imwe Yesu m'ndirongozgenge."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Nga ndi mama pakususutizga mwana wake;",
        "Muchetamiske nyanja mayigna muzikiske,",
        "Ndimwe nkhwantha pa nyanja,",
        "Yesu m'ndirongozgenge."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Para ndiri pafupi kufika ku mulima;",
        "Masuzgo ghakulenge mtende mwa Imwe pera;",
        "Ndipulike mukuti \"Rutanga reka kopa.\""
      ],
      "number": 4
    }
  ]
}