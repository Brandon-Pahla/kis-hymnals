{
  "number": 91,
  "language": "tum",
  "title": " 91 Mlimo Wa Sabata Wamara",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 91 Mlimo Wa Sabata Wamara",
        "ANOTHER SIX DAYS' WORK"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Tamalizga mlimo withu,",
        "Layamba sabata liphya,",
        "Moyo wane'we pumula,",
        "Chiuta wakutumbike."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Moyo wane uchindike,",
        "Imwe mkutipa wanangwa,",
        "Tikupokera thumbiko,",
        "Kuruska mazuwa ghose."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Marumbo ghithu ghakwere,",
        "Ghakhale ngati ndi sembe,",
        "Kuti imwe Fumu yithu,",
        "Mtumbike umoyo withu."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Kupumula kuli ngati,",
        "Bambiro la vya kuchanya,",
        "Uko mpingo wampumula,",
        "Pakumara suzgo lose."
      ],
      "number": 5
    }
  ]
}