{
  "number": 215,
  "language": "tum",
  "title": " 215 Kwithu Kwambula kKusintha",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 215 Kwithu Kwambula kKusintha",
        "C.H. 616 WAIT AND MURMUR NOT"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Kwithu kwambula kusintha chitima nacho kulije,",
        "Inya nkhwithu kwakutowa n'njani ntha wangakhumbako?"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Refrain:",
        "Linda (Lindizga) Ungadinginyikanga",
        "Linda (Lindizga) Ungadinginyikanga",
        "Linda (Lindizga) Linda (Lindizga)",
        "Leka k'dinginyika (dinginyika)"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Para iwe wafyenyeka na katundu mzito m'charu,",
        "Ukudoka kufikako linda kudinginyika cha."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Para m'nthowa muli minga Fumu yikavwala minga,",
        "Para uli na chitima lyo chikamkola kale."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Chita mlimo zizipizga romba uleke kuluwa,",
        "Dazi la mtende likwiza linda kudinginyika cha."
      ],
      "number": 6
    }
  ]
}