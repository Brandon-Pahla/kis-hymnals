{
  "number": 209,
  "language": "tum",
  "title": " 209 Ndendenge Namwe Chiuta",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 209 Ndendenge Namwe Chiuta",
        "O FOR A CLOSER WALK!"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Ndendenge namwe Chiuta,",
        "Fumu ya mutende,",
        "Ungweru undiwalire mnthowa ya kuchanya."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Izani Mzimu Mutuwa,",
        "Nthumi ya mutende,",
        "Nkhutinkha uheni wandipatula na Imwe."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Nkhawa na nyengo yiweme,",
        "Ndipo nkhukumbuka,",
        "Mtima ukuwawa charu ntha chingandithaska.."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Nkhutemwa vikozgo m'charu,",
        "Kwali viwe uli,",
        "Mndovwire kuvileka ndisope Imwe pera."
      ],
      "number": 5
    }
  ]
}