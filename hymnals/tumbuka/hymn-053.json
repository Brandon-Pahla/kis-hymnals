{
  "number": 53,
  "language": "tum",
  "title": " 53 Mzimu Ndi Mlongozgi",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 53 Mzimu Ndi Mlongozgi",
        "HOLY SPIRIT, FAITHFUL GUIDE"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Mzimu ndiyo mrongozgi,",
        "mu nthowa za ukhristu,",
        "Mtirongozgenge ndimwe,",
        "mu mapopa ghakotya.",
        "Mitima yikusangwa",
        "pakupulika mazgu,",
        "Wakuyowoya Mzimu,",
        "\"we mayinga werera.\""
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "M'bwezi withu muweme,",
        "muwe nase pafupi,",
        "Mngatilekanga tekha,",
        "m'charu chakofya ichi,",
        "Pakupoma mayigha",
        "tikwendamo na wofi,",
        "Kweni mazgu ghakuti,",
        "'we mayinga werera."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Para mlimo wamara,",
        "pakulindizga Fumu,",
        "Kunwekera kuchanya,",
        "mu malurombo ghithu.",
        "Kasi mazina ghithu",
        "ghali kulembekamo?",
        "Nweni mazgu ghakuti,",
        "'we mayinga werera."
      ],
      "number": 4
    }
  ]
}