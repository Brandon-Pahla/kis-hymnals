{
  "number": 86,
  "language": "tum",
  "title": " 86 Tijikame Tose",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 86 Tijikame Tose",
        "OWORSHIP THE KING"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Jikamani pamaso pa Fumu,",
        "Mwimbe chitemwa cha Fumu Yesu,",
        "Ndiyo nguru yithu na m'vikiriri,",
        "Mwa wa kuchanya ko, muchindikike."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Mose mwimbenge nkhongono zake,",
        "Wezi kutemwa kwake kwa ise,",
        "Wakugadima nga leza m'mawingu,",
        "Fumu Yesu yavwara vituwa tu!."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Chitemwa chake ncha kuzizwiska,",
        "Dazi likuwalira pa wose.",
        "M'mironga na m'mapiri na m'vinkhwawu",
        ", Muvura muvyose na mjumi wuwo."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Tilje nkhongono Fumu yithu,",
        "Tikugomezga kwa imwe pera,",
        "Mu uchizi winu mtisungilire",
        "Mwomboli na mlengi na mbwezi withu."
      ],
      "number": 5
    }
  ]
}