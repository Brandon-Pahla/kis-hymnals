{
  "number": 62,
  "language": "tum",
  "title": " 62 Fumu Nkhumtemwani",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 62 Fumu Nkhumtemwani",
        "MYJESUS I LOVE THEE"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Fumu nkhumtemwani na mtima wose.",
        "Nkhureka vya charu chifukwa chinu,",
        "Mponoski muweme gomezgo lane,",
        "Chitemwa nkhumtemwa charuska kale."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Nkhumtemwani chifukwa mkanditemwa,",
        "Imwe mkafwira ine pa mphinjika,",
        "Mukugowokera viheni vyose,",
        "Chitemwa nkhumtemwa charuska kale."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Ndichali na umoyo na panyifwa,",
        "Ndichindike imwe mponoski wane,",
        "Pakuzilirika kwane pakufwa,",
        "Chitemwa nkhumtemwa charuska kale."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "M'malo gha uchindami na kusangwa,",
        "Ndamkumchindikani kwa myirayira,",
        "Ndamkuvwara mphumphu ya kugadima,",
        "Chitemwa nkhumtemwa charuska kale."
      ],
      "number": 5
    }
  ]
}