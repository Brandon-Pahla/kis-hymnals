{
  "number": 372,
  "language": "tum",
  "title": " 372 Ndawakwananga Ndapona",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 372 Ndawakwananga Ndapona"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Nda wakwananga ndapona,",
        "goliwoli lasutuka,",
        "Landiyenyanga m'singo'mo,",
        "Yesu Wakandiwombola."
      ],
      "number": 2
    },
    {
      "type": "chorus",
      "lines": [
        "Ndasutuka, ndawombokwa,",
        "Yesu wakhara mkati'mo",
        "wakuya nane mziyoni,",
        "Watuwa wakuhenera."
      ]
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Vikondo vya murwani 'mwe",
        "pafupi kundimarizga",
        "Ndamwene na nyumba yane,",
        "Yesu, wakandiwombola"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Pakusutulika kwane",
        "Wangelo walulutira,",
        "wahemba wandiwurira vya pa",
        "mtima vya ku Yesu."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "M'Yerusalemu kucanya",
        "mphomo Yikapulikikwa,",
        "Wangelo, watuwa wose",
        "wakundihenera nadi."
      ],
      "number": 5
    }
  ]
}