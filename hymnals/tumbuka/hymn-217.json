{
  "number": 217,
  "language": "tum",
  "title": " 217 Fumu Mkunditemwa",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 217 Fumu Mkunditemwa",
        "SOMETHING FOR JESUS"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Fumu mkunditemwa mkandifwira",
        "Ntha tindimkaneni Fumu yane,",
        "Muchitemwa ndize ndimuzomereni,",
        "Na zithero ndize ndimpeni 'mwe."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Pa chizumbe chinu mlomo ndimwe,",
        "Nkhugomezga Imwe Fumu Yesu,",
        "Ndiyeghe mphinjika kuzomera Imwe,",
        "Ndirombe kwa Imwe ndiwe winu."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Mundipe mutima ndimkozgeni,",
        "Dazi liri lose ndiwoneske,",
        "Nchito za chitemwa ndiposo lusungu,",
        "Nipenje wakuzgewa wize kwinu."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Vyose ndiri navyo mukundipa,",
        "Mu umoyo wane ndiwe winu,",
        "akukam'wonani ine ndawombokwa,",
        "Kwandanda ndamkhala namwe Fumu."
      ],
      "number": 5
    }
  ]
}