{
  "number": 269,
  "language": "tum",
  "title": " 269 Mponoski VWakuzizwiska Ndi Yesu",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 269 Mponoski VWakuzizwiska Ndi Yesu",
        "A WONDERFUL SAVIOUR IS JESUS"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Mponoski wakuzizwiska ndi Yesu",
        "wakuzizwiska kwa ine",
        "Wakundibisa wamoyo mjalawe",
        "ndamwona vyakukondweska."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Refrain:",
        "Wakundibisa wamoyo mjaLawe,",
        "Kundibisa kumasuzgo,",
        "Wakundibisa muchitemwa Chake",
        "Kuikapo woko Lake ku ikapo woko Lake"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Mponoski wakuzizwiska ndi Yesu",
        "wakundithura katundu,",
        "Wakundivikilira ntha ndingawa",
        "wakundipaso nkhongono."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Wakundipa vitumbiko vinandi",
        "nakundipaso urunji,",
        "NKhwimba kuyichindika Fumu",
        "yane nadi Muponoski ngwane."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Apo ndamkuvwala utuwa wake",
        "kukumana nayo mdera,",
        "ponosko chakuzizwiska nadi",
        "ndamukwimba na watuwa."
      ],
      "number": 6
    }
  ]
}