  "title": " 97 Tapulika Makani",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 97 Tapulika Makani",
        "WE HAVE HEARD"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Tapulika makani,",
        "Yesu ndi mponoski,",
        "M'pharazge nakusangwa,",
        "Yesu ndi mponoski,",
        "Mukaphare ku wose,",
        "mukalowoke nyanja,",
        "Mwatumika na Fumu,",
        "Yesu ndi mponoski."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Tandazgani makani,",
        "Yesu ndi mponoski,",
        "Ku wose wakwananga,",
        "Yesu ndi mponoski,",
        "Mu virwa, m'nyanja mose,",
        "mvinkhwawu, na mmapiri,",
        "Charu chose chikondwe,",
        "Yesu ndi mponoski."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Mwimbengeso vya lyo,",
        "wose wapulike,",
        "Wakafwa wakaghuma,",
        "ngwa moyo, ngwa moyo,",
        "Para mtim√° wafipa,",
        "pakupenja lusungu,",
        "Kusuka zakwananga,",
        "Yesu ndi mponoski."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Mphepo zichemerezge,",
        "chomene nkhanwa,",
        "\"Mwe muzipokerere,",
        "nakwimba chomene,",
        "\"Chiponosko chikuru,",
        "chizira wose m'charu.",
        "Wakuwapenja wose,",
        "Yesu ndi mponoski."
      ],
      "number": 5
    }
  ]
}