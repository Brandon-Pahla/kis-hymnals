{
  "number": 173,
  "language": "tum",
  "title": " 173 Ndiwenge Na Chigomezgo",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 173 Ndiwenge Na Chigomezgo",
        "C.H. 245 O FOR A FAITH!"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Ndiwenge na chigomezgo chakukhola nadi,",
        "Chambula kusunkhunyika muwukavu,",
        "Panji mumasuzgo m'wukavu na m'suzgo."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Icho ntha chamsinginika para nkhutimbika",
        "M'vyakuwinya na m' vitima chegamire,",
        "Pa Fumu Chiuta chegamire Fumu."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Chigomezgo chakuwala m'mayigha kuwalo,",
        "Chambula wofi m'visuzgo muvyakofya,",
        "Mu chisi nkhayiko mu chisi nkhayiko."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Chakuzizipizga m'charu kuweka msinjiro,",
        "Chakupheska  zakwananga  zakwananga,",
        "Na mauchenjezi gha charu chapasi."
      ],
      "number": 5
    }
  ]
}