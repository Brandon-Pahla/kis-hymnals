{
  "number": 189,
  "language": "tum",
  "title": " 189 Tirije Msumba Mu Charu",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 189 Tirije Msumba Mu Charu",
        "C.H. 308 NO ABIDING CITY HERE"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Tirije msumba mucharu wakukhaliska kwanadi;",
        "Kweni tikondwere ise tik'khumba msumba wukwiza;",
        "Tik'khumba msumba wukwiza."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Tirije msumba mucharu tik'khumba msumba wunyake;",
        "Uko Fumu yikukhala wakuwala myirayira",
        "Wakuwala myirayira."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "\"Mwe ndi malo ghamutende ndiko nkhwakupumulira;",
        "Ndiwenge na mapapindo 'mwe mphanyi ndadukirako;",
        "Mwe mphanyi ndadukirako."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Chetama umoyo wane Fumu yikumanya nyengo;",
        "Ndichitenge khumbo lake ndiyo wandipumuzgenge",
        "Ndiyo wandipumuzgenge."
      ],
      "number": 5
    }
  ]
}