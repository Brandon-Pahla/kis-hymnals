      "type": "verse",
      "lines": [
        "### 364 Tiwonge Yehova Withu"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Tiwonge Yehova withu,",
        "tiwonge Yehova withu,",
        "Chiuta wa Sirayeli."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Wati wawe na lusungu,",
        "wati wawe na lusungu,",
        "Ku awo vyalema lero."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Wati waponoske wose,",
        "wati waponoske wose,",
        "Awo wakutemwa lyo."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Wati wawombole awo,",
        "wati waombole awo,",
        "Wakulilira kwa lyo."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "5.",
        "Wakuchema walkwananga,",
        "wakuchema wakwanangd",
        "Wazgoke wakunyoroka."
      ],
      "number": 6
    },
    {
      "type": "verse",
      "lines": [
        "6.",
        "Zina lake likuzgike,",
        "zina lake likuzgike,",
        "Ku mazuwa na mazuwa."
      ],
      "number": 7
    }
  ]
}