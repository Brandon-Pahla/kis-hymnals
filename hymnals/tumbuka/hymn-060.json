{
  "number": 60,
  "language": "tum",
  "title": " 60 Yesu Chimwemwe Chithu",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 60 Yesu Chimwemwe Chithu",
        "JESUS, THOU JOY OF LOVING HEARTS"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Yesu chimwemwe cha mtima,",
        "Mwe lumuli lwa tawanthu,",
        "Vya m'charu ntha tikukhuta,",
        "Vyinu ndivyo tikukhumba."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Nenesko linu likwima,",
        "'Mwe, mtaski wa wakulira,",
        "Awo wakumusangan,",
        "Wakuti mukupa vyose."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Ndimwe chibama tikurya,",
        "Tamba kale tiryengeso.",
        "Ndimwe chisimi tikumwa,",
        "Mizimu yithu yimweso."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Tawamayinga tadoka,",
        "Mutinjizgem'chipumulo,",
        "Tichenge pakumwonani,",
        "Timkorani na chimwemwe."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "5.",
        "A Yesu mukhale nase,",
        "Mutipe ungweru winu,",
        "Mdima umare, tiwone,",
        "Kungweruka kwinu, Fumu"
      ],
      "number": 6
    }
  ]
}