{
  "number": 235,
  "language": "tum",
  "title": " 235 Lizaso Dazi Lakupumula",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 235 Lizaso Dazi Lakupumula",
        "AGAIN THE DAY RETURNS"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Lizaso dazi lakupumula;",
        "Lakutumbikika na Chiuta;",
        "Wakapumula ku ntchito zake;",
        "Naseso tipumule mu mtende."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Tiyeni tose pa dazi ili;",
        "Timumanye makhumbiro ghake;",
        "Watipulikenge m'sumu zithu;",
        "Na kupokera marombo ghithu."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Fumu yithu m'khazge khutu linu;",
        "Mpulike rombo lwa twana twinu;",
        "Mukumbukire phangano linu;",
        "Muwataske wose wakuzgewa."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Imwe Fumu pa chizumbe chinu;",
        "Mutipe uchizi winu mwekha;",
        "Mwa imwe tikusanga ungweru;",
        "Mutitolere kwinu kuchanya."
      ],
      "number": 5
    }
  ]
}