{
  "number": 218,
  "language": "tum",
  "title": " 218 Lufura Ndimoza",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 218 Lufura Ndimoza",
        "ONE FOUNDATION"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Lufura lwa mpingo ndumoza ndiye Yesu",
        "Chilengiwa chiphya, mwa maji na m'mazgu;",
        "Wakikha kuchanya kwizakayamba mpingo",
        "Kugula na ndopa, nakufwa pa mphinjika."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Kukwana mitundu yose m'charu chapasi;",
        "Chiponosko chiri munthowa yimo pera,",
        "Zina lake ili ndilo lapanga mpingo;",
        "Chigomezgo chimo nakuzizipizgaso."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Wanji wakuseka mpingo wa Yesu Fumu;",
        "Para mpingo ukusuzgika mkuyezgeka,",
        "Watuwa wamkusungilira chigomezgo",
        "Vitima vya m'mara, tayamkumwemwetera."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Pakati pa visu2 na pakuyezgeka;",
        "Mpingo ukulindizga lye kuponoska,",
        "Na paumaliro kulira kwamkumara.",
        "pingo wake wamkuruska kuumaliro."
      ],
      "number": 5
    }
  ]
}