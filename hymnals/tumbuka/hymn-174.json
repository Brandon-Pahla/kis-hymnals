{
  "number": 174,
  "language": "tum",
  "title": " 174 Chigomezgo Nchakukondweska",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 174 Chigomezgo Nchakukondweska",
        "C.H. 387 HOW CHEERING"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Chigomezgo nchakukondweska,",
        "Pakwenda pa charu,",
        "Chikutikhozga pakwenda mu charu cha mdima,",
        "Chikutikhozga pakwenda mu charu cha mdima."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Chikurongora kuli mtende,",
        "Uko kuli wake,",
        "Tamukumana na wake nakukharilira,",
        "Tamukumana na wake nakukharilira."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Charu chambura zakwananga,",
        "Viyezgo kulije,",
        "Kwamkuwa kukondwa pera kwambura chivundi,",
        "Kwamkuwa kukondwa pera kwambura chivundi."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "M'charu icho ntha tamkulira,",
        "Nyifwaso kulije,",
        "Wose awo wam'njirako wamkozga wangelo,",
        "Wose awo wam'njirako wamkozga wangelo.  5.",
        "Duka moyo wane ukondwe,",
        "Mtaski wakwizaso,",
        "Tikudoka kum'wonani m'chikaya chituwa,",
        "Tikudoka kum'wonani m'chikaya chituwa."
      ],
      "number": 5
    }
  ]
}