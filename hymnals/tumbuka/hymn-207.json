{
  "number": 207,
  "language": "tum",
  "title": " 207 Khamuwona Pa Mphinjika",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 207 Khamuwona Pa Mphinjika",
        "SEALS MY PARDON"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "1.",
        "Nkhamuwona pa mphinjika,",
        "Kuphupha na nyifwa,",
        "lyo wakundilawiska,",
        "Pasi pa mphinjika."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "2.",
        "Nkhutondeka na kuluwa,",
        "Ivyo nkhaviwona,",
        "Para ndine nkhamukoma,",
        "Vimanyikwe vyekha."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "3.",
        "Wakandilawisiskaso,",
        "Nga nkhundiphalira,",
        "Nyifwa iyi, ndopa izi,",
        "N'vyakukuponoska."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "4.",
        "Juvyo lane likamkoma,",
        "Liheni nkhanira,",
        "Chamchindindi cha uchizi,",
        "Chandigowokera."
      ],
      "number": 5
    }
  ]
}