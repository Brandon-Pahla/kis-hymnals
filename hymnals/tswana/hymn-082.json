{
  "number": 82,
  "language": "tn",
  "title": "82 Morena Mo Mosong",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Morena Mo Mosong O Tla",
        "Utlwa lentswe ja me;",
        "Go wena ke tla lebisa,",
        "Merapelo ya me."
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Mo’a Wa Gago O Nkgogele",
        "Ditseleng tsa tshiamo:",
        "Tsela tsotlhe tsa tirelo",
        "Di thokgamisiwe."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Batho Ba ’Go Ratang Ba Tla",
        "Bona tsholofetso;",
        "Modimo Mothatayotlhe,",
        "O tla ba femela."
      ],
      "number": 3
    }
  ]
}