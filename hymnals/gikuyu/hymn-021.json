{
  "number": 21,
  "language": "ki",
  "title": "21. Ukaani Tukumie Jesu",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "1.",
        "ũkaani tũkumie Jesũ,",
        "Nĩwe waatũhonokirie,",
        "Nĩtwĩheane kũrĩ we,",
        "Na indo ciothe.",
        "2.",
        "Rĩĩtwa rĩa Jesũ nĩrĩega,",
        "Na norĩo rĩtũkenagia,",
        "Tondũ nĩagatũhootithia,",
        "Kũrũa mbaara.",
        "3.",
        "Mwathani tũmenyerere.",
        "ũtũteithie kũiguana,",
        "Nĩguo tũgaatũra nawe,",
        "Kũu igũrũ.",
        "4.",
        "Kwoguo twathiĩ kũu igũrũ",
        "Tũkahanana na Jesũ,",
        "Natũkagaĩrũo muoyo,",
        "ũtagaathira."
      ],
      "number": 1
    }
  ]
}