{
  "number": 85,
  "language": "ki",
  "title": "85. Ni Utuku Gukiriitwo",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "1.",
        "Nĩ ũtukũ gũkirĩĩtwo,",
        "Thĩ yothe ĩĩ toro;",
        "No Mariamu na Jusufu,",
        "Meroreire Mwana mũthaka.",
        "”Koma, Mwana mwega”",
        "Koma, Mwana mwega.",
        "2.",
        "Nĩ ũtukũ; Gũkirĩĩtwo;",
        "Arĩithi mokĩrio,",
        "Nĩ ũtheri wa rũĩmbo rũega,",
        "Ciumĩte igũrũ- matuinĩ,",
        "“Kristo nĩ mũũku thĩ,",
        "Mũkũũri nĩ mũciare”",
        "3.",
        "Nĩ ũtukũ gũkirĩĩtwo,",
        "ũkeengi ũgĩũka thĩ",
        "Wa mwana ũcio wa igũrũ;",
        "Nĩatũreheire ngoga ciake.",
        "“Kristo nĩ mũũku thĩ,",
        "Mũkũũri nĩ mũciare”",
        "4.",
        "Nĩ ũtukũ gũkirĩĩtwo,",
        "Araika makĩina;",
        "“Ngumo ya ũhoreri wake,",
        "ũroiga kũrĩ andũ othe,",
        "Kristo nĩ mũũku thĩ,",
        "Mũkũũri nĩ mũciare."
      ],
      "number": 1
    }
  ]
}