{
  "number": 348,
  "language": "tw",
  "title": "Asafon' Wɔ Fapem K'ro",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Asafon' wɔ fapem k'ro",
        "Ͻne Kristo, ne W'ra;",
        "ɔnam nsu ne asɛm so",
        "Yɛ N'abɔde fofro;",
        "Ofi s'ro bɛhwehwɛɛ no,",
        "Yɛɛn' ayef'ro kronkron;",
        "ɔde Ne mogya tɔɔ no,",
        "Na ne nkwa nti n'Owui."
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Wɔapaw no aman nyinaam',",
        "Na ɔyɛ k'ro wiase;",
        "Ne nkwagye botae no yɛ",
        "'W'rade k'ro, gyidi k'ro;",
        "Din kronkron k'ro na ohy'ra,",
        "Odi 'duan kronkron k'ro,",
        "Na ɔde 'nidaso k'ro",
        "Nante wɔ ɔdom mu."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Sɛ nipa di ne ho fɛw",
        "N'ahohia kɛse mu;",
        "Sɛ tammfo dw'riw botan a",
        "ɔhome so no mpō a;",
        "Ahotefo de gyidi",
        "Su sɛ: 'nkɔsi da bɛn?",
        "Ͻrenkyɛ na 'nadwo su",
        "Adan 'nɔpa dwonto."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ne brɛ ne nsɔhwɛ nyinaa",
        "Ne ne ko kɛse mu,",
        "ɔtwɛn kosi bere a",
        "Daa asomdwoe bɛba;",
        "Na 'nuonyam 'nidaso a",
        "ɔretwɛn no aba,",
        "Ma 'safo nkonimdifon'",
        "Anya daa ɔhome."
      ],
      "number": 4
    }
  ]
}