  "title": "Nea Yɛde MaW' Biara",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Nea yɛde maW' biara,",
        "Yɛ W'ankasa Wo de;",
        "Nea yɛwɔ nyinaa wɔ Wo nko,",
        "Woayɛ yɛn so hwɛfo."
      ],
      "number": 1
    }
  ]
}