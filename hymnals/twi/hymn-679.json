{
  "number": 679,
  "language": "tw",
  "title": "Nyame Ntra Me T'rim'",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Nyame ntra me t'rim',",
        "ne m'adwene mu;",
        "ɔtra m'niwam',",
        "ne m'adehwɛ mu;",
        "Nyame ntra m'anom',",
        "ne me kasa mu;",
        "Nyame ntra me komam',",
        "ɛne me ntease no mu."
      ],
      "number": 1
    }
  ]
}