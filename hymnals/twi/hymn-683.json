{
  "number": 683,
  "language": "tw",
  "title": "Yesu, Gyina Yɛn Mu",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Yesu, gyina yɛn mu",
        "Wo wusɔr' tumim';",
        "Ma saa som b're yi nyɛ",
        "B're a wɔatew ho."
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Hom' Wo Honhom Kronkron",
        "Gu koma biaram';",
        "Pam suro ne 'w'rɛhow",
        "Fi ɔkra biaram'."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Saa na 'hokeka mu,",
        "Yetu yɛn kwan no;",
        "Wɛn kosi dā a enni",
        "Awiei 'nɔpa no.\""
      ],
      "number": 3
    }
  ]
}