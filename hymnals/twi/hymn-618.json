{
  "number": 618,
  "language": "tw",
  "title": "Monsɔr' Nnyina Mma Yesu!",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Monsɔr' nnyina mma Yesu!",
        "Asɛnnua ho nsraafo;",
        "Momma Ne frankaa no so,",
        "Ͻnsɛ sɛ ɛyera;",
        "'Fi nim mu kɔ nkonim mu,",
        "Obedi N' dɔm anim",
        "Kosi sɛ 'tamfo agu,",
        "Na Kristo ne 'Wura."
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Monsɔr' nnyina mma Yesu!",
        "Munti t'robɛnto nne;",
        "Monkɔ ko kɛsen' anim",
        "N'anuonyam da yi mu.",
        "Mo mmarima, monsom No,",
        "Nko ntia 'tamfo dɔm pii",
        "Nnya kokod'ru amanem',",
        "Momfa ahoɔden nko."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Monsɔr' nnyina mma Yesu!",
        "N'ahoɔden no mu nko;",
        "'Honam bedi mo huammɔ",
        "Muntwer' mo ankasa.",
        "Monhyɛ 'sɛmpa akode,",
        "Monwɛn wɔ mpaebɔ mu;",
        "ɔsom mų oo, amane mų oo,",
        "Monnan mō akyi nky'rɛ da."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Monsɔr' nnyina mma Yesu!",
        "ɔko no renkyɛ pii,",
        "Ͻnnɛ ɔko gyegyeegye,",
        "ɔkyena nkonim no",
        "Nea odi mu nkonim no",
        "Benya nkwa abot'ri,",
        "Na ɔne 'nuonyam Hen' no",
        "Bedi ahen' daa daa."
      ],
      "number": 4
    }
  ]
}