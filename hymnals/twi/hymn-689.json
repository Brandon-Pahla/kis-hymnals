  "number": 689,
  "language": "tw",
  "title": "Da Biara Mesrɛ Nneɛma",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Da biara mesrɛ nneɛma",
        "mmiɛnsa, Wura:",
        "Sɛ ma minhu Wo yie,",
        "ma mennɔ Wo yie,",
        "Na minni W'akyi yie da biara."
      ],
      "number": 1
    }
  ]
}