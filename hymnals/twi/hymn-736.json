{
  "number": 736,
  "language": "tw",
  "title": "N41 Dare To Be A Daniel",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Gyina nokwar' asεm so,",
        "Suo Nyame mm'ra mu,",
        "Di gyidifo no nso ni!",
        "Wɔnkamfo Daniel kuw!"
      ],
      "number": 1
    },
    {
      "type": "chorus",
      "lines": [
        "Si wo bo sε Daniel,",
        "Si wo bo gyina;",
        "Nya botae a ase tim,",
        "Na yi no adi!"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Atumfo pii ayera,",
        "Wɔantum' annyina,",
        "Na wɔyε Nyame ‘safo",
        "Sε wɔf'raa Daniel kuwm'!"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Aberan akεse a",
        "Wɔafa nsase yi,",
        "Sε wohyia Daniel kuw a,",
        "Wɔbεhwehwe ase."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ma ‘nidahε frankaa so,",
        "Di nkonim kɔse!",
        "Di Satan ne ne dεm so,",
        "Na teɔm ma Daniel kuw!"
      ],
      "number": 4
    }
  ]
}