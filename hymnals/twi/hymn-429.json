{
  "number": 429,
  "language": "tw",
  "title": "Sika Yerusalem a",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Sika Yerusalem a",
        "Nufusu ne 'wo wom',",
        "Wo ho adwen ma koma",
        "Ne nne my yaw tu y'ra;",
        "Minnim, O minnim 'nigye",
        "Kronkron a ɛwɔ hɔ,",
        "Anuonyam a ɛhy'rɛn ne",
        "'Dɛw a enni nsɛso."
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Sion adampɔn no mu",
        "'H'rusi dwom ahyɛ ma;",
        "Mogya 'dansefo kuw ne",
        "'Bɔfo dɔm pii hy'rɛn hɔ,",
        "ɔhen' no wɔ wɔn mu daa;",
        "'Dekyee hann no yɛ dwoo,",
        "Anuonyam haban pii wɔ",
        "'Hy'rafon' didibea hɔ."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Dawid agua no wɔ hɔ,",
        "Nkonimdifo no nteam",
        "Ne ponton' asefo dwom",
        "No nyinaa hyɛ hɔ ma;",
        "Na wɔn a wɔne wɔn W'ra",
        "Ako adi nim no,",
        "Wɔahyɛ wɔn ntade fitaā a",
        "Wɔrenworɔ bio da."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "O, ɔdɛw ne hhyira man,",
        "Nyame aponfo fie;",
        "O, ɔdɛw ne nhyira man",
        "A koma p're hwehwɛ",
        "Yesu, fa yɛn bra 'sasč a",
        "Ahome wɔ so no.",
        "Nhyira nka Wo ne Agya",
        "Ne Honhom do daa ny'naa."
      ],
      "number": 4
    }
  ]
}