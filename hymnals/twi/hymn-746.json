  "title": "N51 I'm So Happy",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "M'ani agye asek'rɔ ne sɔ;",
        "Yesu ayi m'adesoa nyinaa kε;",
        "Afei mɔto dwom mmere nnyinaa",
        "Yesu ayi m'adesoa nyinaa kε.",
        "Kan me bεn'adesoa hyɔ",
        "Me koma so,",
        "Yesu yii me bεne maa me asomdwoe.",
        "Afei mɔto dwom mmere nyinaa,",
        "Yesu ayi m'adesoa nyinaa kε koraa!",
        "Yesu ayi m'adesoa nyinaa kε",
        "Mani agye!",
        "Yesu ayi m'adesoa nyinaa kε!"
      ],
      "number": 1
    }
  ]
}