{
  "number": 27,
  "language": "tw",
  "title": "Mo a Mo Komam Tew",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Mo a mo komam tew,",
        "Munni dɛw, monto dwom,",
        "Momma frankaa no so wɔ wim,",
        "Kristo asɛnnua no."
      ],
      "number": 1
    },
    {
      "type": "chorus",
      "lines": [
        "Di dɛw! Di dɛw!",
        "Momfa nnaase nto dwom!"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Momma mo nne so den",
        "Sɛ po 's'rɔkye 'yeyi,",
        "Monto dwom a efi tete,",
        "Tete mm're mu nnwom no."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "S'ro 'bɔfo nnwontofo,",
        "'Sase so 'hotefo,",
        "Momfa dɛw nto ah'rusi dwom",
        "Nokwar' 'hosɛpɛ mu."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "ɔbra kwantu yi mu,",
        "Monkɔ so nto dwom daa;",
        "Mmofra, mpanyin, awia, 'nadwo,",
        "'Nigye ne mmusu mu."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Munyi s'ro Hen' ayɛ,",
        "W'ra a yɛsom No no;",
        "Agya, ɔba, Honhom Kronkron,",
        "Nyame teasefo no."
      ],
      "number": 5
    }
  ]
}