{
  "number": 264,
  "language": "tw",
  "title": "Anka Manya Nkwa Gyaframā a",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Anka manya nkwa gyaframā a",
        "Ͻdɛw tete hotefo mun';",
        "Ͻmaa wɔn kra kɔn dɔɔ s'ro man,",
        "Yɛɛ ateetee ne mmusum' komm."
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Abram komam' honhomɔā ɛhyɛɛn'",
        "Agy'rae maa Won' wɔ he, Aw'ra?",
        "Ͻmaa Paulo koma dii yaw,",
        "De Nyame ahoɔden hyerɛn."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Honhomɔa efi tete b're so,",
        "Kaa Wo dɔ no ky'rɛky'rɛ W'akwan,",
        "Ͻhy'rɛn Yesaia nwoma mu,",
        "Na Dawid nso de too ne nnwom."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ͻnnɛ de Wo dom no nyɛ den,",
        "Sɛ dá a Elia tee ne tum' no?",
        "B're a Mose anim hyerɛn,",
        "Na Hiob dii sɔhwɛ so nim no?"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Yehowa, kae tete b're so,",
        "Nyan W'adwuma ne Wo dom no;",
        "Yɛde yɛn koma remaW' yi,",
        "Hwie Honhom Kronkron gu yɛn so."
      ],
      "number": 5
    }
  ]
}