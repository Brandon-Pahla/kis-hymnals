{
  "number": 506,
  "language": "tw",
  "title": "Yɛn Nyame Ne Abankɛse",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Yɛn Nyame ne abankɛse",
        "Kyɛm a enni huammɔ da;",
        "Yɛn boafo a yɛwɔ ne No,",
        "Adasamma nsuy'ri mu,",
        "Yɛn tamfo dedaw no",
        "Hwehwɛ mmusu ara,",
        "N'adwen ne tumi so,",
        "N'abufuw ne nitan,",
        "Obi nsɛ no 'sase so."
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Yɛn ankasa 'hoɔden mu no,",
        "Yɛn ap'redi bɛyɛ kwa,",
        "Barima pa bi wɔ y'afa",
        "Onyame na ɔpaw No;",
        "Wubisa Onniko",
        "ɔne Kristo Yesu,",
        "Aw'rade Sabaot,",
        "Mfesanten: ɔsankra;",
        "Na etwa sɛ Odi nim."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Ahonhom fi hyɛ wiase ma",
        "Repɛ yɛn amene a",
        "Yɛrensuro, Nyame ama",
        "N'asɛm nam yɛn so di nim;",
        "Sum hen' no fa 'bufuw,",
        "Yɛn ho rempo N'anim;",
        "Yebegyina pintinn,",
        "'Fisɛ ne sɛe reba;",
        "Asɛm k'ro beyi n'ahwe."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Asɛm a ɛtra 'sase so",
        "Tum' biara no tim hɔ daa;",
        "Honhom ne 'kyɛde no wɔ yɛn,",
        "Nea ɔwɔ y'afa no mu.",
        "Ahode, abusua,",
        "Ne nkwa tiawa yi nkɔ;",
        "Wokum yɛn honam a,",
        "Nyamč asɛm tim hɔ daa,",
        "N'ahenni tim hɔ daapem."
      ],
      "number": 4
    }
  ]
}