{
  "number": 219,
  "language": "tw",
  "title": "Yesu Ba 'Nuonyam Mu Sɛ",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Yesu ba 'nuonyam mu sɛ",
        "'W'ra ne ahen mu Hen',",
        "O hwɛ anwanwasɛm a",
        "Kyerɛw Kronkron no ka;",
        "N'anim bɛhy'rɛn sɛ owia",
        "Ne t'ri so yɛ fitaa;",
        "N'ani hy'rɛn sɛ 'gyaframa,",
        "Ne nan te sɛ kɔɔb're."
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ne nne sɛ nsubɔnten no",
        "Begyigye dennennen,",
        "Akodu ɛpo bun mu",
        "Aky'rɛ abɔde ny'naa;",
        "Saa nteam nwanwa yi mu no,",
        "Kristom 'wufo benyan",
        "Akohyia wɔn Agyenkwa",
        "Wɔ wim, anuonyam mu."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Na yɛn a yɛwɔ gyidi",
        "N'ahoyi dɔ mu no",
        "Behu sɛ yɛanya Ne s'ro",
        "Anuonyam pa no bi;",
        "Ne wusɔre tumi no",
        "Benyan yɛn akɔ nea",
        "Saa dɔnhw're nwanwa no mu",
        "Yebehu N'anim no."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "O ma Wo mmae no nyɛ ntɛm,",
        "Wo Adekyee Ns'romma!",
        "Awurade, ma yɛnte",
        "T'robɛnto nne aky'ri;",
        "Wo nk'rɔfo hwɛ Wo kwan sɛ",
        "Yɛbɛyɛ W'ayef'ro;",
        "Na brč a Wobɛba bio no,",
        "Yɛaf'ro aba Wo nkyɛn."
      ],
      "number": 4
    }
  ]
}