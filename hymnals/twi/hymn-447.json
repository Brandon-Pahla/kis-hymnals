{
  "number": 447,
  "language": "tw",
  "title": "Fi Tete Na Wɔabɔ Nguankuw",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Fi tete na wɔabɔ nguankuw",
        "Apete mmepɔ 'tifi,",
        "Akwan no mu yɛ yaw ne sum;",
        "Wɔafa nsɔhwɛ huhu mu;",
        "Seesei frɛ no gu so ara",
        "rema kɔkɔbɔ nsɛm no;",
        "Nkabom, gyidi no ɔdɔ ma",
        "Kuw ketewa no di dɛw."
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Wɔhwehwɛ nokware hann a",
        "Ͻnam ne kwan so seesei;",
        "Wodi mm'ra nsɛm du no ny'naa so,",
        "Wɔyɛ kronkron ne kokwar';",
        "Wɔde nkwasɛm no yɛ aduan,",
        "Ͻsom wɔn bo yɛ wɔn dɛ;",
        "Wotie wɔn W'ra ahyɛde ny'naa,",
        "Koto Ne nan ase brɛoo."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Hann mu hann, kųro a ɛyɛ fɛ,",
        "Saa sika kųro kronkron no;",
        "Ͻ'nkyɛ, wɔbɛhyɛn n'apon mu,",
        "Akɔfa n'anuonyam no;",
        "ɔkra no nya bɔne, owu,",
        "Ne yaw mu 'hofadi hɔ;",
        "Wobehu adan pa no yie,",
        "Nea 'hotefo di hen' daa."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "ɔnam mununkum mu resian,",
        "N'ahotefo sɔr' fi 'wum;",
        "Wɔn a wɔagye wɔn no nkonim",
        "Ho dwom gyigye wɔ ewim;",
        "Yɛn ani gyina W'ahoyi,",
        "Bra, O Agyenkwa, bra ntɛm!",
        "'Nidaso pā ɛhyɛ yɛn den,",
        "Bɛfa Wo mma no kɔ fie."
      ],
      "number": 4
    }
  ]
}