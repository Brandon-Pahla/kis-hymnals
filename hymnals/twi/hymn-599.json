{
  "number": 599,
  "language": "tw",
  "title": "Munni 'H'rusi, 'Gyidifo",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Munni 'h'rusi, 'gyidifo",
        "Momfa mo akanea.",
        "Adesae akɔ anim",
        "Na 'sum kabii rebɛn;",
        "'Yef'rokun' no resɔre",
        "Ͻrenkyɛ Obepue",
        "Monsɔr', monwɛn, mommɔ mpae",
        "'Frɛ no bɛba 'dasum."
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Bepɔ so 'wɛmfo, mompaem",
        "Sɛ 'yef'rokun' abɛn.",
        "Munkohyia No wɔ kwan mu",
        "Monto Haleluya;",
        "Aponto no ad'ru so",
        "Wɔabuebue apon no;",
        "Monsɔr' mo anuonyam mma,",
        "'Yef'rokun' no abɛn."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "'Hotefo a boasetɔm'",
        "Moasoa mo 'sɛnnua ha",
        "Bɛtrā ase  adi hen' daa",
        "Brč a 'w'rɛhow nni hɔ bio.",
        "'Nuonyam ahengua no so,",
        "Mubehu' Guammaan' de",
        "Mo sika' henkyɛw agu",
        "N'anim nkonimdi mu."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Yesu, yɛn 'nidaso ne",
        "Hwehwɛde, bra seesei;",
        "'Wia a yɛatwɛn Wo akyɛ",
        "Pue sum wiase yi so!",
        "Yɛma koma ne nsa so",
        "Srɛ Wo sɛ, ma yenhu",
        "Dā a 'sase benya nkwagyč a",
        "Ͻde yɛn ba Wo nkyɛn."
      ],
      "number": 4
    }
  ]
}