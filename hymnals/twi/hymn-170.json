{
  "number": 170,
  "language": "tw",
  "title": "Mommra, 'Gyidifo, Monto",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Mommra, 'gyidifo, monto",
        "Nkonim anigye dwom,",
        "Nyame de Israel afi",
        "W'rɛhowm' aba dɛw mu;",
        "Yakob mma afi Farao",
        "Kɔndua d'rud'ru n'ase",
        "Afa 'sase kesee so,",
        "Ͻpo Kɔkɔɔ no mu."
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Akra 'h'rusi da ne nnɛ,",
        "Kristo apue afi",
        "Nnansa wu mu asɔre",
        "Sɛnea owia repue.",
        "Yɛn bɔne horow nyinaa",
        "Mu tumi retu y'ra",
        "Ne hann anim,",
        "Nea ɔsɛ Nnaase ne nkamfo daa."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Mmere mu b're kunini",
        "De anuonyam da hann",
        "Ne afahyɛ mu 'fahyɛ,",
        "Ba bɛky'rɛ n'anigye,",
        "De dɛw brɛ agyidifo",
        "A wɔde nokwar' dɔ",
        "To dwom a egyigye yie",
        "Yesu wusɔre ho."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ͻnnɛ asuafo no mu,",
        "Kristo puei b'rč a ɔde",
        "N'asomdwoe pa a ɛtra",
        "Nimdeɛ ny'naa so no mae.",
        "Owu apono anaa",
        "Damoa mu sum kabii,",
        "Awɛmfo anaa ɔbɔ",
        "Antum' ankora No."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "'Haleluya!' yɛteam ky'rɛ",
        "Yɛn Hen' a Onwu da;",
        "Nea nkonim mu",
        "Obubuu Asaman apon' no.",
        "'Haleluya!' yɛkamfo Agya ne ɔba no,",
        "'Haleluya!' yɛto bio",
        "Ma Honhom Kronkron no."
      ],
      "number": 5
    }
  ]
}