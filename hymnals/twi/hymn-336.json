{
  "number": 336,
  "language": "tw",
  "title": "Bura Bi Wɔ Hɔ Mogya Ma",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Bura bi wɔ hɔ mogya ma",
        "Efi Immanuel mfem';",
        "Nnebɔn'yɛfo bɛtɔ mu a,",
        "Wɔn ho fi nyinaa pa."
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Owifo no, ɔde dɛw huu",
        "'Bura no ne b're so;",
        "Hɔ na me ho fi te sɛ nō a,",
        "Mɛh'ro me bɔn' nyinaa."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Guammá a Worewu! Wo mogya",
        "Tumi no rempa da,",
        "Kosi sɛ Nyame mma benya",
        "Nkwa agyae bɔne yɛ."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Efi dá a mihuu nsu no a",
        "Efi N'akrum sen no,",
        "Nkwagye dɔ no n'ayɛ m'adwen",
        "N'ɛbɛyɛ m'adwen daa."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Wura, minim sɛ Woasiesie,",
        "Ewom, memfata de,",
        "Woasiesie mogyā atɔde pa,",
        "Sika sanku amam'!"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Mɛto Wo gye tum' no ho dwom",
        "Anuonyam dwom dɛdɛ.",
        "Dá a me tɛkrɛma pemee yi",
        "Benyan afi ɔdam'."
      ],
      "number": 6
    }
  ]
}