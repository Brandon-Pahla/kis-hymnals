{
  "number": 733,
  "language": "tw",
  "title": "N38 Sinners Jesus Will Receive",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Kristo bεgye bɔn'yɔfo!",
        "Ka ɔdom nsεm yi kyerε",
        "Wɔn a wɔafom s'ro kwan so,",
        "Nenam kwa, hwehwe ase."
