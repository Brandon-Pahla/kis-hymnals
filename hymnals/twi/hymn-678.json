{
  "number": 678,
  "language": "tw",
  "title": "Nyame Ntra Me T'rim'",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "Nyame ntra me t'rim', ɛne me",
        "ntease no mu;",
        "ɔtra m'aniwam' ne m'adehwɛ mu,",
        "Nyame ntra m'anom',",
        "ne me kasa mu;",
        "ɔntra ne komam ne",
        "m'adwene mu;",
        "Nyame ntra m'awiei ne me",
        "Ntetewmdi mu."
      ],
      "number": 1
    }
  ]
}