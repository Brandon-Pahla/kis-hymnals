{
  "number": 690,
  "language": "tw",
  "title": "'W'ra Yɛsrɛ Fa Wo Nhiyira Pɔn",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "'W'ra yɛsrɛ fa Wo nhiyira pɔn yɛn,",
        "B'rč a yefi Wo som mu rekɔ yi;",
        "Ky'rɛ yɛn kwan nnɛ ɔbra ko yi mu;",
        "Gye yɛn W'ahennin', 'yeyi nka Wo.",
        "Amen."
      ],
      "number": 1
    }
  ]
}