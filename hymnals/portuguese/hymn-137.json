{
  "number": 137,
  "language": "pt",
  "title": "Anunciai Pelas Montanhas",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Anunciai Pelas Montanhas"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Côro:",
        ":",
        "Anunciai pelas montanhas,",
        "E proclamai por terra e mar;",
        "Anunciai pelas montanhas",
        "Que Cristo vai voltar."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Minha'lma, com Cristo,",
        "Jamais desanimou,",
        "Porque Ele ajuda e guia,",
        "E a senda me mostrou."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Meu Mestre divino,",
        "Conduz pela mão;",
        "De pão sacia a alma,",
        "E escuta a oração."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "E nós, que O amamos,",
        "Devemos proclamar",
        "Que Cristo veio ao mundo,",
        "E a todos quer salvar."
      ],
      "number": 5
    }
  ]
}