{
  "number": 134,
  "language": "pt",
  "title": "Breve Jesus Voltará",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Breve Jesus Voltará"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Servos de Deus, a trombeta tocai:",
        "Breve Jesus voltará!",
        "A todo o mundo a mensagem levai:",
        "Breve Jesus voltará!"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Côro:",
        ":",
        "Breve virá! Breve virá!",
        "Breve Jesus voltará!"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ide as nações e depressa anunciai:",
        "Breve Jesus voltará!",
        "Gratos, alegres, as novas cantai:",
        "Breve Jesus voltará!"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Montes e vales, o som ecoai:",
        "Breve Jesus voltará!",
        "Ondas do mar a canção entoai:",
        "Breve Jesus voltará!"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Guerras e fome nos dão a entender:",
        "Breve Jesus voltará!",
        "Graves sinais já nos fazem saber:",
        "Breve Jesus voltará!"
      ],
      "number": 6
    }
  ]
}