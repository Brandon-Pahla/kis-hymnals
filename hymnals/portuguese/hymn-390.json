{
  "number": 390,
  "language": "pt",
  "title": "Olhando Para Cima",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Olhando Para Cima"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Ó, mantém teu olhar pra o Céu,",
        "Pra sentir o poder do amor;",
        "Eis que Deus responderá",
        "A oração do pecador",
        "Que buscar Sua mão."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Côro:",
        ":",
        "Ó, mantém teu olhar pra o Céu,",
        "Só Jesus dará poder.",
        "Sua paz hás de então sentir,",
        "E conforto receber."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "A Jesus deves confiar",
        "Teus pesares, teu prantear,",
        "E verás que lá do Céu",
        "Hás de forças receber",
        "Pra vencer todo mal."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Ao olhar para o Salvador,",
        "Nova vida vais receber,",
        "E em luz sempre andarás,",
        "E desejo hás de ter",
        "De servir a Jesus."
      ],
      "number": 5
    }
  ]
}