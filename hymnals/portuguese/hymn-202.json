{
  "number": 202,
  "language": "pt",
  "title": "Eis uma Fonte",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Eis uma Fonte"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Eis que uma fonte aberta está:",
        "O sangue de Jesus.",
        "Quem nela se lavar, à cruz,",
        "Sem manchas ficará.",
        "Sem manchas ficará,",
        "Sem manchas ficará;",
        "Quem nela se lavar, à cruz,",
        "Sem manchas ficará."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "O sangue do Cordeiro tem",
        "Poder pra libertar",
        "O pecador que a Cristo vem,",
        "Ao Salvador sem-par.",
        "Ao Salvador sem-par,",
        "Ao Salvador sem-par;",
        "O pecador que a Cristo vem,",
        "Ao Salvador sem-par."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Desde que aceitei a salvação,",
        "Por Cristo, meu Senhor,",
        "Tem sido a excelsa redenção",
        "Meu tema e meu vigor.",
        "Meu tema e meu vigor,",
        "Meu tema e meu vigor;",
        "Tem sido a excelsa redenção",
        "Meu tema e meu vigor."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "E quando à eterna glória eu for,",
        "Melhor eu cantarei;",
        "A Cristo, Rei e Salvador,",
        "Pra sempre exaltarei.",
        "Pra sempre exaltarei,",
        "Pra sempre exaltarei;",
        "A Cristo, Rei e Salvador",
        "Pra sempre exaltarei."
      ],
      "number": 5
    }
  ]
}