{
  "number": 182,
  "language": "pt",
  "title": "Cristo te Chama",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Cristo te Chama"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Cristo te chama com mui terno amor;",
        "Ó pecador, vem atender!",
        "DEle não fujas com fútil temor;",
        "Vem a Jesus te render!"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Côro:",
        ":",
        "Ó pecador eis o Senhor!",
        "Vem e atende com fé",
        "A chamada de amor."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Cristo te chama pra vir descansar;",
        "Ó pecador, vem atender!",
        "Teu grande peso te quer minorar;",
        "Vem a Jesus te render!"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Cristo deseja, pois, te perdoar;",
        "Ó pecador, vem atender!",
        "Tudo Ele fez para te resgatar;",
        "Vem a Jesus te render!"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Cristo de novo Se põe a chamar;",
        "Ó pecador, vem atender!",
        "Corre depressa, sim, vem te entregar;",
        "Nada te deve deter!"
      ],
      "number": 6
    }
  ]
}