{
  "number": 394,
  "language": "pt",
  "title": "De Ti Careço, ó Deus",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### De Ti Careço, ó Deus"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "De Ti careço, ó Deus,",
        "Habita, pois, em mim,",
        "E vencedor serei",
        "Fiel até o fim."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Côro:",
        ":",
        "Jesus, meu Salvador,",
        "Sê comigo aqui,",
        "Bendize-me agora;",
        "Eu creio em Ti."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "De Ti careço, ó Deus,",
        "De Ti, meu Salvador.",
        "Ó, vem fazer de mim,",
        "Humilde servidor."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Em Tua santa lei,",
        "Senhor, vem-me instruir;",
        "E Teu tão grande amor,",
        "Almejo então sentir."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Meu grande Protetor",
        "És Tu, na provação;",
        "Concedes-me vigor",
        "E eterna salvação."
      ],
      "number": 6
    }
  ]
}