{
  "number": 72,
  "language": "pt",
  "title": "Saudai o Nome de Jesus (2)",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Saudai o Nome de Jesus (2)"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Saudai o nome de Jesus;",
        "Ó anjos, vos prostrai.",
        "O Filho do glorioso Deus,",
        "Com glória coroai!",
        "O Filho do glorioso Deus,",
        "Com glória coroai!"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Ó escolhida geraçăo",
        "Do bom e eterno Pai,",
        "O grande Autor da salvaçăo,",
        "Com glória coroai!",
        "O grande Autor da salvaçăo,",
        "Com glória coroai!"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ó perdoados por Jesus,",
        "Alegres adorai;",
        "O Deus de paz, o Deus de luz,",
        "Com glória coroai!",
        "O Deus de paz, o Deus de luz,",
        "Com glória coroai!"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Ó raças, tribos e naçőes,",
        "Ao Rei divino honrai;",
        "A quem quebrou os vis grilhőes,",
        "Com glória coroai!",
        "A quem quebrou os vis grilhőes,",
        "Com glória coroai!"
      ],
      "number": 5
    }
  ]
}