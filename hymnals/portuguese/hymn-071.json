  "title": "Saudai o Nome de Jesus (1)",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### Saudai o Nome de Jesus (1)"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Saudai o nome de Jesus;",
        "Ó anjos, vos prostrai,",
        "Ó anjos, vos prostrai.",
        "O Filho do glorioso Deus,"
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Côro:",
        ":",
        "Com glória, glória, glória,",
        "Glória, com glória coroai!"
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ó escolhida geraçăo",
        "Do bom e eterno Pai,",
        "Do bom e eterno Pai,",
        "O grande Autor da salvaçăo,"
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Ó perdoados por Jesus,",
        "Alegres adorai,",
        "Alegres adorai;",
        "O Deus de paz, o Deus de luz,"
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "0 raças, tribos e naçôes,",
        "Ao Rei divino honrai,",
        "Ao Rei divino honrai!;",
        "A quem quebrou os vis grilhőes,"
      ],
      "number": 6
    }
  ]
}