{
  "number": 2,
  "language": "rn",
  "title": "  2 Umwami Ageze Kw'Irembo",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "### 2 Umwami Ageze Kw'Irembo"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Umwam'ageze kw'irembo,",
        "Yabambiw'abanyayaha Aje gukorany'abera,",
        "bajane mw'ijuru."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Icitabirizo:",
        "Araje, araje, araje, ub'ari",
        "kw'irembo Ari hafi, ari hafi,",
        "ub'ashitse kw'irembo."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ivyerekana kw'azoza,",
        "ntibisiba kuboneka,",
        "Kandi tugira duhabwe,",
        "kubaho tudapfa."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Ntah'indwano zizohera,",
        "amahoro ntazohora.",
        "Kirets'ivyaha n'urupfu,",
        "Vyatsinzwe na Yesu."
      ],
      "number": 5
    },
    {
      "type": "verse",
      "lines": [
        "Maze mw'isi yagizwe nsha,",
        "tuzobah'ibihe vyose,",
        "Kudapfa kwakuye gupfa.",
        "Dushiz'agahinda."
      ],
      "number": 6
    }
  ]
}