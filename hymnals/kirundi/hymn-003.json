{
  "number": 3,
  "language": "rn",
  "title": "  3 Nzobona Yesu Umukiza",
  "stanzas": [
    {
      "type": "verse",
      "lines": [
        "###3 Nzobona Yesu Umukiza"
      ],
      "number": 1
    },
    {
      "type": "verse",
      "lines": [
        "Nzobona Yes’Umukiza,",
        "Mu nyonga ziw’icese,",
        "Nzohimbarwa ndamubonye,",
        "Kw’ari We yampfiriye."
      ],
      "number": 2
    },
    {
      "type": "verse",
      "lines": [
        "Icitabirizo:",
        "Nzobona mu nyonga hiwe,",
        "Turi kumwe mw’ijuru,",
        "Nzobona Yes'Umukiza,",
        "n'Icubahiro ciwe."
      ],
      "number": 3
    },
    {
      "type": "verse",
      "lines": [
        "Ndamwiyumvira mu bwenge,",
        "ariko simubona,",
        "Umus'uhiriw'uzoza,",
        "ndamubonan ubwiza."
      ],
      "number": 4
    },
    {
      "type": "verse",
      "lines": [
        "Nzonezerwa ndamubonye,",
        "Aya marushwa aheze,",
        "Hagorow’ibihetamye,",
        "Umuc’utamanzuye.",
        "Nzohimbarwa ndamubonye,",
        "Nzomubona mumenye.",
        "Nzobon’Umukiza wanje",
        "Yesu Kristo yankunze."
      ],
      "number": 5
    }
  ]
}